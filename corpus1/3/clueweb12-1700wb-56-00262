      for(var i=0; i < google_ads.length; i++) {
        var domain_split = google_ads[i].visible_url.split('/');
        var root_domain = domain_split[0];
        s += '<br><a class="hgi_clicktrack ' + ad_title_class + '" href="' + google_ads[i].url + '" target="_blank">' +
          google_ads[i].line1 + '</a><br><a class="hgi_clicktrack ' + ad_text_class + '" href="' + google_ads[i].url +  '" target="_blank">' +
          google_ads[i].line2 + ' ' + google_ads[i].line3 + '</a><br>' +
          '<a class="hgi_clicktrack ' + ad_url_class + '" href="' + google_ads[i].url + '" target="_blank">' +
          root_domain + '</a><br>';
      }
    }
    document.write(s);
    return;
  }

// -->
</script>
