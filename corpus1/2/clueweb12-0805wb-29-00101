      } );
    } );

  


    // This function determines which navigational menu items should be shown
    // and selected on this page based on the article's information.
    function setUpNavMenu ( )
    {
      // Show or hide our menus based on what page we're on.
      var ArticleId       = 8331;
      var Tier1ArticleId  = 49;
      var Tier2ArticleId  = 0;
      var Tier3ArticleId  = 0;

      if ( Tier3ArticleId > 0 )
      {
        // Show the submenu containing this article
        $( '#' + Tier3ArticleId + " > ul" ).show( );

        // ...and show the parent menu of that menu.
        $( '#' + Tier2ArticleId + " > ul" ).show( );

        // Make the parent menu blue
        $( '#' + Tier2ArticleId ).addClass( 'menuItemActive' );

        // Put our arrow on this item, or on the menu which should hold this
        // sub-item if it's not part of the menu.
        if ( $( '#' + ArticleId ).get( ).length > 0 )
        {
          $( '#' + ArticleId ).addClass( 'subMenuItemSelected' );
        }
        else
        {
          $( '#' + Tier3ArticleId ).addClass( 'subMenuItemSelected' );
        }
      }
      else if ( Tier2ArticleId > 0 )
      {
        // Show the submenu containing this article
        $( '#' + Tier2ArticleId + " > ul" ).show( );

        // If this article has a submenu, show it, too.
        $( '#' + ArticleId + " > ul" ).show( );

        // Make the parent menu blue
        $( '#' + Tier2ArticleId ).addClass( 'menuItemActive' );

        // Put our arrow on this item
        if ( $( '#' + ArticleId ).get( ).length > 0 )
        {
          $( '#' + ArticleId ).addClass( 'subMenuItemSelected' );
        }
        else
        {
          $( '#' + Tier2ArticleId ).addClass( 'menuItemSelected' );
        }
      }
      else
      {
        // Put our arrow on this item
        // $( '#' + ArticleId ).addClass( 'menuItemSelected' );
        if ( $( '#' + ArticleId ).get( ).length > 0 )
        {
          $( '#' + ArticleId ).addClass( 'menuItemSelected' );
        }
        else
        {
          $( '#' + Tier1ArticleId ).addClass( 'menuItemSelected' );
        }

        // Show the main sub-menu for this item
        $( '#' + ArticleId + ' ul.menu_tier2' ).show( );
      }
    }
  </script>




