			$(document).ready(function() {

				(function() {
					
					$('#current-incentives').show();

					var ngIntegration = NextGenIntegration(),
						department = 'New';

					if (department !== 'New') {
					
						nextGenVDP.hideIncentiveDom();
					
					} else {

						CM.subscribe(ngIntegration.events.incentivePublished, function(incentives) {
							nextGenVDP.applyIncentivesToDOM(incentives);
						});

						ngIntegration.getIncentivesByVIN({
							vinList			: ['2FMDK3K95CBA69547'],
							paCode 			:  '05246',
							dealerZipCode 	:  '74133'
						});
					}

				}());
			});	
		</script>
