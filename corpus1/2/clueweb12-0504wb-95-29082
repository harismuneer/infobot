function marks(id,type) {

	if (!document.images) return false;
	for (i=1;i<=id;i++) {
	    if (type=="show") {
			document.images["mark"+i].src=mark_show.src;
			document.images["ratex"].src=ylw_show.src;
		}
	    else if (type=="hide") {
			document.images["mark"+i].src=mark_hide.src;
			document.images["ratex"].src=ylw_hide.src;
		}
	}
	
}

function switchimg(type) {
	
	if (!document.images) return false;
	if (type=="show") {
		
		document.images["v2released"].src=v2_show.src;
		
	}
	else if (type="hide") {
		
		document.images["v2released"].src=v2_hide.src;
		
	}

}

