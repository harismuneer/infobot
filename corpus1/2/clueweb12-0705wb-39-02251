   PAGESETUP.addControl(function() {
 		switch (EDMUNDS.Util.getQueryString('actionPerform')) {
 			case 'account-activation':
 				PAGESETUP.scope.modalPanel.launch({"url":" http://www.edmunds.com/era/lb/accountActivation.jsp?email=" + EDMUNDS.Util.getQueryString('email') + "&msg=" + EDMUNDS.Util.getQueryString('msg')},"Account Activation", [550,365]);
 				break;
 			case 'reset-password':
 				PAGESETUP.scope.modalPanel.launch({"url":" http://www.edmunds.com/era/secure/lb/resetPassword.jsp?email=" + EDMUNDS.Util.getQueryString('email') + "&msg=" + EDMUNDS.Util.getQueryString('msg')},"Reset Password", [550,350]);
