<script type="text/javascript">

    $(function () {

        $("#trimId").html("<option value='' />");

        //        $.getJSON("/CarReviews/GetSeriesByMakeModelYearJson?make=Ford&model=Edge&year=2013", null, function (data) {
        //            data = $.map(data, function (item, a) {
        //                return "<option value=" + item.Id + ">" + item.Name + "</option>";
        //            });
        //            $("#trimId").html(data.join(""));
        //        });

        $.getJSON("/api/car-reviews/get-series-by-make-model-year-json/?make=ford&model=edge&year=2013", null, function (data) {
            $.each(data, function (id, option) {
                $("#trimId").append($('<option></option>').val(option.Id).html(option.Name));
            });
        });

    });
</script>

<script type="text/javascript">


    function submitReviewForm() {

        var error = '';

        actionTarget = "/CarReviews/CarRatings/";

        if ($('#make').val().length >= 1) {
            $('#vehiclemake').val($('#make option:selected').text());
            actionTarget = actionTarget + $('#make option:selected').text() + "/";
        };

        if ($('#model').val().length >= 1) {
            actionTarget = actionTarget + $('#model').val() + "/";
        } else {
            if ($('#make').val().length >= 1) {
                actionTarget = actionTarget + "all" + "/";
                error = '* At least Make and Model are required.'
            }
        };

        if ($('#year').val().length >= 1) {
            actionTarget = actionTarget + $('#year').val() + "/";
        } else {
            if ($('#model').val().length >= 1) {
                actionTarget = actionTarget + "all" + "/";
            }
        };

        document.forms[0].action = actionTarget;

        if (error.length == 0) {
            document.forms[0].submit();
        } else {
            $('#errMsgMakeModel').css('display', 'block');
        }


    }

    

</script>
