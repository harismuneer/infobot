-55c","248":"Drum+Set","249":"Orchestral+Percussion","10":"Piano","250":"Acoustic+or+Electronic+Piano","274":"AvantGrand+N2","277":"AvantGrand+N3","285":"CP300","314":"CP33","317":"KBP-300","318":"KBP-500","259":"MODUS+F01","184":"MODUS+F11","261":"MODUS+H01","185":"MODUS+H11","273":"MODUS+R01","272":"P-155","320":"P-200","207":"P-250","321":"P-65","322":"P-80","218":"P-85","323":"P-90","57":"PF-1000","197":"PF-500","258":"YDP-140","296":"YDP-141","251":"YDP-160","297":"YDP-161","298":"YDP-181","319":"YDP-S30","283":"YDP-S31","294":"YDP-V240","11":"Band+Orchestra","9":"Electone","292":"ELB-01","213":"ELS-01","214":"ELS-01C","215":"ELS-01X"};
var groupUrl	= "http:\/\/www.yamahamusicsoft.com\/en\/instrument\/{group}\/category\/DVD+Video\/product\/1024535?currency=USD";
var modelUrl	= "http:\/\/www.yamahamusicsoft.com\/en\/instrument\/{group}\/{model}\/category\/DVD+Video\/product\/1024535?currency=USD";

function onGroupChange(groupId)
{
	var $instrumentModel	= jQuery('#insModel');
	var $form				= jQuery('#instrumentSelector');

	initModelDropdown(groupId, '-1');
	httpHelper.setCookie('cookie-instrument', groupId);

	if (groupId > 0)
		$form.attr('action', groupUrl.replace(/\{group\}/i, names[groupId]));
	else
		$form.attr('action', '');

	if ($instrumentModel.children('option').length == 1 && groupId > 0)
	{
        if(jQuery('#form_data_format').length)
			$form.attr('action', $form.attr('action') + '&data_format=' + jQuery('#form_data_format').val());

		window.location = $form.attr('action');
	}
}

function onModelChange(groupId, modelId)
{
	httpHelper.setCookie('cookie-instrument', modelId);

	var $form				= jQuery('#instrumentSelector');
	var modelName			= (modelId > 0) ? names[modelId] : '';

	$form.attr('action', modelUrl.replace(/\{group\}/i, names[groupId]).replace(/\{model\}/i, modelName));

	// specify the source of action
	if ($form.attr('action').indexOf('model_selector') == -1)
		if ($form.attr('action').indexOf('?') == -1)
			$form.attr('action', $form.attr('action') + '?model_selector=1');
		else
			$form.attr('action', $form.attr('action') + '&model_selector=1');

	if (modelId > 0)
	{
		if(jQuery('#form_data_format').length)
		{
            if ($form.attr('action').indexOf('?') == -1)
				$form.attr('action', $form.attr('action') + '?');
			else
            	$form.attr('action', $form.attr('action') + '&');

			$form.attr('action', $form.attr('action') + 'data_format=' + jQuery('#form_data_format').val());
		}

		window.location = $form.attr('action');
	}
}

function gaInstrumentTrack(instrumentName)
{
			if (pageTracker && instrumentName) {
		pageTracker._trackPageview('/selectInstrumentModel/' + instrumentName + '/');
	}
	}

function initModelDropdown(groupId, modelId)
{
	var $instrumentModel	= jQuery('#insModel');
	var $instrumentGroup	= jQuery('#insGroup');
	var $groupSelected		= jQuery($instrumentGroup).children("option:selected");

	$instrumentModel.children('option').remove();

	htmlHelper.addOptions($instrumentModel[0], jQuery.extend({'-1'
		: 'Select Your '
		  +(($groupSelected.val() > 0) ? $groupSelected.text() : 'Instrument')
		  +' Model'
		}, jQuery(modelData[groupId]).length ? modelData[groupId] : {}));
	htmlHelper.selectOption($instrumentModel[0], modelId);

	if (groupId <= 0)
		$instrumentModel.attr('disabled', 'disabled').attr('class', 'disabled');
	else
		$instrumentModel.removeAttr('disabled').attr('class', '');
}

function submit_instrument_selector()
{
	var insModel = 0;
	var insGroup = 0;

	var inputs = document.getElementsByTagName('select');

	for(var i = 0; i < inputs.length; i++)
	{
    	if(inputs[i].name == 'insModel')
        	insModel = inputs[i].value;
    	if(inputs[i].name == 'insGroup')
        	insGroup = inputs[i].value;
	}

	if(insModel > 0 && insGroup > 0)
    	onModelChange(insGroup, insModel);
	else if(insGroup > 0)
	{
		onGroupChange(insGroup)
		var $form = jQuery('#instrumentSelector');

		if(jQuery('#form_data_format').length)
		{
            if ($form.attr('action').indexOf('?') == -1)
				$form.attr('action', $form.attr('action') + '?');
			else
				$form.attr('action', $form.attr('action') + '&');

			$form.attr('action', $form.attr('action') + 'data_format=' + jQuery('#form_data_format').val());
		}
		window.location = $form.attr('action');
	}
}

function validateSelector()
{
	return false;
}


