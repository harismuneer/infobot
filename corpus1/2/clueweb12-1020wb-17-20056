            selector = "#tabHome";
        }
        else if (localPathname.indexOf("/Audio") >= 0 || localPathname.indexOf("/Movie-Reviews") >= 0) {
            selector = "#tabMovieReviews";
        }
        else if (localPathname.indexOf("/Podcasts") >= 0) {
            selector = "#tabPodcasts";
