<script type="text/javascript">
function getContent(url, target) {
  if (window.XMLHttpRequest) {
    req = new XMLHttpRequest();
  } else if (window.ActiveXObject) {
    req = new ActiveXObject("Microsoft.XMLHTTP");
  }
  if (req != undefined) {
    req.onreadystatechange = function() {loadContent(url, target);};
    req.open("GET", url, true);
    req.send("");
  }
}  
function loadContent(url, target) {
  if (req.readyState == 4) { // only if req is "loaded"
    if (req.status == 200) { // only if "OK"
      document.getElementById(target).innerHTML = req.responseText;
    } else {
    }
  }
} 
function makeGo(){
	var makeIndex = document.getElementById('makesCarSearchWidget').selectedIndex;
	var makeName = document.getElementById('makesCarSearchWidget').options[makeIndex].text;
	if (makeIndex != 0){
		_gaq.push(['_trackEvent', 'Reviews and Editorial', makeName]);
		window.location.href = "http://www.autoweek.com/section/" + makeName +"&template=newcars";
	}
}
function modelGo(){
	var makeIndex = document.getElementById('makesCarSearchWidget').selectedIndex;
	var modelIndex = document.getElementById('modelsCarSearchWidget').selectedIndex;
	var makeName = document.getElementById('makesCarSearchWidget').options[makeIndex].text;
	var modelName = document.getElementById('modelsCarSearchWidget').options[modelIndex].text;
	if (makeIndex != 0 && modelIndex != 0){
		_gaq.push(['_trackEvent', 'Reviews and Editorial', makeName, modelName]);
		window.location.href = "http://www.autoweek.com/section/" + modelName + "&template=newcar&make=" + makeName;
	}
}
	$('#makesCarSearchWidget').change(function () {
		if($("#makesCarSearchWidget option:selected").val()!="default")
		{
			getContent('/section/mmDropdown2Home?makeID='+document.getElementById('makesCarSearchWidget').value,'modelsCarSearchWidgetSpan');
		}
	});
</script>
				</form>
			</div>
		</div>

