<script type="text/javascript">	
//<![CDATA[
function validateTrialOrder(remoteURL,productId){
	var isNonMinOrder = 1;
	var url = remoteURL + '/trailorder/ajax.html?sourcingProductId=' + productId + '&from=minisite_detail';
	var callback ={
		success:function(o){
			var JSON = YAHOO.lang.JSON.parse(o.responseText.replace(/&quot;/g,"\"").replace(/&amp;/g,"&"));
			if(o.responseText != "" && JSON["status"] =="success"){
				clk('TOexpo_m');
				if(isNonMinOrder == 1){
					get("trial-order-zone").style.display = "";
					get("trial-order-link").href = JSON["orderURL"];
				}else{
					get("non-minOrder").style.display = "";
					get("trial-order-zone-2").style.display = "";
					get("trial-order-link-2").href = JSON["orderURL"];
				}
			}
		},
		failure:function(o){},
		timeout:5000
	}
	setTimeout("", 100);
	(new AE.run.ajaxProxy()).asyncRequest("GET", url, callback,"",{proxy:remoteURL + "/ajax_proxy.html",domain : "alibaba.com"});
	if(isNonMinOrder){
		YUE.on("trial-order-help", "mouseover", function(){
			get("trial-order-tip").style.display = "";
		});
		YUE.on("trial-order-help", "mouseout", function(){
			get("trial-order-tip").style.display = "none";
		});
	}else{
		YUE.on("trial-order-help-2", "mouseover", function(){
			get("trial-order-tip-2").style.display = "";
		});
		YUE.on("trial-order-help-2", "mouseout", function(){
			get("trial-order-tip-2").style.display = "none";
		});
	}
};



//]]>
</script>
<script type="text/javascript">
//<![CDATA[
var disableEv = new AE.tool.disableEvent;
disableEv.init({ targetTag : ['img'], disableEvent : ['contextmenu', 'selectstart', 'copy', 'mousedown' ] });
var disableEv2 = new AE.tool.disableEvent;
disableEv2.init({ targetTag : ['div'], targetSubTag : ['img'], disableEvent : ['contextmenu', 'selectstart', 'copy', 'mousedown' ] });

YUE.on(window,'load',function(){
		if(document.images){
				var random = (new Date()).getTime();
				new Image().src="http://tracelog.www.alibaba.com/null.gif?refresh=test&t=" + random;
		}
})

YUE.onDOMReady(function(){
	var adjustImgSize = function(el,maxWidth){
		var oriWidth,oriHeight,attrWidth,attrHeight;
		if((oriWidth = el.getAttribute('ori-width')) && (oriHeight = el.getAttribute('ori-height'))){
			if(oriWidth > maxWidth){
				el.width = maxWidth;
				el.height = maxWidth*oriHeight/oriWidth;
			}else{
				el.width = oriWidth;
				el.height = oriHeight;
			}
		}else{
			attrWidth = el.width;
			attrHeight = el.height;
			el.width = maxWidth;
			el.height = maxWidth*attrHeight/attrWidth;
		}
	};
	
	var richBox = get('richTextContainer'), imgList = richBox.getElementsByTagName('img'), imgItem, oriWidth, oriHeight, maxWidth, regionContainer,attrWidth;
	regionContainer = YUD.getRegion('richTextContainer');
	maxWidth = regionContainer.right - regionContainer.left - 20;
	for (var i = 0, j = imgList.length; i < j; i++) {
		imgItem = imgList[i];
		attrWidth = imgItem.width;
		if(attrWidth == 0){
			YUE.on(imgItem,'load',function(){
				if(imgItem.width > maxWidth){
					adjustImgSize(imgItem,maxWidth);
				}
				YUE.removeListener(imgItem,'load');
			},this,true);
		}else{
			if(attrWidth > maxWidth){
				adjustImgSize(imgItem,maxWidth);
			}
		}
	}
	
		validateTrialOrder("http://wholesale.alibaba.com","215423732");
});
//]]>
</script>



