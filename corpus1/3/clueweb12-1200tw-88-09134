<script type="text/javascript">
  jQuery(function($) {
    var request;

    $('.artist_backpage_songs_container').delegate('.artist_backpage_songs_row', 'click', function(e){

      var $tag = $(e.target.tagName), $self = $(this), $song_container = $('.song_spotlight_container'), $play_btn = $self.find("a[data-song-id]")

      if(!$tag.is("li,span,div,.standard_play_button") || $(e.target).closest(".btn_group").length){
        return;
      }

      if($tag.is("li") && $play_btn.length > 0){
        playSongNow( $play_btn.attr("data-song-id") )
      }

      request = $.ajax({
        url: $self.attr('data-url'),
        type: 'get',
        success: function(html) {
          $song_container.html(html);
          $song_container.effect('highlight');
          $('.artist_backpage_songs_row.active').removeClass('active');
          $self.addClass('active');
        },
        beforeSend: function() {
          if ( request != null ) {
            request.abort();
          }
          $self.addClass('loading');
        },
        complete: function() {
          $self.removeClass('loading');
        }
      });
    }).delegate(".btn_group .btn_toggle", "click", function(e){

        var $parent = $(this).closest(".btn_group")

        if(!$parent.hasClass("expanded")){
          $parent.addClass("expanded")
          $parent.find("ul:first").alignTo($parent,"tr-tl",{left:5})

          window.setTimeout(function(){
            $(document.body).bind("click", function handler(e){
              var $target = $(e.target)
              if($target.is(".btn_toggle") && ($target.parents(".btn_group")).toArray().indexOf($parent[0]) != -1 ){
                //
              }else{
                $(this).unbind("click", handler)
                $parent.removeClass("expanded")
              }

            })
          },0)

        }else{
          $parent.removeClass("expanded")
        }


      })

  });
</script>
