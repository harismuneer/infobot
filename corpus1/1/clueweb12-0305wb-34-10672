		if( paymentCalculatorPane != null ) {
			// Initialize
			paymentCalculatorPane.startup();
			dojo.connect( inventorySearchResults, "onTriggerPrice", function( price ) {
				if( !(price < 0.01) ) {
					paymentCalculatorPane.setVehiclePrice( price );
					paymentCalculatorPane.calculate( true );
				}
			});
		}

		// Create Disclaimer Pane
