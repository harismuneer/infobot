function URLencode(sStr) 
{
   return escape(sStr).replace(/\+/g, '%2C').replace(/\"/g,'%22').replace(/\'/g, '%27').replace(/\//g, '%2F').replace(/\./g, '%2E');
}

function goLtr(ltr)
{
	var goLtrURL = "http://www.lyrics.net/artists/" + ltr +"/1";
	
	if (ltr == '$')
		goLtrURL = "http://www.lyrics.net/new-lyrics.aspx";
	
	location.href = goLtrURL;
}

function doSearch()
{
	var searchURL = "http://www.lyrics.net/lyrics/" + URLencode(document.mainform.SEARCHEB.value);

	
		location.href = searchURL;
	
		
	return false;
}
// -->

