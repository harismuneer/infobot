	// Set the position of the social bookmark widget
	var tempLeft = $('#containOuter').offset().left - soc_bk_width + 10;
	var tempHeight = ($(window).height() / 2) - (soc_bk_height / 2);
	if (tempLeft < 0) {
		$("#soc_bk").addClass("in_doc");
	} else {
		$("#soc_bk").addClass("side_scroll");
	}
	$('#soc_bk').css('left',tempLeft).css('top',tempHeight).fadeIn();
		
	$(window).resize(function() {
		// Set the position of the social bookmark widget
		tempLeft = $('#containOuter').offset().left - soc_bk_width + 10;
		tempHeight = ($(window).height() / 2) - (soc_bk_height / 2);
		$('#soc_bk').css('left',tempLeft).css('top',tempHeight);
		if (tempLeft < 0) {
			$("#soc_bk").removeClass("side_scroll").addClass("in_doc");
			$("addthis_button_facebook_like").attr("fb:like:layout","button_count");
		} else {
			$("#soc_bk").removeClass("in_doc").addClass("side_scroll");
		}
	});
});
