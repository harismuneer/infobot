				jQuery(function ($) {
		var thickboxScriptUrl = 'http://static.dealer.com/v8/global/js/thickbox/thickbox-3.1-mod.js?1253129070000',
			detailsAnimating = false;

		$('select[name=make], select[name=model]').change(function () {
		    window.location = $('option', this).eq($(this)[0].selectedIndex)[0].value;
		});

		// prevent accidental form submission
		$('.incentivesFilterForm form').submit(function () {
			return false;
		});

		// make incentive details section expandable/collapseable
		/*
		* Changes made to resolve https://jira.dealer.ddc/browse/JIRA-7526
		* toggle is designed to be used with apply to pass in the jQuery $(this) object
		*/
		var toggle = function() {
				if ($(this).is('.incentiveDetailsOpen') && !detailsAnimating) {
					detailsAnimating = true;
					$('.incentiveDetailsWrap div', this).stop().css({height: 'auto'}).slideUp(function () {
						$(this).parents('.incentiveDetails').removeClass('incentiveDetailsOpen');
						detailsAnimating = false;
					});
				} else if (!detailsAnimating) {
					detailsAnimating = true;
					$('.incentiveDetailsWrap div', this).stop().css({display: 'none', height: 'auto'}).slideDown(function () {
						$(this).parents('.incentiveDetails').addClass('incentiveDetailsOpen');
						detailsAnimating = false;
					});
				}
			};
		$('.incentiveDetails').live('click', function(e){
			if (!$(e.target).hasClass('trims') && (!$(e.target).hasClass('dialog') && !$(e.target).hasClass('thickbox'))) {
				toggle.apply($(this));
				e.preventDefault();
			}
		});

		// pop-up incentive lead form
		$('.incentiveTools .info').live('click', function () {toggle
			if (!window.tb_remove_proxy && $('#showroom').size()) {
				window.tb_remove_proxy = window.tb_remove;
				window.tb_remove = function () {
					$('body').removeClass('incentivePopup');
					window.tb_remove_proxy.apply(this, arguments);
				};
			}

			if (!window.tb_show) {
				var self = this;
				jQuery.getScript(thickboxScriptUrl, function () {
					$(self).click();
				});
			} else {
				if ($('#showroom').size()) {
					$('body').addClass('incentiveShowroomPopup');
				}
				tb_show($('a', this).attr('title'), $('a', this).attr('href'));
			}
			return false;
		});

		// collapseable sections
		$('.collapseHeader').live('click', function (e) {
			$(this).toggleClass('sectionCollapse');
			$(this).next('.collapseable').toggle().toggleClass('collapse');
		});
	});

