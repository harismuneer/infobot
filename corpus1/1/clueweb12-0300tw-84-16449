<script type="text/javascript">
$(document).ready(function(){
	var saveButtonDiscography	= $('#saveDiscography');
	var saveButton				= $('#save-attributes');
	var personID				= 22514;
	
			
	// LOAD LYRICS MODAL
	$('.tracklist').click(function(e){
		var target		= (e.target)?e.target:e.srcElement;		
		var trackID		= e.target.id;
		
		if (trackID != '') {
			var trackTitle	= $('#'+trackID).html();
			$('object, .widget, .object').remove(); // remove videos otherwise causes scrolling conflict
			// set dialog settings	
			$("#dialogLyrics").dialog({
				bgiframe: true,
				autoOpen: false,
				height: 600,
				width: 850,
				draggable: false,
				modal: false,
				position: ['center',90],
				close: function(event, ui) { dialogContent.html(''); }
			});
			// load lyric and forms
			var dialog			= $('#dialogLyrics');
			var dialogContent	= $('#dialogContentLyrics');
			/*$('#ad-header-728').hide();*/
			dialog.dialog('option', 'title', 'Lyric / video '+trackTitle);
			dialogContent.show();
			dialog.dialog('open');
			$.post('/ajax/dialog.php', { type:41961, value1:trackID, value2:true }, function(callback){
				dialogContent.html(callback);
			});
			return false;
		}
	});
	
	});
</script>	
