    .block(function() { gapi.plusone.go(); });

    function plusOned(res) {

      if(res.state == "on") {
        trackSplitTestAction('Social Events', '+1', null);
      }

    }

    $LAB
    .script('http://platform.twitter.com/widgets.js')
    .block(function (){

      twttr.events.bind('tweet', function() {
        trackSplitTestAction('Social Events', 'Tweet', null);
      });
      twttr.events.bind('follow', function() {
        trackSplitTestAction('Social Events', 'Follow', null);
      });

    });
    $LAB
