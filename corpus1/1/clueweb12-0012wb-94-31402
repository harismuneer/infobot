<script language="JavaScript" type="text/javascript">  //   <!--
function showwind(parm,xwid,xhgt)  {
	var imywidth;
	var imyheight;
	windwidth = xwid
	windhgt =  xhgt
	imywidth = (window.screen.width/2) - ((windwidth/2) + 10);
	imyheight = (window.screen.height/2) - ((windhgt/2) + 20);
    if (parm == 1)  var win2 = window.open("thisday/tcrpt1.htm","popwin","toobar=no,height="+windhgt+",width="+windwidth+",resizable=yes,left=" + imywidth + ",top=" + imyheight + ",screenX=" + imywidth + ",screenY=" + imyheight + ",scrollbars=yes,titlebar=no")
	if (parm == 2)	var win2 = window.open("txt/jokesub2.php?caller=navbutn","jokewind","height="+windhgt+",width="+windwidth+",resizable=no,left=" + imywidth + ",top=" + imyheight + ",screenX=" + imywidth + ",screenY=" + imyheight + ",scrollbars=yes,titlebar=no")
	if (parm == 3)	var win2 = window.open("txt/navtips.php","navwind","height="+windhgt+",width="+windwidth+",resizable=yes,left=" + imywidth + ",top=" + imyheight + ",screenX=" + imywidth + ",screenY=" + imyheight + ",scrollbars=yes,titlebar=no");
