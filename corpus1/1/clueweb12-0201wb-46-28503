			<script type="text/javascript">
				var  navBoxesCookie = "001100";
									//setting the cookie on the default, not sure we need this:
					setCookie('showMenuItems','001100','','');  
									
				//alert(navBoxesCookie);
				//alert("1");
				
				function showHidePortletOptions(linkID,listID,whichpanel){
					//	alert("Current navBoxesCookie:" + navBoxesCookie);
					//	alert("first part:" + navBoxesCookie.substring(0,whichpanel) + "\r\nlast part:" + navBoxesCookie.substring(whichpanel+1,navBoxesCookie.length) + "\r\nlength:" + navBoxesCookie.length);
					if (document.getElementById(listID).style.display=="none"){
						navBoxesCookie = navBoxesCookie.substring(0,whichpanel) + "1" + navBoxesCookie.substring(whichpanel+1,navBoxesCookie.length);
						document.getElementById(listID).style.display="block";
						document.getElementById(linkID).className="collapseLinkOpen";
					}
					else{
						//setCookie('showOtherMenuItems','false',(new Date()).toGMTString(),'');
						navBoxesCookie = navBoxesCookie.substring(0,whichpanel) + "0" + navBoxesCookie.substring(whichpanel+1,navBoxesCookie.length);
						document.getElementById(listID).style.display="none";
						document.getElementById(linkID).className="collapseLinkClosed";
					}
					setCookie('showMenuItems',navBoxesCookie,'','');
					//	alert("New navBoxesCookie:" + navBoxesCookie);
				}
			</script>
