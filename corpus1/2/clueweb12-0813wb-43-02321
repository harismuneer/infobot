<script type="text/javascript">
	function SessionKeepAlive()
	{
		var oImage = document.getElementById("imgSessionKeepAlive");
		if (oImage)
		{
			var oDate = new Date();
			var strTimestamp = escape(oDate.toString());
			oImage.src = "/Controls/Shared/SessionKeepAliveHandler.ashx" + "?par=" + strTimestamp;
		}
	}
	
	window.setInterval("SessionKeepAlive()", 600000 );
</script>
<script language="javascript">
	function RedirectClick( strUrl)
	{
		window.location=strUrl;
	}
	
	if (window.addEventListener)
    {    
        window.addEventListener("load", resize, false);    
    }
    else if (window.attachEvent)
    {
        window.attachEvent("onload", resize);
    }
</script>

    
</div>


