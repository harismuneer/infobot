	$(function () {
		var currentImageID = 1;
		var previousImageID = 1;
		var nextSlide = currentImageID + 5;
		var currFirstImage = 1;
		var maxImageID = 19 - 1;

		$('#nextImage').click(function (e) {
			currentImageID = currentImageID + 1;
			if (currentImageID > maxImageID) {
				currentImageID = 1;
				currFirstImage = 1;
				nextSlide = currentImageID + 5;
				$("#foo2_next").click();
			}
			if (currentImageID >= nextSlide) {
				nextSlide = currentImageID + 5;
				currFirstImage = currentImageID;
				$("#foo2_next").click();
			}
			//alert(currentImageID);
			$("#imageThumb_" + currentImageID).click();
			//alert(currentImageID);
		}),

		$('#prevImage').click(function (e) {
			currentImageID = currentImageID - 1;
			if (currentImageID < 1) {
				currentImageID = maxImageID;
				nextSlide = currentImageID + 1;
				currFirstImage = currentImageID - 4;
				$("#foo2_prev").click();
			}
			if (currentImageID < currFirstImage) {
				nextSlide = currentImageID + 1;
				currFirstImage = currentImageID - 4;
				$("#foo2_prev").click();
			}
			//alert(currentImageID);
			$("#imageThumb_" + currentImageID).click();
			//alert(currentImageID);
		}),

		SwitchMainDetailImg = function (photo,imageNum) {
			var photoLocationName = "NewCarDetailPhoto";
			document.images[photoLocationName].src = photo;
			ABTL.pageContent.reload();
			currentImageID = parseInt(imageNum);
			$("#imageStatus_" + currentImageID).addClass('selected');
			$("#imageStatus_" + previousImageID).removeClass('selected');
			previousImageID = currentImageID;
			//alert(currentImageID);
			currSection = Math.floor(currentImageID / 5);
			if (currSection < (currentImageID / 5)) {
				currSection = currSection + 1;
			}
			nextSlide = (currSection * 5) + 1;
			currFirstImage = (currSection * 5) - 4;
		};

		$(".gallery-pic .display").hover(
		  function () {
			$(".gallery-pic .display .arrow-e, .gallery-pic .display .arrow-w").css("opacity","0.7");
		  },
		  function () {
			$(".gallery-pic .display .arrow-e, .gallery-pic .display .arrow-w").css("opacity","0");
		  }
		);

	});
</script>
