 function quantcastPropValidator(property) {
     return (property !='undefined' && typeof(property)=='string' && property.length > 0);
 }

 function quantcastLabelBuilder(){
     if(quantcastPropValidator(s.prop6) && quantcastPropValidator(s.prop17)) {
         return 'Property.' + s.prop17 + '.Site Section.' + s.prop6;

     }
 }

 _qoptions={
     qacct:"p-42Bk1WKC7O3WM",
