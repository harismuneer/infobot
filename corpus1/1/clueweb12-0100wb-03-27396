<script language="JavaScript" type="text/JavaScript">
	function makeSelected()
{
				//get the id of the selected td
				var selectedTdID = document.getElementById('hdnSelectedTdID').value;
				//now get the selected TD
				if (selectedTdID=="") return;

				var selectedTD = document.getElementById(selectedTdID);
				if (selectedTdID=="home") {selectedTD.style.visibility='hidden'}
				else {
				//now set the td's class to selected and roll overs to do nothing
				selectedTD.className = 'selectedClass';
				//next 2 lines may not work, in which case we'll have to make a mouseover/out function for the td that checks selected first
				selectedTD.onmouseover='';
				selectedTD.onmouseout='';}
}
</script>


