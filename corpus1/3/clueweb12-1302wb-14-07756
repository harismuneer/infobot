<script language="javascript" type="text/javascript">
function encodeString(stringValue){
 var encodedString = ''; 
 if(stringValue != ''){
   var encodedString = unescape(encodeURIComponent(stringValue)); 
 }
 return(encodedString); 
}
function initializeShareThis(title, url, summary, iconPath, widgetContainerID){
 try
 {    if(typeof(SHARETHIS) != 'undefined')
    {
        if(SHARETHIS != null)
        {
            var sharethisObject = SHARETHIS.addEntry(
            {
                title:encodeString(title),
                url:encodeString(url),
                summary:encodeString(summary),
                icon:encodeString(iconPath)
            }, {button:false});
            var element = document.getElementById(widgetContainerID);
            sharethisObject.attachButton(element);
        }
    }
 }
 catch(err){}
}
</script>

