<script type="text/javascript">
    var currentPlaylist = playlists[0].id ;
    var text='';

    if(document.getElementById('edit-text2') != null) {
        text = document.getElementById('edit-text2').value;
    }
    
    function buildUrl(id) {
        switch(id) {
            case 'thSongNameArtistMatch':
                var url = base_url + "search/artistmatch?q=" + text + "&sort=SongTitle";
                break;
            case 'thSongName':
                                    var url = base_url + "search/song?q=" + text + "&sort=SongTitle";
                                break;
            case 'thArtistNameSong':
                                    var url = base_url + "search/song?q=" + text + "&sort=ArtistName";
                                break;
            case 'thAlbumName':
                var url = base_url + "search/album?q=" + text + "&sort=AlbumTitle" + "";
                break;
            case  'thArtistNameAlbum':
                var url = base_url + "search/album?q=" + text + "&sort=ArtistName";
                break;
            case 'thArtistNameOnly':
                var url = base_url + "search/artist?q=" + text + "&sort=ArtistName";
                break;
            case 'thAlbumNameArtistDetails':
            case 'thSongNameArtistDetails' :
            case 'thSongNameAlbumDetails':
                fullUrl = location.href;
                urlParts = fullUrl.split("?");
                url = urlParts[0];
                break;
        }
        return url;
        }

    function sortOptions(id) {
        var sort='';
        switch(id) {
            case 'thAlbumNameArtistDetails':
                sort = "?sort=AlbumTitle"
                break;
            case 'thSongNameArtistDetails':
            case 'thSongNameAlbumDetails':
                sort = "?sort=SongTitle"
                break;
        }
        return sort;
    }
    
    function showDownUp(th) {
        
        if (th != null) {
            thClass = th.getAttribute("class");
            sort = sortOptions(th.id);
            if (thClass == "sort act-width down" ) {
                th.setAttribute("class", "sort act-width up"); 
                url = buildUrl(th.id) + sort + '&order=ASC' + "&page=1";
                window.location.href = url;
            } else {
                th.setAttribute("class", "sort act-width down");
                url = buildUrl(th.id) + sort + '&order=DESC' + "&page=1";
                window.location.href = url;
            }
        }
    }

</script>

