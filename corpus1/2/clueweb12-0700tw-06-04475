    var SWITCH_NDXS = {};
    var SWITCH_TOTALS = {};

    function switchEl(elm, ndx, t)  {
        elm = $(elm);
        var els = elm.children();
        SWITCH_TOTALS[elm] = els.length;
        if (t)  {
            $(t).children().removeClass('active');
            $($(t).children()[ndx]).addClass('active').blur();
        }

        for (var i=0; i < els.length; i++)  {
            if (i == ndx)  {
                $(els[i]).show();
            } else  {
                $(els[i]).hide();
            }
        }
        SWITCH_NDXS[elm] = ndx;
    }

    function switchNext(elm)  {
        var idx = SWITCH_NDXS[elm] || 0;
        var totals = SWITCH_TOTALS[elm];
        if (idx < (totals - 1))  {
            return switchEl(elm, (idx + 1));
        } else {
            return switchEl(elm, 0);
        }
    }

    function switchPrev(elm)  {
        var idx = SWITCH_NDXS[elm] || 0;
        var totals = SWITCH_TOTALS[elm];
        if (idx > 0)  {
            return switchEl(elm, (idx - 1));
        } else {
            return switchEl(elm, (totals - 1));
        }
    }

    

       var rarticle_links; var article_links;
       function showRelevantLinks()  {
            article_links.style.display = 'none';
            rarticle_links.style.display = 'block';
        }

        function nextArticle()  {
            showRelevantLinks();
            if (RELATED_ARTICLES_P == RELATED_ARTICLES_MAX -1)
                RELATED_ARTICLES_P = -1;
            RELATED_ARTICLES_P++;
            playRelatedArticle();
        }

        function prevArticle()  {
            showRelevantLinks();                        
            if (RELATED_ARTICLES_P <= 0)
                RELATED_ARTICLES_P = RELATED_ARTICLES_MAX - 1;

            RELATED_ARTICLES_P--;
            playRelatedArticle();
        }

        function backToSelf()  {
            rarticle_links.style.display = 'none'
            article_links.style.display = 'block';

            article_title.innerHTML = main_article_title;
            article_source.innerHTML = main_article_source;
            article_title.href = main_article_source_url;
            article_text.innerHTML = main_article_text;

        }
        function playRelatedArticle()  {
            article_links.style.display = 'none';
            rarticle_links.style.display = 'block';
            article_title.innerHTML = RELATED_ARTICLES[RELATED_ARTICLES_P][0];
            article_title.href = RELATED_ARTICLES[RELATED_ARTICLES_P][3];
            article_text.innerHTML = RELATED_ARTICLES[RELATED_ARTICLES_P][1];
            article_source.innerHTML = '<a href="' + RELATED_ARTICLES[RELATED_ARTICLES_P][3] + '">' + RELATED_ARTICLES[RELATED_ARTICLES_P][2] + '</a>';
        }

        function readCurrentFullArticle()  {
            window.location = RELATED_ARTICLES[RELATED_ARTICLES_P][3];
        }
    

