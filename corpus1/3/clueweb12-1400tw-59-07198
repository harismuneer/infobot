								function populate_wff(urlval,divval,loading,fncname){
								try{
								var xmlhttp=false;
								var status=0;
								if(loading != 'no'){
								var str_temp = '<table cellpadding="0" cellspacing="0" width="100%">';
								str_temp = str_temp + '<tr><td valign="middle" align="center" style="font-family:Arial;font-size:13px;font-weight:bold;color:#000000;">Loading';
