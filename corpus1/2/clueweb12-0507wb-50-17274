<script>
var guFormVehicleType = 'retail';
var vehAbstractId = '';
var deeplink = false;
var brochureCode = '100';
window.onload = init;
vehAbstractId = 'EDG13';
function checkSelect(select) {
var select_element = document.getElementById(select),
error_element = document.getElementById("selectError"),
result = false;
if (deeplink) {
result = true;
} else {
if (select_element && select_element.value.length > 0) {
result = true;
error_element.style.display = "none";
if (typeof brochuresMap[select_element.value].metrics !== "undefined") {
oMetricsTracker.storeNameplateInfo.apply(oMetricsTracker, brochuresMap[select_element.value].metrics);
}
if (typeof brochuresMap[select_element.value].brochure !== "undefined") {
document.getElementById('brochureByMailStep2').style.display = "";
_widgets.context.SelectedContext.set('getUpdatesForm.forVehicle', {
make: "Ford",
year: brochuresMap[select_element.value].brochure.year,
model: brochuresMap[select_element.value].brochure.model
});
} else {
document.getElementById('brochureByMailStep2').style.display = 'none';
}
} else {
error_element.style.display = "";
//FIRE BROCHURE ERROR METRICS
oMetricsTracker.trackMicroData("brochuresError");
_widgets.context.SelectedContext.resetValue('getUpdatesForm.forVehicle', true);
}
}
return result;
}
function init() {
// Use the no-cache header so that logging results cannot be 'captured' by akamai.
setCommercial('retail');
document.getElementById("vehicleSelect1").style.display = "none";
document.getElementById("step2Text").style.display = "none";
document.getElementById("vehicleSelect2").style.display = "block";
}
// com trucks
function downloadBrochure(){
var util = td_site.u;
var brochure = document.getElementById('brochureCode5');
var brochureval = "";
if(brochure) {
brochureval = brochure.options[brochure.selectedIndex].value;
}
if (checkSelect('brochureCode5')) {
oMetricsTracker.trackMicroData("commBrochuresOnLoadDownload");
document.getElementById('brochureSelectVehicleStep2').style.display = "none";
showstep(2);
document.getElementById("pdfImage").style.display = "none";
util.addListener(document.getElementById("pdfLink"), ["click"], function (e) {
callPdfService(brochureval);
util.preventDefault(e);
});
document.getElementById("pdfLink").innerHTML = brochuresMap[brochureval].name;
document.getElementById("pdfLinkButton").style.visibility = "visible";
ngbs.legacy(function (message_bus) {
var $ = jQuery,
$share_button = $("a.share", document.getElementById("step2")),
download_parts = /(\/.*\/)(for:)?([^\/]+)?\/?/.exec(document.location.pathname);
message_bus.send($share_button, "update", {
summary: "Download a brochure or request a brochure by mail for " + brochuresMap[brochureval].name,
url: __params.baseURL + download_parts[1] + "for:" + brochureval + "/"
});
});
}
}
function downloadPDF(id){
var util = td_site.u,
bDownloadBrochure = false,
zip = trim(document.getElementById("zip").value),
image = document.getElementById("pdfImage"),
pdf_link = document.getElementById("pdfImageLink"),
pdf_button = document.getElementById("pdfLink");
// check that vehicle dropdown is present
if (vehAbstractId == '') {
// check that a vehicle is selected
bDownloadBrochure = checkSelect('brochureCode4');
} else {
// vehicle page where vehicle is already set
bDownloadBrochure = true;
setBrochureVeh();
}
if (bDownloadBrochure) {
//FIRE PDF DOWNLOAD CONFIRM METRICS
oMetricsTracker.trackMicroData("brochuresOnLoadDownload");
showstep(2);
if (typeof brochuresMap[id].thumb !== "undefined") {
image.style.display = "";
image.src = brochuresMap[id].thumb;
image.alt = brochuresMap[id].name + " Brochure";
} else {
image.style.display = "none";
}
util.addListener(pdf_link, ["click"], function (e) {
callPdfService(id, zip);
util.preventDefault(e);
});
pdf_button.innerHTML = brochuresMap[id].name;
document.getElementById("pdfLinkButton").style.visibility = "visible";
ngbs.legacy(function (message_bus) {
var $ = jQuery,
$share_button = $("a.share", document.getElementById("step2")),
path = document.location.pathname,
hasFor = /\/for\:.+\//.test(path)
download_parts = /(\/.*\/)(for:)?([^\/]+)?\/?/.exec(path);
var newURL = hasFor ? path : download_parts[1] + "for:" + id + "/"
message_bus.send($share_button, "update", {
image: image.src,
summary: "Download a brochure or request a brochure by mail for the " + brochuresMap[id].name,
url: __params.baseURL + newURL
});
});
}
}
function callPdfService(id, zip) {
//FIRE BROCHURE DOWNLOAD ON CLICK METRICS
if (false) { // Fiarly certain that these two metrics calls do exactly the same thing.
oMetricsTracker.trackMicroData("commBrochuresOnLoadDownload");
} else {
oMetricsTracker.trackMicroData("brochuresOnClickDownload");
}
openWin(brochuresMap[id].pdf + (zip ? "&postalCode=" + zip : ""),'nb');
}
// check the appropriate checkbox in the send email flip
function setFlipNameplate(code) {
if (code) {
jQuery("input.nameplate-id").prop("checked", false);
jQuery("input.nameplate-id[value$='|" + code + "']").eq(0).prop("checked", true);
}
}
function setBrochureVeh(ele) {
var option = ele ? ele.options[ele.selectedIndex] : null;
var id = option ? option.value : vehAbstractId;
vehAbstractId = id;
if (typeof brochuresMap[id].pdf !== "undefined") {
document.getElementById("step1Has").style.display = "block";
document.getElementById("step1HasNo").style.display = "none";
} else {
document.getElementById("step1Has").style.display = "none";
document.getElementById("step1HasNo").style.display = "block";
}
if (typeof brochuresMap[id].brochure !== "undefined") {
document.getElementById("brochureByMailStep1").style.display = "block";
} else {
document.getElementById("brochureByMailStep1").style.display = "none";
}
var oThumb = document.getElementById("brochureVehicleThumb");
oThumb.src = brochuresMap[id].vehicle;
oThumb.alt = brochuresMap[id].name;
oThumb.style.display = "block";
var code = option ? jQuery(option).data("code") : brochureCode;
setFlipNameplate(code);
checkSelect('brochureCode4');
}
function setCommercial(el) {
var ele1 = document.getElementById("brochureSelectVehicleStep1");
var ele2 = document.getElementById("brochureSelectVehicleStep2");
var ele3 = document.getElementById("brochureByMailStep2");
var showFord = -1;
if (el && el.innerHTML) {
showFord = el.innerHTML.indexOf('Retail');
}
if (el === 'retail' || showFord != -1) {
guFormVehicleType = 'retail';
ele1.style.display = "block";
ele2.style.display = "none";
document.getElementById("brochureByMailStep2").style.display = "block";
ele3.className = "broucherMailStepFord";
document.getElementById("fordBrochure").className = "genericBtn1";
document.getElementById("truckBrochure").className = "genericBtn";
var brochureCode4 = document.getElementById("brochureCode4");
if (brochureCode4) {
brochureCode4.options[0].selected = true;
}
if (deeplink) {
downloadPDF(vehAbstractId);
}
} else {
guFormVehicleType = 'commercial';
ele1.style.display = "none";
ele2.style.display = "block";
ele3.className = "broucherMailStepTruck";
document.getElementById("vehicleSelect2").style.display = "block";
document.getElementById("brochureByMailStep2").style.display = "none";
document.getElementById("fordBrochure").className = "genericBtn";
document.getElementById("truckBrochure").className = "genericBtn1";
var brochureCode5 = document.getElementById("brochureCode5");
if (brochureCode5) {
brochureCode5.options[0].selected = true;
}
if (deeplink) {
downloadBrochure();
}
}
}
//*****************************************************************************************************************************
function showstep(step) {
function checkElement(sName) {
if (document.getElementById(sName) != null) {
document.getElementById(sName).style.display = "none";
}
}
if (step==1){
document.getElementById("nextStepsWidget").style.display="none";
}
if (step==2){
document.getElementById("nextStepsWidget").style.display="block";
}
checkElement("step1")
checkElement("step2")
var obj = document.getElementById("step" + step);
if (obj != null)
obj.style.display = "block";
window.scrollTo(0, 0);
}
function getAnotherBrochure(step) {
if (vehAbstractId != '') {
var newLoc = location.protocol + "//" + location.host + "/brochures";
window.location = newLoc;
} else {
showstep(1);
var fordBrochure = document.getElementById('fordBrochure');
if (fordBrochure && (fordBrochure.className == "genericBtn")) {
document.getElementById('brochureSelectVehicleStep2').style.display = "block";
}
}
}
function trim(str) {
if (str.length >0) {
return str.substring(Math.max(str.search(/\S/), 0), str.search(/\S\s*$/) + 1);
} else {
return str;
}
}
function openWin( html, name, wval, hval, scroll) {
var features;
if (wval != '' && hval != '' && wval != undefined && hval != undefined) {
features = 'width='+wval+',height='+hval+',toolbar=no,status=no,menubar=no,scrollbars='+scroll+',resizable=no';
} else {
features = 'width=940,height=620,toolbar=no,status=no,menubar=no,scrollbars='+scroll+',resizable=no';
}
var nW = window.open(html, name, features);
}
