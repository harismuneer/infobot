<script>

function setzipval(no)
{
	var um = jQuery('#usedmiles').val();
	//alert(um);
	//jQuery(".um").val('hello');
	
	//var milesdata = jQuery("#usedmiles_2").val(um);
	//alert(milesdata);
	//return false;
	
	if($("#zipval").val() == '')
	{
		$("#required_check").addClass("txtred");
		return false;
	}
	else
	{
		$("#required_check").removeClass("txtred");
	}

	if(no == 0)
	{
		$("#usedzip_"+no).val($("#zipval").val());
	}
	else
	{	$("#usedzip_"+no).val($("#zipval").val());
		$("#usedmiles_"+no).val($("#usedmiles").val());
	}
}

	
