<script>
(function($) {

$(document).bind('user.load', function() {
	$('#quicksignup').hide();
	if (User.hasFlag('FAILTHIS')) {
		
		$('#quickevents').show();
		
		var seState = Location.getState();
		var seZip = Location.getZip();
		if (seState && seZip) {
			var evDash = eventLoader.load({"limit":"1","state":seState,"zip_radius[0]":seZip,"zip_radius[1]":"200","orderby":"zip_radius","dash":"1",
"event_type": etState});	
		}
		
		
	} else {$('#mobilesignup').show();}
});

$('#quicksignup form').bind('submit', function(e) {
	e.preventDefault();
	if (validForm(this,'Please enter a valid email and zip')) {
		$("#mobEM").val($('#quicksignup_email').val());
		$("#mobZIP").val($('#quicksignup_zip').val());		
		var data = {email: $('#quicksignup_email').val(), zip: $('#quicksignup_zip').val()};
		console.log('create new user...');
		console.log(data);
		User.create(data);
	}
});

$('#mobilesignup form').bind('submit', function(e) {
	User.addFlag('mobile');
	$('#mobilesignup').fadeOut('fast', function() {

		/* $('#quickevents').fadeIn('fast'); */
		
	});
	return true;
});

if (User.authenticated()) {User.load();}

})(jQuery);
</script>

