 $(".yellow_btn_date").bind({
       click: function() {
   				if(flag == 0 )
       			{
	       			flag = 1;
		       		var load_next_date_arr =$(this).attr("id").split('_');
		       		var load_next_date = load_next_date_arr[1];
		       		var notIn = $("#notInCat").val();
		       		var count = $("#count").val();
		       		var topStoryflag = $("#topStoryflag").val();
					var disqus_shortname = 'leftlane';
	      		
	      			$.post(WP_SITEURL+"/wp-content/themes/lln5/ajaxindexnew.php",{nextdate: load_next_date, notInCat: notIn ,count: count, tfalg :topStoryflag },
					function(data){
					
					$("#main_content").height('auto');	
					$("#top_stories").append(data);
					$.getScript ("http://" + disqus_shortname + ".disqus.com/count.js");
					
					$("#load_"+load_next_date).empty().html($("#loadText_"+load_next_date).val());				
					var loadhref = $(".yellow_btn_date a").attr('id');					
					var loadhrefarr = loadhref.split('_');
					$("#count").val(loadhrefarr[1]);
								
					var nxtpage = $("#next_page_"+ load_next_date).val();
					$(".yellow_btn_date").attr('id',"load_"+nxtpage);
					
					var tsflg = $("#ajaxtopStoryflag_"+ load_next_date).val();
					$("#topStoryflag").val(tsflg);
					
					 $('.right_column').height("auto");
                     $('.left_column').height("auto");
                     flag = 0;                     
		}); }
		}
 });
 
 
 
 /*@Ankur 09 Jan 2012 */
 $(".trimOptionPopupClass").bind({

       mouseenter: function(e) {


       	var num = this.id; 
       	var idarr = num.split('_'); 
       	var offset = $(this).offset();
		var fuckjavascriptcreator=offset.top - $(window).scrollTop() + 25;
		$("#popup_page_overlay").css("top",fuckjavascriptcreator+"px");
		
       	//@Ankur 09 Jan 2012 - check if the "i" is trim or option
       	if(idarr[0]=="options")
       	{
       		$('#popup_page_overlay').empty().html($("#optioninfo_"+idarr[1]).html());
       	}
       	else if(idarr[0]=="trim")
       	{
       		$('#popup_page_overlay').empty().html($("#triminfo_"+idarr[1]).html());
       	}
	 
		$('#popup_page_overlay').fadeIn();
		
       },
       mouseleave: function() {
      
       	$('#popup_page_overlay').fadeOut();
       }
 });
 
 
 
 
 
 
 
 
 </script>

</body>
</html> 
		


<script>
/*$(document).ready(function(){ 	
