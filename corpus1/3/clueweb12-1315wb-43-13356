jQuery(document).ready(function() {
		$("form#loginToolbar").unbind();
	$("form#loginToolbar").submit(function(e) {
		ajaxLoadingPanel_show();
		var data = '';
		$("form#loginToolbar input, form#loginToolbar textarea, form#loginToolbar select").each(function(i){
			if ((this.type=="checkbox" || this.type=="radio"))
			{
				if (this.checked)
				{
					data += encodeURIComponent(this.name)+'='+encodeURIComponent(this.value)+'&';
				}
			}
			else data += encodeURIComponent(this.name)+'='+encodeURIComponent(this.value)+'&';
		});
		data += 'ajax=1';
		var action = '';
		if ($("form#loginToolbar").attr('action').length)
			action = $("form#loginToolbar").attr('action');
		else action = 'http://www.kovideo.net/hush-hush-i-will-survive-remix-lyrics-the-pussycat-dolls-742294.html';

		$.ajax({
		  type: "POST",
		  url: action,
		  data: data,
		  success: function(msg) {
			ajaxLoadingPanel_hide();
			if (msg.indexOf('var keep_' + 'output = true;')==-1)
			{	
				if (''=='append')
				{
					$('#messages').html($('#messages').html()+msg);
				}
				else if (''=='prepend')
				{
					$('#messages').html(msg+$('#messages').html());
				}
				else 
				{
					//alert('replacing messages');
					$('#messages').html(msg);
				}
			}
			else $('#messages').html(msg);
			initializeNavigationBar(1);
		  },
		  failure: function(msg)
		  {
			ajaxLoadingPanel_hide();
			alert(msg);
		  }
		 });
		 e.preventDefault();
	});
});

</script>

