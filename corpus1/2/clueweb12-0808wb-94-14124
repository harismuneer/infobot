<script type="text/javascript">
var is_quaternary = false;
var quaternary_id = -1;
var login_error = '';
var default_ask_text = 'What\'s the best brand of Dog Beds?';
function replace_default(element, focused)
{
	var askBox = $(element);
	if(!focused)
	{
		askBox.addClass('input-blurred');
		if(askBox.val() == '')
		{
			askBox.val(default_ask_text);
		}
	}
	else
	{
		askBox.removeClass('input-blurred');
		if(askBox.val() == default_ask_text)
		{
			askBox.val('');
		}
	}
}
</script>
