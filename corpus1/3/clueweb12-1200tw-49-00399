<script type="text/javascript">


    function closecomments() {
        new Effect.SlideUp('hud_comments', { duration: .2 });
        window.location.reload();
    }



    function opencomments(id) {
        document.getElementById('hudcomments_id').value = id;
        document.getElementById('hudcomments_refreshtype').value = "new";
        eo_Callback('callback_hud_comments');
        new Effect.SlideDown('hud_comments', { duration: .2 });
    }

    function deletecomment(id) {
        document.getElementById('hudcomments_id').value = id;
        document.getElementById('hudcomments_refreshtype').value = "delete";
        eo_Callback('callback_hud_comments');
        new Effect.SlideDown('hud_comments', { duration: .2 });
    }

    function commentsbusy_on() {
        document.getElementById('hudcomments_busy').style.display = "block";
        return true;
    }

    function commentsbusy_off() {
        document.getElementById('hudcomments_busy').style.display = "none";
        return true;
    }

  


</script>
