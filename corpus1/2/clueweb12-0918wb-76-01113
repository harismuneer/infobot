        var pairList = googVar.split("_");
        for (var i=0; i<pairList.length; i++) {
            var pair = pairList[i];
            if (pair.length < 1) {
                continue;
            }
            var nameValPair = pair.split("-");
            if(!activeVars[nameValPair[0]]) {
                // not active any more; strip it out
                continue;
            }
            if(userVars[nameValPair[0]] !== undefined) {
                // in both... needs changing?
                // if new value evaluates to false, put in nothing
