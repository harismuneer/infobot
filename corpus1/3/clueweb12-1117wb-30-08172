var numMatches = 7;

//this does not need to be changed
function checkMatch(selectGroup) {
  if (selectGroup[selectGroup.selectedIndex].value == "correct") {
     return 1;
  }
  else {
     return 0;
  }
}


function checkQuiz() {
  var numCorrect = 0;

  for(i=0; i<numMatches; i++) {
     numCorrect += checkMatch(document.matching.elements[i]);
  }

  //document.matching.output.value = document.matching.length;
  document.matching.output.value = "You got " + numCorrect + " correct.\n\n";

}

function showAnswers() {
  document.matching.output.value +=
	"The correct answers are: \n" +
	"\tStep One - (E) States hold elections and a popular vote is recorded.  \n" +
	"\tStep Two - (A) A states' electoral votes are given to the candidate who wins a states' popular votes. \n" +
	"\tStep Three - (F) Electors meet at the electoral college.  \n" +
	"\tStep Four - (B) Electoral vote is taken.  \n" +
	"\tStep Five - (G) Congress counts the electoral vote.  \n" +
	"\tStep Six - (C) Person with the majority of electoral votes is declared the winner. \n" +
	"\tStep Seven - (D)New President is inaugurated on January 20th.  \n" ;
}
</script>

