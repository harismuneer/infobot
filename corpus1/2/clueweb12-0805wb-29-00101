	    var flashvars = {
	}
	var params = {
	    base: ".",
	    wmode: "transparent",
	    allowfullscreen: "true"
	}
	var attributes = {}


	// Dave, same for potw2.swf, works like the video swf above
	swfobject.embedSWF("/potw/potw2.swf", "potw", "365", "375", "9.0.0", false, flashvars, params, attributes);
	// arguments: url, div id, width, height, min flash version
	</script>
	
	
	



	
<script language="javascript" type="text/javascript">

    // /////////////////////////////////////                  document ready function             ///////////

    $(document).ready(function() {

        //make IE6 support 24bit png graphics 
        $('body').pngFix();

        // canvasSwap for rollover graphics. Simply place a copy of the "on" state of the graphic into the same directory as the "off" state, and name the "on" state with "_on" before the extension. "On" state must be same format, gif for gif, png for png. For example: the graphic "logintab.png" has an on-state graphic "logintab_on.png" that resides adjacent in img dir. Then simply add a unique class to the off-state img (use base image name to keep things straight) and make an entry below following the same syntax as the list of graphics to which rollovers apply. That's it. UPDATE: though we have pngfix, IE6 will not activate the click on a 24-bit png. 
        $.fn.canvasSwap.defaults.suffix = '_on';
        $.fn.canvasSwap.defaults.ie6_support = false; // don't turn it on... 
        $('img.logintab').canvasSwap();
        $('img.searchtab').canvasSwap();
        $('img.login-close-x').canvasSwap();
        $('img.membership').canvasSwap();
        $('img.email').canvasSwap();
        $('img.print').canvasSwap();
        $('img.twitter').canvasSwap();
        $('img.facebook').canvasSwap();
        $('img.linkedin').canvasSwap();
        $('img.rss').canvasSwap();
        $('img.itunes').canvasSwap();
        $('img.more').canvasSwap();
        $('img.readmore').canvasSwap();
        $('img.enlarge').canvasSwap();
        $('img.logout').canvasSwap();
        $('img.text-smaller').canvasSwap();
        $('img.text').canvasSwap();
        $('img.text-larger').canvasSwap();

        //preloading...
        $('#nav-band-content img').preload({  // we are looking specifically at this ID div, for images
            find: '.gif', // use a string or regex to match part of the src
            replace: '_on.gif' // replacement to generate alternate URL; this coincides with the _on suffix we are using in canvasSwap above to generate graphic rollovers.
        });




        //highlight divs on mouseover
        $('div.hilite_div').hover(function() { $(this).css('background', '#d8e8f6') },function() { $(this).css('background', 'none') }
);

        //highlight more's w/o links on mouseover
        $('span.continue').hover(
function() { $(this).css('color', '#cc6600') },
function() { $(this).css('color', '#558dbe') }
);


        //login & search box
        $('img.logintab').click(function() {
            $('div.loginbox').slideToggle('slow');
            $('div.searchbox').fadeOut('fast');
            return false;
        });



        $('img.searchtab').click(function() {
            $('div.searchbox').slideToggle('slow');
            $('div.loginbox').fadeOut('fast');
            return false;
        });

        $('img.login-close-x').click(function() {
            $('div.loginbox').fadeOut('slow');
            $('div.searchbox').fadeOut('slow');
            return false;
        });


        // Text resizing script
        // Reset Font Size
        var originalFontSize = $('.articles p').css('font-size');
        $("img.text").click(function() {
            $('.articles p').css('font-size', originalFontSize);
            $('.articles ul').css('font-size', newFontSize);
            return false;
        });
        // Increase Font Size
        $("img.text-larger").click(function() {
            var currentFontSize = $('.articles p').css('font-size');
            var currentFontSizeNum = parseFloat(currentFontSize, 10);
            var newFontSize = currentFontSizeNum + 2; //you could use a multiplier (and divider for Decreasing) but since we are set to px and ems, let's just increase or decrease by 2 sizes
            $('.articles p').css('font-size', newFontSize);
            $('.articles ul').css('font-size', newFontSize);
            return false;
        });
        // Decrease Font Size
        $("img.text-smaller").click(function() {
            var currentFontSize = $('.articles p').css('font-size');
            var currentFontSizeNum = parseFloat(currentFontSize, 10);
            var newFontSize = currentFontSizeNum - 2;
            $('.articles p').css('font-size', newFontSize);
            $('.articles ul').css('font-size', newFontSize);
            return false;
        });




    }); //close doc ready function
</script>

