function swapbox(url)
{
	iBox.hide();
	setTimeout(function() {iBox.showURL(url);},1000);
}

iBox.addEvent(iBox, 'show', function() {
	var newinput = document.createElement("input");
	newinput.type = "hidden";
	newinput.name = "NEXTURL";
	newinput.id = "NEXTURL";
	newinput.value = location.href + "?registered=true";
	setTimeout(function() {if($("#survey_1661")) {$("#survey_1661").appendChild(newinput);}},3000);
});
</script>


