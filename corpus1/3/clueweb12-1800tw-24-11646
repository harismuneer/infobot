  <script>
  var gt = gt||{};gt.sidebar = gt.sidebar||{};
  gt.sidebar.slidePos = 0;
  gt.sidebar.slideTimer = null;
  gt.sidebar.slider = function(){
  	var sbWidgetWrap = $('.sidebarWidgetWrapper .sidebarWidgetBody');
  	sbWidgetWrap.removeClass('widgetSlide-'+(gt.sidebar.slidePos+1));
  	gt.sidebar.slidePos++;
  	gt.sidebar.slidePos=gt.sidebar.slidePos>=5?0:gt.sidebar.slidePos;
  	sbWidgetWrap.addClass('widgetSlide-'+(gt.sidebar.slidePos+1));
  };
  gt.sidebar.sliderChange = function(pos){
  	var sbWidgetWrap = $('.sidebarWidgetWrapper .sidebarWidgetBody');
  	gt.sidebar.sliderKill();
  	pos=pos-1;
  	sbWidgetWrap.removeClass('widgetSlide-'+(gt.sidebar.slidePos+1));
  	gt.sidebar.slidePos=pos;
  	gt.sidebar.slidePos=gt.sidebar.slidePos>=5?0:gt.sidebar.slidePos;
  	sbWidgetWrap.addClass('widgetSlide-'+(gt.sidebar.slidePos+1));
  	gt.sidebar.sliderStart();
  };
  gt.sidebar.sliderStart = function(){
  	var sbwContInput = $('.sidebarWidgetWrapper .sidebarWidgetBody .widgetSlideContainer input').is(':focus');
  	if(!sbwContInput) {
  		//console.log('start');
  		gt.sidebar.slideTimer = setInterval("gt.sidebar.slider()",5500);
  	}
  };
  gt.sidebar.sliderKill = function(){
  	//console.log('stop');
  	clearInterval(gt.sidebar.slideTimer);
  };
  $(function(){
  	var sbwMenu = $('.sidebarWidgetWrapper .sidebarWidgetBody .widgetMenu a'), sbwCont = $('.sidebarWidgetWrapper .sidebarWidgetBody .widgetSlideContainer');
  	sbwCont.hover(gt.sidebar.sliderKill,gt.sidebar.sliderStart).focusin(gt.sidebar.sliderKill).focusout(gt.sidebar.sliderStart);
  	sbwMenu.click(function(){
  		var self = $(this), slideNum = parseInt(self[0].id.split('_')[1],10);
  		//console.log(slideNum);
  		gt.sidebar.sliderChange(slideNum);
  		return false;
  	});
  	gt.sidebar.sliderStart();
  	gt.sidebar.origTop = $('#moduleContent13230202 .sidebarWidgetWrapper').offset().top;
  	$(window).scroll(function(){
  		gt.sidebar.scrollTop = $(window).scrollTop();
  		if(gt.sidebar.scrollTop>(gt.sidebar.origTop-10)) {
  			$('#sectionContent5508670').css({position:'fixed',top:0});
  			$('#sectionContent5060903').css({position:'fixed',top:315});
  		} else {
  			$('#sectionContent5508670').css({position:'',top:''});
  			$('#sectionContent5060903').css({position:'',top:''});
  		}
  	});
  });
  </script>
  
