                           google_ad_slot = "7900102285";
                           google_ad_width = 728;
                           google_ad_height = 15;
                           //-->
                           </script>
                           <script type="text/javascript"
                           src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
                           </script>
                           <!--"''"--></DIV></i></b></font>
                        </p>
                        <p>
                           <table border="0" cellpadding="0" cellspacing="0" align="Left">
                              <tr>
                                 <td align="center" background="/imagelib/sitebuilder/layout/spacer.gif" width="300"><font face="Arial,Helvetica,sans-serif" size="1" color="black"><b><i>Earthworks at the Battle of Asheville</i></b></font></td>
                              </tr>
                              <tr>
                                 <td align="center" background="/imagelib/sitebuilder/layout/spacer.gif"><img alt=" Civil War Battle of Asheville Earthworks.jpg" border="" src="sitebuildercontent/sitebuilderpictures/.pond/battleofashevilleearthworksnc.jpg.w300h224.jpg" vspace="0" hspace="5" width="300"><br></td>
                              </tr>
                              <tr>
                                 <td align="center" background="/imagelib/sitebuilder/layout/spacer.gif" width="300"><font face="Arial,Helvetica,sans-serif" size="1" color="black"><b><i>North Carolina Office of Archives & History</i></b></font></td>
                              </tr>
