  <script type="text/javascript"> 
    var google_adnum = 0; 
    
    function google_ad_request_done(google_ads) {
      var ad_code = '';
      if (google_ads.length == 1) {
        ad_code += '';
        ad_code += '<p class="adsense_credit"><a href="' +
        google_info.feedback_url + '\" class="googleClickout">Ads by Google</a></p>';
        ad_code += '<p class="ad_title"><a href="' +
        google_ads[0].url + '" class="googleClickout">' +
        google_ads[0].line1 + '</a></p><p>' +
        google_ads[0].line2 + '&nbsp;' +
        google_ads[0].line3 + '<br /><a class="google_line_3 googleClickout" href="' +
        google_ads[0].url + '">' +
        google_ads[0].visible_url + '</a></p>';
        if (google_ads[0].bidtype == "CPC") {  
          google_adnum = google_adnum + google_ads.length;
        }
      }

      if (google_ads.length == 3) {
        var number_of_ads = 3;
        for (var i = 0; i < number_of_ads; ++i) {
          if (i == number_of_ads-3) {
            ad_code += '<p class="adsense_credit"><a href="' +
            google_info.feedback_url + '" class="googleClickout">Ads by Google</a></p>';
          }
          ad_code += '<div class="adsense-new adsense_top adsense_10">';
          ad_code += '<span class="ad_title"><a href="' +
            google_ads[i].url + '" class="googleClickout">' +
            google_ads[i].line1 + '</a></span> <span class="ad_abstract">' + 
            google_ads[i].line2 + '&nbsp;' +
            google_ads[i].line3 + '</span> <a class="google_line_3 googleClickout" href="' + 
            google_ads[i].url + '">' +
            google_ads[i].visible_url + '</a></div>';
        }
      
        if (google_ads[0].bidtype == "CPC") {  /* insert this snippet for each ad call */
          google_adnum = google_adnum + google_ads.length;
        }
      }
      document.write(ad_code);
    }

