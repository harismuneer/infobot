            alert("You did not enter an e-mail address; that won't work.")
            document.jsubform.jemail.focus()
            return false
        }
        if (document.jsubform.jemail.value.length < 3) {
            alert("The email address you entered is too short; please try again.")
            document.jsubform.jemail.focus()
            return false
        }
        if (document.jsubform.jemail.value.indexOf("@") == -1)   {
            alert("The e-mail address does not have an \"@\" symbol in it; please try again. ")
            document.jsubform.jemail.focus()
            return false
        }
