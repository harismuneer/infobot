<script src="http://yui.yahooapis.com/3.4.1/build/yui/yui-min.js"></script>

<script src="http://o.aolcdn.com/os_merge/?file=/aol/jquery-1.4.4.min.js&file=/aol/jquery.cookie.js&file=/aol/jquery.getjs-1.0.min.js&file=/aol/jquery.inlinecss-1.0.min.js&file=/aol/jquery.truncate-1.0.min.js&file=/aol/jquery.openwindow-1.0.min.js&file=/aol/jquery.shorturl.min.js&&file=/aol/jquery.multiauth-2.0.min.js&file=/aol/jquery.globalheader-1.5.min.js&file=/aol/jquery.globalsearchbox-1.5.min.js&file=/aol/jquery.aolphotogallery-1.0.js&file=/pets/js/22-sly-aol-comments.js" type="text/javascript"></script>

<script charset="utf-8">

var fba = "187601264685519";
(function($) {

var siteDevId = 'ao1NivzL-hyp20CR';

var handleAuth = function(json) {

            var $elem = $(this.authLink),
				response = json.response,
				userAttr;

            if (response.statusCode === 200) {
               // Login
                userAttr = response.data.userData.attributes;
                var hi = 'Hi, '  + userAttr.displayName;

                // Change Header
                $('#aol-header-auth-link').html(hi + '. Logout');

                // Change all other mini-profiles
               $('div.login .mini-profile b.toggle-auth').each(
                    function() {

                        $(this).children('b:first').html(hi + ' (');

                        $(this).children('a').attr('class', 'auth-logout-link').html('Sign Out');

                        $(this).children('b:last').html(')');
                    }
               );

               if (response.data.userData.attributes.pictureUrl) {
                   $('.mini-profile .aol-comments-avatar-img').attr('src', userAttr.pictureUrl);
               }

                if($.cookie('pets') == null && false) {

			$.post("/login/",
			          {"oat":response.data.token.a,
                                   "loginId":userAttr.loginId,
                                   "displayName":userAttr.displayName
                                  },
                                  function(data) {
                                      //console.log(data);
                                      if (!data || !data.u) { return; }
                                      var uploadForm = $('#photouploadform');
                                      if (uploadForm && uploadForm.hasClass('hide')) {
                                          uploadForm.attr('src', '/photouploadform');
                                          uploadForm.removeClass('hide');

                                          $('#fakeUpload').remove();
                                      }
                                 },
                                  "json");
		}

                var loginError = $('fieldset.user div.error:first');
                if (loginError) {
                    loginError.closest('fieldset').removeClass('error');

                    $('fieldset.user div.legend').removeClass('hide');

                    loginError.hide();
                }

                $('#my-profile-link').removeClass('hide');

            } else {
               
                //console.log('dumping pets cookie');

                // Logout
                $.cookie('pets', null, {path: '/', domain: 'www.pawnation.com'});

                // Change Header
                $('#aol-header-auth-link').html('Sign In / Register');

                $('#my-profile-link').addClass('hide');

                var addPetLink = $('#aol-addpet-auth-link');
                if (addPetLink) {

                    $('.mini-profile b.toggle-auth b:first').html('');
              
                    $('.mini-profile b.toggle-auth a').attr('class', 'auth-login-link').html('Sign In &raquo;<b class="auth-icons"></b>');

                    $('.mini-profile b.toggle-auth b:last').html('');
  
                    $('.mini-profile .aol-comments-avatar-img').attr('src', 'http://o.aolcdn.com/os/aol-comments/images/comment-bubble');
               
                    $('#emailAddress').attr('value', '');
              }
          }  
}

$('#aol-header-auth-link').multiAuth({devId: siteDevId, getTokenCallback: handleAuth});

$('#aol-comments-auth-link').click(function() {
    $('#aol-header-auth-link').trigger('click');
});

$('#aol-addpet-auth-link').click(function() {
    $('#aol-header-auth-link').trigger('click');
});

})(jQuery);



