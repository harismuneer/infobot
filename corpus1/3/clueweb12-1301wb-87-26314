    <script type="text/javascript" src="/outils/js?js=/js/calculateur.js,/js/ag.js&expires=1331750439624"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            jQuery.fn.qtip.zindex = 9999999;

            initCalculateur();
            $(".tabs-calculateur").tabs();
            $(".calculator select").selectmenu();

            $("#tour").tabs();
            $("#tabs").tabs({
                show: function(event, ui) {
                    // lazy load images
                    var panel = $(ui.panel);
                    if($(ui.tab).attr("href") == "#configuration"){
                        panel = $(ui.panel).find("ul.color-list-wrapper");
                    }

                    panel.find("img").each(function(){
                        var $this = $(this);
                        var original = $this.attr("original");
                        if(original != null && original.length > 0){
                            $this.attr("src", original);
                        }
                    });
                }
            });

            $("#exteriorColor .color-wrapper a").colorbox({
                speed: 500,
                current: "Image {current} / {total}",
                returnFocus: false,
                maxWidth: "90%",
                maxHeight: "90%"
            });
            $("#interiorColor .colorInt-wrapper a").colorbox({
                speed: 500,
                current: "Image {current} / {total}",
                returnFocus: false,
                maxWidth: "90%",
                maxHeight: "90%"
            });

            $("#exteriorColor").tabs({
                show: function(event, ui) {
                    // lazy load images
                    $(ui.panel).find("img").each(function(){
                        var $this = $(this);

                        var src = $this.attr("src");
                        var original = $this.attr("original");
                        if(src != original){
                            $this.attr("src", original);
                        }
                    });
                }
            });
            $("#exteriorColor .color-wrapper").tabs();
            $("#interiorColor").tabs({
                show: function(event, ui) {
                    // lazy load images
                    $(ui.panel).find("img").each(function(){
                        var $this = $(this);

                        var src = $this.attr("src");
                        var original = $this.attr("original");
                        if(src != original){
                            $this.attr("src", original);
                        }
                    });
                }
            });
            $("#interiorColor .colorInt-wrapper").tabs();
            
            $("#slideshow-gallery").awesomeGallery({autoPlay: false, play: true, zoom: true, prevTemplate: '<a><span></span></a>', nextTemplate: '<a><span></span></a>', zoomTemplate: '<a class="ag-zoom"><span></span></a>', loading: '/img/lasalle/loading.gif'});

            var trimsSelect = $("#trimsSelect");
            $("#trimsSelect").selectmenu();

            var links = new Array();
          
          links[0]='http://www.lasalleford.com/en/new/car/2012/ford/edge/se-fwd/312';
          
          links[1]='http://www.lasalleford.com/en/new/car/2012/ford/edge/sel-fwd/312';
          
          links[2]='http://www.lasalleford.com/en/new/car/2012/ford/edge/sel-awd/312';
          
          links[3]='http://www.lasalleford.com/en/new/car/2012/ford/edge/limited-fwd/312';
          
          links[4]='http://www.lasalleford.com/en/new/car/2012/ford/edge/limited-awd/312';
          
          links[5]='http://www.lasalleford.com/en/new/car/2012/ford/edge/sport-awd/312';
          
            trimsSelect.change(function(){
                window.location = links[$(this).prop("selectedIndex")];
            });

            $("#formApp input").placeholder();

            initForm($("#formApp"), "", null, "Thank You for your interest. Your request has been received and we will be contacting you shortly for a follow-up. Your complete satisfaction is our priority.", null,
                function(context, form){
                    form.find("input, textarea").qtip("destroy");
                    $('.loadable').show();
                    $('.loadable').empty().html('<img src="'+context+'/img/lasalle/loading.gif" />');
                    form.find(".ui-state-error").removeClass("ui-state-error");

                    return true;
                },
                function(context, form, data){
                    if(data != null){
                        var hasErrors = false;
                        $.each(data, function(i,error){
                            var lControllerId = error["id"];
                            var lMessage = error["value"];
                            var lLevel = error["type"];

                            if(lLevel != 0){
                                hasErrors = true;

                                form.find("[name='"+lControllerId+"']").qtip({
                                    content: {
                                       text: lMessage
                                    },
                                    position: {
                                       my: 'left center',
                                       at: 'right center'
                                    },
                                    style: {
                                       classes: 'ui-tooltip-red'
                                    }
                                });
                                form.find("[name='"+lControllerId+"']").addClass("ui-state-error");
                            }
                        });
                    }
                }
            );

            //Load google plus one script
            window.___gcfg = {lang: 'en'};
            (function() {
                var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
                po.src = 'https://apis.google.com/js/plusone.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
            })();


            window.fbAsyncInit = function() {
                FB.init({status: true, cookie: true, xfbml: true});
            };
            (function() {
                var e = document.createElement('script'); e.async = true;
                e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js#xfbml=1';
                document.getElementById('fb-root').appendChild(e);
            }());
        });
    </script>

