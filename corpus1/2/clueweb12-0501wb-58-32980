    (function($) {
        var createNotification = function() {
            $.post('/wishlist/Create.aspx', { s: 'Ford_Edge_LS.Available_LS.Transferring_LS.Pending-Review' }, function() {
                var text = 'You will receive email alerts about this search.  You can manage your email alerts in the <a href="/myWishlist.xhtml">Wishlist section</a>.'
                var html = reportSummary = '<table style="width:250px" border=0 cellpadding=0 cellspacing=0 class=txt10Red><tr><td>' + text + '</td></tr><tr><td align=right></td></tr></table>';
                openStaticModalWindow(html, 'Email Alert', 400, 100);
            });
        };
        $(function($) {
            $('.emailAlert').click(function() {
                checkLogin(function(loggedIn) {
                    if (!loggedIn)
                    //showLogin({ callBack: createNotification });
                        showSubscribe();
                    else
                        createNotification();
                });
                return false;
            });
        });
    })(jQuery);

    function showSubscribe() {        
        openWindow({
            url: "/EmailAlertSubscription/Index.aspx?sp=Ford_Edge_LS.Available_LS.Transferring_LS.Pending-Review",
            modal: true,
            closable: true,
            width: 400,
            height: 180,
            title: "Email Alert"
        });        
    }              
