         <div style='line-height: 90%; font-size: 18pt; color: #555; font-weight: bold; border-bottom: solid 1px #444; width: 98%;'>John McCain vs.  Barack Obama <span style="padding-left:50px"><small><small><small><em>Last updated: 11-04-08 (FINAL PRE-ELECTION)</em></small></small></small></small></span></div>



        <div style='border: solid 0px #555555;'>
        <SCRIPT>
            // Initial Views: 2000, NEUTRAL, SWING
            cookie_vals = "cookieset=0";
            var states = new Array("AK","AL","AR","AZ","CA","CO","CT","DC","DE","FL","GA","HI","IA","ID","IL","IN","KS","KY","LA","MA","MD","ME","MH","MI","MN","MO","MT","NC","ND","NE","NH","NJ","NM","NV","NY","OH","OK","OR","PA","RI","SC","SD","TN","TX","UT","VA","VI","VT","WA","WI","WV","WY","MS");

            var state_index = new Array();
            var state_settings = new Array();
            var state_vals = new Array();
            var init_state_string = "";
            var state_string = "";

            for (var i = 0; i < states.length; i++)
            {
                state = states[i];
                state_index[state] = i;

                init_state_string = init_state_string + "-";
            }

            state_string = getCookie("state_string");
            if( state_string == null ) state_string = init_state_string;

            //alert(state_string);

            function set_state(state,val)
            {
                idx = state_index[state];

                state_string = state_string.substring(0,idx) + val + state_string.substring(idx+1);
                //alert("idx: " + idx + " state_string: " + state_string);

                setCookie("state_string", state_string, 365 );
            }

            function reset_state()
            {
                state_string = init_state_string;
                setCookie("state_string", state_string, 365 );
            }

            init_view = getCookie("initial_state");
            if( init_view != '2004' && init_view != 'SWING' && init_view != 'NEUTRAL' ) init_view = '2004';
            else cookie_vals = "cookieset=1&initialview=" + init_view;

            var got_states = false;
            for (var i = 0; i < states.length; i++)
            {
                state = states[i];
                idx = state_index[state];
                this_st = state_string.charAt(idx);


                if( this_st == 'R' || this_st == 'D' || this_st == 'S' )
                {
                    if( !got_states )
                    {
                      cookie_vals = "cookieset=1&initialview=" + init_view;
                    }
                    got_states = true;
                    cookie_vals = cookie_vals + "&set" + state + "=" + this_st;
                }
            }



            //document.write("value=\""+cookie_vals+"\"");

            // infomap.swf
            var map_name = '/imgs/pollingmap91608.swf';

            insert_map(map_name,cookie_vals);
            //alert('Inserting Map' + map_name + " cookie_vals: " + cookie_vals);

       </SCRIPT>
         <br>
