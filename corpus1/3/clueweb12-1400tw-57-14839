function countdown() {
	var t = 1301702400 - Math.floor(new Date().getTime()/1000);
	var h = Math.floor(t/3600);
	var m = Math.floor((t - 3600*h)/60);
	var s = t - 3600*h - 60*m;
	var r = "";
	if (t > 0) {
		if (h > 9) r += h+":"; else r += "0"+h+":";
		if (m > 9) r += m+":"; else r += "0"+m+":";
		if (s > 9) r += s; else r += "0"+s;
	}
	document.getElementById('countdown-clock').innerHTML = r
	setTimeout("countdown()", 1000);
}
