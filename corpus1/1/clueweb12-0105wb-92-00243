			<script type="text/javascript">// <![CDATA[
			$(window).load(function() {
				updateSocialLinks();
			});
			$(function() {
				startSlide = days_between(new Date(2011,3,12),new Date()) + 1;
				if (window.location.hash) startSlide = window.location.hash.replace('#','');

				$('#bucketslider').slides({
					next: 'next',
					prev: 'prev',
					generateNextPrev: true,
					generatePagination: false,
					start: startSlide,
					animationComplete: function(current) {
						window.location.hash = '#' + current;
						updateSocialLinks();
					}
				});
			});
			function updateSocialLinks() {
				var newlink = $('#bucketslider .slide').filter(":visible").find('.url').attr('href');
				$('.item_options .facebook').attr('href','http://www.facebook.com/sharer.php?u='+newlink);
				$('.item_options .twitter').attr('href','http://twitter.com/home/?status=I checked off another item on the Civil War To Do List! '+newlink+' via @civilwartrust');
			};
			function days_between(date1, date2) {
				var ONE_DAY = 1000 * 60 * 60 * 24;
				var date1_ms = date1.getTime();
				var date2_ms = date2.getTime();
				var difference_ms = Math.abs(date1_ms - date2_ms);
				return Math.round(difference_ms / ONE_DAY);
			};
			// ]]></script>
