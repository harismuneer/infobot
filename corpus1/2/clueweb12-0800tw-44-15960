function onSaveArticleRatingSuccess(result, ctx) {
  if(result != 'no change') {
    var resSplit = result.split("|");
    var moduleID = resSplit[0];
    var articleID = resSplit[1];
    var avgRatingScore = resSplit[2];
    var ratingCount = resSplit[3];
    var ratingMessage = resSplit[4];
    var avgRatingHtml = resSplit[5];
    var userRatingHtml = resSplit[6];
    var spanAvgRatingScore = document.getElementById("avgratingscore_" + moduleID + "_" + articleID);
    var spanRatingCount = document.getElementById("ratingcount_" + moduleID + "_" + articleID);
    var spanRatingMessage = document.getElementById("ratingmsg_" + moduleID + "_" + articleID);
    var spanAverageRating = document.getElementById("avgrating_" + moduleID + "_" + articleID);
    var spanUserRating = document.getElementById("userrating_" + moduleID + "_" + articleID);
    if(spanAvgRatingScore) spanAvgRatingScore.innerHTML = avgRatingScore;
    if(spanRatingCount) spanRatingCount.innerHTML = ratingCount;
    if(spanRatingMessage) spanRatingMessage.innerHTML = ratingMessage;
    if(spanAverageRating) spanAverageRating.innerHTML = avgRatingHtml;
    if(spanUserRating) spanUserRating.innerHTML = userRatingHtml;
  }
}
function onSaveArticleRatingFailure(result, ctx) {
  alert("An error occurred when saving your rating.  Please try again.\n" + result);
}
