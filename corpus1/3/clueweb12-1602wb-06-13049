			(function () {
				
                    var analyticsAccounts = jQuery.trim('UA-11178976-4').split(','),
				 	googleAnalyticsSetDomainName = ('false' !== 'false' ? 'false' : 'none'),
					googleAnalyticsSetAllowLinker = ('false' === 'true'),
					gaManager = new DDC.GoogleAnalyticsManager(analyticsAccounts);

					gaManager.add('UA-9899509-3');
															
											gaManager._setDomainName(googleAnalyticsSetDomainName);
				
						if (googleAnalyticsSetAllowLinker) {
							gaManager._setAllowLinker('true');
						}
					
						gaManager.disable('UA-9899509-3')
							._trackPageview()
							.disableAll()
							.enable('UA-9899509-3')
							._trackPageview('/CA/BC/en_CA/PR/null/USED_VEHICLE_DETAILS')
							.enableAll();
												
				window.gaManager = gaManager;
				window.gaPageTracker = gaManager.get(analyticsAccounts[0]);
				window.gaDDCPageTracker = gaManager.get('UA-9899509-3');
			}());
		
					
