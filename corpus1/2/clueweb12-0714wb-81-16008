	function loadVideo(playerUrl, autoplay) {
		/*
		swfobject.embedSWF(
		  playerUrl + '&rel=0&border=0&fs=1&showinfo=0&autoplay=' +
		  (autoplay ? 1 : 0), 'player', '346', '246', '9.0.0', false,
		  false, { allowfullscreen: 'true' });
		*/
		
