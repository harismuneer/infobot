slides[0].elems[4] = new newElement("D8", "timer", "0.0", "div", "255", "0", "0", "50", "false", "up", wavesElements, "revealtrans(Duration=0,Transition=0)  shadow(color=#000000,direction=225,enabled=1)");
slides[0].elems[5] = new newElement("D9", "timer", "0.0", "div", "255", "0", "0", "50", "false", "up", wavesElements, "revealtrans(Duration=0,Transition=0)  glow(color=#000000,strength=5,enabled=1)");
slides[0].elems[6] = new newElement("I2", "timer", "0.0", "image", "255", "0", "0", "50", "false", "up", wavesElements, "revealtrans(Duration=0,Transition=0) ");
slides[0].elems[7] = new newElement("I4", "timer", "0.0", "image", "255", "0", "0", "50", "false", "up", wavesElements, "revealtrans(Duration=0,Transition=0) ");
slides[0].elems[8] = new newElement("D14", "timer", "0.0", "div", "255", "0", "0", "50", "false", "up", wavesElements, "revealtrans(Duration=0,Transition=0)  glow(color=#ffff00,strength=2,enabled=1)");
