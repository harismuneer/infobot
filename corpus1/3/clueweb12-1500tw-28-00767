function usefontdef()
{
	document.getElementById('storybody').style.fontSize = "12px";
	document.getElementById('byline').style.fontSize = "12px";
	document.getElementById('fontdef').style.color = "#000000";
	document.getElementById('fontmid').style.color = "#AAAAAA";
	document.getElementById('fontbig').style.color = "#AAAAAA";
}

function usefontmid()
{
	document.getElementById('storybody').style.fontSize = "14px";
	document.getElementById('byline').style.fontSize = "14px";
	document.getElementById('fontdef').style.color = "#AAAAAA";
	document.getElementById('fontmid').style.color = "#000000";
	document.getElementById('fontbig').style.color = "#AAAAAA";
}

function usefontbig()
{
	document.getElementById('storybody').style.fontSize = "16px";
