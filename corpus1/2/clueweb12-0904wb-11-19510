function popUp(url, width, height)
{
settings=
"toolbar=no,location=no,directories=no,"+
"status=no,menubar=no,scrollbars=yes,"+
"resizable=no,width="+width+",height="+height;

MyNewWindow=window.open(url,"",settings);
}

