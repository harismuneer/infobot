<script type="text/javascript">
	function trackcomments(_commentId, _pageUrl, _actionTaken, _commentMessage, _userName, _userId) {
		$.ajax({
			type: 'POST',
			url: '/fb/track_comments.php',
			data: {
				commentId: _commentId,
				pageUrl: _pageUrl,
				actionTaken: _actionTaken,
				userName: _userName,
				userId: _userId,
				commentMessage: _commentMessage
			}
		});
		return;
	}

  // Facebook
	window.fbAsyncInit = function() {
    FB.init({appId: '116910738327239', status: true, cookie: true, xfbml: true, oauth: true});
		FB.Event.subscribe('edge.create', function(targetUrl) {
			pageTracker._trackSocial('Facebook', 'Like', targetUrl);
		});
		FB.Event.subscribe('edge.remove', function(targetUrl) {
			pageTracker._trackSocial('Facebook', 'Unlike', targetUrl);
		});
		FB.Event.subscribe('comment.create', function(targetUrl) {

			var commentQuery = FB.Data.query("SELECT text, fromid FROM comment WHERE post_fbid='" + targetUrl.commentID +
					"' AND object_id IN (SELECT comments_fbid FROM link_stat WHERE url='" + targetUrl.href + "')");
			var userQuery = FB.Data.query("SELECT name FROM user WHERE uid in (select fromid from {0})", commentQuery);
			FB.Data.waitOn([commentQuery, userQuery], function () {
				var commentRow = commentQuery.value[0];
				var userRow = userQuery.value[0];
				trackcomments(targetUrl['commentID'], targetUrl['href'], 'create', commentRow.text, userRow.name, commentRow.fromid);
			});
			
			pageTracker._trackSocial('Facebook', 'Comment created');

		});
		FB.Event.subscribe('comment.remove', function(targetUrl) {
			trackcomments(targetUrl['commentID'], targetUrl['href'], 'remove', '', '', '');
			pageTracker._trackSocial('Facebook', 'Comment removed');
		});
		FB.Event.subscribe('message.send', function(targetUrl) {
			pageTracker._trackSocial('Facebook', 'Send', targetUrl);
		});
  };
  (function() {
    var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
  }());
</script><noindex follow>
