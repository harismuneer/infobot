			<script type="text/javascript">
								
				function disableField(whichField){
					document.getElementById(whichField).readOnly="true";
					document.getElementById(whichField).style.color="#000";
				}
				
				function initialize() {
									}
				
				function externalLinks() {
					if (!document.getElementsByTagName) {
						return;
					}
					var anchors = document.getElementsByTagName("a");
					for (var i = 0; i < anchors.length; i++) {
						var anchor = anchors[i];
						if ((anchor.getAttribute("href") && anchor.getAttribute("rel") != null && (anchor.getAttribute("rel").indexOf("external") >= 0 || anchor.getAttribute("rel").indexOf("nofollow") >= 0)) && anchor.getAttribute("href").indexOf(wgServer) == -1) {
							anchor.target = "_blank";
							//alert(anchor.getAttribute("href") + '\n' + wgServer);
						}
					}
				}
				
				function setCookie(cookieName,cookieValue,expiresDate,path){
					expiresParam = '';
					pathParam = '';
					if (expiresDate != ''){
						expiresParam = ";expires=" + expiresDate;
					}
					if (path != ''){
						pathParam = ";path=" + path;
					}
					document.cookie = "" + cookieName + "=" + cookieValue + expiresParam + pathParam + "";
				}
				function getCookie(whichCookie){
					var cookieString = document.cookie;
					var cookieValue = "";
					//alert("cookieString: " + cookieString);
					if (cookieString.length > 0 && cookieString.indexOf(whichCookie) >= 0) {
						var cookiePosition = cookieString.indexOf(whichCookie);
						//alert("cookiePosition: " + cookiePosition);
						var endPositionCookiePair = cookieString.indexOf(";",cookiePosition);
						//alert("endPositionCookiePair(before): " + endPositionCookiePair);
						if (endPositionCookiePair == -1) {
							endPositionCookiePair = cookieString.length;
						}
						//alert("endPositionCookiePair(after): " + endPositionCookiePair);
						var cookiePairString = cookieString.substring(cookiePosition,endPositionCookiePair);
						//alert("cookiePairString: " + cookiePairString);
						var cookiePairArray = cookiePairString.split("=");
						//alert("cookiePairArray[1]: " + cookiePairArray[1]);
						//alert("cookiePairArray[1].length: " + cookiePairArray[1].length);
						
						if (cookiePairArray[1].length > 0) {
							cookieValue = cookiePairArray[1];
						}
						else {
							cookieValue = "";
						}
					}
					else {
						cookieValue = "";
					}
					return cookieValue;
				}
				
				function getDefaultUsername(){
					//return document.cookie;
					return "";
				}
				function headerEmailPassword(){
					if (document.userlogin && (document.userlogin.wpName.value).length > 0) {
						if (document.userlogin.wpName.value == 'Username') {
							document.h_userlogin.wpName.value = '';
						}
						else {
							document.h_userlogin.wpName.value = document.userlogin.wpName.value;
						}
					}
					else {
						if ((document.h_userlogin.wpName.value).length == 0 || document.h_userlogin.wpName.value == "Username") {
							document.h_userlogin.wpName.value = getCookie("UserName");
						}
					}
					//alert(document.h_userlogin.wpName.value);
					document.h_userlogin.submit();
					//return;
				}
				
				function getQuerystringValue(valueToRead) {
					myQueryString = document.location.search;
					tempQueryString = myQueryString.toLowerCase();
					valueToRead = valueToRead.toLowerCase();
					if (myQueryString == "" || tempQueryString.indexOf(valueToRead + '=') == -1) myQueryString = top.document.location.search;
					if (myQueryString == "") return -1;
					tempQueryString = myQueryString.toLowerCase();
					if (tempQueryString.indexOf(valueToRead + '=') != -1) {
						myValue = "";
						valuePositionInString = tempQueryString.indexOf(valueToRead + '=')+(valueToRead+'=').length;
						valueEndPosition = tempQueryString.indexOf('&',valuePositionInString);
						if (valueEndPosition != -1) {
							myValue = myQueryString.substring(valuePositionInString,valueEndPosition);
						}
						else {
							valueEndPosition = tempQueryString.length;
							myValue = myQueryString.substring(valuePositionInString,valueEndPosition);
						}
						return myValue;
					}
					else {
						return -1;
					}
				}
				
							</script>
