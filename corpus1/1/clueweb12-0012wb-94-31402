function setc(name,value) {         // once
    var exp = new Date()
    curdate = exp
    exp.setTime(exp.getTime() + 5000 * 24 * 60 * 60 * 1000)
    var cdate = new Date()
    var curcookie = name + "=" + escape(value) + "; expires=" + exp.toGMTString() + "; path=/; domain=.bbhq.com"
    document.cookie = curcookie;
}
