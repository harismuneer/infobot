                    var bedName = $(this).val();
                    if (unspecifiedTrimChecked && bedName == 'Unspecified' && !unspecifiedTrimAttribute) {
                        unspecifiedTrimAttribute = true;
                    }
                    scopedBeds.push(bedName);
                });
                this.beds = scopedBeds;
            } else{
                if (unspecifiedTrimChecked) {
                    unspecifiedTrimAttribute = true;
                }
            }

            if (unspecifiedTrimChecked && !unspecifiedTrimAttribute) {
                unspecifiedTrimChecked = false;
            } else {
                unspecifiedTrimAttribute = false;
            }

            var checkedTrimRearWheelsCount = $('#truck_rear_wheel_filter input:checked').size();
            var totalRearWheelsCount = $('#truck_rear_wheel_filter input').size();
            var scopedRearWheels = new Array();

            if (totalRearWheelsCount != checkedTrimRearWheelsCount && checkedTrimRearWheelsCount != 0) {
                anyTrimFilterAttrChecked = true;
                $('#truck_rear_wheel_filter input:checked').each(function(index, value) {
                    var rearWheelName = $(this).val();
                    if (unspecifiedTrimChecked && rearWheelName == 'Unspecified' && !unspecifiedTrimAttribute) {
                        unspecifiedTrimAttribute = true;
                    }
                    scopedRearWheels.push(rearWheelName);
                });
				this.rearWheels = this.rearWheels;
            } else {
                if (unspecifiedTrimChecked) {
                    unspecifiedTrimAttribute = true;
                }
            }

            if (unspecifiedTrimChecked && !unspecifiedTrimAttribute) {
                unspecifiedTrimChecked = false;
            } else {
                unspecifiedTrimAttribute = false;
            }

            var checkedTrimDoorsCount = $('#truck_door_filter input:checked').size();
            var totalDoorsCount = $('#truck_door_filter input').size();
            var scopedDoors = new Array();

            if (totalDoorsCount != checkedTrimDoorsCount && checkedTrimDoorsCount != 0) {
                anyTrimFilterAttrChecked = true;
                $('#truck_door_filter input:checked').each(function(index, value) {
                    var doorName = $(this).val();
                    if (unspecifiedTrimChecked && doorName == 'Unspecified' && !unspecifiedTrimAttribute) {
                        unspecifiedTrimAttribute = true;
                    }
                    scopedDoors.push(doorName);
                });
				this.doors = scopedDoors;
            } else {
                if (unspecifiedTrimChecked) {
                    unspecifiedTrimAttribute = true;
                }
            }

            if (unspecifiedTrimChecked && !unspecifiedTrimAttribute) {
                unspecifiedTrimChecked = false;
            }
            // make sure that the Unspecified Trim is removed if any of the
            // other trim attributes do not have Unspecified checked

            if (!unspecifiedTrimChecked) {

             var foundUnspecifiedTrim = false;
                for (var i=0; i< this.styles.length - 1; i++) {
                    if (this.styles[i] == 'Unspecified trim') {
                        this.styles[i] = this.styles[i+1];
                        foundUnspecifiedTrim = false;
                    }
                }
                if (foundUnspecifiedTrim) {
                    this.styles.length = this.styles.length - 1;
                } else{
                    if (this.styles[this.styles.length -1] == 'Unspecified trim') {
                        this.styles.length = this.styles.length - 1;
                    }
                }
            }

            if (!anyTrimFilterAttrChecked) {
                this.styles = [];
                this.cabins = [];
                this.beds = [];
                this.rearWheels = [];
                this.doors = [];
            }
            this.frequency = $("#listingsSubscriptionFrequency").val();
            if (this.frequency == null || this.frequency == '') {
                this.frequency = $("input[name=frequency]:checked").val();
            }
        };
  </script>

