	$(document).ready(function() {
		$(".biggerText").click(function(){
			var currentFontSize = $('#lyric .lyric-center p').css('font-size');
			var currentFontSizeNum = parseInt(currentFontSize);
			if(currentFontSizeNum < 20){
				var newFontSize = currentFontSizeNum + 2;
				$('#lyric .lyric-center p').css('font-size', newFontSize);
			}
			return false;
		 });

		  // Decrease Font Size
		$(".smallerText").click(function(){
			var currentFontSize = $('#lyric .lyric-center p').css('font-size');
			var currentFontSizeNum = parseInt(currentFontSize);
			if(currentFontSizeNum > 12){
				var newFontSize = currentFontSizeNum - 2;
				$('#lyric .lyric-center p').css('font-size', newFontSize);
			}
			return false;
		});
	});
