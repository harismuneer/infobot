<script type="text/javascript">
var variants = 0;

$(document).ready(function() {
    var auctionId = 173;
    
    	// format auction end time with countdown
	var auctionCurObj = {"currentTime":"04\/13\/2012 22:26:59"};
	var auctionEndObj = {"auctionId":173,"auctionEndTime":"01\/09\/2012 20:45:00"};
	var auctionEndTime = null;
	var dateTimeCounter = null;
	
	try {
		if('auctionEndTime' in auctionEndObj && 'auctionId' in auctionEndObj) {
			auctionId = ((isDefined(auctionEndObj['auctionId'])) ? parseInt(auctionEndObj['auctionId']) : null);
			auctionCurTime = ((isDefined(auctionCurObj['currentTime'])) ? auctionCurObj['currentTime'].toString() : null);
			auctionEndTime = ((isDefined(auctionEndObj['auctionEndTime'])) ? auctionEndObj['auctionEndTime'].toString() : null);
			$('p.auctionEndTime').html(auctionEndTime.toString());// just in case counter cannot run?
		} else {
			throw 'Missing or corrupted Auction data!';
		}
		
		// instantiate counter...
		dateTimeCounter = new DateTimeCounter();
		dateTimeCounter.setClassElements('auctionEndTime')
			.setStartDate(auctionCurTime.toString())
			.setTargetDate(auctionEndTime.toString())
			.setStepIncrement(-1)
			.setEndMessage('This Auction has now finished!')
			.setTargetDateIsGMT(false)
			.init();
		
	} catch(ex) {//ERRORS? because javascript throws exception on array key missing!
		exceptionHandler(ex, 'Missing Auction data');
		return false;
	}
	
        
	// load the social media share panel (load like this to avoid delays in the page load time)
	$('#sharePanel').load('/auction/get-share-panel', {auctionId: auctionId});// fail silently
	
	delete(auctionEndObj);
	delete(auctionId);
	delete(auctionEndTime);
	
	//show full images in fancybox on mouseup
	$('a.defaultProdImg').mouseup(function() {
		if ($(this).attr('href').length > 0) {
			$(this).fancybox({
				padding: 0,
				margin: 0,
				href: $(this).attr('href').toString(),
				type: 'image',
				transitionIn: 'elastic',
				transitionOut: 'elastic',
				speedIn: 600,
				speedOut: 200,
				overlayShow: true,
				hideOnOverlayClick: true,
				hideOnContentClick: true,
				titleShow: false,
				autoScale: true,
				autoDimensions: true
			});
		}
	});
	
	// swap thumbs for main image and update output
	$('div.product-thumbs a.productThumb').click(function(event) {
		//disable default event...with IE8 fix
		if(event.preventDefault) {
			event.preventDefault();
		} else {
			event.returnValue = false;
		}
		try {
			if ($(this).attr('href').length > 0 && 
				$(this).attr('title').length > 0) {
				$('a.defaultProdImg').attr('href', $(this).attr('href').toString().replace('_websiteProductGalleryMain', ''));
				$('img.websiteProductGalleryMain').attr('src', $(this).attr('href').toString());
				$('img.websiteProductGalleryMain').attr('alt', $(this).attr('title').toString());
				$('img.websiteProductGalleryMain').attr('title', $(this).attr('title').toString());
			} else {
				throw 'The required image attributes do not exist.';
			}
		} catch (e) {
			exceptionHandler('Unable to show larger image version', e);
		}
	});
	
	$('ul#tabs li a').click(function(event){
		//disable default event...with IE8 fix
		if(event.preventDefault) {
			event.preventDefault();
		} else {
			event.returnValue = false;
		}
		var targetHref = $(this).attr('href').toString();
		if ($(targetHref).length > 0) {
			$('div.tabs-wrap div.subTab').each(function(index) {
				if ($(this).is(':visible')) {
					$(this).hide();
				}
			});
			$(targetHref).fadeIn();
		}
		delete targetHref;
		return false;
	});
		// view full bid list
		$('.showBidList').fancybox({
			padding: 10,
			margin: 10,
			width: 750,
			height: 465,
			href: '/auction/view-bids?auctionId=173',
			type: 'iframe',
			speedIn: 600,
			speedOut: 200,
			overlayShow: true,
			hideOnOverlayClick: false,
			hideOnContentClick: false,
			titleShow: false,
			autoScale: false,
			autoDimensions: false
		});
			// misc functionality
		$('a.cmdWatch, a#cmdBuyNow, form#bidPlace #cmdPlaceBid').click(function(event) {
			if (event) {
				//disable default event...with IE8 fix
				if(event.preventDefault) {
					event.preventDefault();
				} else {
					event.returnValue = false;
				}
			}
			
			window.location = "/customer/login";
			
			return false;
		});
	});
</script>

	</div>
	
	
