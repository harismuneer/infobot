jQuery(document).ready(function() {
		$("form#add_comment_form").unbind();
	$("form#add_comment_form").submit(function(e) {
		ajaxLoadingPanel_show();
		var data = '';
		$("form#add_comment_form input, form#add_comment_form textarea, form#add_comment_form select").each(function(i){
			if ((this.type=="checkbox" || this.type=="radio"))
			{
				if (this.checked)
				{
					data += encodeURIComponent(this.name)+'='+encodeURIComponent(this.value)+'&';
				}
			}
			else data += encodeURIComponent(this.name)+'='+encodeURIComponent(this.value)+'&';
		});
		data += 'ajax=1';
		var action = '';
		if ($("form#add_comment_form").attr('action').length)
			action = $("form#add_comment_form").attr('action');
		else action = 'http://www.kovideo.net/hush-hush-i-will-survive-remix-lyrics-the-pussycat-dolls-742294.html';

		$.ajax({
		  type: "POST",
		  url: action,
		  data: data,
		  success: function(msg) {
			ajaxLoadingPanel_hide();
			if (msg.indexOf('var keep_' + 'output = true;')==-1)
			{	
				if ('prepend'=='append')
				{
					$('#post_comment').html($('#post_comment').html()+msg);
				}
				else if ('prepend'=='prepend')
				{
					$('#post_comment').html(msg+$('#post_comment').html());
				}
				else 
				{
					//alert('replacing post_comment');
					$('#post_comment').html(msg);
				}
			}
			else $('#messages').html(msg);
			initializeNavigationBar(1);
		  },
		  failure: function(msg)
		  {
			ajaxLoadingPanel_hide();
			alert(msg);
		  }
		 });
		 e.preventDefault();
	});
});

