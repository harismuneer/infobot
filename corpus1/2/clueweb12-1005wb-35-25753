      startList = function()
      {
        if (document.all&&document.getElementById)
        {
          navRoot = document.getElementById("dc-nav");
          for (i=0; i<navRoot.childNodes.length; i++)
          {
            node = navRoot.childNodes[i];
            if (node.nodeName=="LI")
            {
              node.onmouseenter=function()
              {
                this.className+="-selected";
              };
              node.onmouseleave=function()
              {
                this.className=this.className.replace("-selected", "");
              };

              var ulElement = node.getElementsByTagName("ul")[0];
              if(ulElement)
              {
                var iframeElement = ulElement.getElementsByTagName("iframe")[0];
                ulElement.style.display = "block";
                iframeElement.style.height = ulElement.offsetHeight;
                iframeElement.style.width = ulElement.offsetWidth - 2;
                ulElement.style.display = "";
              }
            }
          }
        }
      };
      function initializePageUI()
      {
        correctPNG();
      }

      YAHOO.util.Event.addListener(window, "load", startList);
      YAHOO.util.Event.addListener(window, "load", initializePageUI);

    </script>
