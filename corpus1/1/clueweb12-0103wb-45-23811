/*
* lowerThird
*/
function lowerThirdAd(id,params) {
var adObject = $('#'+id);
var speed = 500;
var cookieName = 'poppers123';
var cookieValue = 'dontshow';
var minutes = 0;
var close = 0;
var openheight = 105;
var closeheight = 0;
var timehandle = null;
if (params.minutes) {
minutes = params.minutes;
}
if (params.close) {
close = params.close;
}
if (params.openheight) {
openheight = params.openheight;
}
if (params.closeheight) {
closeheight = params.closeheight;
}
$(adObject).css('overflow','hidden');
$(adObject).css('position','fixed');
$(adObject).css('bottom','0px');
var adInner = $(adObject);
var adcookie = readCookie();
if (! adcookie) {
// find the hidden stuff
$(adInner).bind('mouseover',function() { 
$(adObject).css('height',openheight+'px');
clearTimeout(timehandle); // dont close when the mouse is in there
});
$(adInner).bind('mouseout',function() { 
$(adObject).css('height',closeheight+'px');
if (close) {
timehandle = setTimeout( function() {
$(adInner).hide('slide', {direction:'down'} ,speed); // restart the autoclose
},close);
}
});
if (params.speed) speed = params.speed;
$(adInner).show('slide',{direction:'down'},speed);
var adAreaClick = $('#ad')[0];
$(adAreaClick).css('cursor','pointer');
$(adAreaClick).bind('click',function() {
createCookie(720);
window.open('http://otto.developershed.com/www/delivery/ck.php?oaparams=2__bannerid=7064__zoneid=0__log=yes__cb=0bc1b59ce2__oadest=http%3A%2F%2Fwww.ironspeed.com%2Fproducts%2FRegistration.aspx%3Fc%3DDevShed');
return false;
//            document.location.href='http://otto.developershed.com/www/delivery/ck.php?oaparams=2__bannerid=7064__zoneid=0__log=yes__cb=0bc1b59ce2__oadest=http%3A%2F%2Fwww.ironspeed.com%2Fproducts%2FRegistration.aspx%3Fc%3DDevShed';
});
var closeHandle = $('#' + params.closeHandle)[0];
$(closeHandle).css('cursor','pointer');
$(closeHandle).bind('click',function() {
if (minutes) createCookie(minutes);
$(adInner).unbind('mouseover');
$(adInner).unbind('mouseout');
$(adInner).hide('slide', {direction:'down'} ,speed);
return false;
});
if (close) {
timehandle = setTimeout( function() {
$(adInner).hide('slide', {direction:'down'} ,speed); // start the autoclose
},close);
}
} else {
$(adObject).css('display','none');
}
function createCookie(minutes) {
if (minutes) {
var date = new Date();
date.setTime(date.getTime()+(minutes*60*1000));
var expires = "; expires="+date.toGMTString();
}
else var expires = "";
document.cookie = cookieName+"="+cookieValue+expires+"; path=/";
}
function readCookie() {
var nameEQ = cookieName + "=";
var ca = document.cookie.split(';');
for(var i=0;i < ca.length;i++) {
var c = ca[i];
while (c.charAt(0)==' ') c = c.substring(1,c.length);
if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
}
return null;
}
return {
createCookie: function(minutes) {
createCookie(minutes);
},
readCookie: function() {
readCookie();
},
eraseCookie: function() {
createCookie(-1);
}
}
}
</script>
