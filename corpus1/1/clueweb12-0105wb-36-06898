            }

            var checkedInstalledOptionIdsCount = $('#installedOptions_filter input:checked').size();
            if (checkedInstalledOptionIdsCount != 0) {
                var scopedInstalledOptionIds = new Array();
                $('#installedOptions_filter input:checked').each(function(index, value) {
                    scopedInstalledOptionIds.push($(this).val());
                });
                this.installedOptionIds = scopedInstalledOptionIds;
            } else {
                this.installedOptionIds = [];
            }

            var checkedTrimsCount = $('#trim_filter input:checked').size();
            var totalTrimsCount = $('#trim_filter input').size();
            var anyTrimFilterAttrChecked = false;
            var unspecifiedTrimChecked = false;

            this.trims = [];
            this.styles = [];
            this.cabins = [];
            this.beds = [];
            this.rearWheels = [];
            this.doors = [];

            if (totalTrimsCount != checkedTrimsCount && checkedTrimsCount != 0) {
                var scopedTrims = new Array();
                var scopedStyles = new Array();
                anyTrimFilterAttrChecked = true;
                unspecifiedTrimChecked = true
                $('#trim_filter input:checked').each(function(index, value) {

                    var trimName = $(this).val().split(",")[1];
                    scopedTrims.push(trimName);
                    scopedStyles.push(trimName);
                });
                this.trims = scopedTrims;
                this.styles = scopedStyles;
            } else {
                this.trims = [];
                this.styles = [];
            }

            var checkedTrimCabinsCount = $('#truck_cabin_filter input:checked').size();
            var totalCabinsCount = $('#truck_cabin_filter input').size();

            var unspecifiedTrimAttribute = false;

            var scopedCabins = new Array();
            if (totalCabinsCount != checkedTrimCabinsCount && checkedTrimCabinsCount != 0) {
                anyTrimFilterAttrChecked = true;

                $('#truck_cabin_filter input:checked').each(function(index, value) {

                    var cabinName = $(this).val();
                    if (unspecifiedTrimChecked && cabinName == 'Unspecified' && !unspecifiedTrimAttribute) {
                        unspecifiedTrimAttribute = true;
                    }
                    scopedCabins.push(cabinName);

                });
				this.cabins = scopedCabins;


            } else {
                if (unspecifiedTrimChecked) {
                    unspecifiedTrimAttribute = true;
                }
            }

            if (unspecifiedTrimChecked && !unspecifiedTrimAttribute) {
                unspecifiedTrimChecked = false;
            } else {
                unspecifiedTrimAttribute = false;
            }

            var checkedTrimBedsCount = $('#truck_bed_filter input:checked').size();
            var totalBedsCount = $('#truck_bed_filter input').size();
            var scopedBeds = new Array();
            if (totalBedsCount != checkedTrimBedsCount && checkedTrimBedsCount != 0) {
