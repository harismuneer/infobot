function fbdeauth(doblock, forceCancel, redirectUrl) {
    $.post('/user/account/action/facebook-deauth/', {forceCancel : forceCancel});
	FB.api(
	{
		method: 'auth.revokeAuthorization',
		uid: '',
		block: doblock
	},
	function(response) {
		if (redirectUrl) {
			window.location = redirectUrl;
		}
		else {
			location.reload();
		}
	}
	);
}

</script>


