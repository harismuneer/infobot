<script language="JavaScript">
function swapFlashLights(flashLightNum) {

for (x=1; x<=4; x++) {
	if (x != flashLightNum) {
		set= document.getElementById('set' + x);
		desc= document.getElementById('desc' + x);
		section= document.getElementById('section' + x);
		set.className= 'flashLightsOff';
		desc.className= 'descOff';
		section.className= 'sectionDefault';
		arrow= document.getElementById('arrow' + x);
		arrow.innerHTML= '&#9658;';
	} else {
			var arrObj=eval("set"+x);
			if(arrObj.length >0){
				for(i=0;i< arrObj.length;i++){
					if(arrObj[i])document.getElementById(arrObj[i]["id"]).src=arrObj[i]["src"];
				}
			}
			set= document.getElementById('set' + x);
			desc= document.getElementById('desc' + x);
			section= document.getElementById('section' + x);
			set.className= 'flashLightsOn';
			desc.className= 'descOn';
			section.className= 'sectionSelected';
			topSection= document.getElementById('section' + (x - 1));
			if (topSection != null) { topSection.className = 'sectionTop'; }
			sectionBg= document.getElementById('sections');
			sectionBg.style.backgroundImage= 'url(http://media.ignimgs.com/media/ign/images/img_around_arrow_' + x + '.gif)';
			arrow= document.getElementById('arrow' + x);
			arrow.innerHTML= '&#9660;';
	}
}
}
// onload, pick a random flashlight row to open
swapFlashLights(1+Math.floor(Math.random()*4));
</script>

    

