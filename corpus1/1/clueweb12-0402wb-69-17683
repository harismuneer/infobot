        <div style="padding:10px;">
    <div id="similarCarsSection" class="ui-corner-all" style="padding:10px; border:1px solid #ccc; background-color:#eee;">
      <h3>Add Similar Cars</h3>
      <div id="renderSimilarModelsSection">

    <script type="text/javascript">

        function showAddModelsToSearchResults() {
                        
            var options = {
            	modal: true,
            	title: "Add Listings to Search Results",
				resizable: false,
                draggable: false
    		};
                            
            $("#addModelsToSearchResultsPopup").dialog(options);
                            
        }

        function addModelListings(modelEntityId, startYear, endYear, isSuggestedCar, isUserChosenCar) {

            // following code triggers a number of UI events that cause logging
            // re-enabled by out-of-band logic once all listings have been loaded
            USAGE_ANALYZER.disable();

            relatedCarFilter.handleChange();
            listingsSearchManager.addEntity(modelEntityId, startYear, endYear, isSuggestedCar, isUserChosenCar);

            // mark as selected
            if ($('#relatedCheckbox' + modelEntityId).attr('checked')) {
                $('#relatedModelThumbnailEntity' + modelEntityId).addClass('selectedThumbnail');
            } else {
                $('#relatedModelThumbnailEntity' + modelEntityId).removeClass('selectedThumbnail');
            }
        }

        function addRelatedModelListings(modelEntityId, startYear, endYear) {
            addModelListings(modelEntityId, startYear, endYear, true, false);
        }

        function addSpecifiedModelListings(modelEntityId, startYear, endYear) {

            if (modelEntityId == null || modelEntityId == '' || modelEntityId.indexOf('d') == -1) {
                alert('Please select a model.');
                return;
            }

            if (modelEntityId == 'd923') {
                alert('Please select a different model.');
                return;
            }

            closeDialog('addModelsToSearchResultsPopup');

            if ($('#relatedModelThumbnailEntity' + modelEntityId).length == 0) {
                $.get('/Cars/inventorylisting/renderIncludeModelListingsButton.action',
                      {'entitySelectingHelper.selectedEntity': modelEntityId,
                       'startYear': startYear,
                       'endYear': endYear},
                      function(data) {
                          $(data).insertBefore('#similarModelClear');
                          $('#relatedCheckbox' + modelEntityId).attr('checked', true);
                          addModelListings(modelEntityId, startYear, endYear, false, true);
                      });
            }
        }

    </script>

