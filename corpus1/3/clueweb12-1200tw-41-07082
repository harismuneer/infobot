	<script Language="JavaScript">	
	var varCounter = 1;
	function showhideComment(counterVal){
		if((counterVal % 10) == 0)
			var varCounter1 = ((counterVal/10)-1);
		else
			var varCounter1 = (counterVal/10);
			
    	if(varCounter <= varCounter1)
         {          
    		var str = (varCounter*10)+1;
                if(counterVal < str+10 )
                var cmp = (counterVal+1);
                else
                var cmp = str+10;
                
                while(str < cmp) {
                var str2 = str.toString();                 
                var divId = "divComment"+str2;
                document.getElementById(divId).style.display="block";                               
                str++;
                }
                document.getElementById("view1").href = "#divComment+((varCounter*10)+1)";
                if(counterVal <= str2)
                {
                   document.getElementById("view1").style.cssText = 'display:none !important;'; 
                }
    			varCounter=varCounter+1;
         }
	}
	</script>	
	
