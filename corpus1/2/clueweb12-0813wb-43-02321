    <script type="text/javascript">
        var msgReceived = false;
        XD.receiveMessage(function(message) {
            var recStr = String(message.data);
            if (recStr.indexOf(',shopper') > -1) {
                var arr = recStr.split(','),
                newH = parseInt(arr[0]); //get the first item which is a number
                $('#stmPos').parent().find('iframe').css({ 'height': newH, 'marginTop': '10px' });
            }
            if (recStr.indexOf(',ad') > -1) {
                var arr = recStr.split(','),
                    newH = parseInt(arr[0]); //get the first item which is a number
                $('#hpPos').parent().find('iframe').addClass('stmAdHeight'); //add the class to the ad iframe
                $('.stmAdHeight').height(newH); //any object with this class is the new height
            }
            if ((!msgReceived) && (recStr.match("^" + "DISPLAYINTERSTITIAL_"))) {
                msgReceived = true;
                $("body").prepend("<iframe id='iframeInterstitialAd' class='interstitial_ad_iframe_off' src='http://forums.automotive.com/70/9074128/transmission/71-ford-c4-problem/index.html?ExecutionPath=/pages/ads/interstitialiframe.aspx&adpagename=ForumsThread&randomnumber=239101&timer=" + message.data + "'></iframe>");
            }
        }, 'http://ad.doubleclick.net');
        
    </script>








<!--End Ad Control-->
</li>
    <li><a href="http://tiv.trade-in-value.com/?sit=3673&ct=ManyLogos-2_250x90" target="_blank" rel="nofollow" title="What's Your Car Trade-in Value">
        <img src="http://static.automotive.com/Community/_SiteConfigs/automotive_com/images/Acom_250x90.gif" alt="Trade-in-Value.com"/></a>
