	if (mmx.length == 1) { mmx = "0" + mmx}
	ddx = currdate.getDate().toString()
	if (ddx.length == 1) { ddx = "0" + ddx}
	mmddyyx = mmx + ddx + currdate.getFullYear().toString().substring(2,4)
	expx.setTime(currdate.getTime() + 990000000000)
	var zthecookie = "firstdte=" + mmddyyx + "; expires=" + expx.toGMTString() + "; path=/; domain=.bbhq.com"
	document.cookie = zthecookie;
