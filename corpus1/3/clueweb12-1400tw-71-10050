<script type="text/javascript" src="/js.ln/messenger.js"></script>
<script type="text/javascript" src="/js.ln/yui/build/container/container-min.js"></script>
<script type="text/javascript">

		var ChatBoxUser = {
			banOpt : 0,
			eraseOpt : 0,
			ownerID : 1313997,
			userID : null		};
		
		var chatTroll = {
		updateChatWindow : function(o){
			
			var messages = Array;
			for(i=0; i<updater.messagesPerPage; i++){	
				if(!YAHOO.lang.isUndefined(o.messages[i])){
					messageObj = o.messages[i];
				}else{
					messageObj = {
						'messageID' : '0',
						'message' : '',
						'userID' : '0',
						'userName' : '',
						'image' : '',
						'sentOn' : '',
						'msgType' : 'chat',
						'topicID' : 0,
						'topicSubject' : ''
					};	
				}
				
				//messageObj.message = unescape(messageObj.message);
				imageDiv = document.getElementById('chatUserImg'+i);
				chatTroll.eatChildren(imageDiv);				
				if(messageObj.msgType == 'chat'){
					imageDiv.innerHTML = messageObj.image;
				}
				
				if(messageObj.userID > 0){
					userLinkNode = chatTroll.makeUserLink(messageObj.userID, messageObj.userName);
					if(messageObj.msgType == 'chat'){
						userNode1 = document.createTextNode(' wrote ');
					}else{
						userNode1 = document.createTextNode(' posted in:');
					}
					userNode2 = document.createTextNode(messageObj.sentOn + ':');			
				}
				else if(messageObj.userID == 0 && messageObj.messageID > 0){					
					userLinkNode =  document.createTextNode('');
					if(messageObj.msgType == 'chat'){
						userNode1 = document.createTextNode('');
					}else{
						userNode1 = document.createTextNode('A guest posted in:');
					}
					userNode2 = document.createTextNode('');					
				}
				else{
					userLinkNode =  document.createTextNode('');
					userNode1 = document.createTextNode('');
					userNode2 = document.createTextNode('');
				}
				brNode = document.createElement('br');
				userDiv = document.getElementById('chatUser'+i);
				chatTroll.eatChildren(userDiv);
			  
				userDiv.appendChild(userLinkNode);	// adds the user's name as link to his profile
				userDiv.appendChild(userNode1);		// just adds the text 'wrote' or 'posted in:' depending on action
				
				if(messageObj.msgType != 'notification'){
					userDiv.appendChild(brNode);	// basically adds a <br>
					userDiv.appendChild(userNode2);	// adds the age of the message
				}
			
				
				messageNode = document.createTextNode(messageObj.message); // adds the message text
				messageDiv = document.getElementById('chatMessage'+i);
				chatTroll.eatChildren(messageDiv);
				
				if(messageObj.msgType == 'notification'){
					smallNode = document.createElement('div');
					topicLinkNode = chatTroll.makeTopicLink(messageObj.topicID, messageObj.topicSubject);
					
					smallNode.appendChild(topicLinkNode);
					messageDiv.appendChild(smallNode);	
				}else{
					messageDiv.innerHTML = messageObj.message;
				}

				// ban
				banLinkDiv = document.getElementById('chatBanLink'+i);
				chatTroll.eatChildren(banLinkDiv);

				// erase
				eraseLinkDiv = document.getElementById('chatEraseLink'+i);
				chatTroll.eatChildren(eraseLinkDiv);
				
				if (messageObj.userID > 0 && messageObj.msgType != 'notification'){
					var showBanNode = ChatBoxUser.banOpt == 1 
						&& messageObj.userID!=ChatBoxUser.userID 
						&& messageObj.userID!=ChatBoxUser.ownerID;
					var showEraseNode = ChatBoxUser.eraseOpt == 1;

					// ban
					banLinkNode = document.createTextNode('');
					if (showBanNode)
						banLinkNode = chatTroll.makeBanLink(messageObj.messageID, messageObj.userID);
					banLinkDiv.appendChild(banLinkNode);

					// erase
					eraseLinkNode = document.createTextNode('');
					if (showEraseNode)
						eraseLinkNode = chatTroll.makeEraseLink(messageObj.messageID, messageObj.userID);
					eraseLinkDiv.appendChild(eraseLinkNode);
				}
			}
		},

		eatChildren : function(node){
			while (node && node.firstChild) 
			    node.removeChild(node.firstChild);						
		},
		
		updateNavWindow : function(o){
			page = o.page;
			messagesPerPage = o.messagesPerPage;
			
			navDiv = document.getElementById('chatNavigator');
			chatTroll.eatChildren(navDiv);
			
			msgFrom = (page * messagesPerPage) + 1;
			msgTo = (page * messagesPerPage) + parseInt(messagesPerPage);
			firstText = 'First';
			prevText = 'Previous';
			nextText = 'Next';
			navNode1 = document.createTextNode(msgFrom + ' to ' + msgTo);
			
			navNodeNextText = document.createTextNode(nextText);
			navNodePrevText = document.createTextNode(prevText);
			navNodeFirstText = document.createTextNode(firstText);
			
			navNodeNextLink = document.createElement('a');
			navNodeNextLink.setAttribute('href', 'javascript:updater.nextPage();');
			navNodeNextLink.setAttribute('title', nextText + ' ' + messagesPerPage);
			navNodeNextLink.appendChild(navNodeNextText);
			
			if(page > 0){
				navNodePrevLink = document.createElement('a');
				navNodePrevLink.setAttribute('href','javascript:updater.prevPage();');
				navNodePrevLink.setAttribute('title', prevText + ' ' + messagesPerPage);
				navNodePrevLink.appendChild(navNodePrevText);
				
				navNodeFirstLink = document.createElement('a');
				navNodeFirstLink.setAttribute('href','javascript:updater.firstPage();');
				navNodeFirstLink.setAttribute('title', firstText + ' ' + messagesPerPage);
				navNodeFirstLink.appendChild(navNodeFirstText);
			}
			else {
				navDiv.appendChild(navNodeFirstText);
				navDiv.appendChild(document.createTextNode('\u00a0'));
				navDiv.appendChild(navNodePrevText);
				navDiv.appendChild(document.createTextNode('\u00a0|\u00a0'));
			}
			
			if (page > 0) {
				navDiv.appendChild(navNodeFirstLink);
				navDiv.appendChild(document.createTextNode('\u00a0'));
				navDiv.appendChild(navNodePrevLink);
				navDiv.appendChild(document.createTextNode('\u00a0|\u00a0'));
			}
			navDiv.appendChild(navNode1);
			navDiv.appendChild(document.createTextNode('\u00a0|\u00a0'));
			navDiv.appendChild(navNodeNextLink);
		},
		
		makeEraseLink : function(messageID){
				var eraseLink = document.createElement('a');
				var linkHref= "javascript: if(confirm('Are you sure you want to erase that message?')){updater.eraseMessage("+messageID+")};"
				eraseLink.setAttribute('href',linkHref);
				eraseLink.setAttribute('title','Erase' );
				var linkStr = document.createTextNode('Erase');
				eraseLink.appendChild(linkStr);
				return eraseLink;
		},
		makeBanLink : function(messageID, userID){
				var banLink = document.createElement('a');
				var linkHref= "javascript: if(confirm('Are you sure you want to temporarily ban this user from the chatbox?')){updater.banUser("+messageID+","+userID+")};"
				banLink.setAttribute('href',linkHref);
				banLink.setAttribute('title','Ban' );
				var linkStr = document.createTextNode('Ban');
				banLink.appendChild(linkStr);
				return banLink;
		},			
		makeUserLink : function(aBUserID, userName){
			var userLink = document.createElement('a');
			var linkHref = '/m'+aBUserID+'/profile/';
			
			userLink.setAttribute('href',linkHref);
			userLink.setAttribute('title',userName);
			var linkStr = document.createTextNode(userName);
			userLink.appendChild(linkStr);
			return userLink;
		},
		makeTopicLink : function(commentID, commentName){
		
			var topicLink = document.createElement('a');
			var slug = $.trim(commentName.replace(/[^\w\d\s-]/g,'').replace(/\s+/g,'-').toLowerCase())
			var linkHref = '/t'+commentID+'/'+slug+'/#lastPostAnchor';
			
			topicLink.setAttribute('href',linkHref);
			topicLink.setAttribute('title',commentName);
			var linkStr = document.createTextNode(commentName);
			topicLink.appendChild(linkStr);
			return topicLink;
		
		}
	};
		
		
		updater.page = 0;
		updater.messagesPerPage = 7;
		updater.aBID = 136656;
				updater.abKey = '';
		
			
		
		function noSubmit() {
			if(checkMessage_comp4f886294c9ba9() && updater.writingActive == 0){
				updater.writeMessage();
			}
			return false;
		}	
		
		function checkMessage_comp4f886294c9ba9(){
			var message = document.form__comp4f886294c9ba9.message.value;			
			message=message.replace(/^\s+|\s+$/g,"");
			if(message.length<1)
				alert('Please write a message!');
			else if(message.length>500)
				alert('That message is too long!');
			else{				
				return true;
			}
			return false;
		}

		// sets an indicator depending on whether the chatbox is currently updating
		var activityHandler = {
		
    		// called when user clicks to activate chat
    		activate : function(e) { 
    			YAHOO.util.Event.preventDefault(e); 
    			updater.pollOn = 1;
    			updater.init(); // active is called from updater.init();
    		},

    		active : function(e) {
    		    DOM.removeClass('message', 'chatTextboxIdle');
    		    DOM.addClass('message', 'chatTextboxActive');
    		    document.getElementById('message').title = '';
    		},
    
    		// called from updater when polling delay hits a certain threshold
    		idle : function(e) {
    			DOM.removeClass('message', 'chatTextboxActive');
    			DOM.addClass('message', 'chatTextboxIdle');
    			document.getElementById('message').title = 'Idle';
    		},
    
        	init : function(e) {
        		YAHOO.util.Event.addListener('message', 'focus', this.activate);
        		YAHOO.util.Event.addListener('message', 'click', this.activate);
        	}
		}

		// initialize chat activity indicator
		activityHandler.init();

    	/* emoticon box code start */

		var timerID;
		
    	var toggleEmoteBox = function(e) {
    		YAHOO.namespace("emotebox"); 
			if(e) 
    			YAHOO.util.Event.preventDefault(e); 
			
    		if(YAHOO.emotebox.overlay.cfg.getProperty("visible") == false) {
    			YAHOO.emotebox.overlay.show();
    			timerID = setTimeout(toggleEmoteBox, 4000);
    		}
    		else {
    			YAHOO.emotebox.overlay.hide();
    			clearTimeout(timerID);
    		}
    	}

		function selectDefaultTab(e) {
			if(e)
				YAHOO.util.Event.preventDefault(e);
			DOM.removeClass('default-tab', 'row1');
			DOM.addClass('custom-tab', 'row1');
			
			DOM.removeClass('custom-tab', 'selected');
			DOM.addClass('default-tab', 'selected');
			emoticonChatUpdateCallback(0, 12, 3, 200, 0);
		}

		function selectCustomTab(e) {
			if(e)
				YAHOO.util.Event.preventDefault(e);
			DOM.removeClass('custom-tab', 'row1');
			DOM.addClass('default-tab', 'row1');
			
			DOM.removeClass('default-tab', 'selected');
			DOM.addClass('custom-tab', 'selected');
			emoticonChatUpdateCallback(0, 12, 3, 200, 1);
		}	

    	var initEmoteBox = function(e) {
    		YAHOO.namespace("emotebox"); 

    		YAHOO.emotebox.overlay = new YAHOO.widget.Overlay("overlay", { context:["emote-selector","tr","br", ["beforeShow", "windowResize"], [0,0]],
    		  visible:false,
    		  width:"120px" } );

    	    		
			var tabBarHtml = '<table width="100%" cellspacing=0 cellpadding=0 class="table"><tr><td id="custom-tab" class="gensmall"><a href="#" id="custom-link">Custom</a></td>';
			tabBarHtml += '<td id="default-tab" class="gensmall"><a href="#" id="default-link">Default</a></td></tr></table>';	
			
    		YAHOO.emotebox.overlay.setHeader(tabBarHtml);
    		YAHOO.emotebox.overlay.setBody('Loading...');
    		YAHOO.emotebox.overlay.render("overlay-container"); //overlay-container is the parent div 		
    		
    		selectCustomTab(); // initialize the emoticon box to the first page

    		YAHOO.util.Event.addListener("default-link", "click", selectDefaultTab);
    		YAHOO.util.Event.addListener("custom-link", "click", selectCustomTab);

    	
    		DOM.addClass('overlay', 'row2');
    		DOM.addClass('overlay', 'fullborderline');
    		DOM.addClass('overlay', 'emoticon-chat-widget');
        		
    		YAHOO.util.Event.addListener("show-emotes", "click", toggleEmoteBox);
    		YAHOO.util.Event.addListener("close-emotes-link", "click", toggleEmoteBox);
    	}	
    	
    	function emoticonChatUpdateCallback(page, num, cols, max, custom)
    	{
    		if (page*num > max || page < 0)
    			return;
    		
    		var emote_callback = {
    			success : emoticonChatUpdateSuccess,
    			failure : emoticonChatUpdateFailure
    		};
    		
    		YAHOO.util.Dom.setStyle('emoticonChatLoading', 'display', 'block');
    		
    		var params = '?aBID=136656&n='+num+'&c='+cols+'&p='+page+'&custom='+custom;
    		var uri = 'http://www.cagealotcastle.com/emoticon_update_chat.spark'+params;
    		YAHOO.util.Connect.setForm();
    		YAHOO.util.Connect.asyncRequest('GET', uri, emote_callback);
    	}

    	function emoticonChatUpdateSuccess(r)
    	{
    		YAHOO.namespace("emotebox"); 
    		if (r.responseText.length>0)
    		{
    			YAHOO.emotebox.overlay.setBody(r.responseText);
    		}
    		//YAHOO.util.Dom.setStyle('emoticonChatLoading', 'display', 'none');
    	}

    	function emoticonChatUpdateFailure(r)
    	{
    		//YAHOO.util.Dom.setStyle('emoticonChatLoading', 'display', 'none');
    	}

		/* Inserts the selected emoticon and closes the box */
    	function insertChatEmoticon(target, shortcut, src) {

    		src = '<img src="'+src+'" border="0" alt="'+shortcut+'" title="'+shortcut+'" />';

    	    //IE support
			if (document.selection) {
				target.focus();
				sel = document.selection.createRange();
				sel.text = ":"+shortcut+":";
			}
			//MOZILLA/NETSCAPE support
			else if (target.selectionStart || target.selectionStart == '0') {
				var startPos = target.selectionStart;
				var endPos = target.selectionEnd;
				target.value = target.value.substring(0, startPos)
				+ ":"+shortcut+":"
				+ target.value.substring(endPos, target.value.length);
			} else {
				target.value += ":"+shortcut+":";
			}
			
			target.focus(); //set cursor back to textarea
			toggleEmoteBox(); // close box
    	}

		// automatically hide the box after set period
    	YAHOO.util.Event.on('overlay', 'mouseover', function() { 
        	clearInterval(timerID); 
        });
    	
    	YAHOO.util.Event.on('overlay', 'mouseout', function() { 
        	if(YAHOO.emotebox.overlay.cfg.getProperty("visible")) { // ignore when box was closed already due to selecting an emoticon
        		timerID = setTimeout(toggleEmoteBox, 2000);
        	}
    	 });    	

    	YAHOO.util.Event.onDOMReady(initEmoteBox);
		
    	/*emoticon box code end*/
    			
		</script>

		<div id="testDiv"></div>
