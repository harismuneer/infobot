      function isiPhone(){
        return ((navigator.platform.indexOf("iPhone") != -1) || (navigator.platform.indexOf("iPod") != -1));
      }
      function equalHeight(group){
        tallest = 0;
        group.each(function(){
          thisHeight = jQuery(this).height();
          if(thisHeight > tallest){
            tallest=thisHeight;
          }
        });
        group.height(tallest);
      }
      function peekHide(){
        jQuery('#peek').css({'background-image':'none'});
      }
      function peekCSS(){
        jQuery('#peek').css({
          'background-attachment':'scroll',
          'background-image':'url(http://www.travelhoppers.com/wp-content/themes/travelhopper-theme/nav_img/peek_frog.png)',
          'background-color':'#FFF',
          'background-repeat':'no-repeat'
        });
      }
      function activeNavbar(){
        jQuery('#navBar a').removeClass('active');
        peekHide();
        if(location.pathname != "/"){
          jQuery('#navBar a[href*="/' + location.pathname.split("/")[1] + '"]').addClass('active');
        } else {
          jQuery('#navBar a:eq(0)').addClass('active')
        }
        var a = jQuery('a.active').attr('id');
        switch(a){
          case 'navHome':
            var p = '8';
          break;
          case 'navDestinations':
            var p = '105';
          break;
          case 'navBrochures':
            var p = '272';
          break;
          case 'navSpecials':
            var p = '415';
          break;
          case 'navBlog':
            var p = '515';
          break;
          case 'navCommunity':
            var p = '612';
          break;
          case 'navPlanning':
            var p = '745';
          break;
          default:
            var p = '8';
        }
        peekCSS();
        jQuery('#peek').css({'background-position': p + 'px 0'});
      }
