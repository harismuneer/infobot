<script>
// go through the blog articles and set all the links to pop in a new window, except javascript links
var link_redirect = document.getElementById('pageRight').getElementsByTagName('a');
for (i=0; i<link_redirect.length; i++) {
	var str_link = String(link_redirect[i]).toLowerCase();
	if (str_link.indexOf("javascript") < 0  && str_link.indexOf("") < 0 && str_link.indexOf("addthis") < 0) {
		link_redirect[i].setAttribute('target', '_blank');
	}
}
</script>
