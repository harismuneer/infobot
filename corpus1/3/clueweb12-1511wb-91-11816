			<script type="text/javascript">
function redirect(select) {
    var link_index;
	var link_class;

  for(link_index=0; link_index<select.options.length; link_index++)
    if(select.options[link_index].selected)
      {
	  	link_class=select.options[link_index].className;
		
		if(select.options[link_index].value!="" && link_class=="_blank")
		{
        window.open(select.options[link_index].value);
		}
		else if(select.options[link_index].value!="" && link_class=="_self")
		{
		window.location.href=select.options[link_index].value;
		} 
        break;
      }
}
</script>



