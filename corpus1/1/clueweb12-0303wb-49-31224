		<!--SET EXELATE CONDITION-->
		
			
			<!--DEFINE ALL NECESSARY VARIABLES-->
			
			
				
				
				
					<!--GATHER VARIABLES IN JAVCASCRIPT-->
					<script type="text/javascript"> 
						var currentPageURL = 'http://www.uticaod.com/news/x740674679/Herkimer-woman-appears-in-new-Nicolas-Cage-movie';
						var removeHTTPfromURL = currentPageURL.split('http://');
						var pageURLVars = removeHTTPfromURL[1].split('/');
						var page_category = pageURLVars[1];
						var page_subcategory = pageURLVars[2];
						var site_city = 'Utica';
						var site_state = 'NY';
						var site_zip_code = '13501';
						var site_region = '001';</script>
					
					<script type="text/javascript">

						var theStory = /^x[0-9]/g;
						var storyArrayIndex = '';
						
						for(i=0; i<pageURLVars.length; i++){
						
							if(pageURLVars[i].match(theStory)){
								storyArrayIndex = parseInt(i);
							}
						
						}
						
						//CHECK STORY START
						if(storyArrayIndex < 3 && storyArrayIndex != '') {
							page_subcategory = '';
						}
						
						//CHECK IF THERE IS A CATEGORY
						if (page_category == undefined) {
							page_category = '';
						}
						
						//CHECK IF THERE IS A SUB CATEGORY
						if (page_subcategory == undefined) {
							page_subcategory = '';
						}
						
						//HANDLE REGIONS
						
							//REGION 1
							if (site_state == 'MA') {
							site_region = '001';
							}

							//REGION 2
							if ((site_state == 'CA') || (site_state == 'CO')) {
							site_region = '002';
							}
							
							//REGION 3
							if ((site_state == 'IA') || (site_state == 'IL') || (site_state == 'MN') || (site_state == 'MO') || (site_state == 'NE') || (site_state == 'OH')) {
							site_region = '003';
							}
							
							//REGION 4
							if ((site_state == 'MI') || (site_state == 'OK') || (site_state == 'TN') || (site_state == 'AR') || (site_state == 'KS') || (site_state == 'LA')) {
							site_region = '004';
							}
							
							//REGION 5
							if ((site_state == 'CT') || (site_state == 'DE') || (site_state == 'NY') || (site_state == 'PA') || (site_state == 'WV')) {
							site_region = '005';
							}						
						
					</script>
					
				
					
				
				
					
					
					
					
					
					
					<!--WRITE HTML-->
					<script type="text/javascript">
					    document.write('<scr'+'ipt type="text/javascript" src="http://loadus.exelator.com/load/?p=298&amp;g='+site_region+'&c=1547567&ctg='+page_category+'&subctg='+page_subcategory+'&state='+site_state+'&city='+site_city+'&Zip='+site_zip_code+'"><'+'/scr'+'ipt>')

					</script>
										
				
				
			
			
			
