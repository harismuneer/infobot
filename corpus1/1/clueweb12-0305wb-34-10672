		dojo.style( rightPanelContainer, "position", "relative" );
		var rightPanelNode = dojo.byId( "rightpaneldiv" );
		dojo.style( rightPanelNode, "position", "relative" );

		// Connect scroll events and pane wipe-in and wipe-out events to scroll check
		dojo.connect( window, "scroll", checkScroll );
		dojo.forEach( [dealerSpecialsPane, compareVehiclesPane, paymentCalculatorPane], function( pane ) {
			if( pane != null ) {
				dojo.forEach( ["onAnimateIn", "onAnimateOut"], function( eventName ) {
					dojo.connect( pane, eventName, checkScroll );
				}, this );
			}
		}, this );
