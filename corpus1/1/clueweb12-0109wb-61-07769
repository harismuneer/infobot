	<script type="text/javascript">
		var rightSep = 'http://www.globalpropertyguide.com/template/images/sep.gif'; // jump menu for cities menu

		jQuery.noConflict();
						
		
		
		window.addEvent('domready',function(){	//domready listener
$$('.target_blank').each(function(el){
 el.setProperty('target','_blank');
});
$$('.auto_off').each(function(el){
 el.setProperty('autocomplete','off');
});
if ($('commentregister')) {
	$('commentregister').addEvent('click', function(e){
		new Ajax('http://www.globalpropertyguide.com/regfromcomment',{ method: 'post', evalScripts: true }).request();
	});
}
			

			OnStart(); //load alului navigator
						getLoginStyles();


function doSignUp() {
	$('signupform').addEvent('submit',function(e){
		e=new Event(e).stop();
		if(isEmail($('signupemail').value)){
			new Ajax('http://www.globalpropertyguide.com/ajax-signup',{
				method: 'post', evalScripts: true, data: $('signupform'), update: $('lg_errorBox'),
				onStart: $('lg_errorBox').setHTML('<'+'img src="./template/images/loading-small.gif" alt="loading"/>')
			}).request();
		}else{
			$('lg_errorBox').setHTML("<"+"span class='gpg_errorMsg'>Please enter a valid email address<"+"/span>");
		}
	});

}
function dopassword(){
	$('passfrm').addEvent('submit',function(e){
		e=new Event(e).stop();
		if(isEmail($('emailrequest').value)){
			new Ajax('http://www.globalpropertyguide.com/ajax-password',{
				method: 'post', evalScripts: true, data: $('passfrm'), update: $('lg_errorBox'),
				onStart: $('lg_errorBox').setHTML('<'+'img src="./template/images/loading-small.gif" alt="loading"/>')
			}).request();
		}else{
			$('lg_errorBox').setHTML("<"+"span class='gpg_errorMsg'>Please enter a valid email address<"+"/span>");
		}
	});
}


			if($chk($('loginfrm'))){

	$('loginfrm').addEvent('submit',function(e){
		e=new Event(e).stop();
		if(isEmail($('lg_user').value)){
			new Ajax('http://www.globalpropertyguide.com/ajax-login',{
				method: 'post', evalScripts: true, data: $('loginfrm'), update: $('lg_errorBox'),
				onStart: $('lg_errorBox').setHTML('<'+'img src="./template/images/loading-small.gif" alt="loading"/>')
			}).request();
		}else{
			$('lg_errorBox').setHTML("<"+"span class='gpg_errorMsg'>Please enter a valid email address<"+"/span>");
		}
	});
	doSignUp();
	dopassword();
}
			
			$('nls_switch').addEvent('click',function(e){e=new Event(e).stop();highlight($('nls_email'));$('nls_email').focus();});
							//load action page dom level scripts
		});
	</script>
	
