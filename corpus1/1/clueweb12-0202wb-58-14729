 <script type="text/javascript">
     $LS(document).ready(function() {
         $LS('.post-comment').mouseover(function() {
             $LS('.view-comment').css('color', '#ff6633');
             $LS('#postComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
             $LS('#postComment a.post-comment').css('background-repeat', 'no-repeat');
         });
         $LS('.post-comment').mouseout(function() {
             $LS('.view-comment').css('color', '');
             $LS('#postComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-normal.gif)');
             $LS('#postComment a.post-comment').css('background-repeat', 'no-repeat');
         });
         $LS('.post-comment').click(function() {
             var commentBox = $LS('#disqus_thread_box').css('display');
             if (commentBox == 'none') {
                 $LS('#disqus_thread_box').css('display', 'block');
                 $LS("#genericArticleCommentId").removeClass("hide").addClass("show");
             }

             if (disqus_shortname == "" || (typeof disqus_shortname == undefined)) {
                 disqus_shortname = "lifescript";
             }

             jQuery.ajax(
            {
                async: true,
                type: "GET",
                url: 'http://' + disqus_shortname + '.disqus.com/embed.js',
                dataType: 'script'
            });

         });

         $LS('.view-comment').mouseover(function() {
             $LS('#postComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
             $LS('#postComment a.post-comment').css('background-repeat', 'no-repeat');
         });
         $LS('.view-comment').mouseout(function() {
             $LS('#postComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-normal.gif)');
             $LS('#postComment a.post-comment').css('background-repeat', 'no-repeat');
         });
         $LS('.view-comment').click(function() {
             var commentBox = $LS('#disqus_thread_box').css('display');
             if (commentBox == 'none') {
                 $LS('#disqus_thread_box').css('display', 'block');
                 $LS("#genericArticleCommentId").removeClass("hide").addClass("show");
             }

             if (disqus_shortname == "" || (typeof disqus_shortname == undefined)) {
                 disqus_shortname = "lifescript";
             }

             jQuery.ajax(
            {
                async: true,
                type: "GET",
                url: 'http://' + disqus_shortname + '.disqus.com/embed.js',
                dataType: 'script'
            });

         });
     });
</script>
        
