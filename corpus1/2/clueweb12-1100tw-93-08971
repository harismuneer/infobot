<script src="//assets.pinterest.com/js/pinit.js" type="text/javascript"></script><script type="text/javascript">


  global_dim_overlay = new dimoverlay();

  Reverb.Navigate.initialize_history();

  CURRENT_PAGE_OBJECT = 'artist_524100';

  // TODO this should go in ajaxutils after some more testing
  (function(ns){
    var loaded_resources = {},
            patterns = {css: /^(.+\.css)/, js: /^(.+\.js)/ },
            selectors = { css: "link[href][type$=css]", js: "script[src]" },
            head = document.getElementsByTagName("head")[0]
    window["loaded_resources"] = loaded_resources

    var get_resource = function(path, type){
      if((path+"").match(patterns[type])){
        return {key: RegExp.$1, path: path, type: type}
      }
    },

            append_resource = function(resource){
              var ref
              if(resource.type == "js"){
                ref = document.createElement('script')
                ref.setAttribute("type","text/javascript")
                ref.setAttribute("src", resource.path)
              }else if(resource.type == "css"){
                ref = document.createElement("link")
                ref.setAttribute("rel", "stylesheet")
                ref.setAttribute("type", "text/css")
                ref.setAttribute("href", resource.path)
              }
              loaded_resources[resource.key] = !0
              if(ref){
                head.appendChild(ref)
              }
            }

    // store all existing resources

    jQuery(document).ready(function(){
      for(var key in selectors){
        if( selectors.hasOwnProperty(key)){
          $$(selectors[key]).each(function(elem){
            var res = get_resource( $(elem).readAttribute("href") || $(elem).readAttribute("src"), key )
            if(res){
              loaded_resources[res.key] = !0
            }
          })
        }
      }

    })

    ns.loadjscssfile = function(filename, filetype){
      var res =  get_resource(filename, filetype)
      if(res && !loaded_resources[res.key]){
        // TODO remove this after finishing with the tests
        //if(window.console){
        //  console.log("appending", filename)
        //}
        append_resource(res)
      }
    }
  })(window);


</script>
