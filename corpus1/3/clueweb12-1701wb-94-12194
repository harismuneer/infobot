function expandComment(intWhich)
{
	document.getElementById("moreComment" + intWhich).style.display = "none";
	document.getElementById("lessComment" + intWhich).style.display = "block";
}

function contractComment(intWhich)
{
	document.getElementById("lessComment" + intWhich).style.display = "none";
	document.getElementById("moreComment" + intWhich).style.display = "block";
}

var xmlhttp

function showRating()
{
xmlhttp=GetXmlHttpObject();
if (xmlhttp==null)
  {
  return;
  }
var url="/submit/getStoryRating.asp?x=17818";
xmlhttp.open("GET",url,true);
xmlhttp.send(null);
xmlhttp.onreadystatechange=stateChanged;
}

function stateChanged()
{
if (xmlhttp.readyState==4)
  {
  var x;
  var info = xmlhttp.responseText;
  var rates = info.split("*");
  x = document.getElementById('tub');
  x.innerHTML=rates[0];
  x = document.getElementById('tdb');
  x.innerHTML=rates[1];
  }
}

function GetXmlHttpObject()
{
if (window.XMLHttpRequest)
  {
  // code for IE7+, Firefox, Chrome, Opera, Safari
  return new XMLHttpRequest();
  }
if (window.ActiveXObject)
  {
  // code for IE6, IE5
  return new ActiveXObject("Microsoft.XMLHTTP");
  }
return null;
}//-->
