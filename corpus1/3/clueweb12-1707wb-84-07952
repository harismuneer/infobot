<script>
$j(document).ready(function(){
var divs = $j('div.allArticles, div.reviewArticles, div.previewArticles, div.relatedArticles, div.newsArticles,div.hotArticles, div.featureArticles');
var divTabs = $j('#allArticles, #reviewArticles, #previewArticles, #relatedArticles, #newsArticles, #hotArticles,#featureArticles');
divTabs.click(function() { 
	if(!$j(this).hasClass('disabledNavBtn')){
		divTabs.removeClass('articleNavBtnSelected');
		$j('.articleNavBar').find('div').removeClass('articleNavDividerSelected');
		$j(this).addClass('articleNavBtnSelected');
		$j(this).removeClass('articleNavBtnOver');
		var divider=$j(this).next();
		divider.addClass('articleNavDividerSelected');
		divs.hide(); 
		$j('.' + this.id).show();
	}return false;
}); $j('.articleNavBtn').hover(function(){
	if(!$j(this).hasClass('disabledNavBtn')){	 
		var divider=$j(this).next();
		divider.addClass('articleNavDividerOver');
		$j(this).addClass('articleNavBtnOver');
	}
}, function(){
	$j(this).removeClass('articleNavBtnOver');
	var divider=$j(this).next();
	divider.removeClass('articleNavDividerOver');
});
}); 
</script>
		
