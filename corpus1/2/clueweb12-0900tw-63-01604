<script type="text/javascript"><!--
	var themeTags=[
		];
	
	jQuery(document).ready(function(){
		var mail = getCookie("scoopemail");
        //showHideMailRegistrationDivs({"status":  "cancel" });

		jQuery("#inputemail").val(mail);
		
		// Any Post to paste ?
				
		// Any post to show fullscreen ?
					jQuery("#viewboxOverlay").click(function(){
				closePostAndViewTopic(1418930253, "/t/raspberry-pi");
			});
			jQuery("#thePostToPrint .htmlfragment").show();
				
				
			});

	function resizeOverlays() {
		jQuery(".onePost").each(function() {
			jQuery(".overlayForCuttingPost", $(this)).css({
				width : $(this).outerWidth(),
				height : $(this).outerHeight()
			});
		});
	}
	
	function getCookie(c_name)
	{
		if (document.cookie.length>0) {
			var c_start=document.cookie.indexOf(c_name + "=");
			if (c_start!=-1) {
			    c_start=c_start + c_name.length+1;
			    c_end=document.cookie.indexOf(";",c_start);
			    if (c_end==-1) c_end=document.cookie.length;
				var val = unescape(document.cookie.substring(c_start,c_end));
				return val.replace(/['"]/g , '');
			}
		}
		return "";
	}
	
	function emailRegistration(action) {
		var auth = getCookie("auth");
		var params = {};
		if (auth == null || auth.length == 0) {
			if (action == "cancel" || (jQuery("#popup").length > 0 && jQuery("#popup").is(':visible'))) {
				var mail = jQuery("#inputemail").val();
				var kaptcha = jQuery("#inputkaptcha").val();
				var kaptchaUrl = jQuery("#inputkaptchaurl").val();
				var url = "/t/raspberry-pi/exportByMail";				
				params = {"view" : "json", "method" : "mail", "action": action, "email": mail, "kaptcha":kaptcha, "kaptchaUrl":kaptchaUrl};
			} else {
				jQuery("#allPage").popup({
						dom: jQuery("#mailinput")[0],
						hideOnClick: false,
						showAnimation: false
					});
				// Nothing to do, show popup
				return;
			}
		} else {
			var url = "/t/raspberry-pi/exportByMail";
			params = {"view": "json", "method" : "mail", "action" : action };
		}
		jQuery.post(url, params, function(data) {
			data = data.response;
			var errors = data.errors;
			if (errors) {
				// Has errors
				if (errors.email) {
					jQuery("#errorsEmail").html(errors.email);
					jQuery("#errorsEmail").show();
				}
				if (errors.kaptcha) {
					jQuery("#errorsKaptcha").html(errors.kaptcha);
					jQuery("#errorsKaptcha").show();
					jQuery("#followKaptcha").attr('src',jQuery("#followKaptcha").attr('src')+Math.random());
				}
				
			} else {
				// Hide all
				jQuery("#allPage").closePopup();
				jQuery("#errors*").hide();
				window.location= data.themeInternalUrl;
			}
		});			
	}
--></script>
