<script type="text/javascript">

var allowSearch = false;

// keyword search box behaviors
function prepSearch(kw)
{
		kw.value='';
	kw.className='';
		kw.onfocus='';
	allowSearch = true;
}

function checkSiteKeyword()
{
		if (!allowSearch || document.siteKeywordForm.keyword.value.replace(/^\s*/, "").replace(/\s*$/, "") == '')
	{
		alert('Please enter one or more keywords into the search box.');
		document.siteKeywordForm.keyword.value='';
		document.siteKeywordForm.keyword.focus(); // calls prepSearch()
		return false;
	}
		return true;
}

function validateSearch()
{
	var defaultKeyword = 'e.g. Elton John, PianoSoft';

	if((jQuery('#searchMain').val() == defaultKeyword) || (String(jQuery('#searchMain').val()).replace(/\s/gi, '') == ''))
	{
		alert('Please enter one or more keywords into the search box.');
		jQuery('#searchMain').val('');
		return false;
	}

	// scope radio value not needed server-side; unset it
	jQuery('input[name=scope]:radio:checked').attr('checked',false);
}
</script>

