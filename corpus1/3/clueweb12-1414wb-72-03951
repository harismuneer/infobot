<script type="text/javascript">
$(document).ready(function() {
	
	$('form#searchFrm fieldset input#cmdSearchQuick').click(function() {
		var searchTxt = $('form#searchFrm fieldset input#searchTxt').val().toString();
		if (isString(searchTxt)) {
			$('form#searchFrm').submit();
		}
		delete(searchTxt);
		
		return false;
	});
	
	$('#searchTxt').focusout(function() {
		if ($(this).val().toString() == '') {
			$(this).val('Search...');
		}
	}).focus(function() {
		if ($(this).val().toString() == 'Search...') {
			$(this).val('');
		}
	});
	
});
</script>
