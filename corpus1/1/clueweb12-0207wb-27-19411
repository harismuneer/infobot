		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_c_code').style.display!='none') && theform['__phone_f_multy_c_code'].type != 'hidden' && theform['__phone_f_multy_c_code'].style.display  != 'none') && theform['__phone_f_multy_c_code'].focus) 
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_c_code').style.display!='none') && theform['__phone_f_multy_c_code'].type != 'hidden' && theform['__phone_f_multy_c_code'].style.display  != 'none') && theform['__phone_f_multy_c_code'].focus) {
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_c_code').style.display!='none') && theform['__phone_f_multy_c_code'].type != 'hidden' && theform['__phone_f_multy_c_code'].style.display  != 'none')) {alert('Country Code*'+unescape('%u000A')+'Please Select')}
		form_submitted=false
		return false		}
 
	}	if(theform['__phone_f_multy_a_code'].value.trim()=='')
	{
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_a_code').style.display!='none') && theform['__phone_f_multy_a_code'].type != 'hidden' && theform['__phone_f_multy_a_code'].style.display  != 'none') && theform['__phone_f_multy_a_code'].focus) 
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_a_code').style.display!='none') && theform['__phone_f_multy_a_code'].type != 'hidden' && theform['__phone_f_multy_a_code'].style.display  != 'none') && theform['__phone_f_multy_a_code'].focus) {
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_a_code').style.display!='none') && theform['__phone_f_multy_a_code'].type != 'hidden' && theform['__phone_f_multy_a_code'].style.display  != 'none')) {alert('Area Code*'+unescape('%u000A')+'It was not specified valid data for:')}
		form_submitted=false
		return false		}
 
	}	if(theform['__phone_f_multy_p_code'].value.trim()=='')
	{
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_p_code').style.display!='none') && theform['__phone_f_multy_p_code'].type != 'hidden' && theform['__phone_f_multy_p_code'].style.display  != 'none') && theform['__phone_f_multy_p_code'].focus) 
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_p_code').style.display!='none') && theform['__phone_f_multy_p_code'].type != 'hidden' && theform['__phone_f_multy_p_code'].style.display  != 'none') && theform['__phone_f_multy_p_code'].focus) {
		if(((document.getElementById('r_phone_f_multy').style.display!='none' && document.getElementById('r___phone_f_multy_p_code').style.display!='none') && theform['__phone_f_multy_p_code'].type != 'hidden' && theform['__phone_f_multy_p_code'].style.display  != 'none')) {alert('Phone number*'+unescape('%u000A')+'It was not specified valid data for:')}
		form_submitted=false
		return false		}
 
	}	if(theform['email'].value.trim()==''
	|| ValidateEmail(theform['email'])==false)
	{
		if((document.getElementById('r_email').style.display!='none' && theform['email'].type != 'hidden' && theform['email'].style.display  != 'none') && theform['email'].focus) 
		if((document.getElementById('r_email').style.display!='none' && theform['email'].type != 'hidden' && theform['email'].style.display  != 'none') && theform['email'].focus) {
		if((document.getElementById('r_email').style.display!='none' && theform['email'].type != 'hidden' && theform['email'].style.display  != 'none')) {alert('E-mail address*'+unescape('%u000A')+'It was not specified a valid e-mail address')}
		form_submitted=false
		return false		}
 
	}	if(theform['residence_country'].selectedIndex==-1 || theform['residence_country'].options[theform['residence_country'].selectedIndex].value.trim()=='')
	{
		if((document.getElementById('r_residence_country').style.display!='none' && theform['residence_country'].type != 'hidden' && theform['residence_country'].style.display  != 'none') && theform['residence_country'].focus) 
		if((document.getElementById('r_residence_country').style.display!='none' && theform['residence_country'].type != 'hidden' && theform['residence_country'].style.display  != 'none') && theform['residence_country'].focus) {
		if((document.getElementById('r_residence_country').style.display!='none' && theform['residence_country'].type != 'hidden' && theform['residence_country'].style.display  != 'none')) {alert('Where are you calling from?*'+unescape('%u000A')+'Please select an option!')}
		form_submitted=false
		return false		}
 
	}	if(theform['list_country_tax_property'].selectedIndex==-1 || theform['list_country_tax_property'].options[theform['list_country_tax_property'].selectedIndex].value.trim()=='')
	{
		if((document.getElementById('r_list_country_tax_property').style.display!='none' && theform['list_country_tax_property'].type != 'hidden' && theform['list_country_tax_property'].style.display  != 'none') && theform['list_country_tax_property'].focus) 
		if((document.getElementById('r_list_country_tax_property').style.display!='none' && theform['list_country_tax_property'].type != 'hidden' && theform['list_country_tax_property'].style.display  != 'none') && theform['list_country_tax_property'].focus) {
		if((document.getElementById('r_list_country_tax_property').style.display!='none' && theform['list_country_tax_property'].type != 'hidden' && theform['list_country_tax_property'].style.display  != 'none')) {alert('What country are you enquiring about?*'+unescape('%u000A')+'Please select country are you enquiring about')}
		form_submitted=false
		return false		}
 
	}	if(theform['how_here'].value.trim()=='')
	{
		if((document.getElementById('r_how_here').style.display!='none' && theform['how_here'].type != 'hidden' && theform['how_here'].style.display  != 'none') && theform['how_here'].focus) 
		if((document.getElementById('r_how_here').style.display!='none' && theform['how_here'].type != 'hidden' && theform['how_here'].style.display  != 'none') && theform['how_here'].focus) {
		if((document.getElementById('r_how_here').style.display!='none' && theform['how_here'].type != 'hidden' && theform['how_here'].style.display  != 'none')) {alert('How did you hear of our service?*'+unescape('%u000A')+'It was not specified "How did you hear about us?"')}
		form_submitted=false
		return false		}
 
	}	if(theform['other_country_tax'].value.trim()=='')
	{
		if((document.getElementById('r_other_country_tax').style.display!='none' && theform['other_country_tax'].type != 'hidden' && theform['other_country_tax'].style.display  != 'none') && theform['other_country_tax'].focus) 
		if((document.getElementById('r_other_country_tax').style.display!='none' && theform['other_country_tax'].type != 'hidden' && theform['other_country_tax'].style.display  != 'none') && theform['other_country_tax'].focus) {
		if((document.getElementById('r_other_country_tax').style.display!='none' && theform['other_country_tax'].type != 'hidden' && theform['other_country_tax'].style.display  != 'none')) {alert('Other, please specify*'+unescape('%u000A')+'Please select country are you enquiring about')}
		form_submitted=false
		return false		}
 
	}	if(theform['img_captcha'].value.trim()=='')
	{
		if((document.getElementById('r_img_captcha').style.display!='none' && theform['img_captcha'].type != 'hidden' && theform['img_captcha'].style.display  != 'none') && theform['img_captcha'].focus) 
		if((document.getElementById('r_img_captcha').style.display!='none' && theform['img_captcha'].type != 'hidden' && theform['img_captcha'].style.display  != 'none') && theform['img_captcha'].focus) {
		if((document.getElementById('r_img_captcha').style.display!='none' && theform['img_captcha'].type != 'hidden' && theform['img_captcha'].style.display  != 'none')) {alert('Please enter the word from the picture:*'+unescape('%u000A')+'Please enter the word from the picture below:')}
		form_submitted=false
		return false		}
 
	}	return true
}
