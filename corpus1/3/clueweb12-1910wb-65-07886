                  referrerIsInventorySearch = false;
                  }

                  if (referrerIsInventorySearch)
                  {
                  $("#back-to-inventory").live('click', function(){window.history.back();});
                  }
                  else
                  {
                  $("#back-to-inventory").attr("href", "/Little-Rock/For-Sale/Used/");
