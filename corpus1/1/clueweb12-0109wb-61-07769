<script>
/**************************************************************

	Script		: Sortable Table
	Version		: 1.4
	Authors		: Samuel Birch
	Desc			: Sorts and filters table elements
	Licence		: Open Source MIT Licence
	Modified by/for: Global Property Guide
**************************************************************/

var sortableTable = new Class({
							  
	getOptions: function(){
		return {
			overCls: false,
			onClick: false,
			sortOn: 0,
			sortBy: 'ASC',
			useMod: false
		};
	},

	initialize: function(table, options){
		
		this.setOptions(this.getOptions(), options);
		this.table = $(table);
		this.headrows = this.table.getElement('tr[class=head]');
		this.tHead = options.useMod ? this.table.getElement('tr[class=head]').getElements('td') : this.table.getElement('thead').getElements('th');
		this.tBody = this.table.getElement('tbody');		
		this.elements = options.useMod ? this.tBody.getElements('tr[class=sortable]') : this.tBody.getElements('tr');
		
		this.elements.each(function(el,i){			
			if(this.options.overCls){
				el.addEvent('mouseover', function(){
					el.addClass(options.overCls);
				}, this);
				el.addEvent('mouseout', function(){
					el.removeClass(options.overCls);
				});
			}
			if(this.options.onClick){
				el.addEvent('click', options.onClick);
			}
		}, this);
		
		//setup header
		this.tHead.each(function(el,i){
			var modified = this.options.useMod;
			if(el.axis){				
				if(!this.options.useMod) el.addEvent('click', this.sort.bind(this,i));
				el.addEvent('mouseover', function(){
					el.addClass('tableHeaderOver');
				});
				el.addEvent('mouseout', function(){
					el.removeClass('tableHeaderOver');
				});
				//
				el.findData = function(elem){
					var child = elem.getFirst();
					if(child){
						return el.findData(child);
					}else{
						return elem.innerHTML.trim().replace(/(<([^>]+)>)/ig,"");
					}
				};
				//
				el.compare = function(a,b){
					if(!modified){
						var1 = el.findData(a.getChildren()[i]);
						var2 = el.findData(b.getChildren()[i]);
					}else{
						var1 = a.getChildren()[i].getProperty('title');
						var2 = b.getChildren()[i].getProperty('title');
					}
					
					if(el.axis == 'number'){
						var1 = parseFloat(var1);
						var2 = parseFloat(var2);
						
						if(el.sortBy == 'ASC'){
							return var1-var2;
						}else{
							return var2-var1;
						}
						
					}else if(el.axis == 'string'){
						var1 = var1.toUpperCase();
						var2 = var2.toUpperCase();
						
						if(var1==var2){return 0};
						if(el.sortBy == 'ASC'){
							if(var1<var2){toReturn = modified? 1 : -1;return toReturn;};
						}else{
							if(var1>var2){toReturn = modified? 1 : -1;return toReturn;};
						}
						toReturn = modified? -1 : 1;return toReturn; 
					}
					
				}				
				if(i == this.options.sortOn){
					this.options.useMod ? this.sort(i) : el.fireEvent('click');
				}
			}
		}, this);
	},
	
	sort: function(index){
		
		if(this.options.onStart){
			this.fireEvent('onStart');
		}
		//
		this.options.sortOn = index;
		var header = this.tHead;
		var el = header[index];
		header.each(function(e,i){
			if(i != index){
				e.removeClass('sortedASC');
				e.removeClass('sortedDESC');
			}
		});
		
		if(el.hasClass('sortedASC')){			
			el.removeClass('sortedASC');
			el.addClass('sortedDESC');
			el.sortBy = 'DESC';
		}else if(el.hasClass('sortedDESC')){
			el.removeClass('sortedDESC');
			el.addClass('sortedASC');
			el.sortBy = 'ASC';
		}else{
			if(this.options.sortBy == 'ASC'){
				el.addClass('sortedASC');
				el.sortBy = 'ASC';
			}else if(this.options.sortBy == 'DESC'){
				el.addClass('sortedDESC');
				el.sortBy = 'DESC';
			}
		}
		//
		
		this.elements.sort(el.compare);
		if(!this.options.useMod){
			this.elements.injectInside(tBody);
		}else{
			this.tBody.setHTML="";
			while(this.tBody.hasChildNodes()){
				this.tBody.removeChild(this.tBody.firstChild);
			}

			this.elements.each(function(e,i){				
				if(i!=0 && i%17==0){		
					this.headrows.clone().injectInside(this.tBody);
				}				
				e.getFirst().setHTML("&nbsp;"+(i+1));
				e.injectInside(this.tBody);
				
			},this);
		}		
		// var exampleFx = new Fx.Morph(this.elements, {
			// duration: 1000
		// });
		// exampleFx.start({
			// 'opacity':[0,1]
		// });
		if(this.options.onComplete){
			this.fireEvent('onComplete');
		}
	}
	

});
sortableTable.implement(new Events);
sortableTable.implement(new Options);

/*************************************************************/


</script>


