        else if (localPathname.indexOf("/Actor") >= 0) {
            selector = "#tabActors";
        }
        else if (localPathname.indexOf("/Store") >= 0) {
            selector = "#tabStore"
        }

        if (selector != "") {
            $(selector).addClass("selected");
