<script type="text/javascript">
	var OnLine;
	function getOnline(i){
		if (i>online.length){
			OnLine=2;
		}
		else{
			OnLine=online[i-1];
		}
	}
	getOnline(1);
</script>

<script type="text/javascript">
	YAHOO.util.Event.onDOMReady(function () {
	    YAHOO.util.Get.script(
	    	'http://hotproducts.aliexpress.com/wscompanyid-storeurl-server.html?ak=9k41ll64b6h0673a559l02l2l29003ba&company_id=200176658&s=0&n=1&mode=get&callbackParamName=storeInfo',
	        {
	        	onSuccess: function (o) {
					if(typeof storeInfo != "undefined" && storeInfo){
						var storeURL = storeInfo.store_url;
						if(get("store-zone-top")){
							get("store-zone-top").style.display = "block";
							get("store-lnk-top").href = storeURL+"?tracelog=minisite_top";
						}
						if(get("store-zone-cp")){
							get("store-zone-cp").parentNode.parentNode.style.display = "";
							get("store-lnk-cp").href = storeURL+"?tracelog=minisite_CP";
							get("store-lnk-cp").innerHTML = storeURL;
						}
						if(get("store-zone-ci")){
							get("store-zone-ci").parentNode.parentNode.style.display = "";
							get("store-lnk-ci").href = storeURL+"?tracelog=minisite_CI";
							get("store-lnk-ci").innerHTML = storeURL;
						}
					}
	        	}
	        }
		);
	});
</script>

