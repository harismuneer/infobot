function changeSource()
{
  var adCall3js = "http://ad.doubleclick.net/adi/sci.tribune/homepage;;ptype=pg;slug=sns-viral-nicolas-cage-movie-pg;rg=ur;ref=ky3com;pos=1;sz=300x250;tile=2;en=NicolasCage;at=NicolasCage;ord=40493268";
