<script type="text/javascript">
	function showReviewLinks() {
		$("#VehicleDetailLinksSpace").css("display", "none");
		$("#VehicleDetailReviewLinks").css("display", "");
		$("#VehicleDetailReviewLinks2").css("display", "");
		$("#VehicleDetailReviewLinks3").css("display", "");
	}

	function hideReviewLinks() {
		$("#VehicleDetailLinksSpace").css("display", "");
		$("#VehicleDetailReviewLinks").css("display", "none");
	}

	function showPricingLinks() {
		$("#VehicleDetailLinksSpace").css("display", "none");
		$("#VehicleDetailReviewLinks2").css("display", "none");
		$("#VehicleDetailReviewLinks3").css("display", "none");
		$("#VehicleDetailPricingLinks").css("display", "");
	}

	function hidePricingLinks() {
		$("#VehicleDetailLinksSpace").css("display", "");
		$("#VehicleDetailReviewLinks2").css("display", "");
		$("#VehicleDetailReviewLinks3").css("display", "");
		$("#VehicleDetailPricingLinks").css("display", "none");
	}

	function showOverviewSection() {
		$("#VehicleDetailOverviewSection").css("display", "");
		$("#VehicleDetailExpertReviews").css("display", "none");
		$("#VehicleDetailConsumerRatings").css("display", "none");
	}

	function showExpertReviewsSection() {
		$("#VehicleDetailOverviewSection").css("display", "none");
		$("#VehicleDetailExpertReviews").css("display", "");
		$("#VehicleDetailConsumerRatings").css("display", "none");
		$("#ExpertReviewLI1").addClass("active");
		$("#ExpertReviewLI2").addClass("active");
		$("#ExpertReviewLI3").addClass("active");
		$("#ExpertReviewLI4").addClass("active");
	}

	function showConsumerRatingsSection() {
		$("#VehicleDetailOverviewSection").css("display", "none");
		$("#VehicleDetailExpertReviews").css("display", "none");
		$("#VehicleDetailConsumerRatings").css("display", "");
		$("#ConsumerRatingLI1").addClass("active");
		$("#ConsumerRatingLI2").addClass("active");
		$("#ConsumerRatingLI3").addClass("active");
		$("#ConsumerRatingLI4").addClass("active");
	}

	$(function () {
		$("#VehicleDetailsOverviewTab").click(function () {
			showOverviewSection();
		});
		$("#VehicleDetailsReviewsTab").click(function () {
			showExpertReviewsSection();
		});
		$("#ReviewTab").mouseover(function () {
			showReviewLinks();
		});
		$("#ReviewTab").mouseout(function () {
			hideReviewLinks();
		});
		$("#VehicleSpecsTab").mouseover(function () {
			showPricingLinks();
		});
		$("#VehicleSpecsTab").mouseout(function () {
			hidePricingLinks();
		});

	});


	// Manage GO button!
	$(function () {
		$('#VDRebateZipform').click(function () {
			var linkLocation = "/";
			var make = $('#incentiveszipmake').val();
			var model = $("#incentiveszipmodel").val();
			var year = $("#incentiveszipyear").val();
			var trim = $("#incentivesziptrim").val();
			var zipcodeVal = $('#zipcode').val();
			var zipLen = zipcodeVal.length;

			if (zipLen != 5) {
				$('#zipErrorMessage').css('display', '');
				$('#zipcode').addClass('input-validation-error');
				return false;
			}

			if (trim.length > 0)
				linkLocation = linkLocation + make.toLowerCase().replace(' ', '-') + '/' + model.toLowerCase().replace(' ', '-') + '/' + year + '/' + trim + '/incentives/';
			else if (make.length > 0)
				linkLocation = linkLocation + make.toLowerCase().replace(' ', '-') + '/' + model.toLowerCase().replace(' ', '-') + '/' + year + '/incentives/';

			document.location = linkLocation;
		});
		$('#VDRebateQuoteForm').click(function () {
			var zipcodeVal = $('#zipcode').val();
			var zipLen = zipcodeVal.length;

			if (zipLen != 5) {
				$('#zipErrorMessage').css('display', '');
				$('#zipcode').addClass('input-validation-error');
				return false;
			}
			else {
				return true;
			}
		});

	});


	$(document).ready(function () {
		$("#SearchMakeModel_MakeListPopup").load("/api/new-cars/makelist-popup/", { actionTarget: "SearchMakeModel" }, function () {
			$("#VehicleDetail_TrimListPopup").load("/api/new-cars/trimlist-popup/", { make: "Ford", model: "Edge", year: "2013" }, function () {
				$('.close-popup').click(function () {
					$('.edit-selection-popup').hide();
					return false;
				});
			});
		});
	});
</script>

