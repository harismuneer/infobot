<script type="text/javascript">
    jQuery.extend(jQuery.expr[':'], { focus: "a == document.activeElement" });
    $(document).ready(function() {
        $(".lnkExpand").parent().find("h3").click(function() {
            $(this).parent().find(".search-para-wrap").toggle();
            if ($(this).parent().find("img").attr("src") == "/images/search-page/tier/exp.png") {
                $(this).parent().find("img").attr("src", "/images/search-page/tier/cnt.png");
            } else {
                $(this).parent().find("img").attr("src", "/images/search-page/tier/exp.png");
            }

        });
        $(".lnkExpand").click(function() {
            $(this).parent().find(".search-para-wrap").toggle();
            if ($(this).find("img").attr("src") == "/images/search-page/tier/exp.png") {
                $(this).find("img").attr("src", "/images/search-page/tier/cnt.png");
            } else {
                $(this).find("img").attr("src", "/images/search-page/tier/exp.png");
            }

        });
        $(".lnkOpenSave").click(function() {
            $("#divSave").show();
        });
        $(".lnkAllMakes").click(function() {
            $("#divAllMake").toggle();
        });
        $(".lnkAllExt").click(function() {
            $("#divAllExt").toggle();
        });
        $(".lnkAllState").click(function() {
            $("#divAllState").toggle();
        });
        if ($(".txtZip").val() == "") {
            $(".txtZip").val("zip code");
        }
        $(".txtZip").focus(function() {
            if ($(".txtZip").val() == "zip code") { $(".txtZip").val(""); }
        });
        $(".txtZip").keypress(function(event) {
            if ($(this).val().length >= 4) {
                if (event.which != 8) {
                    var zip = $(this).val();
                    if ((zip.length + 1) <=5){zip= zip +String.fromCharCode(event.which);}
                    var pageURL = '/lease/Ford/Edge/search.aspx?';
                    $(".divLoading").show();
                    window.location = pageURL + "zip=" + zip //$(this).val() + String.fromCharCode(event.which);
                }
            }
        });

        $(".cbOnlyDealer input").click(function() {
            if ($(this).is(":checked")) {
                $(".cbIncludeDealer input").attr("checked", "true");
            }
        });
        $(".cbIncludeDealer input").click(function() {
            if (!$(this).is(":checked")) {
                $(".cbOnlyDealer input").attr("checked", "");
            }
        });

    });
</script>
