  load = function(url, id, cb) {
    if (d.getElementById(id)) {return;} //if it's already there, don't bother
    js = d.createElement(s); js.async=true; js.src = url; js.id = id;
    fjs.parentNode.insertBefore(js, fjs);
    if (cb) {$(js).load(function(){cb();}); }
  };
  
  load(('https:' == document.location.protocol ? '//ssl' : '//www') + '.google-analytics.com/ga.js', 'ga');      
  load('//connect.facebook.net/en_US/all.js#xfbml=1', 'fbjssdk');
  load('//apis.google.com/js/plusone.js', 'gplus1js');
  load('//platform.twitter.com/widgets.js', 'tweetjs',function(){
    twttr.events.bind('tweet', function(event) {
      _gaq.push(['_trackEvent', "twitter", "tweetshared", event.target.title]);
    });  
    twttr.events.bind('follow', function(event) {
      _gaq.push(['_trackEvent', "twitter", "followed", event.data.screen_name]);
    });
    twttr.events.bind('retweet', function(event) {
       _gaq.push(['_trackEvent', "twitter", "retweeted", "http://twitter.com/intent/tweet?in_reply_to="+event.data.source_tweet_id]);
    });  
  });

  
$(".shareCEM").click(function(){
  var el = $(this);
  _gaq.push(['_trackEvent', el.data("type"), "emailshared", el.data("slug")]);
});


(function(){
  function loadChartbeat() {
    window._sf_endpt=(new Date()).getTime();
    var e = document.createElement('script');
    e.setAttribute('language', 'javascript');
    e.setAttribute('type', 'text/javascript');
    e.setAttribute('src',
       (("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://static.chartbeat.com/") +
       "js/chartbeat.js");
    document.body.appendChild(e);
  }
  var oldonload = window.onload;
  window.onload = (typeof window.onload != 'function') ?
     loadChartbeat : function() { oldonload(); loadChartbeat(); };
})();

}(jQuery,document,'script'));
</script>



</div>
</body>
</html>