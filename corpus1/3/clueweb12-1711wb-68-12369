<script type="text/javascript">
$(function(){
  var state=1;
  var prevstate=0;
  var rot = function(){ 
    var arts = $(".header-article");
    $(arts[prevstate]).css({'zIndex':3});
    $(arts[state]).css({'zIndex':2}).show();
    $(arts[prevstate]).fadeOut('slow');
    prevstate = state;
    state++;
    if(state>=arts.length) state=0;
  }
  setInterval(rot, 8000);
});

</script>

