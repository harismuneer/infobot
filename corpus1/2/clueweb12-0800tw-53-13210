                        var ysc_check_target = function() {
                                if (Y.Media.Topstory) {
                                    var node_sharing = Y.one('.yom-socialchrome-login .status_now');
                                    if (!node_sharing) {
                                        return;
                                    }
                                    var optin_status = (node_sharing && node_sharing.get('innerHTML') === 'ON') ? true : false ;
                                    var enable_focus = node_sharing.hasClass('enable_focus') && optin_status ;
                                    var args = {
                                        sharing: optin_status,
                                        focus: enable_focus
                                    };
                                    Y.fire('ysc_sharing:status_changed', null, enable_focus);
                                    clearTimeout( ysc_timer );
                                } else if ( ysc_retry < 3 ) {
                                    setTimeout( ysc_check_target, 1000);
                                    ysc_retry++;
                                }
                            };
