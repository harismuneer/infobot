	<div class="fbnotify">	
	<fb:login-button scope="email" onlogin="$.post('/user/account/facebook-resync-user-data/'); $('#collectFold').slideUp(); _gaq.push(['_trackEvent', 'UserGenContent', 'Quotes', 'FacebookEmailSuccess']);">Use my Facebook e-mail to contact me</fb:login-button>
