'$':function(){
  var elements = new Array();
  for (var i = 0; i < arguments.length; i++) {
    var element = arguments[i];
    if (typeof element == 'string')
      element = document.getElementById(element);
    if (arguments.length == 1)
      return element;
    elements.push(element);
  }
  return elements;
},

'getElementsByClassName':function(classname,tag) {
 if(!tag) tag = "*";
 var anchs =  document.getElementsByTagName(tag);
 var total_anchs = anchs.length;
 var regexp = new RegExp('\\b' + classname + '\\b');
 var class_items = new Array()
 
 for(var i=0;i<total_anchs;i++) { //Go thru all the links seaching for the class name
  var this_item = anchs[i];
  if(regexp.test(this_item.className)) {
   class_items.push(this_item);
  }
 }
 return class_items;
},

'addEvent': function( obj, type, fn ) {  
     if (obj.addEventListener) {  
         obj.addEventListener( type, fn, false );  
           
     }  
     else if (obj.attachEvent) {  
         obj["e"+type+fn] = fn;  
        obj[type+fn] = function() { obj["e"+type+fn]( window.event ); }  
         obj.attachEvent( "on"+type, obj[type+fn] );    
     }  
     else {  
         obj["on"+type] = obj["e"+type+fn];  
     }  
 }, 

'goTo':function(url) {

window.open(url, '_top');

},

reflowFlag:false,

'forcePageReflow':function(flag) {

if (this.browser == 'Microsoft Internet Explorer'){

//Force page reflow for IE
var nb = this.$("navbar_nb");
var nbClass = nb.className;
nb.className = nbClass;
if(flag){
this.reflowFlag = true;
}
}

},

infowindow:'',

'openPopup':function(u){
var h=491;
var w=454;
var l=478;
var t=133;

if (screen.width <= 800){t=19;l=309;}
if(this.infowindow) {this.infowindow.close();}
this.infowindow = window.open(u,'infowindow','width='+w+',height='+h+',left='+l+',top='+t+',location=no,resizable=yes,menubar=no,status=no,toolbar=no,scrollbars=yes');
},

'getCookie':function(name) {
var dc = document.cookie;
var prefix = name + "=";
var begin = dc.indexOf("; " + prefix);
if (begin == -1) {
begin = dc.indexOf(prefix);
if (begin != 0) return null;
} else
begin += 2;
var end = document.cookie.indexOf(";", begin);
if (end == -1) end = dc.length;
return unescape(dc.substring(begin + prefix.length, end));
},

setCookie:function( name, value, expires, path, domain, secure )
{
var today = new Date();
today.setTime( today.getTime() );
if ( expires )
{
expires = expires * 1000 * 60 * 60 * 24;
}
var expires_date = new Date( today.getTime() + (expires) );
document.cookie = name + "=" +escape( value ) +
( ( expires ) ? ";expires=" + expires_date.toGMTString() : "" ) +
( ( path ) ? ";path=" + path : "" ) +
( ( domain ) ? ";domain=" + domain : "" ) +
( ( secure ) ? ";secure" : "" );
},

'ga':function() {

var question = this.$("navbar_nb_question");
                if (question.value == '') {

                        question.select();
                        question.focus();
                        return false;
                }
                else {
				this.rs();
				this.$("navbar_question_form").reset();
                return true;
                }

},

'allTrim': function(str) {
                return str.replace(/^\s+|\s+$/g, '');
            },
			
'rs':function() {
var percent = "%";
var pound = "#";
var regExp = new RegExp("["+percent+"]","g");
var regExp2 = new RegExp("["+pound+"]","g");
var query = this.$("navbar_nb_question").value;
query = query.replace(regExp,"%25");
query = query.replace(regExp2,"%23");
query = this.allTrim(query);
parent.top.location="http://personal.fidelity.com/global/search/inquira/resultsindex.shtml?question="+query;
},

