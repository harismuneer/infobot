    function UpdatePageSize(url, pagesize) {

        //set pagesize cookie
        var exdate = new Date();
        exdate.setDate(exdate.getDate() + 7);
        var c_value = "ps=" + pagesize + "; path=/;expires=" + exdate.toUTCString();
        document.cookie = c_value;
        location.href = url;
    }


    function AjaxReplacement(data, sliderInitUrl, outermin, outermax, min, max) {
        
        ajaxUpdateListComplete(data);

        
        if (outermin == undefined
        || outermax == undefined
        || min == undefined
        || max == undefined) {
        initSliderDefault(sliderInitUrl);
        }
        else {
        initSlider(sliderInitUrl, new PriceSliderValues(10, min, max, outermin, outermax, 'GBP', 1));
        }
        

    }
</script>
