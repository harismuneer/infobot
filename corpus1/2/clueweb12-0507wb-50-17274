<script type="text/javascript">
function dartClickEvent(id,url){
//these are for nav items
var dartmetrics = "";
var bOldmetrics = false;
if(typeof oMetricsTracker === "undefined"){
bOldmetrics = true;
dartmetrics = ngbsMetricsTracker;
}else{
dartmetrics = oMetricsTracker;
}
switch(id){
case "applyforcredit":
try{
dartTracker.trackEventOnClick("credapp", dartmetrics.nameplate(), 1, dartmetrics.getBrandType(), dartmetrics.revealNameplate());
if(bOldmetrics){
dartmetrics.applyForCreditFooterLinkMetrics();
}else{
dartmetrics.trackMicroData("applyForCreditFooterLink");
}
}catch(ee){}
break;
default:
break;
}
document.location = url;
}
</script>
