  <tr>
    <td valign="top"><div><h1>Flags of
          Afghanistan    </h1>
    </div>
	<div style="float:left; padding:5px;">
                   <script type="text/javascript">
google_ad_client = "pub-0543837335525010";
google_ad_width = 336;
google_ad_height = 280;
google_ad_format = "336x280_as";
google_ad_type = "text_image";
google_ad_channel = "4747863437";
