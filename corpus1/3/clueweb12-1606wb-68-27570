                  var referrerPage = document.referrer.toLowerCase();
                  var referrerPageNew = referrerPage.indexOf('for-sale/new');
                  var referrerPageUsed = referrerPage.indexOf('for-sale/used');
                  var referrerIsInventorySearch = (referrerPage.indexOf('inventorysearch.aspx') != -1) || (referrerPageNew != -1) || (referrerPageUsed != -1);

                  if(referrerPage.charAt(referrerPageNew+14) == "" || referrerPage.charAt(referrerPageUsed+15) == ""){
                  }else{
                  referrerIsInventorySearch = false;
                  }

                  if (referrerIsInventorySearch)
                  {
                  $("#back-to-inventory").live('click', function(){window.history.back();});
                  }
                  else
                  {
                  $("#back-to-inventory").attr("href", "/Tulsa-OK/For-Sale/Used/");
