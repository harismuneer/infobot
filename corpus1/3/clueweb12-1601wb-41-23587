    $(document).ready(function(){
//	$(".sr-action-buttons").fadeTo(0, .5);
//        $("#social_links a").hover(
//            function() {
//                $(this).children("img").fadeTo(250, 1);
//            },
//            function() {
//                $(this).children("img").fadeTo(250, .5);
//            }
//        );
	if(!$.browser.msie) {
	    $(".sr-action-buttons").fadeTo(0, .5);	    
	    $(".sr-row").mouseenter(function(evt) {
		$(this).find(".sr-action-buttons").fadeTo(250, 1);
	    });
	    $(".sr-row").mouseleave(function(evt) {
		$(this).find(".sr-action-buttons").fadeTo(250, .5);
	    });
	}
	
	$(".sr-arrow").fadeTo(0, .2);
	$(".sr-row").mouseenter(function(evt) {	  
	    $(this).find(".sr-arrow").fadeTo(250, 1);
	});
	$(".sr-row").mouseleave(function(evt) {	  
	    $(this).find(".sr-arrow").fadeTo(250, .2);
	});
	//
	//$('.searchfilter .head').click(function() {
	//	$(this).next().slideToggle('fast');
	//	return false;
	//}).next(); //.hide();
	//$("#filter_container").html('<img src="/media/img/loading-small.gif" alt="loading" style="vertical-align: middle">&nbsp;&nbsp;Loading search filters...');
	$("#filter_container").load(escape("/inventory/search-filters/class/new/make/Ford/model/Edge/"));
        $(".uibutton").button();
	
	
	
	$(".goauto-button:not(.ui-state-disabled)")
	    .hover(
		    function(){ 
			    $(this).addClass("ui-state-hover"); 
		    },
		    function(){ 
			    $(this).removeClass("ui-state-hover"); 
		    }
	    )
	    .mousedown(function(){
			    $(this).parents('.goauto-buttonset-single:first').find(".goauto-button.ui-state-active").removeClass("ui-state-active");
			    if( $(this).is('.ui-state-active.goauto-button-toggleable, .goauto-buttonset-multi .ui-state-active') ){ $(this).removeClass("ui-state-active"); }
			    else { $(this).addClass("ui-state-active"); }	
	    })
	    .mouseup(function(){
		    if(! $(this).is('.goauto-button-toggleable, .goauto-buttonset-single .goauto-button,  .goauto-buttonset-multi .goauto-button') ){
			    $(this).removeClass("ui-state-active");
		    }
	});
	
	
		
		
    });
    function update_limit(elem) {
	url="/inventory/search/class/new/make/Ford/model/Edge/?page=1&orderby=&orderdir=&limit="+$(elem).val();
	window.location=url;
    }
