var Application = 
 {
  lastId: 0,
  getNewId: function() {
    Application.lastId++;
    return "window_id_" + Application.lastId;
  }
  }

	function openW(mypage, myname, w, h, params)
	{
		var winl = (screen.width - w) / 2;
		var wint = (screen.height - h) / 2;
		winprops = 'height='+h+',width='+w+',top='+wint+',left='+winl+params
		swin = window.open(mypage, myname, winprops)
		if (parseInt(navigator.appVersion) >= 4) { swin.window.focus(); }
		
	}
    </script>

    

