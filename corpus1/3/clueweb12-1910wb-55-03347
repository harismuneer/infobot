<script type="text/javascript">
function replace_ads() {
	jQuery('#header .ad_728x90 .doubleclick_ad, #sidebar_right .ad_300_250 .doubleclick_ad, #universal-footer .doubleclick_ad').each(function() {
		var ad_tile = jQuery(this).attr('id').split('_').pop();
		var qv= window.ord || Math.floor(Math.random()*1E16);
		var js_url = jQuery('#ad_src_'+ ad_tile).html() + 'ord='+qv;
		iframe = document.createElement('iframe');
		iframe.src = src="about:blank";
		iframe.scrolling="no";
		iframe.height=jQuery(this).height();
		iframe.width='100%';
		iframe.frameBorder=0;
		iframe.style.border=0;
		iframe.allowTransparency="true";
		iframe.backgroundColor="transparent";
		iframe.hspace=0;
		iframe.vspace=0;
		iframe.marginHeight=0;
		iframe.marginWidth=0;
		jQuery(this).html('');
		jQuery(this).append(iframe);
		iframe.src = 'http://cbskluv.wordpress.com?load_ad=1&ad_url='+js_url;
	});
}
</script>
