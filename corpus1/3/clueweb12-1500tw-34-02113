    <script id="a12r3" type="text/javascript">
        
        function _m(x, k) {
		    var r = "";
			for(var i=0; i < x.length; i++) {
			    r += String.fromCharCode(x.charCodeAt(i) ^ k[i % k.length]);
			}
			return r;
		}
        
