    $.ajax(
        {
            type: 'GET',            url: '/user_location_instructions',            success: function(msg)
            {
                // Strip Devel HTML from the redirect link if needed.
                var str_position;
                str_position = msg.indexOf('<');
                if (str_position > 1)
                {
                    msg = msg.substring(0, str_position);
                }
                if (msg.match("redirect: "))
                {
                    msg = msg.replace(/redirect: /g, "");
                    msg = msg.replace(/^"/g, "");
                    msg = msg.replace(/"$/g, "");
                    window.location = msg
                }
                else
                {
                    switch (msg)
                    {
                        case '"popup_picker"':
                            empire_user_location_show_prov_theatre_picker()
                            break;
                        default:
                            break;
                    }
                }
            }
        }
    );
</script>    </div>    
