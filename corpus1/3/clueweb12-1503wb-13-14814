        <script type="text/javascript">
        home_url = 'http://www.thecornerofficedenver.com';

        // Sound
        backSound = null;
        soundManager.url = 'http://www.thecornerofficedenver.com/v100/wp-content/themes/CornerOffice/swf/';
        soundManager.debugMode = false;
        soundManager.onload = function() {
            backSound = soundManager.createSound({
              id: 'backSound',
              url: 'http://www.thecornerofficedenver.com/v100/wp-content/uploads/mp3/c3f60377fba07ac73282dbef2a4ce9db.mp3',
              volume: 70,
              autoLoad: false,
              autoPlay: false,
              loops: 5,
              onload: soundLoadedFunction
            });
        };
        function soundLoadedFunction() {
            backSound.play();
        }
        
        jQuery(function() {
            jQuery('#sound-button-on-off').click(function(){
                if (backSound.paused) backSound.play();
                else backSound.pause();
                return false;
            });

            jQuery.imgpreload([
                'http://www.thecornerofficedenver.com/v100/wp-content/themes/CornerOffice/images/section-gallery.png',
                'http://www.thecornerofficedenver.com/v100/wp-content/themes/CornerOffice/images/section-events.png',
                'http://www.thecornerofficedenver.com/v100/wp-content/themes/CornerOffice/images/section-drink.png',
                'http://www.thecornerofficedenver.com/v100/wp-content/themes/CornerOffice/images/section-food.png'
            ], {});
        });

        </script>

	<script>
    jQuery(document).ready(function(){
       jQuery(".iform").width(450);
       jQuery(".iform label").width(150);
       jQuery("#birthday").datePicker({clickInput:true, startDate: '04/15/1922'});
       jQuery("#wedding_anniversary").datePicker({clickInput:true, startDate: '04/15/1922'});
       jQuery(".dp-choose-date").hide();
    });
    function isEmailValid(email){
        var e = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
        return e.test(email);
    }
    function sendForm(){
        jQuery("#imessageOK, #imessageERROR").hide();
        jQuery(".required").removeClass("required");
        if(jQuery("#name").val()==""){jQuery("#name").addClass("required");return false;}
        if(jQuery("#address").val()==""){jQuery("#address").addClass("required");return false;}
        if(jQuery("#phone").val()==""){jQuery("#phone").addClass("required");return false;}
        if(jQuery("#email").val()==""){jQuery("#email").addClass("required");return false;}
        if(!isEmailValid(jQuery("#email").val())){jQuery("#email").addClass("required");return false;}
        if(jQuery("#birthday").val()==""){jQuery("#birthday").addClass("required");return false;}
	if(jQuery("#city").val()==""){jQuery("#city").addClass("required");return false;}       

        jQuery("#SendaMessage").val("Please Wait...");
        var AddDetails="";
        AddDetails+="&name="+encodeURIComponent(jQuery("#name").val());
        AddDetails+="&email="+encodeURIComponent(jQuery("#email").val());
        AddDetails+="&phone="+encodeURIComponent(jQuery("#phone").val());
        AddDetails+="&birthday="+encodeURIComponent(jQuery("#birthday").val());
        AddDetails+="&address="+encodeURIComponent(jQuery("#address").val());
        AddDetails+="&wedding_anniversary="+encodeURIComponent(jQuery("#wedding_anniversary").val());
	if(jQuery("#GeneralInterest").attr("checked")==true){
    		AddDetails+="&GeneralInterest="+encodeURIComponent(jQuery("#GeneralInterest").val());	
	}

	if(jQuery("#PrivateDining").attr("checked")==true){
    		AddDetails+="&PrivateDining="+encodeURIComponent(jQuery("#PrivateDining").val());	
	}

	if(jQuery("#Events").attr("checked")==true){
    		AddDetails+="&Events="+encodeURIComponent(jQuery("#Events").val());
	}

	if(jQuery("#Specials").attr("checked")==true){
    		AddDetails+="&Specials="+encodeURIComponent(jQuery("#Specials").val());
	}

        
        jQuery.ajax({
            type: "POST",
            url: "http://www.thecornerofficedenver.com/v100/wp-content/themes/CornerOffice/inquiry-send.php",
            data: AddDetails,
            success: function(data){

                if(data=='OK'){
                    jQuery(".iform ul").slideUp(function(){
                        jQuery("#imessageOK").fadeIn();
                    });
                } else {
                    jQuery("#imessageERROR").slideDown().append('<!--'+data+'-->');
                    jQuery("#SendaMessage").val("Send a Message!");
                }
            }
        });
    }
</script>

