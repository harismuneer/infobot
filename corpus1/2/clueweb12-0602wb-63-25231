<script>
	function showArtist()
	{
		document.getElementById("albumsmenu").style.display = "none";
		document.getElementById("lyricsmenu").style.display = "none";
		document.getElementById("artistmenu").style.display = "block";
		//document.getElementById("a").style.background=('url(../images/main-navigation-li-active.gif) no-repeat scroll bottom center');
	}
	function showLyrics()
	{
		document.getElementById("albumsmenu").style.display = "none";
		document.getElementById("artistmenu").style.display = "none";
		document.getElementById("lyricsmenu").style.display = "block";
	}
	function showAlbums()
	{
		document.getElementById("lyricsmenu").style.display = "none";
		document.getElementById("artistmenu").style.display = "none";
		document.getElementById("albumsmenu").style.display = "block";
	}
	function shownone()
	{
		document.getElementById("lyricsmenu").style.display = "none";
		document.getElementById("artistmenu").style.display = "none";
		document.getElementById("albumsmenu").style.display = "none";
	}
</script>	
