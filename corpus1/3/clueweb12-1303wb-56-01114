<script language="javascript" type="text/javascript">
    // <![CDATA[
    // A copy of this function also exists in SiteTemplate.inc for chat pages
    function setOmnitureEvars(sender, e, module) {
        e.cancelBubble = true;
        if (e.stopPropagation) {

            e.stopPropagation();
        }

        if (sender == undefined || sender == null)
            sender = typeof (e.target) == 'undefined' ? e.srcElement : e.target;


        if (window.sc == undefined) {
            includeOnTheFly("http://www.lawyers.com/Assets/Scripts/s_code.js", setOmnitureEvars, [sender, e, module]);
            return;
        }

        if (module != undefined)
            switch (module) {
            case "LGSS_Form": //Simple search widget
                sc.events = sc.linkTrackEvents = "event4";
                sc.linkTrackVars = "events";
                sc.tl(true, 'o', 'Search_Go_Button_Click');
                break;

            case "Search_Form_SortBy": //Search form sort by drop down
                var sortOption = sender.options[sender.selectedIndex].text;
                switch (sortOption) {
                    case "Relevance" : sc.eVar52 = "Sort By: R"; break;
                    case "Oldest First" : sc.eVar52 = "Sort By: O"; break;
                    case "Newest First" : sc.eVar52 = "Sort By: N"; break;
                }
                sc.linkTrackVars = "eVar52";
                sc.tl(true, 'o', 'Search_Form_SortBy');
                break;

            case "Search_Form_DateFilter": //Search form date filter
                sc.eVar53 = "With Date";
                sc.linkTrackVars = "eVar53";
                sc.tl(true, 'o', 'Search_Form_DateFilter');
                break;

            case "Forum_Filter_WithReadStatus": //Forum Filter With Read Status
                var value = sender.options[sender.selectedIndex].value;
                sc.eVar54 = value == "False" ? "read" : "not read";
                sc.linkTrackVars = "eVar54";
                sc.tl(true, 'o', 'Forum_Filter_WithReadStatus');
                break;

            default:
                break;
        }
        return true;
    }
    // ]]>
</script>


        <script  language="javascript"  type="text/javascript"> var s_code=sc.t();if(s_code)document.write(s_code); </script>	
