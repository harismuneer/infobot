// convert IP dot-decimal format to long int
function ip2long(ip) {
      var octets = ip.split('.');
      var iplong = 0;
      with (Math) {
            iplong = (parseInt(octets[0]) * pow(256,3)) + (parseInt(octets[1]) * pow(256,2)) + (parseInt(octets[2]) * 256) + parseInt(octets[3]);
      }
      return iplong;
}

// bitshift left
function lshift(num, bits) {
      return num * Math.pow(2,bits);
}

// bitshift right
function rshift(num, bits) {
      return num / Math.pow(2,bits);
}

// check if dot-decimal IP falls within CIDR network
function network_contains_ip(ip, network) {
      var longIP = ip2long(ip);
      var cidr = network.split('/');

      var subnet = ip2long(cidr[0]);
      var bits   = parseInt(cidr[1]);
      var mask   = lshift(-1, (32 - bits));

      subnet &= mask;

      return (longIP & mask) == subnet;
}
      
var ip_blocks = {
      onnetwork:[
                // STAFF SYSTEMS (192.220.128.0 ... 192.220.135.255)
                '192.220.128.0/21',
                // PUBLIC MACHINES (192.220.136.0 ... 192.220.143.255)
                '192.220.136.0/21',
                // REGIONAL WIFI (198.107.160.128 ... 198.107.160.255)
                '198.107.160.128/25',
                // NEIGHBORHOOD WIFI (198.107.161.0 ... 198.107.161.255)
                '198.107.161.0/24',
               ],

      central:  [
                 // CENTRAL STAFF (192.220.128.x, 192.220.128.x, 192.220.131.x, 192.220.136.x)
