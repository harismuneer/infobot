<script type="text/javascript">
    $(document).ready(function() {
        centerBottom();
        $("a.fancyframelink").fancybox({
        	'type': 'iframe',
        	'width': 420,
        	'height': 420,
        	'scrolling': 'no'
        });
        //setTimeout("if(!$('.bottomnav .forms').is(':visible'))ToggleForm('.bottomnav .question', '.bottomnav .buttonbar .requestinfo a');", 30000);
    });
	$(window).resize(centerBottom);
	function centerBottom() {
		$(".bottomnav").css("margin-left", (($(document).width() - $(".bottomnav").width()) / 2) - parseInt($(".bottomnav").css("padding-left").replace("px", "")));
		$("div.bottomnav").show();
	}

	function CloseBottomNav() {
		$(".bottomnav").fadeOut();
	}

	function ToggleForm(selector, elemSelector) {
		$(".bottomnav .innerForm").hide();
		$(selector).show();
		$(".bottomnav .buttonbar li a").each(
			function(){
				this.className = this.className.replace(" selected", "");
			}
		);
		cl = $(elemSelector).attr("class") || "";
		$(elemSelector).attr("class", cl + " selected");
    
		if(!$(".bottomnav .forms").is(":visible"))
			CloseBottomNav();
	}
</script>
