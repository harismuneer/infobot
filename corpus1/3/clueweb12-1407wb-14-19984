	<script type="text/javascript">
	// <![CDATA[
	var vtype = "new";
	var qsForm = document.getElementById('QuickSearchForm');
	
		function changeVCondition() {
			isnew = false;
			if( qsForm.condition.options) {
				isnew = qsForm.condition.value == 'new';
			} else {
				isnew = qsForm.condition[0].checked;
			}
			if(isnew) {
				if(vtype == "new") return;
				vtype = "new";
				qsForm.action = qsNewVehiclesLink;
				populateSelects();
			} else {
				if(vtype == "used") return;
				vtype = "used";
				qsForm.action = qsUsedVehiclesLink;
				populateSelects();
			}
		}
		
	
		function populateAuto2MakeSelect(desc,value) {
			var s = qsForm.make;
			s.length = 0;
			if ( desc.length <= 0 ) {
				s.options[0] = new Option("Select Make", "");
				return false;
			}
			s.options[0] = new Option("Select Make", "");
			for ( var i=0;i<desc.length;i++ ) {
				s.options[i+1] = new Option( desc[ i ], value[ i ] );
			}
			if(desc.length == 1) {
				s.selectedIndex = 1;
			} else {
				s.selectedIndex = 0;
			}
		}

		function populateAuto2ModelSelect( smake, desc, value ) {
			var s = qsForm.model;
			s.length = 0;
			if ( smake == "" || desc[smake].length <= 0 ) {
				qsForm.model.options[0] = new Option("Select Model", "");
				return false;
			}
			s.options[0] = new Option("Select Model", "");			
			
			for ( var i=0;i<desc[smake].length;i++ ) {
				s.options[i+1] = new Option( desc[smake][ i ], value[smake][ i ] );
			}
		}

		function populateSelects() {
				
				isVConditionNew = qsForm.condition[0].checked;
			
			if ( isVConditionNew ) {
				populateAuto2MakeSelect(new_makes_desc,new_makes_value);
				populateAuto2ModelSelect( qsForm.make.value, new_models_desc, new_models_value );				
			} else {
				populateAuto2MakeSelect(used_makes_desc,used_makes_value);
				populateAuto2ModelSelect( qsForm.make.value, used_models_desc, used_models_value );
			}
		}

		function makeSelected(){
			var makeValue = qsForm.make.value;
			if ( isVConditionNew ) {
				populateAuto2ModelSelect( qsForm.make.value, new_models_desc, new_models_value );
			} else {
				populateAuto2ModelSelect( qsForm.make.value, used_models_desc, used_models_value );
			}
		}

		var isVConditionNew = false;
		var new_makes_desc = new Array(1);
		var new_makes_value = new Array(1);
		var new_models_desc = new Array();
		var new_models_value = new Array();
		var used_makes_desc  = new Array();
		var used_makes_value  = new Array();
		var used_models_desc  = new Array();
		var used_models_value  = new Array();
	
			new_makes_desc[0] = 'Ford';
