<script>  

var gigyaCheckin = { action: "checkin" };
var pageView = { action: "viewpage"  };
var gigyaTweep = { action: "tweep" };
var gigyaFriend = { action: "facebookfriend" };
var loginState = null;


    gigya.services.socialize.addEventHandlers({
        onLogin: onLogin,
        onLogout: onLogout
    });
 

        function onLoad() {          			
			gigya.services.gm.notifyAction(pageView);
	        		       }  
	        		       
    function refreshUI()
    {
        gigya.services.socialize.getUserInfo({
            callback: renderUI
        });
    }
    
	function onLogout()
	{
		renderUI();
		gigya.services.gm.showAchievementsUI({containerID: 'divAchievements', excludeChallenges:'',width: 298});
	}
	
    function onLogin(user)
    {
        renderUI(user);
        gigya.services.gm.showUserStatusUI(userStatusParams); 
    }
    
    
    function renderUI(user)
    {
		if(typeof user != "undefined" && typeof user.user != "undefined")
		{
			user = user.user;
		}
		else
		{
			user = null;
		}
		
        if(user != null && user.isLoggedIn)
        {
            document.getElementById("zap-status").innerHTML = "<a href=\"http://www.zap2it.com/about/zap-rewards-profile,0,3057266.htmlstory\">Welcome, " + user.firstName + "</a>";
            document.getElementById("zap-logout").style.display = "";
        }
        else
        {
            document.getElementById("zap-status").innerHTML = "Sign in to connect with Zap2it:";
            document.getElementById("zap-logout").style.display = "none";
        }
    }
    
    function logout()
    {
        gigya.services.socialize.logout({
            forceProvidersLogout: false
        });
    }
	
	function onLoadHandler()
	{
		gigya.socialize.showAddConnectionsUI({
			containerID: "addConnectionsUI",
			showEditLink: false,
			showTermsLink: false,
			hideGigyaLink: true,
			width: 110,
			height: 30,
                        disabledProviders: 'orkut, renren, kaixin, vkontakte, mixi',
            UIConfig: '<config><body><controls><snbuttons buttonsize="20" /></controls></body></config>'
		});
	}
      
      

var userStatusParams = {
        containerID: 'divUserStatus', width: 298
		}
        var achievmentsParams = {
            containerID: 'divAchievements', width: 298
		}
        var leaderboardParams = {
            containerID: 'divLeaderboard', width: 298
        }
		var notificationParams = {
		}

 		function publishFeed(linkback, title, src) {

		    // Constructing a UserAction Object
		    var act = new gigya.services.socialize.UserAction();	    
			act.setLinkBack(linkback); // Adding a Link Back		
			act.setTitle(title); // Setting the Title			
			 act.addMediaItem({ // Adding a Media
				'src': src,
				'href': linkback,
				'type': 'image'
			 });
			   act.setActionName("shared"); //
		    
		    // Parameters for the publishUserAction method, 
			var params = 
			{
			    userAction:act, // including the UserAction object
			    scope: 'internal', // the Activity Feed will be published interanlly (site scope) only (not to social networks).
				privacy: 'public',
			};
			
            // Publish the User Action
			gigya.services.socialize.publishUserAction(params);

		}	
		
		


    function init() {
        refreshUI(); onLoadHandler(); onLoad();
    }
    
window.onload = init;


 // start script to check follow zap 
function twitterFollower(response) {  
if (typeof(response) == 'object' && response.hasOwnProperty("ids")) {
		var totalFollows = response.ids;
		var userFollows;
		var i=0;
		for ( property in totalFollows ) {
			userFollows += '|' + totalFollows[i] + '|';
			i++;
			}			
		var pos=userFollows.indexOf("34741439")
		if (pos>=0) {
    			gigya.services.gm.notifyAction(gigyaTweep);
		} 
	} 
} 
function twFriend(response) {
	if ( response.errorCode == 0 ) {        
		var user = response['user'];
		var ifTwitter = user.identities['twitter']; 
			if ( ifTwitter != null ) {  
				var twitterID = user.identities['twitter'].providerUID;		
				jQuery.ajax({
 					type: 'GET',
        			url: 'https://api.twitter.com/1/friends/ids.json',
        			data: {'user_id':twitterID,'callback':'twitterFollower'},
        			dataType: 'script',
        			cache: true,
        			success: function(response){ twitterFollower(response);}
    				});
        	}
  	}
}
// end script to check follow zap 
     // Start Friends badge check
function isFbFriend(isAlready) {
    if ( isAlready.errorCode == 0 ) {             
        if ( null!=isAlready.achievements && isAlready.achievements.length>0) {        
            var achievements;        
            for (var index in isAlready.achievements) {
                var currAchievement = isAlready.achievements[index];
                achievements += currAchievement ['challengeID'] + currAchievement ['level'] ;                 
            }            
            var achPos=achievements.indexOf("facebook1");
            var twPos=achievements.indexOf("tweep1");
            if (achPos=="-1")
            {
// add FB Friend Badge
function fbFriend(response) {  
    if ( response.errorCode == 0 ) {             
        var user = response['user'];
var totalLikes = user['likes'];
        var userLikes;
        var i=0;
        for ( property in totalLikes )        
{
userLikes += '|' + totalLikes[i].name + '|';
i++;
}
var pos=userLikes.indexOf("|Zap2it|")
if (pos>=0)
{
	gigya.services.gm.notifyAction(gigyaFriend);
} 
    }
}
gigya.services.socialize.getUserInfo({callback:fbFriend,extraFields:'likes'});
//end add FB Friend Badge
                } 
            if (twPos=="-1") 
            {
            gigya.services.socialize.getUserInfo({callback:twFriend,includeAllIdentities:true});
			}   
        }
    }
};     
var fbFriendParams = { 
	callback:isFbFriend,details: "basic"
};
gigya.services.gm.getChallengeStatus(fbFriendParams);
            // End friend badges check


