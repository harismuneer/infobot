  en4.core.runonce.add(function() {
    var url = '/utility/advertisement';
    var processClick = window.processClick = function(adcampaign_id, ad_id) {
      (new Request.JSON({
        'format': 'json',
        'url' : url,
        'data' : {
          'format' : 'json',
          'adcampaign_id' : adcampaign_id,
          'ad_id' : ad_id
        }
      })).send();
    }
  });
</script>

