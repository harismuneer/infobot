<script type="text/javascript"> 
function validateZipCode(zip, countrycode) {
		zip = zip.replace(" ", "");
		zip = zip.toUpperCase();
		var recheck;
		var errmsg="";
		switch(countrycode) {
			case 'US':	
				recheck=/^\d{5}$/;
				errmsg="Please enter a 5 digit zipcode";
				break;
			case 'NL':
				recheck=/^\d{4}[A-Z]{2}$/;
				errmsg="Please enter a valid zipcode, e.g 1076DX";
				break;			
			case 'UK':
				recheck=/^[A-Z]{1,2}[0-9][0-9A-Z]{0,1}[0-9][A-Z]{2}$/;
				errmsg="Please enter a valid postcode, e.g BN72AN";
				break;
			case 'AU':	
			case 'NZ':				
				recheck=/^\d{4}$/;
				errmsg="Please enter a 4 digit postcode";
				break;	
			case 'IN':				
				recheck=/^\d{6}$/;
				errmsg="Please enter a 6 digit postcode";
				break;					
			case 'CA':	
				recheck=/^[ABCEGHJKLMNPRSTVXY]{1}\d{1}[A-Z]{1}\d{1}[A-Z]{1}\d{1}$/;
				errmsg="Please enter a valid postcode, e.g T2X1V4";
				break;		
			};
		if (errmsg!="") {
			if (zip.search(recheck)==-1) return errmsg;
		}
		return "";
}
</script>	
<script type="text/javascript"> 
        $(function() {
            var offset = $("#Result").offset();
            var topPadding = 15;
            $(window).scroll(function() {
                if ($(window).scrollTop() > offset.top) {
                    $("#Result").stop().animate({
                        marginTop: $(window).scrollTop() - offset.top + topPadding
                    });
                } else {
                    $("#Result").stop().animate({
                        marginTop: 0
                    });
                };
            });
        });
	function searchDoctor(zip){
		var errmsg = validateZipCode(zip, "US");
		if (errmsg!="") {
			alert(errmsg);
			return false;
		}
		$('#searchResults').html('<img alt="Loading Image" src="http://www.choosenatural.com/wp-content/plugins/doctors_widget/loading.gif">');
		 jQuery.ajax({
			url: "http://www.choosenatural.com/wp/wp-admin/admin-ajax.php",
			type: 'POST',
			data: {
			action: 'search_doctor_by_zip_callback',zipcode:jQuery('#zipcode').val(),postid:2221,countrycode:'US',
			},
			dataType: 'html',
			success: function(response) {
			$('#searchResults').html(response);
			}
		});
		return false;
	}
	function searchDoctorByIP(latitude,longitude){ 
		$('#searchResults').html('<img alt="Loading Image" src="http://www.choosenatural.com/wp-content/plugins/doctors_widget/loading.gif">');
		 jQuery.ajax({
			url: "http://www.choosenatural.com/wp/wp-admin/admin-ajax.php",
			type: 'POST',
			data: {
			action: 'search_doctor_by_lat_lon_callback',lat:latitude,lon:longitude,postid:2221,
			},
			dataType: 'html',
			success: function(response) {
			$('#searchResults').html(response);
			}
		});	
	}				
	searchDoctorByIP('40.3484','-79.9221');
</script>		
