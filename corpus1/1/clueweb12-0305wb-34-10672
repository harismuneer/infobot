			if( styleIds.length > 4 ) {
				styleIds.length = 4;
			}
			var styleIdsString = styleIds.join();
			var link = '/j/i/29895/CompareFord.html?trimIds=' + styleIdsString;
			jazel.util.HTMLHelper.gotoUrl( link );
		};
		// Check Scroll of right column
		var checkScroll = function() {
			var initialY = dojo.coords( rightPanelContainer, true ).y;

			var minY = 0;
			var maxY = dojo.coords( rightPanelContainer ).h - dojo.coords( rightPanelNode ).h;

			var scrollY = dijit.getViewport().t;
			var deltaY = Math.min( Math.max( scrollY - initialY + 4, 0 ), maxY );

			dojo.style( rightPanelNode, "top", deltaY + "px" );
		};
		
		// Process query string and set default drop down values.
		((function(){var searchState = new jazel.util.FormPersist( {fieldId:arguments[1]} );})('search-string-state', 'search-state'));

