        function switchElem(nr)  {
            var i = 0;
            var elem = document.getElementById('elem' + i);
            while (elem != null)  {
                elem.style.display = (i == nr) ? "block" : "none";
                document.getElementById('e' + i).className = (i == nr) ? "h": "";
         i++;
                elem = document.getElementById('elem' + i);
            }
        }
