  <script type='text/javascript'>
    function google_ad_request_done(google_ads) {
      var ad_code = '';
      if (google_ads.length == 2) {
        var number_of_ads = 2;
        for(var i = 0; i < number_of_ads; ++i) {
          if (i == number_of_ads-2) {
            ad_code += '<p class="adsense_credit"><a href="' +
            google_info.feedback_url + '" class="googleClickout">Ads by Google</a></p>';
          }
          ad_code += '<p class="ad_title"><a href="' +
          google_ads[i].url + '" class="googleClickout">' +
          google_ads[i].line1 + '</a></p><p class="ad_abstract">' +
          google_ads[i].line2 + '&nbsp;' +
          google_ads[i].line3 + '<br /><a class="google_line_3 googleClickout" href="' +
          google_ads[i].url + '">' +
          google_ads[i].visible_url + '</a></p>';
        }
      
        if (google_ads[0].bidtype == "CPC") {  /* insert this snippet for each ad call */
          google_adnum = google_adnum + google_ads.length;
        }
      }

      document.write(ad_code);
    }

    google_ad_client = "ca-pub-9879162776784828";
    google_ad_output = 'js';
    google_max_num_ads = '2';
    google_ad_type  = "text";
    google_targeting = "content";
    google_ad_channel = "3901574437";
    google_feedback = 'on';          
    google_skip = google_adnum;
  
