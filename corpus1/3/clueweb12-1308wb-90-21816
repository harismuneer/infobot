		$('galleryDisplay').setStyle({display:'block'});
		$('galleryDisplay').innerHTML="<div style='margin-top:40px;'><embed height='240' width='320' wmode='transparent' align='bottom' quality='high' bgcolor='#FFFFFF' name='extSpinIzmo' id='extSpinIzmo' src='http://media.dealeron.com/photo/flix/SSCUSA/Ford/Edge/2012/5OD-4 SE/360.swf' type='application/x-shockwave-flash'/></div>";
		$('zoomMedia').setStyle({display:'block',left:'455px'});
