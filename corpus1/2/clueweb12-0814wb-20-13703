<div id="fb">
	<script type="text/javascript">
		
		window.fbAsyncInit = function() {
			FB.init({
				appId:'384d4d01492d29c0c0db054fec93cc90', 
				status:true,
				cookie:true, 
				xfbml:true
			});
		};
		(function() {
    		var e = document.createElement('script'); e.async = true;
    		e.src = document.location.protocol +
      		'//connect.facebook.net/en_US/all.js';
    		document.getElementById('fb-root').appendChild(e);
  		}());
	
		function facebookLogin() 
		{
			var loginUrl = 'https://www.facebook.com/login.php?api_key=384d4d01492d29c0c0db054fec93cc90&cancel_url=http%3A%2F%2Fsportsshow.comedycentral.com%2Fcrabapple%2Ffragments%2Ffacebook%2Flogin&display=popup&fbconnect=1&next=http%3A%2F%2Fsportsshow.comedycentral.com%2Fcrabapple%2Ffragments%2Ffacebook%2Flogin&return_session=1&session_version=3&v=1.0&req_perms=publish_stream';
			loginUrl = loginUrl.replace('loggedOut%3Dtrue', '');
			window.open(loginUrl,"popup","height=240,width=575,modal=yes,alwaysRaised=yes");
			return false;
		}
		function getCookie(c_name)
		{
       		if (document.cookie.length>0) {
            	c_start=document.cookie.indexOf(c_name + "=");
            	if (c_start!=-1) {
               		c_start=c_start + c_name.length+1;
               		c_end=document.cookie.indexOf(";",c_start);
               		if (c_end==-1) c_end=document.cookie.length;
               			return unescape(document.cookie.substring(c_start,c_end));
            	}
        	}
        	return false;
		}
		function refreshAfterLogin()
		{
			var hash = window.location.hash;
			var href = window.location.href;
			if (hash != "") window.location.href = href.replace(hash, ""); 
			window.location.reload();
		}
		function showFacebookProfile()
		{
			var cookie = getCookie('fbs_384d4d01492d29c0c0db054fec93cc90');
			if (cookie) cookie = cookie.replace('"', "");
			var url = 'http://sportsshow.comedycentral.com/crabapple/fragments/facebook/user?'+ cookie;
						$(".fbContent").load(url, function(response, status, xhr) {
				if (response.indexOf("error") > 0) {
					return;
				}
			});
		}
		function showLoginButton()
		{
			FB.logout(function(response){
				if ($("#fbloggedin").val() == "true") refreshAfterLogin();
			});
			deleteFacebookCookie();
			$(".fbContent").html('<a class="connectLoginButton" href="javascript:void(0);" target="_blank" onclick="return facebookLogin();"><span>Login</span></a>');
			
		}
		function facebookLogout() {
			FB.getLoginStatus(function(response) {
				if (response.session) {
					var url = 'http://www.facebook.com/logout.php?api_key=384d4d01492d29c0c0db054fec93cc90&;session_key={s}&next=http%3A%2F%2Fsportsshow.comedycentral.com%2Fcrabapple%2Ffragments%2Ffacebook%2Flogout';
					url = url.replace("{s}", response.session.session_key);
					window.open(url,"popup","height=240,width=575,modal=yes,alwaysRaised=yes");
				}
				else {
					var session = FB.getSession();
					if (session!=null) {
						var url = 'http://www.facebook.com/logout.php?api_key=384d4d01492d29c0c0db054fec93cc90&;session_key={s}&next=http%3A%2F%2Fsportsshow.comedycentral.com%2Fcrabapple%2Ffragments%2Ffacebook%2Flogout';
						url = url.replace("{s}", session.session_key);
						window.open(url,"popup","height=240,width=575,modal=yes,alwaysRaised=yes");
					}		
				}
			});
			
			var session = FB.getSession();
			if (session!=null) {
				var url = 'http://www.facebook.com/logout.php?api_key=384d4d01492d29c0c0db054fec93cc90&;session_key={s}&next=http%3A%2F%2Fsportsshow.comedycentral.com%2Fcrabapple%2Ffragments%2Ffacebook%2Flogout';
				url = url.replace("{s}", session.session_key);
				window.open(url,"popup","height=240,width=575,modal=yes,alwaysRaised=yes");
			}
			else {
				var url = 'http://www.facebook.com/logout.php?api_key=384d4d01492d29c0c0db054fec93cc90&;session_key={s}&next=http%3A%2F%2Fsportsshow.comedycentral.com%2Fcrabapple%2Ffragments%2Ffacebook%2Flogout';
				url = url.replace("session_key={s}", "");
				window.open(url,"popup","height=240,width=575,modal=yes,alwaysRaised=yes");
			}
			return false;
		}
		function deleteFacebookCookie()
		{
			var name = 'fbs_384d4d01492d29c0c0db054fec93cc90';
			document.cookie = name + "=" + ";expires=Thu,01-Jan-1970 00:00:01 GMT";
		}
		function getFacebookCookie() 
		{
			return getCookie('fbs_384d4d01492d29c0c0db054fec93cc90');
		}
		$(document).ready(function() {
			var cookie = getCookie('fbs_384d4d01492d29c0c0db054fec93cc90');
			if (cookie) showFacebookProfile();
			$(".fbContent").show();
		});
		
	</script>
