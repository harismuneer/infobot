<script type="text/javascript">


jQuery(document).ready(function() {

		// scrolls buttoms
		jQuery('#footer .navega span.totop').click(function(){jQuery.scrollTo( '.sf-menutop', 800 );});

		jQuery("#top-minimenu ul li a").hover(function() {
			jQuery(this).stop().animate({color: "#000"}, 500);
		},function() {
			jQuery(this).stop().animate({color: "#888"}, 500);
		});
		
		jQuery(".ktt-columnpost-comments a").hover(function() {
			jQuery(this).stop().animate({backgroundColor: "#666"}, 300);
		},function() {
			jQuery(this).stop().animate({backgroundColor: "#bbb"}, 800);
		});
		
		jQuery("#mainmenu ul.sf-navbar ul.sub-menu li a ").hover(function() {
			jQuery(this).stop().animate({color: "#720"}, 300);
		},function() {
			jQuery(this).stop().animate({color: "#999"}, 300);
		});
		
		jQuery(".ktt-columnpost-item .sombrita").hover(function() {
			jQuery(this).find('img').stop().animate( {backgroundColor: "#f0f0f0"}, 500 );
		},function() {
			jQuery(this).find('img').stop().animate({backgroundColor: "#fff"}, 300);
		});
		
		jQuery(".widget .recentcomments li .comment, #authorpage .lastcomments li").hover(function() {
                jQuery(this).stop().animate({color: "#000 !important"}, 500);
                },function() {
                jQuery(this).stop().animate({color: "#777"}, 500);
                });
});




</script>
		
			
						
