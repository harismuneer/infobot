  trackEvent('Footer Links', 'Footer Loaded', window.g_clickTrackFooterType);

  var footer = $('footer');
  if(footer.readAttribute('data-recording-clicks') != "true") {
    footer.observe("click", function(evt) {
      var href = evt.element().readAttribute('href');
      if(href) {
        trackEvent('Footer Links', 'Click: ' + href, window.g_clickTrackFooterType);
      }
    });
    footer.writeAttribute('data-recording-clicks', "true");
  }
</script>

