    <script type="text/javascript">
			$(document).ready(function(){
    		var adapter = new Object(),
					event = "",
					webPageName = 'Vehicle';
				switch (webPageName.toLowerCase()) {
					case "newinventorysearch":
					case "usedinventorysearch":
						adapter = new CM.SisterInventoryPage();
						event = "updatePage_complete"
						break;
					case "vehicle":
						adapter = new CM.SisterVehiclePage();
						break;
					case "mobile":
						var vehicleId = 0;
						adapter = new CM.SisterMobileVehiclePage(vehicleId);
						break;
				}
				var options = {
						adapter: adapter,
						event: event,
						webPageName: webPageName
					},
					sisterIntegration = new CM.SisterIntegration(options);
				$.event.trigger("Sister_Data_Loaded", sisterIntegration);
      });
		</script>
