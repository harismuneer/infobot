<script type="text/javascript">
    (function($) {

        $('.mod-blog-entry-contents').each(function(index) {
            var contentheight = $(this).innerHeight();
            var parentdivheight = $(this).parents('div').innerHeight();
            var toppadding = (parentdivheight - contentheight) / 2;
            $(this).children('.mod-blog-entry-title').css({ 'padding-top': toppadding });
        });

        $('.table_only').parent().removeClass('brdr1');

        var int = window.setInterval(generateOutput, 1000);
        var count = 0;

        function generateOutput() {
            newH = $('.stmAdHeight').height();
            $('.mod-blog-entries').height(newH - 75);
            count++;
            if (count > 2) {
                window.clearInterval(int);
            }
        }
    })(jQuery);
</script>
