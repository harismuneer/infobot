  <script type="text/javascript">
        var listingsListingDealsSubscription_overview = new ListingDealsSubscription();
        listingsListingDealsSubscription_overview.initialize = function() {

                this.email = $('#listingsSubscriptionEmail_overview').val();
            this.zip = $('#newSearchHeaderForm_zip').val();

            if (this.zip == null || this.zip == '') {
                this.zip = $('#listingsSubscriptionZip').val();
            }

            this.distance = $('#distance').val();

            this.entityId = $('#selectedEntity').val();
            this.entityId2 = $('#selectedEntity2').val();

            if (this.entityId == null || this.entityId == '') {

                this.entityId = 'd923';
                this.entityId2 = '';
            }

			this.showNegotiable = $('#pricelessCheckbox').attr("checked");

            var transmissionFilterRadio = $('#transmissionFilterDiv input:radio:checked').val();
            if (transmissionFilterRadio == 'M') {
                this.transmission = false;
            } else if(transmissionFilterRadio == 'A') {
                this.transmission = true;
            } else {
                this.transmission = null;
            }

            var checkedColorsCount = $('#exterior_color_filter input:checked').size();
            var totalColorsCount = $('#exterior_color_filter input').size();
            if (totalColorsCount != checkedColorsCount && checkedColorsCount != 0) {
                var scopedColors = new Array();
                $('#exterior_color_filter input:checked').each(function(index, value) {
                    scopedColors.push($(this).val());
                });
                this.colors = scopedColors;
            } else {
                this.colors = [];
            }

            var checkedWheelSystemsCount = $('#wheel_system_filter input:checked').size();
            var totalWheelSystemsCount = $('#wheel_system_filter input').size();
            if (totalWheelSystemsCount != checkedWheelSystemsCount && checkedWheelSystemsCount != 0) {
                var scopedWheelSystems = new Array();
                $('#wheel_system_filter input:checked').each(function(index, value) {
                    scopedWheelSystems.push($(this).val());
                });
                this.wheelSystems = scopedWheelSystems;
            } else {
                this.wheelSystems = [];
            }


            var checkedEnginesCount = $('#engine_filter input:checked').size();
            var totalEnginesCount = $('#engine_filter input').size();
            if (totalEnginesCount != checkedEnginesCount && checkedEnginesCount != 0) {
                var scopedEngines = new Array();
                $('#engine_filter input:checked').each(function(index, value) {
                    scopedEngines.push($(this).val());
                });
                this.engines = scopedEngines;
            } else {
                this.engines = [];
