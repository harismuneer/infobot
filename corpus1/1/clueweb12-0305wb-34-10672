		if( nextStepsPane != null ) {
			// Handle "action" event
			dojo.connect( nextStepsPane, "onAction", function( linkTarget ) {
				var action = linkTarget.toUpperCase();
				switch( action ) {
				case "PRINT":
					jazel.util.HTMLHelper.printPage();
					break;
				case "CONTACTUS":
					contactUsFormPane.openDialog();
					break;
				case "EMAIL":
					emailFormPane.openDialog();
					break;
				case "QUICKQUOTE":
					quickQuoteFormPane.openDialog();
					break;
				case "MAKEANOFFER":
					makeAnOfferFormPane.openDialog();
					break;
				case "TESTDRIVE":
					testDriveFormPane.openDialog();
					break;
				case "TRADEINVALUE":
					tradeInValueFormPane.openDialog();
					break;
				}
