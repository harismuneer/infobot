$('#poem_title').keyup(function(e) {
if(e.keyCode == 13) {
showLoader();
if(jQuery.trim($("#poem_title").val())==''){alert('Please enter search value.');}else{
$('#searchcont').fadeIn(1500);
$('#searchajtitle').html('Search Result - '+$("#poem_title").val());
$('#searchajtxt').html('You have searched for poem titles containing "'+$("#poem_title").val()+'". Please find below the list of poems having "'+$("#poem_title").val()+'" in their poem title. plesae click on the poem title to browse the poem'); 
$.ajax({
type: "POST",
url: "search_poem.php",
data: "val="+jQuery.trim($("#poem_title").val()),
success: function(msg){
 $("#searchcont").html(msg).hide().fadeIn('slow');
}
});
}return false;
}
 });
