function ValidateEmail(theinput)
{
	s=theinput.value.trim()
	if(s.trim() == '')
	{
	return true
	}
	if(s.search)
	{
		return (s.search(new RegExp('^[_a-z0-9-]+(\\.[_a-z0-9-]+)*@[a-z0-9-]+(\\.[a-z0-9-]+)*(\\.[a-z]{2,6})$','gi'))>=0)
	}
