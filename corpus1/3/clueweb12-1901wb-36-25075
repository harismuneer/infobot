preloadedImage5.src="../southerncharmproto/menu_header_online_store_orange.gif";
var preloadedImage6 = new Image();
preloadedImage6.src="../southerncharmproto/menu_header_online_store.gif";
var preloadedImage7 = new Image();
preloadedImage7.src="../southerncharmproto/menu_header_information_orange.gif";
var preloadedImage8 = new Image();
preloadedImage8.src="../southerncharmproto/menu_header_information.gif";
function switchImages(_onOff) {
if (_onOff=="on")
document.home.src=preloadedImage1.src;  
else
document.home.src=preloadedImage2.src;  
}
function switchImages1(_onOff) {
if (_onOff=="on")
document.custom.src=preloadedImage3.src;  
else
document.custom.src=preloadedImage4.src;  

}
function switchImages2(_onOff) {
if (_onOff=="on")
document.online_store.src=preloadedImage5.src;  
else
document.online_store.src=preloadedImage6.src;  

}
function switchImages3(_onOff) {
if (_onOff=="on")
document.information.src=preloadedImage7.src;  
else
document.information.src=preloadedImage8.src;  

}
