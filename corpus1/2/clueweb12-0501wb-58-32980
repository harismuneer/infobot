    _(document).ready(function() {
        _('.rollover').hover(
        function() {                        
            var imgUrl = _(this).children().attr('src');            
            var newImageUrl = imgUrl.replace('.gif', '_over.gif');
            _(this).children().attr('src', newImageUrl);
        },
        function() {
            var imgUrl = _(this).children().attr('src');
            var newImgUrl = imgUrl.replace('_over', '');
            _(this).children().attr('src', newImgUrl);
        });
    });    
</script>

