                <script type="text/javascript">

            // IE8 bug where if the form is hidden by default you can't submit with the enter key
            // Ref: http://stackoverflow.com/questions/964734/hitting-enter-does-not-post-form-in-ie8
            $('signin_menu').hide();
			
            function displayLoginBox() {
                $('signin_menu').appear({ duration: 0.3 });
            }

            Event.observe(document, 'click', function(event) {

                var targetel = Event.element(event);

                while (targetel && targetel.id != "signin_menu") {
                    targetel = targetel.parentNode;
                }

                if (targetel == null) {
                    if($('signin_menu') != undefined){
                        $('signin_menu').hide();
                    }
                }
            });
        </script>
        		
