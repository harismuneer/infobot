function showVideo(ele, link)
{
	var videoObj = '<object height="344" width="425" class="video_object"><param value="' + link + '" name="movie"/><param value="true" name="allowFullScreen"/><embed height="344" width="425" allowfullscreen="true" type="application/x-shockwave-flash" src="' + link + '"/></object>';
	ele.html(videoObj);
}

function stopVideo(ele)
{
	ele.html('');
}

