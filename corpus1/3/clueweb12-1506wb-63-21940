<script>
(function($) {

$(document).bind('user.load', function() {
	console.log('user loaded');
	$('#logged_out').hide();
	$('#logged_in').show();

	if (User.firstname()) {$('#userbar_name').text(User.firstname());} else {$('#userbar_name').text(User.email());}
});

$('#login').bind('click', function(e) {
	e.preventDefault();
	User.get(function() {
		$('#logged_in').effect('highlight', {}, 1000);
	}, true);
});

$('#logout').bind('click', function(e) {
	e.preventDefault();
	$('#logout_spinner').show();
	User.logOut();
});

if (User.authenticated()) {User.load();}

})(jQuery);


</script>

