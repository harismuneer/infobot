<script type="text/javascript">
$(document).ready(function() {
	$('form#newsletterSignUpForm').submit(function() {
		if ($('form#newsletterSignUpForm #title option:selected').val().toString() == '') {
			window.alert('You must select a valid Title!');
			$('form#newsletterSignUpForm #title').focus();
			return false;
		} else if ($('form#newsletterSignUpForm #email').val().toString() == 'Email Address' || $('form#newsletterSignUpForm #email').val().toString().indexOf('@') == -1 || $('form#newsletterSignUpForm #email').val().toString().length == 0) {
			window.alert('You must enter a valid Email Address!');
			$('form#newsletterSignUpForm #email').focus();
			return false;
		} else if ($('form#newsletterSignUpForm #name').val().toString() == 'Name' || $('form#newsletterSignUpForm #name').val().toString().length == 0) {
			window.alert('You must enter a valid Name!');
			$('form#newsletterSignUpForm #name').focus();
			return false;
		}
		
		return true;
	});
});
</script>			</div>
