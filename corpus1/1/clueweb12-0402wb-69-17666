    function submitAskAutoClubQuestion2() {
      var redirectUrl = '/Cars/autoclub/members/newTopic.action?startUrl=%2FCars%2Fautoclub%2Fforum-ford-edge-ac846&autoClub=846&selectedEntity=d923&topic=';
      checkEmptyField('askQuestionTopic2', 'Start a new Ford Edge question.');
      if ($('#askQuestionTopic2').val() != 'Start a new Ford Edge question.') {
        redirectUrl += encodeURIComponent($('#askQuestionTopic2').val());
      };

      document.location.href = redirectUrl;
    }
  </script>


  <div class="ui-corner-all" style="border:1px solid #8598C0;padding:12px 8px;margin:0 0 25px;background-color:#eaf4fd;">




        
