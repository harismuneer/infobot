<script type="text/javascript">
/*<![CDATA[*/
			
			jQuery.fn.flashBack.addConfig(1, {
				container: '#semanticslideshowwrap',
				getData: function(){return ''}
			});

				(function(){
		jQuery.fn.flashBack.addConfig(2, {
			container: '.modelRibbon',
			beforeReady: function (flashObj) {
				if (flashObj) {
					var $_this = jQuery(this),
						handlers = jQuery('.handler a:not(.override)', this),
						buttons = handlers.filter(function () {var t = $_this.text(); return (t === 'prev' || t === 'next')}),
						click = function() {
							var txt = $(this).text();
							if (!$(this).hasClass('active')) flashObj.select(txt.toString().toLowerCase());
							if (txt != 'prev' && txt != 'next') {
								handlers.removeClass('active');
								$(this).addClass('active');
							}
							return false;
						},
						deactivate = function() {return false};
					// assign flash interface to html handlers
					handlers.click(click);
					// assign js interface to flash object
					flashObj.update = function (txt) {
						txt = txt.toLowerCase();
						if (txt === 'disable') {
							buttons.unbind('click', click).bind('click', deactivate);
						} else if (txt === 'enable') {
							buttons.unbind('click', deactivate).bind('click', click);
						} else if (txt && handlers.find('.active').text().toLowerCase() != txt) {
							handlers.removeClass('active').filter(function (){
								return jQuery(this).text().toLowerCase() === txt;
							}).addClass('active');
						}
					};
				}
			}
		});
	})();

				(function(){
		jQuery.fn.flashBack.addConfig(2, {
			container: '.modelRibbon',
			beforeReady: function (flashObj) {
				if (flashObj) {
					var $_this = jQuery(this),
						handlers = jQuery('.handler a:not(.override)', this),
						buttons = handlers.filter(function () {var t = $_this.text(); return (t === 'prev' || t === 'next')}),
						click = function() {
							var txt = $(this).text();
							if (!$(this).hasClass('active')) flashObj.select(txt.toString().toLowerCase());
							if (txt != 'prev' && txt != 'next') {
								handlers.removeClass('active');
								$(this).addClass('active');
							}
							return false;
						},
						deactivate = function() {return false};
					// assign flash interface to html handlers
					handlers.click(click);
					// assign js interface to flash object
					flashObj.update = function (txt) {
						txt = txt.toLowerCase();
						if (txt === 'disable') {
							buttons.unbind('click', click).bind('click', deactivate);
						} else if (txt === 'enable') {
							buttons.unbind('click', deactivate).bind('click', click);
						} else if (txt && handlers.find('.active').text().toLowerCase() != txt) {
							handlers.removeClass('active').filter(function (){
								return jQuery(this).text().toLowerCase() === txt;
							}).addClass('active');
						}
					};
				}
			}
		});
	})();

				(function($) {
		var sliderLength = parseInt('50' || '0', 10),
			val = parseInt(sliderLength / 2, 10),
			widgetSelector = $('#flashWidget3a').parents('.widgetWrap'),
			UI = $('.slider', widgetSelector),
			config = {
								container: widgetSelector,
				beforeReady: function(flashObj) {
					if (config.debug && typeof window.console === "object" && typeof console.log === "function") {
						console.log('[coverflow] - this:',this);
					}
                    $(widgetSelector).each(function() {
                        UI.slider({
                            handle: '.handle',
                            max: sliderLength,
							min: 1,
                            step: 1,
                            slide: function (e, ui) {
                                if (ui.value != val) {
                                    val = ui.value;
                                    flashObj.update(ui.value);
                                }
                            }
                        });
                        $(UI).slider('value', Math.round(sliderLength / 2));
                    });
				},
				afterReady: function(flashObj) {
					flashObj.UI = UI;
					UI.update = function(val) {
						UI.slider('value', val);
					};
                    flashObj.update(val);
                        $(".fvCoverflowSlider .disabled").removeClass('disabled').each(function() {
                            $(this).bind('click', function() {
                                                                var computedVal = $(this).hasClass('prev') ? UI.slider('value') - 1 : $(this).hasClass('next') ? UI.slider('value') + 1 : 0
                                    val = (computedVal < 1) ? 1 : (computedVal > sliderLength ? sliderLength : computedVal);
                                UI.slider('value', val);
                                flashObj.update(val);
                                return false;
                            });
                        });
                        $('.slider', widgetSelector).slider('enable');
                    }
			};
		jQuery.fn.flashBack.addConfig(3, config);
	})(jQuery);

						jQuery(function(){
			$('.flip3d .viewAll').unbind('click').bind('click', function(ev){
				var href = $(ev.target).attr('href');
				if (href) {
					window.top.location.href = this.href;
				}
				return false;
			});
		});
		
				jQuery(function($){
		$('.buttonBlock li').each(function(i){
			$('a', this).click(function(){
								if (window.gaManager) {
					gaManager._trackEvent("button-block-style1", "click", $(this).attr('href'), i);
				} else if (window.gaPageTracker) {
					gaPageTracker._trackEvent("button-block-style1", "click", $(this).attr('href'), i);
				}
			});
		});
	});

			
		
			
				var trebuchet_bold_gradient = {
										src: 'http://static.dealer.com/v8/global/images/sifr/trebuchet_bold_gradient.swf'
									};

			
				var georgia = {
										src: 'http://static.dealer.com/v8/global/images/sifr/georgia.swf'
									};

			
			sIFR.delayCSS  = true;
			sIFR.activate(trebuchet_bold_gradient,georgia);

		
		
			
				jQuery(function ($) {
					


// for vista brown white color variation



				});

			
		

				jQuery(function($) {
							});

				jQuery(function($) {
		// smartbrowse setup
		var trueInventoryType,
			forceMakeSelection = [],
			getAnimatedVehicleTotals = function () {
					return ($('object[name=animatedVehicleTotals]').get(1) && $('object[name=animatedVehicleTotals]').get(1).spinTo ) ?
						$('object[name=animatedVehicleTotals]').get(1) : $('object[name=animatedVehicleTotals]').get(0);
			},
			animatedVehicleTotalsInterval,
			firstInterval = true,
			intervalTimeout = 800,
			firstIntervalTimeout = 1600,
			setAnimatedVehicleTotals = function (vehicleCount) {
				clearInterval(animatedVehicleTotalsInterval);
				animatedVehicleTotalsInterval = setInterval(function () {
					firstInterval = false;
					clearInterval(animatedVehicleTotalsInterval);
					getAnimatedVehicleTotals().spinTo({target: vehicleCount});
				}, firstInterval ? firstIntervalTimeout : intervalTimeout);
			},
			smartbrowse = new DDC.GenericSmartbrowse({
				inventoryTypes: {
						'new': {name: 'new', inv: '/new-inventory/index.htm', helios: '/404.html?type=2', ajax: '/smartbrowse/ajax/new.htm'}
	,'used': {name: 'used', inv: '/used-inventory/index.htm', helios: '/404.html?type=1', ajax: '/smartbrowse/ajax/used.htm'}
	,'certified': {name: 'certified', inv: '/404.html', helios: '/404.html?certified=true', ajax: '/smartbrowse/ajax/certified.htm'}
	,'certified-used': {name: 'certified-used', inv: '/404.html', helios: '/404.html?certified=true', ajax: '/smartbrowse/ajax/certified.htm'}
	,'commercial': {name: 'commercial', inv: '/404.html', ajax: '/smartbrowse/ajax/commercial.htm'}
	,'commercial-new': {name: 'commercial-new', inv: '/404.html', ajax: '/404.html'}
	,'commercial-used': {name: 'commercial-used', inv: '/404.html', ajax: '/404.html'}
	,'wholesale': {name: 'wholesale', inv: '/404.html', ajax: '/404.html'}
	,'wholesale-new': {name: 'wholesale-new', inv: '/404.html', ajax: '/404.html'}
	,'wholesale-used': {name: 'wholesale-used', inv: '/404.html', ajax: '/404.html'}
	,'exotic-new': {name: 'exotic-new', inv: '/404.html', ajax: '/404.html'}
	,'exotic-used': {name: 'exotic-used', inv: '/404.html', ajax: '/404.html'}
	,'helios': {name: 'helios', inv: '/404.html', ajax: '/404.html'}
				},
				params: {
					detect: 'false',
					reset: 'InventoryListing',
						'showYear': 'true'
	,'showMake': 'true'
	,'showModel': 'false'
	,'showAllModels': 'false'
	,'showTrim': 'false'
	,'showBodyStyle': 'false'
	,'showMileage': 'false'
	,'showPrice': 'true'
	,'showLocation': 'false'
	,'showHighywayMpgs': 'false'
				},
				responseProcessors: [
					DDC.GenericSmartbrowse.responseProcessors.ajax.removeNullKeyEntries,
					DDC.GenericSmartbrowse.responseProcessors.ajax.nameValueReplacement(/^GMC Pontiac Buick$/, 'GMC')
					,DDC.GenericSmartbrowse.responseProcessors.ajax.compliance(['ford'])                    
				],
				requestProcessors: [
																
						DDC.GenericSmartbrowse.requestProcessors.ajax.compliance(['ford'])
									],
				view: function(response) {
					var k, k2, i, itemCount, currentVehicleCount, totalVehicleCount, vehicleCount = 0, vehicleCountHtml = '',
						selectedItemCount = 0,
						vehicleTypeLabel, select, el, params, selectedIndex, newParams = {}, bodyStyleHref, bodyStyleImagesHtml,
						bodyStyleIncludeArray = ['Convertible', 'Coupe', 'Hatchback', 'Hybrid', 'Sedan', 'SUV', 'FULLSIZE PICKUP', 'Van', 'Mini-Van', 'Wagon'];

					if( forceMakeSelection.length > -1 ){
						if( ( jQuery.inArray(this.getInventoryType().name, forceMakeSelection) !== -1 ) &&
								(typeof this.getParams().SBmake == 'undefined' || this.getParams().SBmake == 'clear' ) ){
							var membersToShow = ['SBmake','SByear','SBprice','SBdefaults'];
							for( member in response){
								if(jQuery.inArray(member,membersToShow) === -1 ){
									response[member] = {};
								}
							}
						}
					}
					for (k in response) {
						if ((k !== 'SBdefaults') && (k !== 'defaults') && (typeof response[k] === 'object') && !(response[k] instanceof Array)) {
							itemCount = 0;
							totalVehicleCount = 0;
							selectedIndex = 0;
							selectedItemCount = 0;
							params = this.getParams();
							el = null;

							if ($('[name=SB' + k.toLowerCase().replace('internetprice', 'price') + ']', '#ajaxSmartbrowse').size()) {
								select = $('[name=SB' + k.toLowerCase().replace('internetprice', 'price') + ']', '#ajaxSmartbrowse');
								el = $(select)[0];
							} else if ($('[name=' + k + ']', '#ajaxSmartbrowse').size()) {
								select = $('[name=' + k + ']', '#ajaxSmartbrowse');
								el = $(select)[0];
							}

							if (el) {
								DDC.GenericSmartbrowse.ui.clearOptions(el);

								for (k2 in response[k]) {
									itemCount++;

									
									// if the current option's key is equal to an SBdefault for this field or it is equal to
									// the current smartbrowse parameters, note the index and set the selected index later
									if (response.SBdefaults) {
										selectedIndex = ((response.SBdefaults[k] === k2) || (!!(params[k] === k2) && !selectedIndex) ? itemCount : selectedIndex);
									} else if (response.defaults) {
										selectedIndex = ((response.defaults[k.replace('bodyStyle', 'bodystyle')] === k2) || (!!(params[k] === k2) && !selectedIndex) ? itemCount : selectedIndex);
									}

									el.options[el.options.length] = new Option(jQuery.trim(response[k][k2]), jQuery.trim(k2));

									if (response.SBdefaults) {
										newParams[k] = response.SBdefaults[k] || 'clear';
									} else if (response.defaults) {
										newParams[k] = response.defaults[k.replace('bodyStyle', 'bodystyle')] || 'clear';
									}

																	}

								
								// if for some reason selectedIndex is out of range, use first index (0)
								el.selectedIndex = (selectedIndex < el.options.length) ? selectedIndex : 0;

								// if there are no options in this set, disable the select element
								if (itemCount === 0) {
									$(el).addClass('disabled').attr('disabled', 'disabled');
								} else {
									$(el).removeClass('disabled');
								}
							}
													}
					}

					
					this.setParams(newParams);

					if (trueInventoryType) {
						this.setInventoryType(trueInventoryType);
					}

											
						$('form', '#ajaxSmartbrowse').attr('action', smartbrowse.getInventoryType().inv);
										endLoading();
				}
			});

		var SEARCH_BY_STOCK_NUMBER = 'Search by stock number',
			extraButtonConstraint = 'all',
			initialParams = smartbrowse.getParams(),
			exceptions = {
					'year': ''.split(',')
	,'make': ''.split(',')
	,'model': ''.split(',')
	,'trim': ''.split(',')
	,'bodystyle': ''.split(',')
	,'mileage': ''.split(',')
	,'price': ''.split(',')
	,'linkedaccountid': ''.split(',')
			},
			loadingTimeout,
			beginLoading = function() {
				$('form select, form [name=invtype]', '#ajaxSmartbrowse').attr('disabled', 'disabled')
					.animate({opacity: 0.5}, 'fast');
				$('form [name=invtype]', '#ajaxSmartbrowse').blur();
				loadingTimeout = setTimeout(function() {
					$('#sb_loading, .sb_loading', '#ajaxSmartbrowse').show().animate({opacity: 0.99}, 'fast');
					$('#sbSubmitBtn, .sbSubmitBtn', '#ajaxSmartbrowse').hide();
				}, 500);
			},
			selectResetTool = '<a href="#" class="selectReset">Change</a>',
			endLoading = function() {
				clearTimeout(loadingTimeout);
				$('#SBstockNumber, .SBstockNumber', '#ajaxSmartbrowse').val($('#SBstockNumber, .SBstockNumber', '#ajaxSmartbrowse').val() || SEARCH_BY_STOCK_NUMBER);
				$('#sb_loading, .sb_loading', '#ajaxSmartbrowse').animate({opacity: 0.01}, 'fast', function () {
					$(this).hide();
					$('#sbSubmitBtn, .sbSubmitBtn', '#ajaxSmartbrowse').show();
				});

				
				$('form select:not(.disabled, .selected), form [name=invtype]', '#ajaxSmartbrowse')
					.animate({opacity: 0.99}, 'fast', function () {
						$(this).attr('disabled', '');
					});
			},
			tracked = false,
			trackEvent = function (eventName, data) {
				// don't track the first call to trackEvent, since the first
				// event is always auto-triggered
				if (window.gaManager && tracked) {
					gaManager._trackEvent('smartbrowse', eventName, data);
				}
				tracked = true;
			},
			commit = function(params, invType, clear) {
				var paramsCopy = jQuery.extend({}, params);

				beginLoading();

				
					trueInventoryType = null;
					smartbrowse.commit({
						inventoryType: invType,
						params: paramsCopy,
						clear: clear
					});
							};

		// setup smartbrowse UI
		// handle inventory type selectors as radio buttons
		$('form input[name=invtype]', '#ajaxSmartbrowse').live('click', function() {
			if (!$(this).is('.selected')) {
				$('form [name=invtype]', '#ajaxSmartbrowse').removeClass('selected');
				$(this).addClass('selected');
				if (extraButtonConstraint != $(this).val() && extraButtonConstraint != 'all') {
					$('#extraButton').addClass('hidden');
				} else {
					$('#extraButton').removeClass('hidden');
				}
				for (var e in exceptions) {
					if (jQuery.inArray($(this).val(), exceptions[e]) !== -1) {
						$('#SB' + e, '#ajaxSmartbrowse').addClass('hidden');
					} else {
						$('#SB' + e, '#ajaxSmartbrowse').removeClass('hidden');
					}
				}
				trackEvent('click', 'type: ' + $(this).val());
				commit(initialParams, $(this).val(), true);
			}
		}).removeClass('selected');

		setTimeout(function () {
			$('form input[name=invtype]:checked', '#ajaxSmartbrowse').click();
		}, 1);

		if ($('form input[name=invtype]', '#ajaxSmartbrowse').length === 1) {
			$('form input[name=invtype]', '#ajaxSmartbrowse').parent().hide();
		}
		// handle inventory type selectors as select element
		$('form select[name=invtype]', '#ajaxSmartbrowse').change(function () {
			for (var e in exceptions) {
				if (jQuery.inArray($(this).val(), exceptions[e]) !== -1) {
					$('#SB' + e, '#ajaxSmartbrowse').addClass('hidden');
				} else {
					$('#SB' + e, '#ajaxSmartbrowse').removeClass('hidden');
				}
			}
			trackEvent('click', 'type: ' + $(this).val());
			commit(initialParams, $(this).val(), true);
		});

		setTimeout(function () {
			$('form select[name=invtype]', '#ajaxSmartbrowse').trigger('change');
		}, 1);

		// handle smartbrowse search parameter select elements
		var selectChange = function () {
				var thisParams = {},
					name = $(this).attr('id') || $(this).attr('name'),
					val = $(this).val();

				
				thisParams[name] = val;
				commit(thisParams);

				trackEvent('click', name.replace('SB', '') + ': ' + val);
			},
			selectReset = function () {
				if ($(this).prev('select').size()) {
					$(this).prev('select').val('clear').change();
				}

				return false;
			};

		if (jQuery.browser.msie) {
			$('form select:not([name=invtype]):not([name=radius])', '#ajaxSmartbrowse').change(selectChange);
		} else {
			$('form select:not([name=invtype]):not([name=radius])', '#ajaxSmartbrowse').live('change', selectChange);
		}

		
		$('#SBstockNumber, .SBstockNumber', '#ajaxSmartbrowse').click(function() {
			if ($(this).val().toLowerCase() === SEARCH_BY_STOCK_NUMBER.toLowerCase()) {
				$(this).val('');
			}
		});

		
		$('.sbSubmitBtn', '#ajaxSmartbrowse').live('click', function() {
			if($('#SBstockNumber, .SBstockNumber', '#ajaxSmartbrowse').length > 0){
				$('#SBstockNumber, .SBstockNumber', '#ajaxSmartbrowse').val((($('#SBstockNumber, .SBstockNumber', '#ajaxSmartbrowse').val() || '').toLowerCase() !== SEARCH_BY_STOCK_NUMBER.toLowerCase()) ? jQuery.trim($('#SBstockNumber, .SBstockNumber').val()) : '');
			}
			trackEvent('submit');

			$('form', '#ajaxSmartbrowse').submit();

			return false;
		});
	});

						jQuery(function ($) {
				var gaManager = new DDC.GoogleAnalyticsManager(['UA-9899509-3']);
			
				gaManager._trackPageview('/CA/MB/en_CA/PR/null/INDEX');
			
				window.gaManager = gaManager;
				window.gaPageTracker = gaManager.get('UA-9899509-3');
				window.gaDDCPageTracker = window.gaPageTracker;
			});

		
				jQuery(function ($) {
		$("#siteNav li").each(function(){
			$("ul li:first", this).addClass("first");
		});
		$('.buttonBlock a').each(function(){
			var calculatedPaddingTop = (($(this).height() - $('span', this).height())/2);
			$('span', this).css({paddingTop: calculatedPaddingTop + 'px'});
		});
		
							
	});
	jQuery(function ($) {
		var isiPad = navigator.userAgent.match(/iPad/i) != null;

		if (isiPad === true) {
			$('.fvCoverflow,.modelRibbon').remove();
		}
	});

	
	setTimeout(function(){
		jQuery.ready();
                    jQuery(function($) {
                DDC.embedVideoPlayer.loadVideoContent();
            });
            }, 1000);

