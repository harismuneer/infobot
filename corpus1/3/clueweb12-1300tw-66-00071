<script>
gigya.services.socialize.addEventHandlers({
        onLogin: onAchieveLogin,
        onLogout: onAchieveLogout
    });

	var loginState = null;

 var achieveParams = {
    height:30  
            ,width:180
            ,UIConfig: '<config><body><controls><snbuttons buttonsize="20" /></controls></body></config>'
            ,showTermsLink:false // remove 'Terms' link  
            ,hideGigyaLink:true // remove 'Gigya' link  
            ,containerID: 'divAchieveConnect' 
,disabledProviders: 'orkut, renren, kaixin, vkontakte, mixi'
        }

function onAchieveLogout()
	{
		displayUserInfo();
	}

function onAchieveLogin(user)
	{
		displayUserInfo(user);
	}

function displayUserInfo(user) {

if(typeof user != "undefined" && typeof user.user != "undefined")
		{
			user = user.user;
		}
		else
		{
			user = null;
		}
		

 if(user != null && user.isLoggedIn)
        {
             document.getElementById("divAchievementsWrapper").style.display = "block";  
             document.getElementById("divAchieveConnectWrapper").style.display = "none";  
             document.getElementById("divAchieveConnect").style.display = "none"; 
   } else {  
  gigya.services.socialize.showAddConnectionsUI(achieveParams);  
  document.getElementById("divAchieveConnectWrapper").style.display = "block";  
  document.getElementById("divAchieveConnect").style.display = "block";  
  document.getElementById("divAchievementsWrapper").style.display = "none";  
}
}
gigya.services.socialize.getUserInfo({callback:displayUserInfo});
</script>



