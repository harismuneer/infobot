function sidebarMenu(header) {
  var linkList = jQuery(header).parent().find('.sidebar_link_list');
  if (linkList.css('display') == 'none') linkList.slideDown();
  else linkList.slideUp();
}


var labels = jQuery('.form_label');
for (var i=0; i < labels.length; i++)
  labels.eq(i).html(labels.eq(i).html().replace(/Â/g,''));

if (document.location.href.indexOf('/event/detail') >= 0) {jQuery('#dashboard_sidebar').css('visibility','hidden');}
if (document.location.href.indexOf('/page/neighborhood') >= 0) {
  jQuery('.module_dashboard #control_set h1').show();
}
if (document.location.href.indexOf('/dashboard/private') >= 0) {
  jQuery('.dashboardCandidates').show();
  jQuery('#myactivity_title').show();
  jQuery('.sidebar_link_list').show();
} else {
  var menuLoads = jQuery('.sidebar_section .menu_load');
  menuLoads.click(function() {
    sidebarMenu(this);
    return false;
  });
  menuLoads.attr('href','#');
}

/* Update blog management buttons */
var writeNewPost = jQuery('img[title="Write New Post"]');
var viewYourBlog = jQuery('img[title="View Your Blog"]');
var editBlogSettings = jQuery('img[title="Edit Blog Settings"]');
if (writeNewPost.length) {
	jQuery('#community .buttonstable').css('color','transparent').css('font-size','0em');
	jQuery('#community .buttonstable td').css('padding-right','20px');
	writeNewPost.attr('style','').attr('src','http://www.democrats.org/files/images/dashboard/dashboard-write-new-post.gif');
	viewYourBlog.attr('style','').attr('src','http://www.democrats.org/files/images/dashboard/dashboard-view-your-blog.gif');
	editBlogSettings.attr('style','').attr('src','http://www.democrats.org/files/images/dashboard/dashboard-edit-blog-settings.gif');
}

jQuery(document).ready(function() {
  jQuery('#calendar-nav').after(jQuery('#search_results_xml_link'));
  jQuery('#search_results_xml_link').css('margin-top','10px').show();

  var calDays = jQuery('.calweekdaycell');
  for (var i=0; i < calDays.length; i++) calDays.eq(i).html(calDays.eq(i).html().charAt(0));
});
</script>
