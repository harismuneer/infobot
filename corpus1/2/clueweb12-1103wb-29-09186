                // GRESHAM PUBLIC LAB (192.220.141.1 ... 192.220.141.30)
                '192.220.141.1/32','192.220.141.2/31','192.220.141.4/30','192.220.141.8/29','192.220.141.16/29','192.220.141.24/30','192.220.141.28/31','192.220.141.30/32',
                // MIDLAND PUBLIC (192.220.139.193 ... 192.220.139.255)
                '192.220.139.193/32','192.220.139.194/31','192.220.139.196/30','192.220.139.200/29','192.220.139.208/28','192.220.139.224/27',
                // MIDLAND STAFF (192.220.133.193 ... 192.220.133.222)
                '192.220.133.193/32','192.220.133.194/31','192.220.133.196/30','192.220.133.200/29','192.220.133.208/29','192.220.133.216/30','192.220.133.220/31','192.220.133.222/32',

                // MIDLAND PUBLIC LAB (192.220.140.193 ... 192.220.140.222)
                '192.220.140.193/32','192.220.140.194/31','192.220.140.196/30','192.220.140.200/29','192.220.140.208/29','192.220.140.216/30','192.220.140.220/31','192.220.140.222/32',
