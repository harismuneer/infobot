		<script type="text/javascript">
			var headline_count;
			var headline_interval;
			var current_headline = 0;
			var old_headline = 0;
			var intervalLength = 10000;
			var isAnimating = false;

			headline_count = jQuery('#socialMediaTwitter p').size();
			jQuery('#socialMediaTwitter p:eq(' + current_headline + ')').css('top', '0');


			headline_interval = setInterval(headline_rotate, intervalLength); //time in milliseconds
			jQuery('#scrollup').hover(function() {
				clearInterval(headline_interval);
			}, function() {
				headline_interval = setInterval(headline_rotate, intervalLength); //time in milliseconds
				if (!isAnimating) headline_rotate();
			});
			
			
			function headline_rotate() {
				isAnimating = true;
				current_headline = (old_headline + 1) % headline_count; 
				jQuery("#socialMediaTwitter p:eq(" + old_headline + ")").animate({top: -105},"slow", function() {
					isAnimating = false;
					jQuery(this).css('top','110px');
				});
				jQuery("#socialMediaTwitter p:eq(" + current_headline + ")").show().animate({top: 0},"slow");  
				old_headline = current_headline;
			}

		</script>
