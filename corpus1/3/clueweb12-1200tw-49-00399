<script type="text/javascript">


    function closehudphotoviewer() {

        new Effect.SlideUp('hud_photoviewer', { duration: .2 });

        //document.getElementById('hud_photoviewer').style.display = "none";
    }



    function openhudphotoviewer() {

        new Effect.SlideDown('hud_photoviewer', { duration: .2 });

        // document.getElementById('hud_photoviewer').style.display = "block";
    }

    function hudphotoviewerbusy_on() {
        document.getElementById('hudphotoviewer_busy').style.display = "block";
        return true;
    }

    function hudphotoviewerbusy_off() {
        document.getElementById('hudphotoviewer_busy').style.display = "none";
        if (document.getElementById('hudphotoviewer_refreshtype').value == "detected") {
            openhudphotoviewer();
            document.getElementById('hudphotoviewer_refreshtype').value = '';
        }
        return true;
    }

    function viewphoto(photoid) {
        document.getElementById('hudphotoviewer_id').value = photoid;
        eo_Callback('callback_hud_photoviewer');
        openhudphotoviewer();
        return true;
    }

    function autodetectphoto() {
        document.getElementById('hudphotoviewer_refreshtype').value = "detect";
        eo_Callback('callback_hud_photoviewer');
        return true;
    }


</script>
