				jQuery(function ($) {
		var height = $('div.truncateComments').height();
		var expand = function () {
				var self = this;
				$('#internetComments div.truncateComments').animate({height: height}, function () {
					$(this).height('');
					$(self).unbind('click').click(collapse).find('span').text('Click here to hide extra content');
				});
				return false;
			},
			collapse = function () {
				var self = this;
				$('#internetComments div.truncateComments').animate({height: 400}, function () {
					$(self).unbind('click').click(expand).find('span').text('Click here to read more Comments');
				});
				return false;
			};
		if (height > 400) {
			$('#internetComments div.truncateComments').height(400);
			$('<a href="#" class="viewMore"><span>Click here to read more Comments</span></a>').appendTo('#internetComments').click(expand);
		}
	});
	
