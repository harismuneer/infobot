              <div id="html5VideoHolder">
                <video id="html5VideoPlayer" width="459" height="257" controls="" style="display:none">
                </video>
                <img id="imgPlayVideo" src="//assets.clickmotive.com/video/play_video.png" alt="Click to play video" style="display:none">
                </img>
              </div>
              <script type="text/javascript">
			$(document).ready(function() {
				if ((navigator.userAgent.indexOf('iPhone') != -1) || (navigator.userAgent.indexOf('iPod') != -1) || (navigator.userAgent.indexOf('iPad') != -1)) {
					$("#videoScreenshot").css("display","none");
						var width = 459 + "px";
						var height = 257 + "px";
			var $videoContainer = $("#videoContainer");
			var $html5VidPlayer = $("#html5VideoPlayer");
			var html5vidPlayerDOM = $("#html5VideoPlayer").get(0);
			var $imgPlayVideo = $("#imgPlayVideo");
			var pauseTime = 0;

			var newVideoPath = "";

			$videoContainer.empty();
			$imgPlayVideo.bind('click', function showVideo() {
			if (typeof(HTML5VideoPath) != "undefined"){
				if (HTML5VideoPath.length != 0) {
				newVideoPath = HTML5VideoPath;
				}
			}

			$imgPlayVideo.hide();
			$videoContainer.html($("#html5VideoHolder").html());
			$html5VidPlayer.show()
			.css("width",width)
			.css("height",height)
			.attr('src','http://assets.clickmotive.com/video/' + newVideoPath + '.mp4');
					html5vidPlayerDOM.play();
			});
			
			$imgPlayVideo.show();

			function hideVideo() {
			$html5VidPlayer.hide();
			$imgPlayVideo.show();
			}

			html5vidPlayerDOM.addEventListener('loadedmetadata', function() {
			this.currentTime = pauseTime;
			}, false);

			html5vidPlayerDOM.addEventListener('pause', function () {
			pauseTime = this.currentTime;
			hideVideo();
			}, false);

			html5vidPlayerDOM.addEventListener('ended', function () {
			hideVideo();
			}, false);
			}else{
			var videoPlayer = new VideoPlayer(
						"http://assets.clickmotive.com/video/",
						459,
						257,
						""
					);

					videoPlayer.loadPlayer(
						"/Ford/2012_Ford_Edge_En_Us",
						"",
						"true",
						"false"
					);
				}
			});
		</script>
            </div>
