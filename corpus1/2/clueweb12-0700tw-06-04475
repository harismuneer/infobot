    <script type="text/javascript">
   //<![CDATA[
        var videoplayer = new Videoplayer({

            playlistItemTemplate: '<li class="playlistitemli list border_color">' +
'<div class="video_facebook"><a href="http://www.facebook.com/sharer.php?u=http://wn.com/<%= title %>&t=<%= title %>" title="Share this video" target="_blank" style="target-name: new; target-new: tab;"></a></div>' +
'<a class="playlistitem ellipsis" id="<%= id %>" href="#"><span class="title"><%= title %></span>...</a>' +
'<span class="playlistitemremove" title="Click to remove from playlist"></span>' +
'<span class="description-content" style="display:none;"><%= tooltipContentBody %></span>' +
'<span class="duration"><%= durationStr %></span>' +
'</li>',
playpositionTemplate: '<%= elapsedTime %> / <%= totalTime %>',
            emptyItemHtml: '<li class="empty">Add any items to Create Playlist by dragging or using the "Add" Icon on each Video thumbnail or selecting more Videos from new Searches to buld your Playlist</li>',
            inputOrdering: true,
            descriptionsholder: '#descriptionsplace',
            replaycurrentCheckbox: '#replaycurrent',
            enableLogging: false
        });
        // this will add all available videos to playlist
        jQuery(function() {
            videoplayer.addVideo($('#mainvideoresults .video-info').slice(0, 14));
                    // jQuery(document).bind('videoplayer.player.playlistchanged', function() {
                    //     jQuery('#id_save_playlist').html('save the playlist').parent().removeClass('disable');
                    // });
            var detailsTemplate = _.template('<div class="video-desc border_color font_color <%= info %>">' +
                '<div class="content"><a class="playlistitem" id="<%= id %>_line" href="#">'+
                '<div class="video-thumb border_color bgr_color  gotop">'+
                '<img class="video-thumbnail" width="120" height="90" alt="" src="<%= thumbnailUrl %>"></div></a>'+
                '<div class="video-title playvideo activate-videos gotop"><%= title %></div>'+
                '<ul><li>Order: <input type="text" size="2" value="<%= order %>" class="details-manual-order" />'+
				'<a href="javascript:void(0);" class="details-manual-order-confirm" style="display:none;" title="Move to new position">Reorder</a></li>'+
                '<li>Duration: <span class="video-duration"><%= durationStr %></span></li>'+
                '<li>Uploaded: <span class="video-duration"><%= dateStr %></span></li></ul>'+
                '<div class="playlistitemremove" title="Click to remove from playlist"></div>' +
                '<div class="video-fulldescription"><%= tooltipContentBody %></div>' +
                '<a href="http://wn.com/<%= url %>" target="_blank" style="target-name: new; target-new: tab;">'+
                'http://wn.com/<%= title %></a><br clear="all" /></div></div>');
            var target = $('#descriptions');
            videoplayer.playerContainer.bind('videoplayer.player.playlistchanged', function(e, playlist){
                target.find('.video-desc').detach();
                for(var i in playlist){
                    var item = playlist[i];
                    var itemId = _.uniqueId('item_');
                    var context = {
                        id: itemId,
                        info: "{source: '"+item.source+"', video_id: '"+item.video_id+"', duration: "+item.duration+", dateStr: '"+item.dateStr+"'}",
                        url: item.title.replace(/ /g, '_'),
                        title: item.title,
                        durationStr: videoplayer.formatTime(item.duration),
                        description: item.description,
                        dateStr: item.dateStr,
                        order: (+i)+1,
                        thumbnailUrl: item.thumbnailUrl,
                        tooltipContentBody: item.tooltipContentBody,
                    };
                    var item_text = detailsTemplate(context);
                    target.append(item_text);
                }
            });                    
        });
      

        // adding more actions
        jQuery(function() {
            // hyphenate video details
            // only use this for english language
            jQuery('.video-info').each(function() {
                Hyphenator.hyphenate(this, 'en');
            });

            jQuery('.videoplayer-play-next').click(function(event) {
                event.preventDefault();
                videoplayer.playNextPlaylistVideo();
            });
            
            jQuery('.videoplayer-play-all').click(function(event) {
                event.preventDefault();
                videoplayer.clearPlaylist();
                videoplayer.addVideo(jQuery('#mainvideoresults .video-info').slice(0, 23));
                videoplayer.playPlaylist();
            });
             jQuery('#restore').click(function(event) {
                    event.preventDefault();
                    videoplayer.clearPlaylist();
                    videoplayer.addVideo(jQuery('#mainvideoresults .video-info').slice(0, 14));
                    videoplayer.playPlaylist();
                });
            window.onPlayVideo = function(event) {
                videoplayer.log('onPlayVideo');
                event.preventDefault();
                var videoData = videoplayer.extractVideoData($(event.currentTarget));
                videoplayer.log(videoData);
                videoplayer.playFromPlaylist(videoData, this.AUTOSTART);
                };

            window.onAddVideo = function(event) {
                videoplayer.log('onAddVideo');
                event.preventDefault();
                var videoData = videoplayer.extractVideoData($(event.currentTarget));
                var playlist = videoplayer.getPlaylist();
                playlist.push(videoData);
                videoplayer.setPlaylist(playlist);
                videoplayer.log('onAddVideo done.');
            };

            jQuery('.addvideo').livequery(function() {
                $(this).click(window.onAddVideo);
            });

            jQuery('.playvideo').livequery(function() {
                $(this).click(window.onPlayVideo);
            });

                    $('#descriptions .playlistitemremove').live('click', function(e) {
                        var toRemove = videoplayer.extractVideoData(this);
                        var playlist = videoplayer.getPlaylist();
                        for (var i = 0; i < playlist.length; i++) {
                            var playlistItem = playlist[i];
                            if(videoplayer.isSameVideo(playlistItem, toRemove)){
                                playlist.splice(i--, 1);
                            }
                        }
                        videoplayer.setPlaylist(playlist);
                        $(this).parent('div.video-desc').detach();
                    }); 
     // getting the video details delete button to work
                    var pl_len = $('#descriptions .video-desc').length;
                    var init_vlist_value = false;
                    var new_vlist_value = false;
                    $('#descriptions .details-manual-order').live('hover', function(e) {
                        $(this).select();
                        if (!init_vlist_value) { init_vlist_value = parseInt($(this).val()); }
                    });
                    $('#descriptions .details-manual-order').live('focus', function(e) {
                        if (!init_vlist_value) { init_vlist_value = parseInt($(this).val()); }
                    });
                        //$('#descriptions .details-manual-order').blur(function(e) {
                        //  $(this).parent().find('.details-manual-order-confirm').hide();
                        //})
                        $('#descriptions .details-manual-order').live('keyup',function(e) {
                            $(this).parent().find('.details-manual-order-confirm').hide();
                            new_vlist_value = parseInt($(this).val());
                            if (isNaN(new_vlist_value)) { return false; }
                            if (new_vlist_value == undefined) { return false; }
                            if (new_vlist_value == parseInt(init_vlist_value)) { return false; }
                            if (new_vlist_value < 0) { return false; }
                            if (new_vlist_value > pl_len) { return false; }
                            if (new_vlist_value == 0) { new_vlist_value = 1; }
                            $(this).parent().find('.details-manual-order-confirm').show();
                        });
                        // add the keypress event
                        $('#descriptions .details-manual-order').live('keypress', function(e) {
                            if(e.keyCode == 13) {
                                var tmp = $('#descriptions .video-desc').eq(init_vlist_value-1);
                                var target = $('#descriptions .video-desc').eq(new_vlist_value-1);
                                var tmpc = tmp.clone(true);

                                target.find('.details-manual-order').val(init_vlist_value);
                                tmp.replaceWith(target);
                                var place = $('#descriptions .video-desc').eq(new_vlist_value-1);
                                tmpc.insertBefore(place);

                                var items = $('#descriptions .video-desc .details-manual-order');
                                for (var i = 0; i < items.length; i++) {
                                    $(items[i]).val(i+1);
                                }

                                videoplayer.switchPlaylistItems(init_vlist_value, new_vlist_value);
                                init_vlist_value = false;
                                new_vlist_value = false;
                                $('#descriptions .details-manual-order-confirm').hide();
                            }
                        });
                        $('.details-manual-order-confirm').live('click', function(e) {
                                tmp = $('#descriptions .video-desc').eq(init_vlist_value-1);
                                target = $('#descriptions .video-desc').eq(new_vlist_value-1);
                                tmpc = tmp.clone(true);

                                target.find('.details-manual-order').val(init_vlist_value);
                                tmp.replaceWith(target);
                                place = $('#descriptions .video-desc').eq(new_vlist_value-1);
                                tmpc.insertBefore(place);

                                var items = $('#descriptions .video-desc .details-manual-order');
                                for (var i = 0; i < items.length; i++) {
                                    $(items[i]).val(i+1);
                                }

                                videoplayer.switchPlaylistItems(init_vlist_value, new_vlist_value);
                                init_vlist_value = false;
                                new_vlist_value = false;
                                $('#descriptions .details-manual-order-confirm').hide();
                        });
     });
    //-->
    </script>

  

    <script type="text/javascript"><!--
    // store search history to be able to control offset
    var currentSearchesOffsets = new Object;

    function find_multiple_video() {
                jQuery('.tabtrigger').offscreentabs('activateTab', 'videos');
                var query = jQuery("#videoquery0").val();
                var fquery = query.replace(/[\W\s]+/i, '_');

                if (currentSearchesOffsets[fquery] == undefined) {
                    currentSearchesOffsets[fquery] = 0;
                }
                window.currentSearchesOffsets = currentSearchesOffsets;
                jQuery.ajax({
				url: '/api/upge/cheetah-article/mvs',
                data: {
                        query: query,
                        offset: currentSearchesOffsets[fquery]
                    },
                    dataType: 'jsonp',
                    success: function(text) {
                            jQuery('#multiple_video').prepend(jQuery(text));
                            window.currentSearchesOffsets[fquery] += 9;
                    },
                    complete: function() {
                        theForm.find('input[type=submit]').attr('disabled', '');                       
                    }
                });
    }
    function load_related_videos() {
        jQuery.ajax({
            url: '/api/upge/cheetah-article/related_videos',
            data: {
			query: 'Chocolate Heart Health',
			},
            dataType: 'jsonp',
            success: function(text) {
            if(!text) { text = 'No related videos'; }
                jQuery('div#rel_videos').html(text);
            }
        });
    }

	
	
	
    function find_more_videos() {
                var query = 'Chocolate Heart Health';
                offset = last_count;
                last_count += 9;
                jQuery.ajax({
                        data: {
                        t: 'cheetah-article/mvs_more.txt',
                        offset: offset,
                        orderby: 'published',
                        corpus: 'current',
                        language_id: '1',
                        query: query
                    },
                    dataType: 'jsonp',
                    url: 'http://upge.wn.com/',
                    success: function(text) {
                            jQuery('#searchresults').append(jQuery(text));
                    }
                });
    }

            var wn_search_mangler = function(theform) {
                find_multiple_video();
                return false;
        }

        var last_count = 24;
        jQuery(function() {
            jQuery('div#container').find('.more_related').click(function(e) {
                find_more_videos();
                find_more_videos();
                e.preventDefault();
            })

            jQuery('.more_related_mvs').live('click', function(event) {
                find_multiple_video();
            });
            
            jQuery('#newssearch').click(function(event) {
                var search_string = jQuery("#videoquery0").val();
                search_string = search_string.toLowerCase();

                // We cannot handle certain characters in searches:
                if(search_string.indexOf('.') >= 0)
                    return true; // submit normally
                if(search_string.indexOf('/') >= 0)
                    return true; // submit normally
                if(search_string.indexOf(':') >= 0)
                    return true; // submit normally

                window.location = 'http://wn.com/' + search_string.replace(' ', '_');
                return false; // do not submit the form
            });
            //jQuery('#multiple-video-search').click(function(e) {
            //    find_multiple_video();
            //    event.preventDefault();
            //});
        });
        //-->
    </script>

    

    



        
