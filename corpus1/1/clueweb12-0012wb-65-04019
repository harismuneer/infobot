<script>
     function state_details(st,yr) {
          var url = "presidential_map/frame_state_details_ajax.php?state=" + st +"&show_year="+yr;
          ajax_load_into_div(url,"state_div");
     }

     function state_details_clear() {
          $("#state_div").html("");
     }

     function share_map_hide() {
          $("#share_map_section").hide();
          $("#share_map_sidebar_info").hide();
     }

     function share_map_show() {
          $("#share_map_section").show();
          $("#share_map_sidebar_info").show();
     }

     function year_summary(yr) {
        fullURL = parent.document.URL;
        if (fullURL.indexOf('mapid=') > 0) {
            var url = "presidential_map/frame_details_ratio.php?show_year="+yr+"&hasmapid=1";
        } else {
            var url = "presidential_map/frame_details_ratio.php?show_year="+yr+"&hasmapid=0";
        }

        ajax_load_into_div(url,"div_summary");
     }

    function show_map_link2(map_number,initial_save) {
        if (initial_save) {
            $("#div_map_link_blurb").html("To share the map you've created above:<br><br>");
        } else {
            $("#div_map_link_blurb").html("To <em>re</em>share the 2012 map currently displayed:<br><br>");
        }
        var url = "http://www.270towin.com/2012_election_predictions.php?mapid="+map_number; 
        var mapLinkURL = url;
        var addthis_share = {url:mapLinkURL,title:'2012 Presidential Election: Electoral Map',description:'This map displays the projections of the sender and does not reflect the opinions of 270toWin.',url_transforms:{shorten:{twitter:'bitly',facebook:'bitly'},add:{referring_service:'{{code}}'},clean:false},shorteners:{bitly:{ username:'akeiter',apiKey:'R_0cec6fa277a2d4a6ef897035402655c5'}},templates:{twitter:'Check out my latest electoral map forecast for the 2012 presidential election {{url}} via @270toWin'}};
        var addthis_config = {pubid:'akeiter',ui_use_addressbook:true,data_track_clickback:true};

        //var tbx = document.getElementById("addthis_toolbox");
        svcs = {facebook: '', twitter: '', email: '', google: '',  compact: ''};
        for (var s in svcs) {
            //tbx.innerHTML += '<a class="addthis_button_'+s+'">'+svcs[s]+'</a>';
            var btnToAdd = '<a class="addthis_button_'+s+'">'+svcs[s]+'</a>';
            $(btnToAdd).appendTo("#addthis_toolbox");
        }

        addthis.toolbox("#addthis_toolbox",addthis_config,addthis_share);

        $("#div_map_link").html("<br>You can also link directly via this URL: &nbsp; <font color='red'>"+mapLinkURL+"</font>");
        
    }

    function hide_map_link() {
        $("#addthis_toolbox").html("");
        $("#div_map_link").html("");
        $("#div_map_link_blurb").html("You can now share your personal 2012 election map with others.  This can be a map where you forecast all 50 states, or you can leave some states undecided that you think will be up for grabs. It is up to you. Share your predictions via social media, email or blog. Just click the &quot;Share Map&quot; button when you are ready to send it out and follow the instructions that will appear here.");
    }

     function initAddThis() {
          addthis.init()
     }

    function copy_map_link() {
        document.getElementById("maplink").focus();
        document.getElementById("maplink").select();
    }

    function year_facts(yr) {
         if( yr == 2012 ) {
            //document.getElementById("year_facts").innerHTML = "";
            $("#year_facts").html("");
            //return;
         } else {
            //document.getElementById("info2").innerHTML = "";
            $("#info2").html("");
            //document.getElementById("state_div").innerHTML = "";
            $("#state_div").html("");

            var url = "presidential_map/year_facts_ajax.php?show_year="+yr;
            ajax_load_into_div(url,"year_facts");
          }
    }

    year_summary(2012);
    year_facts(2012);
    hide_map_link();

    initial_state = "2008";
    current_state = "";
    current_year = 2012;


    function show_map_link(args) { // runs when a map is uploaded
        var current_mapnumber = args;
        if( current_mapnumber != null ) {
            show_map_link2(current_mapnumber,true);
        }
    }

    function show_sharing_links(args) { // runs when a map is retrieved
        var patt = new RegExp("(.*)([A-Z]{2,2}),([A-Z]{3,3})$");
        var m = patt.exec(args);
        var spts = "";

        show_map_link2(qsURL,false);

        if( m != null ) {
            args = m[1];
            spts = "&me=" + m[2] + "&ne=" + m[3];
        }

        var patt = new RegExp("undefined");
        if( !patt.test(args) ) {
             var url = "http://www.270towin.com/presidential_map/scenario_builder.php?state_list=" + args + spts;
             ajax_load_into_div(url,"info2");
        }
    }

    function state_press(args) {
        var patt = new RegExp("(.*)([A-Z]{2,2}),([A-Z]{3,3})$");
        var m = patt.exec(args);
        var spts = "";

        hide_map_link();

        if( m != null ) {
            //alert("got match: me:" + m[2] + " ne:" + m[3] );
            args = m[1];
            spts = "&me=" + m[2] + "&ne=" + m[3];
        }

        var patt = new RegExp("undefined");
        if( !patt.test(args) ) {
             var url = "http://www.270towin.com/presidential_map/scenario_builder.php?state_list=" + args + spts;
             ajax_load_into_div(url,"info2");
        }
    }

    function state_setup(args) {
        var patt = new RegExp("(.*)([A-Z]{2,2}),([A-Z]{3,3})$");
        var m = patt.exec(args);
        var spts = "";

        if( m != null ) {
            args = m[1];
            spts = "&me=" + m[2] + "&ne=" + m[3];
        }

        var patt = new RegExp("undefined");
        if( !patt.test(args) ) {
             var url = "http://www.270towin.com/presidential_map/scenario_builder.php?state_list=" + args + spts;
             ajax_load_into_div(url,"info2");
        }
    }

    function update(args) {
        current_year = args;
        if( args == 2012 ) {
            share_map_show();
        } else {
            current_state = "";
            share_map_hide();
        }
        state_details_clear();
        year_facts(args);
        year_summary(args);
        //hide_map_link();
    }

    function state_click(args) {
         // NEUTRAL, DEMOCRAT, REPUBLICAN, SWING, VCLOSE,COMP
          if( current_state != args ){
         
             state_details(args,current_state);
             current_state = args;
          }
    }

    function state_result(args) {
        set_state(current_state,args);
    }

    function state_view(args) {
          setCookie("initial_state", args, 365 );
          reset_state();
    }

    function installVer(args) {
          alert('Wrong Version');
          window.location = 'http://www.270towin.com/wrong_flash.php';
    }
</script>

