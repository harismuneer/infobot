<script type="text/javascript" src="/js/request.js"></script>


<script type="text/javascript">
// <![CDATA[
	function validateQQ(form) {
		var qqFields = form.getElementsByTagName('INPUT');
		for ( var i in qqFields ) {
			if ( qqFields[i].type == "text" ) {
				if ( qqFields[i].value.length == 0 ) { alert( "Please fill all fields in Quick Quote Form before submitting." ); return false; }
			}
		}
		
		//check zip
		var zipExp = new RegExp("[0-9]{1,}","g");
		if ( form.customerzip && !zipExp.test(form.customerzip.value) ) { alert("Please enter numerical values only on Quick Quote Zip Code."); }
		
		//check phone
		var phone0Exp = new RegExp("[0-9]{3}","g");
		var phone1Exp = new RegExp("[0-9]{3}","g");
		var phone2Exp = new RegExp("[0-9]{4}","g");
		if ( !phone0Exp.test(form.customerdayphone0.value) || !phone1Exp.test(form.customerdayphone1.value) || !phone2Exp.test(form.customerdayphone2.value) ) {
			alert("Please enter 10 numeric values only on Quick Quote Phone Numbers."); 
			return false;
		}
		form.customerdayphone.value = form.customerdayphone0.value + "-" + form.customerdayphone1.value + "-" + form.customerdayphone2.value;
		
		//check email
		var emailExp = new RegExp(".@.","g");
		if ( !emailExp.test(form.customeremail.value) ) { alert("Please enter a valid email address on Quick Quote Email."); return false; }
		
		//check survey questions
				
		
		//check model
		if ( form.model.value == "" || form.model.value == "Select Model" ) { alert("Please select a Quick Quote Vehicle Model."); return false; }
		form.a.value = "submit";
		return true;
	}
	
	var alreadyLoaded=false;
	function loadVehicles()	{
		try {
			if( !alreadyLoaded ) {
				alreadyLoaded=true;
				var request = getRequest();
				request.open( "get", "/accountVehicles?getType=1&year=2012&mergeNewer=false&mergeOlder=false", false );
				request.send( null );
					
				var oSelect = qqForm.elements[ 'model' ];
				
				oSelect.options.length = 0;
				oSelect.options[0] = new Option( "Select Model:", "", true, true );
				var makes = request.responseText.split( ";;" );

				for ( var i = 0; i < makes.length; i++ ) {
					var make = makes[i].split( "::" );
					var makeName = make[0].split( "~~" );
					var oGroup = document.createElement( 'optgroup' );
					oGroup.label = makeName[ 1 ];
					var models = make[1].split( ",," );

					for( var j = 0; j < models.length; j++ ) {
						var modelName = models[j].split( "~~" ); 
						var oOption = document.createElement( 'option' );
						oOption.value = modelName[ 1 ]; 
						oOption.innerHTML = modelName[ 1 ]; 
						oGroup.appendChild( oOption );
					}
					oSelect.appendChild( oGroup);
				 }
			}
		}catch(Exception) {}
	}
// ]]>
</script>

<div id="quickQuoteContainer" style="background-image:url(&quot;/img/1735730/Ford-QQ.jpg&quot;); background-repeat:no-repeat;">
	<div class="content">
		<div class='quickQuote'>
			<div class="clsHeading">Quick Quote</div>
			<div class="rightPanelBox">
			
				<form id='quickQuoteForm' name='quickQuoteFormModule' method='post' onsubmit='return validateQQ( this )' action='/d/www.davesinclairford.com/handleOffer/o/88/i/38823'>
					<input type="hidden" name="a" value="" />
					<input type="hidden" name="make" value="" />
					<div class="quoteFirstName">
						<div class="quoteLabel">First Name*</div>
						<input type="text" maxlength="32" value="" id="customerfirstname" name="customerfirstname" onchange="modified=true;" />
