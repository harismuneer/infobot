<div id="fb-root"></div>
<script>
	var social_init = new $.Deferred(); // See http://trac.flixsterqa.com/wiki/Dev/header for detailed explanation of these 4 deferreds 
	var fb_ready = new $.Deferred();    
	var fb_connected = new $.Deferred(); 
	var fb_login = new $.Deferred();     
	
	window.fbAsyncInit = function() {
		FB.init({
			appId: '326803741017', 
			status: true, 
			cookie: true, 
			xfbml: true,
			oauth: true,
			channelUrl: "http://www.rottentomatoes.com/facebook/channel.html"
		});		

		// FB scripts ready. (Does not imply logged in)
		fb_ready.resolve();		
		
		$(".rt_fblogin").live("click", function(){
			var scope = $(this).attr("scope");
			if (scope == undefined) {
				scope = 'email,publish_actions';
			}
			FB.login(function(response) {
				var fbresp = response;
				if(response.status == "connected"){
					//if(console != undefined) {
					//	console.log("logincallback");
					//	console.log(document.cookie);
					//}
											
					social_init.resolve();
					fb_connected.resolve(); 			
					fb_login.resolve();
					
					// Trigger social event on body
					$(document).ready(function(){
						$("body").trigger("social", fbresp);
						$("body").trigger("social-login", fbresp);	
					});
				}				
			 }, {scope: scope});
		});
		
		
		
		
		// Called only once on page load
		FB.getLoginStatus(function(response){
			if(response.status == "connected"){

				social_init.resolve();
				fb_connected.resolve(); 					
				
				$(document).ready(function(){
					$("body").trigger("social");				
				});				
			}
		});
		
		
		
		// Logout link handler
		$(".logoutlink").live("click", function(){
			_gaq.push(['_trackEvent', 'Facebook', 'Logout', 'Header Logout']);
			
			FB.getLoginStatus(function(response){
				if(response.status == "connected"){
					FB.logout(function(response) {
						document.location='/logout/';
					});
				}
				else{
					document.location='/logout/';					
				}
			});
			
			return false; // Discontinue existing href action
		});
		
	};
	
	// Load User navigation menu (top right) if logged in
	social_init.done(function(){		
		$.ajax( {
			url : '/home/header/', dataType : "html",
			data : null
		}).success( function(response, status) {	
			$("#user_navigation").html(response);			
		});
	});	
	

	(function($){		
		// Async loading of all.js
		var e = document.createElement('script'); e.async = true;
		e.src = document.location.protocol +
		   '//connect.facebook.net/en_US/all.js';
		 document.getElementById('fb-root').appendChild(e);
		
		// Async loading of plusone.js	
	    var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
	    po.src = 'https://apis.google.com/js/plusone.js';
	    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);

		
		/** JS Driven Hover Delay using HoverIntent **/
		var hoverConfig = {    
	   			over: function(){
	   				$(this).addClass('hover');
	   				$('#searchformnew .autocomplete').hide();	
	   			},     
	   			timeout: 200,    
	   			out: function(){
	   				$(this).removeClass('hover');
	   			}     
		};	
		$('.drop_menu > li').removeClass("no-js").hoverIntent(hoverConfig);
	
	})(jQuery);
	
	$("#header_login_link").click(function(e){
		e.preventDefault();
		e.stopPropagation();
		
		if($("#loginBox").length == 0){
			var loginEle = $("<div id='loginBox' style='display:none;'></div>");
			loginEle.append("<iframe id='loginIframe' src='https://www.rottentomatoes.com/user/account/login-iframe/' frameborder='0' scrolling='no' style='width: 700px; height: 320px;'></iframe>");			
			$("body").append(loginEle);
						
			// Load CSS
			if(document.createStyleSheet)
				document.createStyleSheet("//images.rottentomatoescdn.com/styles/jqueryui/rt-theme/jquery-ui-1.8.13.custom-dialog.css?v=prod20120210");
			else
				$("head").append("<link rel='stylesheet' href='//images.rottentomatoescdn.com/styles/jqueryui/rt-theme/jquery-ui-1.8.13.custom-dialog.css?v=prod20120210' type='text/css' />");
			
			// Load JS
			$.getScript("//images.rottentomatoescdn.com/js/jquery/ui/ui-1.8.7.custom.min.js", function(){
				$('#loginBox').dialog({ 
					modal: true, 
					title: "Log In to Rotten Tomatoes",
					height: 420,
					width: 760,
					zIndex: 65531,
					//autoOpen: false,
					dialogClass: "rt-theme"
				});
			});	
		}
		else{
			$("#loginBox").dialog("open");
		}		
	});
	

</script>

	
	























 




