var profileText = jQuery('#widget_profile_widget > p');
profileText.html("<span>Share some basic information about yourself and why you support the Democratic Party.</span><br/>" + profileText.html());

var blogText = jQuery('#widget_community_widget > p');
blogText.html("<span>Share your thoughts and experiences with other Democrats. Whether it's a photo, a personal story, or your thoughts, you can share it through your own blog.</span><br/>" + blogText.html());

