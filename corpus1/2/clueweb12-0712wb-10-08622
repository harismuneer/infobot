<script language="javascript">

addNamespace('CARS.gSearch');


CARS.gSearch = {
// defaults
page: 1, qTerm: '', form: 'form.gSearch-form', resultsContainer:'div#gSearch-results-container',
// functions
	init: function() {
		$(CARS.gSearch.form).each(function(index) {
			var that = this;
			this.id = this.id || 'form-' + String((new Date()).getTime()).replace(/\D/gi,'');
			$(this).submit(function() {
				CARS.gSearch.page = 1;
				CARS.gSearch.query(CARS.gSearch.page,that); 
				return false; 
			});
			$("a#blog-search",this).click(function() { CARS.gSearch.query(CARS.gSearch.page,that); return false; });
		});
		// There should only be one results pane on the page, but use .each() here for safety's sake
		$(CARS.gSearch.resultsContainer).each(function(index) {
			var that = this;
			$('a.next',this).click(function() {
				CARS.gSearch.page += 1;
				CARS.gSearch.query(CARS.gSearch.page);
				return false;
			});
			$('a.previous',this).click(function() {
				CARS.gSearch.page -= 1;
				CARS.gSearch.query(CARS.gSearch.page);
				return false;
			});
			$('a.close',this).click(function() {
				$(that).hide('slow');
				return false;
			});		
		});
	},
	query: function(page,w) {
		CARS.utility.logit(w);
		if (typeof(w) !== 'undefined') { CARS.gSearch.qTerm = w.q.value; }
		CARS.gSearch.page = page || 1;
		var that = $('#gSearch-results-container');
		$.getJSON("http://www.cars.com/go/blogs/google-search-data/askcars.jsp?callback=?", { q: CARS.gSearch.qTerm, pg: CARS.gSearch.page }, function(data) {
			var container = $("#gSearch-results", that);
                        $("#gSearchTitle strong").text(CARS.gSearch.qTerm);
			container.empty();
			$.each(data.results, function(i,result) {
				container.append("<div class=\"result\"><a href=\"" + result.url + "\">" + result.title + " </a><p>" + result.snippet + "</p></div>");
				
			});
			that.show('slow');
		});

	}
}

</script>                
			


