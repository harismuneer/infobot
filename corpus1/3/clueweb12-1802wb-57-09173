	window.CliDisplayUserName = function CliDisplayUserName() {
		var currUserName = '';
		try {
    		currUserName = CliUIGetCookie('uifd').replace(/["<>]*/g,'');
		} catch (e) {}
		if (currUserName != '') {
	    	$('#accountMenu > li > a.userName').text(currUserName);
		}
	}

	PageCallback.addCallback(function() {

				try {
			AjaxHistory.setPages('/SVOD/COMMON/STATICPAGES/HISTORYIFRAME/flip.html', '/SVOD/COMMON/STATICPAGES/HISTORYIFRAME/flop.html');
			AjaxHistory.initialize();
		} catch(e) {
			CliDebug(e.message,true);			}

				try {
			ZoneManager.getInstance().getZone('appnav').addComponent('CliInputBoxesManager', new CliInputBoxesManager() );
		} catch(e) {}
		
		CliDisplayUserName();
		
	},true);
	
	PageCallback.addCallback(function() {
		if(!ZoneManager.getInstance().getZone('content').getComponent('CliSearchSuggestController')) {
			ZoneManager.getInstance().getZone('content').addComponent('CliSearchSuggestController', new CliSearchSuggestController("/suggest/go-pc", ZoneManager.getInstance().getZone('appnav')) );
		}
		ZoneManager.getInstance().getZone('content').addComponent('CliECustomerSurvey', new CliECustomerSurvey());
	}, false);
	
