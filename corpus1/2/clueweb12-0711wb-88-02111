				if (!$(this).is('.videoLoading')) {
					trackEvent('showroom', 'click', 'video button');
					$('#videoButton').addClass('videoLoading');
					jQuery.viewer(jQuery.extend({selectedTab: 'viewerVid'}, viewerParams));
				}
				return false;
			});
			// alias tb_remove so we can stop showing the video loading graphic when the viewer is hidden
			var proxyTbRemove = setInterval(function () {
				if (window.tb_remove) {
					window.tb_remove_old = window.tb_remove;
					window.tb_remove = function() {
						$('#videoButton').removeClass('videoLoading');
						window.tb_remove_old.apply(window, arguments);
						// DNA1331110 - ddcrandallm - 2010.jan.05
						// manually stop video after thickbox is unloaded
						setTimeout(function () {
							for (var i = 0; i < DDC.embedVideoPlayer.videos.length; i++) {
								try {
									document.getElementById(DDC.embedVideoPlayer.videos[i])
										.stopMe();
								} catch (e) {}
							}
						}, 750);
					}
					clearInterval(proxyTbRemove);
				}
			}, 500);
			
