<script language="javaScript">
function hideShowDetail(divname, elementName, index)
{
	if(document.getElementById(divname).style.display == '')
		document.getElementById(divname).style.display = 'none';
	else
		document.getElementById(divname).style.display = ''; 

	var thisElement = document.getElementById(elementName);
	var descLimited = document.getElementById("limitedDesc-" + index);
	var descFull = document.getElementById("fullDesc-" + index);
	if(thisElement.innerHTML == "Show details")
	{
		thisElement.innerHTML = "Hide details";
		thisElement.className = 'show-details open';
		descLimited.style.display = "none";
		descFull.style.display = "";
	}
	else if(thisElement.innerHTML == "Hide details")
	{
		thisElement.innerHTML = "Show details";
		thisElement.className = 'show-details';
		descLimited.style.display = "";
		descFull.style.display = "none";
	}	
}
</script>












































	
	








		
