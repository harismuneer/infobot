// criteo js
var crtg_nid = "1393";
var crtg_cookiename = "cto_newsquest";

// get the current cookie value
var crtg_content = $.cookie(crtg_cookiename);

// random number
var crtg_rnd = Math.floor(Math.random()*99999999999).toString();

var q = {networkId: crtg_nid, cookieName : crtg_cookiename, rnd: crtg_rnd, varName: 'crtg_content'}
queryString = qs.objToQueryString(q);
// build the javascript
