<script type="text/javascript" charset="utf-8">
  $(function(){

    //Tabs
    $("a", "ul.expertReviewNav").click(function(e){
      e.preventDefault();
      var idToToggle = $(this).attr("href").split("#")[1];
      $("li", "ul.expertReviewNav").removeClass("active");
      $(this).parent("li").toggleClass("active");
      $(".tabContent").addClass("hidden");
      $("#" + idToToggle).toggleClass("hidden");
    })
  })
</script>

