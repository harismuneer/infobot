<script language="javascript">		

$(document).ready(function(){
	
	function equalHeight(group) {
		var tallest = 0;
		group.each(function() {
			var thisHeight = $(this).outerHeight(true);
			if(thisHeight > tallest) {
				tallest = $(this).height();
			}
		});
		group.height(tallest);

	
	}
	equalHeight($(".column"));

});

</script>



