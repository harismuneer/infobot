	jQuery(function($) {
		$('body').click(function (e) {
			if ($(e.target).attr('href') === '#detailsDisclaimer') {
				jQuery.scrollTo($(e.target).attr('href'), 750);
				$('#detailsDisclaimer').css({backgroundColor: 'white'})
					.animate({backgroundColor: '#FFDF7F'}, 1500)
					.animate({backgroundColor: 'white'});
				return false;
			}
			else if ($(e.target).attr('href') === '#detailsDisclaimer2') {
				jQuery.scrollTo($(e.target).attr('href'), 750);
				$('#detailsDisclaimer2').css({backgroundColor: 'white'})
					.animate({backgroundColor: '#FFDF7F'}, 1500)
					.animate({backgroundColor: 'white'});
				return false;
			}
		});
	});

			
		
			
