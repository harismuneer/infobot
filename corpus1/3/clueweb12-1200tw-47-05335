    <script type="text/javascript">
    //<![CDATA[
    // set the pluck file to the proper photo.
          var hrst_liid = 1;
        var hst_rotation_label = '';
    new hstrot(6,1,'hst_galleryitem_index',0,6,1);
    hst_galleryitem_index.setup();
    
    function hst_KeyCheck(e) {
      var KeyID = (window.event) ? event.keyCode : e.keyCode;
      switch(KeyID)
      {
        case 37:
        hst_galleryitem_index.prev(); // arrow left
        break;

        case 39:
        hst_galleryitem_index.next(); // arrow right
        break;
      }
    }
    
    document.onkeyup = hst_KeyCheck;

    //]]>
    </script>
   
