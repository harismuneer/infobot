				jQuery(function ($) {
		$('div.photo').slideshow({
						includeStockPhotos: ('true' === 'true')
		});
		
		var showSpin = function () {
			var href = $(this).attr('href');
			if ($('#spin').size() === 0) {
				$('<div />').attr('id', 'spin').appendTo('#viewer .widgetWrap');
			}
			try{
				SiSTeR.Pause();
			} catch(e) {};
			$('#viewer a.zoom').trigger('zoomOut');
			setTimeout(function () {
				$('#viewer div.widgetWrap').height(260);
				$('#viewer div.widgetWrap > *:not(#spin):not(#helpSpins)').hide();
				$('#helpSpins').show();
				$('#spin').attachQuickTimeMovie(href).css({height: '', width: ''}).show();
				setTimeout(function () {
					$('#spin embed').css({left: 'auto'});
				}, 1000);
			}, 750);
			return false;
		};
		$('#media .interior, #media .exterior').unbind('click').click(showSpin);
	});

