	<script type='text/javascript'>
		jQuery(function() {
			jQuery('.sudownedslider_slider').jCarouselLite({
				btnNext: '.next-page',
				btnPrev: '.prev-page',
				scroll:	5,
				visible: 5
				
				});
				
			jQuery('.sudownedsliderf_slider').jCarouselLite({
				btnNext: '.next-page',
				btnPrev: '.prev-page',
				scroll:	5,
				visible: 9
				
				});
				
			jQuery('.sudownedslider_post').mouseenter(function(){
				jQuery(this).find('.sudownedslider_hider').stop(true, true).animate({
					height: '100px'
				}, 500);
			}).mouseleave(function(){
				jQuery(this).find('.sudownedslider_hider').stop(true, true).animate({
					height: '8px'
				}, 1000);
			});
			
			
			
			jQuery('.sudownedslider_chiclet').mouseenter(function(){
			
				var loc = jQuery(this).offset();
				//var offset = ($(window).width() - 1000) / 2;
				//var newpos = loc - offset;
				var cur = jQuery(this).find('.sudownedslider_desc');
				
				jQuery('.sudownedtip').remove();
				var src = jQuery(this).find('img').attr('src');
				var link = jQuery(this).find('a').attr('href');
				jQuery('#content-wrapper').append('<div class="sudownedtip" style="position: absolute;"><a href="'+link+'"><img src="'+src+'"></a><span>'+jQuery(cur).html()+'</span></div>');
				jQuery('.sudownedslider_chiclet').find('img').css('z-index', 4);
				jQuery('.sudownedslider_chiclet').find('a').css('z-index', 4);
				jQuery(this).find('img').css('z-index', -1);
				jQuery(this).find('a').css('z-index', -1);
				loc.left = loc.left - 7;
				jQuery('.sudownedtip').offset(loc);
				jQuery('.sudownedtip').find('img').mouseleave(function(){
					jQuery('.sudownedtip').remove();
				});
				
				
				//jQuery(cur).stop(true, true).animate({
				//	height: '160px',
				//	width: '240px',
				//	left: '-10px',
				//	top: '-10px'
				//}, 500);
				
				
