<script>
  jQuery(document).ready(function(){
     
    
    var $tabs = $('#product_details').tabs(); 
    
    $('a#details').click(function() { 
        $tabs.tabs('select', 0); 
        $('#marker-details').scrollTo();
        return false;
    });
    
    $('a#reviews').click(function() { 
        $tabs.tabs('select', 1);
        $('#marker-reviews').scrollTo();  
        return false;
    });
       
    $("a#video").click(function () {
       $("#videotester").trigger('click');
    });
    
    Galleria.loadTheme('http://cdn.shopify.com/s/files/1/0046/5192/t/4/assets/script.js?101627');
    $("#product_images").galleria({
        width: 660,
        height: 500,
        popupLinks:true,
        data_config: function(img) {
        return {
        title: $(img).attr("title"),
        description: $(img).attr("alt")
        };
      }
    });
    

		
    jQuery(window).scroll(function(event) { // Scroll means when the window is scrolling

				var windowLoc = jQuery(window).scrollTop(); // Grab the value in pixels of the scrollbar position for the window
				var detailsLoc = jQuery('span#marker-details').offset().top - 1;
				var reviewsLoc = jQuery('span#marker-details').offset().top - 1;
				//var featureLoc = jQuery('#nav-positioning').offset().top - 120;
				//var techLoc = jQuery('#product-features').offset() == null ? 999999 : jQuery('#product-features').offset().top - 120;
				// If #product-features [position] doesn't exist, then asign 999999, else figure out the position minus 120 pixels
				
				//jQuery('#brand').html("Window: " + windowLoc + "Details Marker: " + detailsLoc);
				
				var detailsTab = jQuery('#details_tab').hasClass('ui-state-active');
				var reviewsTab = jQuery('#reviewss_tab').hasClass('ui-state-active');
				
				
				if ((windowLoc < detailsLoc)) 
				{ 
					jQuery('.product_nav ul li a div#icon').removeClass('active');
					jQuery('.product_nav ul li a#overview div#icon ').addClass('active');	
					//log('overview');
				}
				
				if ((windowLoc > detailsLoc) && (detailsTab)) 
				{ 
					jQuery('.product_nav ul li a div#icon').removeClass('active');
					jQuery('.product_nav ul li a#details div#icon ').addClass('active');	
					//log('overview');
				}
				
				if ((windowLoc > reviewsLoc) && (reviewsTab)) 
				{ 
					jQuery('.product_nav ul li a div#icon').removeClass('active');
					jQuery('.product_nav ul li a#reviews div#icon ').addClass('active');	
					//log('overview');
				}

			
		});
    
  });
</script>





