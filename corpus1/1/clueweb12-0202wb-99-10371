        $('.sfooter .pane a').bind('mouseleave', hideSxSlide);

        function showSxSlide() {
          $(this).find('img').stop().animate({ opacity:0 }, 400);
        }

        function hideSxSlide() {
          $(this).find('img').stop().animate({ opacity:1 }, 400);
