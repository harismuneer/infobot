<script type="text/javascript">

var FFGallery = 0;

function cycleGallery(slidenum) {

	var curimages = jQuery("#FFGallery img");
	var tag = jQuery("#FFGallery a.covering span");
	var menu = jQuery("#FFGalControls p");

	if (slidenum >= curimages.length) {
		slidenum = 0;
	}
	
	FFGallery = setTimeout('cycleGallery(' + (slidenum + 1) + ')', ((10 * 1000) + 1150));
	
	curimg = curimages.eq(slidenum);
	curimg.css({top:0});
	curtitle = curimg.attr("alt");
	curlink = curimg.attr("link");
	curheight = curimg.height();
	tag.html(curtitle).parent().attr("href", curlink); 
	tag.css({display:"block", opacity:0}).fadeTo(500, 1);
	menu.eq(slidenum).addClass("contactive");
	curimg.fadeIn(500, function() {
		curimg.animate({ top:"-"+(curheight*.5) }, (10 * 1000), "linear", function() {
			tag.fadeOut(500);
			curimg.fadeOut(500, function() {
				curimg.css({top:0});
				menu.eq(slidenum).removeClass("contactive");
			});
		});
	});
}

jQuery(document).ready(function($) {

	var curimages = $("#FFGallery img");
	var tag = jQuery("#FFGallery a.covering span");
	
	$("#FFGallery img").each(function(number) {
		leftm = (curimages.length - number) * (110/curimages.length) - 22;
		$("#FFGalControls").append("<p>" + (number+1) + "</p>");
	});
	
	$("#FFGalControls p").hover(function() {
		$(this).addClass("contover");
	}, function() {
		$(this).removeClass("contover");
	});
	
	$("#FFGalControls p").click(function() {
		if (FFGallery != 0) { clearTimeout(FFGallery); curimages.stop(); tag.stop(); }
		$("#FFGalControls p").removeClass("contactive");
		curimages.hide();
		slidetodisplay = parseInt($(this).text()) - 1;
		cycleGallery(slidetodisplay);
	});
	
	$("#FFGalControls p").eq(0).click();
	
			$("#content .post object").each(function() {
			ewidth = $(this).width();
			eheight = $(this).height();
			$(this).wrap("<div class='embedwrap' style='width:" + ewidth + "px;'></div>");
		});
		if ($(".gallery-icon").length > 0) {
			$(".gallery-icon").each(function() {
				$(this).find("a.fancybox").attr("title", $(this).find("a.fancybox img").attr("alt"));
			});
		}
		
});

</script>
 
