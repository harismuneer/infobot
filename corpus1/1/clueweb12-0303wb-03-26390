    <script type="text/javascript">
        $(document).ready(function() {
            $("#lnkCloseNotify").click(function() {
                $(".notify").hide();
            });
            $("#lnkNotify").click(function() {
                $(".notify").show();
            });
            $(".txtNotifyEmail").click(function() {
                $(this).val("");
                $(this).css("color", "#000");
            });
            $("#lnkCompare").click(function() {
                var strID = ""
                $(".cbCompare").each(function(index) {
                    if ($(this).attr('checked')) {
                        if (strID != "") {
                            strID += "," + $(this).val();
                        } else {
                            strID = $(this).val();
                        }
                    }
                });
                if (strID != "") {
                    window.location = "http://www.swapalease.com/functions/compare.aspx?id=" + strID;
                }

            });
            $(".cbCompare").click(function() {
                if (!$(this).is(':checked')) {
                    $.ajax({ url: "/functions/compare?type=remove&id=" + $(this).val() });
                }
            });
        });
    </script>
