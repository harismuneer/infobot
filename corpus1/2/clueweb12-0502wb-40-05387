       version=0;     //To get the version number of the browser
	    // document.write(version +'<br>');
       if (navigator.appVersion.indexOf("MSIE")!=-1){
       temp=navigator.appVersion.split("MSIE");
       version=parseFloat(temp[1]);
       }
	   bName = navigator.appName; //To get the browser name
	   //document.write(bName +'<br>');
	   // document.write(version +'<br>');
	   //RUN THIS ON IE
if(bName == "Microsoft Internet Explorer"){
       //document.write('MSIE Detected<br>');
       if(version > 6){
	      //var oReq = new ActiveXObject("Microsoft.XMLHTTP");
          var oReq = new XMLHttpRequest();
          oReq.open("GET", url,false);
          oReq.send(null);
      }else{
	     // document.write('Version before 7<br>');
	       var oReq = new ActiveXObject("Microsoft.XMLHTTP");
           // var oReq = new XMLHttpRequest();
           oReq.open("GET", url,false);
           oReq.send(null);
		  // return oReq;
      }   
}else{     //RUN this on Firefox
     var xmlHttp = new XMLHttpRequest(); 
