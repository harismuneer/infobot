	<!-- replace 'Bessemer' with 'Pell City' in h1 on pages that have 'dealer3281' in their href -->
	<script type="text/javascript">
	//<!CDATA[
		var loc = window.location.href.toLowerCase();
		if (loc.match("dealer3281") != null) {
			$('div.searchHeader h1[ID*=pagetitle]').text($('div.searchHeader h1[ID*=pagetitle]').text().replace('Bessemer','Pell City'));
		}
	//]]>
	</script>

