	<script type="text/javascript">

	var jQue = jQuery.noConflict();



	jQue(window).scroll(function() {	

		var stickyEl = jQue('#shareLayer');

		var windowTop = jQue(window).scrollTop();

		var elTop = jQue(stickyEl).offset().top;

		

		if (windowTop > 450)  {

			jQue(stickyEl).addClass('sticky').stop();

			return false;    

		}

		else {

			jQue(stickyEl).removeClass('sticky');  

			return false;    

		}

	});	

	</script>

	

