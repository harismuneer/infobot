    /* <![CDATA[ */
   function openDealerPopUp() { 
		$('#popUpContainer').toggle();
   }
	   
    $(document).ready(function(){         
        $('#popUpContainer').click(function(){
            $('#popUpContainer').toggle();
        });
        
    });
	
	function openDealerPopUpAlt() { 
		$('#popUpContainerAlt').toggle();
   }
	   
    $(document).ready(function(){         
        $('#popUpContainerAlt').click(function(){
            $('#popUpContainerAlt').toggle();
        });
        
    });
