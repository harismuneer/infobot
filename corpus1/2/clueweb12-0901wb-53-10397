											<script type="text/javascript">
/* @ANKUR - 23 aug 2011
	tab = carquotesearch added in function
*/
function setquotemake(name, tab)
{
	if(tab=="quotesearch"){
	$("#quote_make_box strong").html(name);
	}
	else if(tab=="quotesearchcalc"){
	$("#quote_make_box_calc strong").html(name);
	}
	else if(tab=="videoquotesearch"){
	$("#videoquote_make_box strong").html(name);
	}
	else if(tab=="carquotesearch")
	{
		//$(".selectbox").find('>div.popup').hide();
	}
	else
	{
		$(".selectbox").find('>div.popup').hide();
	}
	
	$(".specific-list").find('li.checked').removeClass('checked');
}
</script>
