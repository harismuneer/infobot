<script language="javascript">
function init_history_tracker() {
    if( document.__history_tracker_form.__counter ) {
        document.__history_tracker_form.__counter.value++;
    }
    if( document.__history_tracker_form.__counter.value > 1 ) {
        // BACK
        var sLocation = document.location.href;
        sLocation = sLocation.replace(/[\?|\&]r=\d+/g,'');
        if( sLocation.indexOf('#') != -1 ) {
           var sSuffix = sLocation.substring(sLocation.indexOf('#'));
           sLocation = sLocation.substring(0,sLocation.indexOf('#'));
        } else {
            var sSuffix = '';
        }
        if( sLocation.indexOf('?') != -1 ) {
            var sep = '&';
        } else {
            var sep = '?';
        }
        var val = Math.floor(10000*Math.random());
        document.location = sLocation+sep+'r='+val+sSuffix;
    }
}

YAHOO.util.Event.onAvailable('__history_tracker_form',init_history_tracker);
</script>
