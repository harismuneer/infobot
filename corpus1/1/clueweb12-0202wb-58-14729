<script type="text/javascript">
    $LS(document).ready(function () {
        $LS('#disqus_thread_box').css('display', 'none');
        if (pageCount != 1) {
            $LS(".pnlSignoff").removeClass("show").addClass("hide");
        }
        else {
            $LS(".pnlSignoff").removeClass("hide").addClass("show");
        }

        $LS("#postCommentId").attr("href", "javascript:showPage(" + (parseInt(pageCount, 10) + 1) + ", " + pageCount + ")");

        var allQueryStrings = window.location.search;
        if (allQueryStrings != "") {
            var queryString = allQueryStrings.substring(1).split("&");
            for (var i = 0; i < queryString.length; i++) {
                var pair = queryString[i].split("=");
                if (pair[0] == "page") {
                    curPage = 1;
                    showPage(pair[1], pageCount);
                }
            }
        }

        $LS('.emailAnchor').mouseover(function () {
            turnOnGenericIcons($LS('.emailImg'), 1);
            $LS('.emailAnchor').css('color', '#ff6633');
        });
        $LS('.emailAnchor').mouseout(function () {
            turnOnGenericIcons($LS('.emailImg'), 0);
            $LS('.emailAnchor').css('color', '');
        });
        $LS('.emailImg').mouseover(function () {
            $LS('.emailAnchor').css('color', '#ff6633');
        });
        $LS('.emailImg').mouseout(function () {
            $LS('.emailAnchor').css('color', '');
        });
        $LS('.printAnchor').mouseover(function () {
            turnOnGenericIcons($LS('.printImg'), 1);
            $LS('.printAnchor').css('color', '#ff6633');
        });
        $LS('.printAnchor').mouseout(function () {
            turnOnGenericIcons($LS('.printImg'), 0);
            $LS('.printAnchor').css('color', '');
        });
        $LS('.printImg').mouseover(function () {
            $LS('.printAnchor').css('color', '#ff6633');
        });
        $LS('.printImg').mouseout(function () {
            $LS('.printAnchor').css('color', '');
        });

        $LS('.post-comment').mouseover(function () {
            $LS('.view-comment').css('color', '#ff6633');
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.post-comment').mouseout(function () {
            $LS('.view-comment').css('color', '');
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/healthcenter/comments2.png)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.post-comment').click(function () {
            onClickComments();
        });

        $LS('.view-comment').mouseover(function () {
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.view-comment').mouseout(function () {
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/healthcenter/comments2.png)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.view-comment').click(function () {
            onClickComments();
        });

    });

    function onClickComments() {
        var commentBox = $('#disqus_thread_box').css('display');
        $('#disqus_thread_box').css('display', 'block');
        $("#genericArticleCommentId").removeClass("hide").addClass("show");
        gIsUgc = true;
        RefreshAllAds();
        TrackPageView();
    }

    function onHoverComments() {
        $LS('.view-comment').css('color', '#ff6633');
        $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
        $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
    }
    function onNonHoverComments() {
        $LS('.view-comment').css('color', '');
        $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/healthcenter/comments2.png)');
        $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
    }

</script>                        

