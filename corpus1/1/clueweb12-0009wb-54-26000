<script>
		var url = 'http://content-api.ign.com/v1/objects/913195.json?networkid=12';
            jQuery.getJSON(url + '&callback=?', function(json) {
               if (typeof(json) != 'undefined') {
                	var obj = json.object;
					var videoCount = obj.totalVideos;
					var imageCount = obj.totalImages;
					var hideThumbs="";
					var imagesToDisplay = 2;
					if(videoCount >0){imagesToDisplay = 1;}


					if(videoCount > 0 || imageCount > 0) {
						$j('#latestMediaWrapper').show();
						if(videoCount > 0){
							var thumb=obj.lastVideoThumbUrl;
							var videoGalleryUrl=obj.videoGalleryUrl;
							$j('.latestmediaBtnVideos').attr('url',videoGalleryUrl);
							$j('.latestmediaBtnVideos').click( function(event) { window.location = $j(this).attr("url"); event.preventDefault(); });
							$j('.img-wrapper').width(313);
							$j('.vid-wrapper').show();
							if(imageCount == 0){
								$j('.latestmediaBtnImages').removeClass('latestmediaBtnImages').addClass('latestmediaBtnImagesOff');


							}
							$j('#video-count').html('<a href="'+videoGalleryUrl+'">See All <strong>'+videoCount+'</strong> Videos</a>');
							$j('#thumbImg3').hide();
						}
						if(videoCount == 0 && imageCount > 0){
							$j('.img-wrapper').width(480);
							$j('.latestmediaBtnImages').addClass('latestmediaBtnImagesOnly');
							$j('.vid-wrapper').hide();
							$j('#thumbImg3').show();
							$j('.latestMediaBorder1','.latestMediaBorder2').width(478);
							$j('#video-count').hide();

						}

						if(videoCount>0 && hideThumbs != 'true' && imageCount > 0){
							$j('#latestMediaWrapper').height(162);
							$j('.latestMediaBorder1','.latestMediaBorder2').height(127);

						}
						var imageDisplayCount = 3;
						if(imageDisplayCount < imagesToDisplay){
						    for(i = 0; i < (imagesToDisplay - imageDisplayCount); i++){
						        $j('#img-'+i).show();
						    }
						}


					}
					$j('.latestMediaThumbVid').html('<a href="'+videoGalleryUrl + '"><img width="140" height="105" class="latestMediaThumb" alt="" src="'+thumb+'"/></a>');
                }
            });
</script>

