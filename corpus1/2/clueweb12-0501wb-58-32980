<script type="text/javascript">
(function($) {
	$("#id-6c7b2159-f620-442c-a920-b59c1fc34605 #insuranceZipcode").mask("99999", {destroyInvalid: false});
	$('#id-6c7b2159-f620-442c-a920-b59c1fc34605 #btnInsuranceSearch').click(function() {
	var z = $('#id-6c7b2159-f620-442c-a920-b59c1fc34605 #insuranceZipcode').val();
        window.location = '/Auto-Insurance/?zipcode=' + z;
    });
    $('#id-6c7b2159-f620-442c-a920-b59c1fc34605 #insuranceZipcode').keypress(function(e) {
        if (e.which == 13) {
            $('#id-6c7b2159-f620-442c-a920-b59c1fc34605 #btnInsuranceSearch').click();
			return false;
		}
    });
})(jQuery);   
</script>

	</div>
	<div class="bottom"></div>
</div>
		<!--/Insurance Quote-->
	
