	function openWindowFor(a) {
		var options = eval("(" + a.getAttribute('window') + ")");
		options.url = options.url || a.getAttribute('href');
		options.title = options.title || a.innerHTML;
		openWindow(options);
		return false;
	}
	function applyWindows() {
		_('.window').click(function(){
			return openWindowFor(this);
		});
	}
	applyWindows();
</script>
