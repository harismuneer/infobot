			jQuery('#icpsignup2857').submit(function(event){

				if (document.location.protocol === "https:") {
					var url = "https://app.icontact.com/icp/signup.php";
					jQuery(this).attr('action', url);
				}
				
				if ( jQuery('#icpsignup2857 input[name="fields_email"]').val() == "" ) {
					jQuery('#icpsignup2857 input[name="fields_email"]').focus();
					alert("The Email field is required.");
					return false;
				}
			});
			jQuery('#icpsignup2857 input[name="fields_email"]').focus(function(event){
				if ( jQuery(this).val() == 'Enter Email Address' ) {
					jQuery(this).val('');
					jQuery(this).css('text-align','left');
				}
			});
			jQuery('#icpsignup2857 input[name="fields_email"]').blur(function(event){
				if ( jQuery(this).val() == '' ) {
					jQuery(this).val('Enter Email Address');
					jQuery(this).css('text-align','center');
				}
