        $("#Gall_"+ numarr[1] ).slideDown("fast");
       },
       mouseleave: function() {
       		var num = this.id;
       		var numarr = num.split('_');
        // hide your menu
     	//$("#Gall_"+ num ).slideUp("fast");
     	$("#Gall_"+ numarr[1] ).slideUp("fast");
       }
 });

