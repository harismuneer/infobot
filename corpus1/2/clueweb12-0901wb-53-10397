$(document).ready(function(){
	

/*function getTheQuote()
{
	document.optionfrm_car_quote.submit();
}*/

	
	var link = window.location.href.split( '#' );
	if(link[1]=="prices")
	{ 
		$('body').addClass("build-price");	
		
		$('#pricegradient').css("z-index", -1);		
		$(".basicfspecs").hide();		
		$("#left_buildnprice").show();
		$(".highlights").width(191);
		$("#left_others").show();
		$("#displaygallery").hide();
		$("#progress").show();
		$("#overview-tab").removeClass('active');
		$("#pricing-tab").addClass('active');
		$(".overview").removeClass('selected');
		$(".pricing").addClass('selected');
		$('.highlights').css("padding-left", 0);
		$('.highlights').css("padding-bottom", 0);
        $('.bottom_content').css("margin-top", 0);
		//$('#gallery_large_image').hide(); //@Ankur 25 feb 2012 commented
		$('.image_video_slider').hide();//@Ankur 25 feb 2012 added
		$('.editorials').width(450); 
		$('.right_column').height("auto");
        $('.left_column').height("auto");
        $('#left_others').height("auto");
        $('.editorials').height("auto");
        $("#otherstab").hide();
        $("#carpricingtab").show();
        
        //@Ankur 20 dec
        $('#carpagetitle').width(665); 
        $('.sub_menu').width(665);
        
      //  $('#buildprcieads').addClass("buildprcieadsclass");
        //$('#buildprcieads').hide();
        $('.content_panel').height('auto');
        
		if($('#left_others').height() > $('.editorials').height())
		{
			$('.editorials').height($('#left_others').height()); 
			$('.content_panel').height($('#left_others').height() );
		}
		else
		{
			$('#left_buildnprice').height($('.editorials').height());
			
		}
		
		if($('.right_column').height() > $('.left_column').height())
		{
		    $('.left_column').height($('.right_column').height());
		    $('#main_content').height($('.right_column').height());
		    //@Ankur 12 jan 2012
		    var lbnpht12 = $('.left_column').height() - ($('.title_bar_carpage').height() + $('.sub_menu').height());	
		$('#left_others').height(lbnpht12 );
		$('.content_panel').height(lbnpht12);
		}
		else
		{ 
		   	$('.left_column').height('auto');
		    $('.right_column').height($('.left_column').height());
		    $('#main_content').height($('.left_column').height());            
		}
    
	}	
	if($('.right_column').height() > $('.left_column').height())
	{
		$('.left_column').height($('.right_column').height());
		$('#main_content').height($('.right_column').height());		
	}
	else
	{	
		$('.left_column').height('auto');
		$('.right_column').height($('.left_column').height());	
		$('#main_content').height($('.left_column').height());
	}
});	
</script>


