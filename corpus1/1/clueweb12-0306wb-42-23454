	myString = new String(currentPage)
	rExp = 'page';
	currentPage = myString.replace(rExp, 'paging');


	var elements = (ie) ? document.all : document.getElementsByTagName('a');

	  for (i=0; i<elements.length; i++){
		 if(elements[i].id == currentPage){
		 	elements[i].style.textDecoration='none';
			elements[i].style.cursor='default';
			elements[i].style.fontWeight='bold';
			elements[i].blur(); //to remove out so user can which page they are on
		 }else{
		 	//change all other paging links back to normal linky look
			 if (elements[i].className==objClass){
				elements[i].style.textDecoration='underline';
				elements[i].style.cursor='pointer';
			 }
		 }
	  }
	}
	
	
	function hideClass(objClass, currentPage){
	//  This function will hide Elements by object Class
	//  Works with IE and Mozilla based browsers
	
	var elements = (ie) ? document.all : document.getElementsByTagName('div');
	  for (i=0; i<elements.length; i++){
		 if(elements[i].id == currentPage){
		 	elements[i].style.display="block";
		 }else{
			 if (elements[i].className==objClass){
			   //alert(elements[i].id);
				elements[i].style.display="none"
			 }
		 }
	  }
	}
	
	window.onload = function(){
		hideClass("pagedivs", "page0");
		pagingClass("paginglink", 'page0');
	}
	
// -->
</script>
</html>