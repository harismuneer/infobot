        var googIndex = docCookie.indexOf('__utmv');
        var googVar = "";
        var newGoogVar = [];
        var changed = false;
        
        if (googIndex >= 0) {
            var googIndexEnd = docCookie.indexOf(';', googIndex);
            if (googIndexEnd < 0) {
                googIndexEnd = docCookie.length;
            }
            googIndex += 7; // Get rid of cookie's name
            var googCookie = docCookie.substring(googIndex, googIndexEnd);
            // Cookie is a bunch of numbers and a period at the beginning, get rid of those
            googVar = googCookie.replace(/^[0-9]*\./, "");
        }
        var pairList = googVar.split("_");
        for (var i=0; i<pairList.length; i++) {
            var pair = pairList[i];
            if (pair.length < 1) {
                continue;
            }
            var nameValPair = pair.split("-");
            if(!activeVars[nameValPair[0]]) {
                // not active any more; strip it out
                continue;
            }
            if(userVars[nameValPair[0]] !== undefined) {
                // in both... needs changing?
                // if new value evaluates to false, put in nothing
                if(userVars[nameValPair[0]]) {
                    newGoogVar.push(nameValPair[0] + '-' + userVars[nameValPair[0]]);
                }
                delete userVars[nameValPair[0]];
            } else {
                // only in the current cookie; just pass it through
                newGoogVar.push(pair);
            }
        }
        for(var name in userVars) {
            if(userVars.hasOwnProperty(name) && userVars[name]) {
                newGoogVar.push(name + '-' + userVars[name]);
            }
        }
        if(newGoogVar.length) {
            newGoogVar.sort();
            newGoogVar = '_' + newGoogVar.join('_') + '_';
        } else {
            newGoogVar = '';
        }
        
        if(newGoogVar != googVar) {
            if(window.console && window.console.log) console.log('_setVar', newGoogVar);
            // _setVar doesn't do anything if given ""; "-" makes it empty everything out
            pageTracker._setVar(newGoogVar || "-");
        }
        
                    
