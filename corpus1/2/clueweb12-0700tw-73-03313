<script type="text/javascript">/* <![CDATA[ */function add_loyalty_points( activity, points, callback ) {
	Loyalty_Points.add_points(activity, points, callback);
}

function remove_loyalty_points( activity, points, callback ) {
	add_loyalty_points(activity, points*-1, callback);
}

function get_loyalty_user( activity, callback ) {
	Loyalty_Points.get_user( activity, callback );
}

function update_loyalty_points( points ) {
	Loyalty_Points.update_points(points);
}

function get_loyalty_point_value( activity ) {
	return Loyalty_Points.get_point_value(activity);
}

function get_loyalty_activity_interval( activity ) {
	return Loyalty_Points.get_interval(activity);
}

function Loyalty_User() {
	this.last_request = 0;
	this.status = 'OK';
	this.error = '';
	this.name = '';
	this.email = '';
	this.points = 0;
	this.activities = {};
}
Loyalty_User.prototype.update = function ( data ) {
	if ( typeof(data.status) == 'undefined' ) {
		return; // if we don't have a status, we can't assume anything else if valid
	}
	this.status = data.status;
	this.error = '';
	if ( typeof(data.last_request) != 'undefined' ) {
		this.last_request = data.last_request;
	}
	if ( typeof(data.error) != 'undefined' ) {
		this.error = data.error;
	}
	if ( typeof(data.name) != 'undefined' ) {
		this.name = data.name;
	}
	if ( typeof(data.email) != 'undefined' ) {
		this.email = data.email;
	}
	if ( typeof(data.points) != 'undefined' ) {
		this.points = data.points;
	}
	if ( typeof(data.activities) != 'undefined' ) {
		// don't want to overwrite any existing activities that aren't set in data
		for ( activity in data.activities ) {
			this.activities[activity] = data.activities[activity];
		}
	}
};

Loyalty_Points = {
	activities: {},
	user: new Loyalty_User,

	add_points: function( activity, points, callback ) {
		if ( !activity ) {
			return;
		}
		var url = '/api/loyalty';
		url = url + '/' + activity;
		if ( points ) {
			url = url + '/' + points;
		}
		jQuery.ajax({
			url: url,
			data: { now: Math.round((new Date()).getTime() / 1000) }, // because IE loves to cache
			success: function(data, textStatus) {
				if ( data ) {
					if ( typeof(data.points) != 'undefined' ) {
						Loyalty_Points.update_points(data.points);
					}
					Loyalty_Points.user.update(data);
					if ( callback ) {
						callback(Loyalty_Points.user);
					}
				}
			},
			error: function(request, textStatus) {
				var data = jQuery.parseJSON(request.response);
				if ( data ) {
					Loyalty_Points.user.update(data);
					if ( callback ) {
						callback(Loyalty_Points.user);
					}
				}
			},
			dataType: 'json'
		});
	},

	get_user: function( activity, callback ) {
		if ( !document.cookie.match(/_ts_logged_in=1/) ) {
			return;
		}
		var url = '/api/loyalty/user';
		if ( activity ) {
			url = url + '/' + activity;
		}
		jQuery.ajax({
			url: url,
			data: {},
			success: function(data, textStatus) {
				if ( data ) {
					Loyalty_Points.user.update(data);
					if ( callback ) {
						callback(Loyalty_Points.user);
					}
				}
			},
			error: function(request, textStatus) {
				data = jQuery.parseJSON(request.response);
				if ( data ) {
					Loyalty_Points.user.update(data);
					if ( callback ) {
						callback(Loyalty_Points.user);
					}
				}
			},
			dataType: 'json'
		});
	},

	update_points: function( points ) {
		jQuery(".number-of-points").text(points);
	},

	get_point_value: function( activity ) {
		var a = this.get_activity(activity);
		if ( a != null ) {
			return a.points;
		}
		return 0;
	},

	get_interval: function( activity ) {
		var a = this.get_activity(activity);
		if ( a != null ) {
			return a.interval;
		}
		return 0;
	},

	get_activity: function( activity ) {
		if ( !(typeof this.activities[activity] == 'undefined') ) {
			return this.activities[activity];
		}
		return null;
	}
};

