<script language="javascript">
        function validateForm() {
            if (document.getElementById('yp_what').value.replace(/^\s+|\s+$/g, '') == '') {
                alert("Please provide a keyword or business name.");
                document.getElementById('yp_what').focus();
                return false;
            }
            else
            {
                return true;
            }
        }
        </script>
