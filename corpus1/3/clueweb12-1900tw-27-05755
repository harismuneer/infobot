  <script type="text/javascript">
    jQuery(document).ready(function() {
      twttr.events.bind('tweet', function(event) {
      	page_url = window.location.pathname;
      	m = /([^\/]+)\/(\d+)-(.+$)/.exec(page_url);
        if(m && m.length >=3 ) {
          save_share("Event", m[2], "ReTweet");
          window.location.reload();
        }
      });
    });
  </script>
  
  


