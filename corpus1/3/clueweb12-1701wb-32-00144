// Test for Microsoft Internet Explorer
//-----------------------------------------------------------------------
function isMSIE() 
//-----------------------------------------------------------------------
{
	return (navigator.appVersion.indexOf("MSIE") != -1)
}


// Netscape CSS Bug Fix
if (document.layers){
	origWidth = innerWidth;
	origHeight = innerHeight;
	}
	
//-----------------------------------------------------------------------
function reloadPage() 
//-----------------------------------------------------------------------
{
	if (innerWidth != origWidth || innerHeight != origHeight)
		location.reload();
}
	
if (document.layers) onresize = reloadPage;	


// Email Link Protection Functions
var user;
var domain;
var suffix;
var linktitle;

// Show and Hide Functions
//-----------------------------------------------------------------------
function hide_main_menu(div_id)
//-----------------------------------------------------------------------
{
	var menu_id;
	
	menu_id = document.getElementById(div_id);
	
	/* Hide the specified menu */
	if (menu_id == null)
	{
		return;
	}
	else 
	{
		menu_id.className = 'hide';
	}
}

//-----------------------------------------------------------------------
function toggle_main_menu(div_id)
//-----------------------------------------------------------------------
{
	var menu_id, old_value;
	
	menu_id = document.getElementById(div_id);
	if (menu_id == null)
		return;
		
	/* Retrive current class value */
	old_value = menu_id.className;
	
	/* First hide all menus */
	if (menu_ids) {
		for(i=0; i<menu_ids.length; i++) {
			document.getElementById(menu_ids[i]).className = 'hide';
			}
	}

	/* set new class value */
	if (old_value == 'hide')
	{
		menu_id.className = 'show';
	}
	else
	{
		menu_id.className = 'hide';	
	}
}	

//-----------------------------------------------------------------------
function show_all_menus()
//-----------------------------------------------------------------------
{
	if (menu_ids) {
		for(i=0; i<menu_ids.length; i++) {
			document.getElementById(menu_ids[i]).className = 'show';
			}
	}
}	

//-----------------------------------------------------------------------
function toggle_display(div_id)
//-----------------------------------------------------------------------
{
	var child;

	/* Hide all menus */
	if (menu_ids) {
		for(i=0; i<menu_ids.length; i++) {
			document.getElementById(menu_ids[i]).className = 'hide';
			}
	}
	
/*	child = document.all(window.event.srcElement.child);*/
	child = document.getElementById(div_id);
	
	if (child == null)
		return;
	else
	{
		if (child.className == 'hide')
		{
			child.className = 'show';
		}
		else
		{
			child.className = 'hide';
		}
	}
}

//-->
