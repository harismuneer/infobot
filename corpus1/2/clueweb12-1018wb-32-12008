        $(document).ready(function () {
            if (DetectMobileQuick()) {
                var cookieValue = $.cookie("spill_mobile");

                if (cookieValue != null && cookieValue != "0") {
                    $.cookie("spill_mobile", "1", { expires: 30, path: "/" });
                    window.location = "http://m.spill.com";
                }
            }
        });
    </script>

    
    
    <script type="text/javascript">
        function searchAudioKeyCheck(e) {
            var characterCode;

            if (e && e.which) {
                characterCode = e.keyCode;
            }
            else {
                characterCode = event.keyCode;
            }

            if (characterCode == 13) {
                submitAudioSearch();
            }
        }

        function submitAudioSearch() {
            var searchElement = document.getElementById("searchAudioText");

            if (searchElement != null) {
                location.href = "/Audio/Default.aspx?search=" + searchElement.value;
            }
        }
    </script>

