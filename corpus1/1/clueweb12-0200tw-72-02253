	if (cnnTrackPath.match("galleries")) {
		var galleryUrlMatch = /.*\//;
		cnnTrackPath = cnnTrackPath.match(galleryUrlMatch);
		var hedMatch = /[^\-]*/;
		cnnTitle = cnnTitle.match(hedMatch);
	}
	var cnnSection = '';
	if (typeof jsmd!='undefined') {
		var cnnMeta=jsmd.mdata.business.mny.section;
		cnnSection=cnnMeta[1];
	}
	if (typeof cnnSource != 'undefined') {
		if (cnnSource=="FORT") {
			var isFortune = true;
		}
	}
	if (window.location.href.match(".fortune")||isFortune) {
