var __squid_like = {
  logged_in: isLoggedIn,
  member_likes: false,
  owner_viewer: false,
  member_favorite: false,
  like_count: 14};

function doSquidLike() {
  buttonObj = $J('.sl_btn');

  if (! buttonObj.hasClass('disabled')) {

    if (__squid_like.logged_in) {
      if (__squid_like.owner_viewer) {
        Squidoo.modal('#owner_liked');
      } else {
        buttonObj.addClass('disabled');
        var new_count;
        if (__squid_like.member_likes) {
          new_count = __squid_like.like_count - 1;
          $J('.sl_count_rt').html(new_count.toString());
          $J('li.social_like').removeClass('social_liked');

          __squid_like.like_count = new_count;
          __squid_like.member_likes = false;
        } else {
          new_count = __squid_like.like_count + 1;
          $J('.sl_count_rt').html(new_count.toString());
          $J('li.social_like').addClass('social_liked');

          __squid_like.like_count = new_count;
          __squid_like.member_likes = true;
        }
        SquidgamingJS.get('/utility/like_lens/' + theLensName, function (result) {
          setTimeout(function() {
              buttonObj.removeClass('disabled');
          }, 5000);
        });
      }
    } else {
      Squidoo.login();
    }
  }
}

