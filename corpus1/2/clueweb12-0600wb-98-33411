var receiveReq = getXmlHttpRequestObject();
var files_xml = '144144.xml'

function show_comment(page)
{
    //alert ("aa");
    if (receiveReq.readyState == 4 || receiveReq.readyState == 0) {
		 //alert ("nn"); 
		receiveReq.open("GET", './comment/getdata.php?files_xml='+ files_xml + '&page=' + page  , true);
		receiveReq.onreadystatechange = handleReceiveChat; 
		receiveReq.send(null);
	}
}


function getXmlHttpRequestObject() {
				if (window.XMLHttpRequest) {
					return new XMLHttpRequest();
				} else if(window.ActiveXObject) {
					return new ActiveXObject("Microsoft.XMLHTTP");
				} else {
					alert('Status: Cound not create XmlHttpRequest Object.  Consider upgrading your browser.');
				}
}

function handleReceiveChat() {
    if (receiveReq.readyState == 4) {
	    var showdata = document.getElementById('showdata');
		var xmldoc = receiveReq.responseXML;
		var list_nodes = xmldoc.getElementsByTagName("list"); 
		var n_list = list_nodes.length;
	    if (n_list > 0)	
    	{	showdata.innerHTML ='';
			var count = xmldoc.getElementsByTagName("count");
			//alert (count[0].firstChild.nodeValue);
			var count_nodes =count[0].firstChild.nodeValue;
			showdata.innerHTML = "<div class=\"total_comment\">comment("+ count_nodes +")</div>";
			var page =Math.ceil(count_nodes/10);
			showdata.innerHTML += "Pages :";
			for (i=1; i<= page;i++)
			{
			showdata.innerHTML += "<a href=\"javascript:\" onclick=\"show_comment("+ i +");\" style=\"text-decoration:none\"  >["+ i + "]&nbsp</a>";
			}  	
		
			for (i = 0; i < n_list; i++) {
				var name = list_nodes[i].getAttribute("name");
				var comment = list_nodes[i].getAttribute("comment");
				var tanggal  = list_nodes[i].getAttribute("tanggal");
				
				showdata.innerHTML += "<div class=list>"; 
				showdata.innerHTML += "<div class=tanggal>" + tanggal + "</div>";
				showdata.innerHTML += "<div class=name>" + name + "</div>";
				showdata.innerHTML += "<div class=comment>" + comment + "</div>";
				//alert (comment[0].firstChild.nodeValue);
				showdata.innerHTML += "</div>"; 
		    }
			showdata.innerHTML += "<br><br>";
		}	
    }
} 	
</script>

<style>
.list {clear:both;margin-top:6px;padding-top:7px;padding-bottom:4px;border-top:1px solid #efefef}
.tanggal {font-size:12px; color:#FF0000;}
.name    {font-size:16px; color:#0000CC;}
.comment {font-size:12px; color:#000000;}
.total_comment {font-size:12px; color:#0000CC;}
</style>


