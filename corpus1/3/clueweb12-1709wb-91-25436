  var timeplot;

  function createTimeplot()
  {
    var eventSource = new Timeplot.DefaultEventSource();
    var timeGeometry = new Timeplot.DefaultTimeGeometry({
      gridColor: new Timeplot.Color("#000000"),
      axisLabelsPlacement: "top"

    });

    var valueGeometry = new Timeplot.DefaultValueGeometry({
      gridColor: "#000000",
      min: 0
    });

    var datasource = "/newsearch/timeplot/549e0abd0912d70e1d9a787199ca3ced";
    var plotInfo = [ Timeplot.createPlotInfo ({
                id: "basic_search_results",
                dataSource: new Timeplot.ColumnSource(eventSource,1),
                valueGeometry: valueGeometry,
                timeGeometry: timeGeometry,
                lineColor: "#1f6f94",
                dotColor: "#000000",
                fillColor: "#1f6f94",
                fillGradient: false,
                showValues: true}) ];

    timeplot = Timeplot.create(document.getElementById("timeplot"), plotInfo);
    timeplot.loadText(datasource, ",", eventSource);
  }

  </script>
