    $(document).ready(function() {
      initEmptyField('askQuestionTopic1', 'Start a new Ford Edge question.');
    });

    function submitAskAutoClubQuestion1() {
      var redirectUrl = '/Cars/autoclub/members/newTopic.action?startUrl=%2FCars%2Fautoclub%2Fforum-ford-edge-ac846&autoClub=846&selectedEntity=d923&topic=';
      checkEmptyField('askQuestionTopic1', 'Start a new Ford Edge question.');
      if ($('#askQuestionTopic1').val() != 'Start a new Ford Edge question.') {
        redirectUrl += encodeURIComponent($('#askQuestionTopic1').val());
      };

      document.location.href = redirectUrl;
    }
  </script>


