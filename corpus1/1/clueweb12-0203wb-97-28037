		document.write('<script type="text/javascript" src="http://ad.doubleclick.net/adj/macon.living/health;c2=health;c3=health_homepage;!category=health;template=article;pos=center6;group=rectangle;sz=300x250;tile='+_krdDartInc+';ord='+_krdDartOrd+'?"><\/script>');
    }
}
</script>

