		fo.addVariable("showExtrasXmlSite", "3news"); //site to get XML from
		fo.addVariable("showExtrasXmlCat", ""); //default video category for extras
		
		if((geo=="yes")||(geo=="geo")){
		fo.addVariable("midRolls", ""); //mid roll points hh:mm:ss
		fo.addVariable("midRollLimit", "60"); //mid roll points hh:mm:ss
		fo.addVariable("postRolls", ""); //mid roll points hh:mm:ss
		}
		else{
		fo.addVariable("midRolls", ""); //mid roll points hh:mm:ss
		fo.addVariable("postRolls", ""); //mid roll points hh:mm:ss
		
		}
		
		fo.addVariable("geo", geo); 
		//is the clip geo blocked 
		fo.addVariable("fullstoryVar", "http://www.3news.co.nz/Dog-race-caller-faces-biggest-challenge-of-his-career/tabid/817/articleID/245897/Default.aspx"); //for bookmarking 
		fo.addVariable("sevenHundred", "no"); //cookies video speed 
		
		var videoReplace = video;
		var intIndexOfMatch = videoReplace.indexOf( "*" );
		 // Loop over the string value replacing out each matching
		 // substring.
		 while (intIndexOfMatch != -1){
		 // Relace out the current instance.
		 videoReplace = videoReplace.replace( "*", "/" )
		// Get the index of any next matching substring.
		intIndexOfMatch = videoReplace.indexOf( "*" );
		 }
		 
		fo.addVariable("flv", videoReplace); //location of video clip 
		fo.addVariable("mp3", ""); //audio file
		fo.addVariable("midroll", ""); //midrolls
		
		fo.addVariable("sloc", sloc); 
		fo.addVariable("flashSect", flashSect); //section eg video browse all 
		fo.addVariable("flashGenre", flashGenre); //genre eg comedy 
		fo.addVariable("flashShow", flashShow); // eg outrageous fortune 
		fo.addVariable("flashCG", flashCG); //full title path eg TV3-Video-BrowseAll-TV3Video-OutrageousFortune0415 
		fo.addVariable("flashSI", flashSI); //stream channel 
		 var ou="http://www.3news.co.nz/video/"+genre+"";
		fo.addVariable("ownershipURL", ou); //stream channel 
		fo.addVariable("channelName", "3news"); //section eg video browse all 
		fo.addVariable("flashLOC", location.href); //current URL 
		fo.addVariable("flashTL", flashTL); //title of media 
		fo.addVariable("flashAID", flashAID); //the ID of the article
		fo.addVariable('width', '640'); 
		fo.addVariable('height', '410'); 
		
		//start date of article - used to determine h264/flv
		fo.addVariable("dateVarDay", "09"); 
		fo.addVariable("dateVarMonth", "03"); 
		fo.addVariable("dateVarYear", "2012"); 
		var  dateVarYear ="2012";
		
		videoDate = new Date("2012/03/09");//get current video date
		videoTime=videoDate.getTime();//convert it to time
		
		nowDate = new Date("2011/07/18");//time 700k started getting encoded
		nowTime = nowDate.getTime();//convert to time
		
		
		if (videoTime > nowTime) {
			fo.addVariable("sevenHundred", "yes"); 
			if(flashSpeed==""){
				flashSpeed = "4";//default speed to 700k if not set yet
			}
		}
		else{
			if(flashSpeed==""){
				flashSpeed = "1";//default speed to 330k if not set yet
			}
		}
		
		fo.addVariable("flashSpeed", flashSpeed); //cookies video speed 
		//removes 56 K option
		fo.addVariable("highEnd", "false"); 
		
		fo.addVariable("fifteenHundred", "no"); 
		fo.addVariable("pausedAtStart", "no"); 
		
		//start date of h264
		fo.addVariable("h264dateVarDay", "25"); 
		fo.addVariable("h264dateVarMonth", "07"); 
		fo.addVariable("h264dateVarYear", "2010"); 
		
		
		//Archive switch over find out if videos start date is before h264 date
		
		fo.addVariable("useArchiveServer", "yes"); 
		fo.addVariable("dateVarDayArc", "01"); 
		fo.addVariable("dateVarMonthArc", "01"); 
		fo.addVariable("dateVarYearArc", "2011"); 
		
		
		//force next video at end of clip
		fo.addVariable("continuousPlay", "true"); 
		//fo.addVariable("picVar", ""); //thumbnail location for favourites 
		//fo.addVariable("dateVar", "Fri, 09 Mar 2012 16:01:00 +1200;");
		if((video=="")||( parseFloat(dateVarYear) < parseFloat("2008") )){
		var novid = '<p class="novid" >Sorry - this video doesn\'t seem to be available - <a href="/Video/VideoAll.aspx"><u>click here</u> to watch more video</a></p>';
		
		document.getElementById('flashPlayerDisp').innerHTML=novid; 
		}
		else{
		fo.write("flashPlayerDisp");
		
		} 
			}
						else {  // flash is too old or we can't detect the plugin
							var alternateContent = 'Oops! Problem with playback. '
							+ 'This content requires the Adobe Flash Player('+requiredMajorVersion+' '+requiredRevision+')<br/>'
							+ '<a href=http://www.macromedia.com/go/getflash/>Get Flash &raquo;</a>';
							
							document.getElementById('probs').innerHTML=alternateContent;
							document.getElementById('probs').className='probs';
							//document.write(alternateContent);  // insert non-flash content
						}}

	}					

			
	function next_vid(){
	recentVideo = new String(getPlayerSpeed("recentVideo"));
	//if(recentVideo.indexOf('41')==-1){
	window.location=next_video_url_v2;
	//}
	}
	setNextVideo();	
	 
		/* ]]> */
	 </script>
