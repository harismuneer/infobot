					function trim(str)
					{
					    if(!str || typeof str != 'string')
					        return null;

					    return str.replace(/^[\s]+/,'').replace(/[\s]+$/,'').replace(/[\s]{2,}/,' ');
					}
					function dfnew(sd,msid)
					{
						try{											
							var datm =sd;
							var datmfm=datm.split(',');
							var datmfm1=datmfm[1].substr(5);
							if(sd=="" || sd == null)return;						
							sd = new Date(sd);
							if(cd.getTime() < sd.getTime())return;
							ss = document.getElementById("dtfrmt"+msid);	
							var chk=0;						
							var diff = Math.abs(cd.getTime() - sd.getTime());			
							diff = ((diff/1000)/60);
							chk= diff%60;								
							if(diff<=1440)
							{
								if(diff < 60)
								{
									if(diff < 2)
										{
										ss.innerHTML=""+diff + " min ago ("+ trim(datmfm1) +")";
										}
									else 
									{
										ss.innerHTML=""+diff + " mins ago ("+ trim(datmfm1) +")";
									}
								}
								else if(diff > 60 && diff < 120 )
								{
								ss.innerHTML=""+Math.floor(diff/60) + " hr ago ("+ trim(datmfm1) +")";
								}				
								else
								{
									if(diff > 120 && diff < 1440)
									{
										if(chk < 25)
										{				
											ss.innerHTML=""+Math.floor(diff/60) + " hrs ago ("+ trim(datmfm1) +")";
										}
										else
										{				
											ss.innerHTML=""+Math.floor(diff/60) + " hrs ago ("+ trim(datmfm1) +")";
										}
									}
									else 
									{
										if(chk < 25)
										{				
											ss.innerHTML=""+Math.floor(diff/60) + " hrs ago ("+ trim(datmfm1) +")";
										}
										else 
										{		
											ss.innerHTML=""+Math.floor(diff/60) + " hrs ago ("+ trim(datmfm1) +")";
										}
									}
								}	
						}
						else 
						{
							if(diff > 1440)
							{							
								if(chk > 24)
								{							
								ss.innerHTML=datm;
								}
								else
								{							
								ss.innerHTML=datm;
								}
							}
						}

						}
					
					catch(ex){}
				
					}
					
