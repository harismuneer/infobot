  <script type="text/javascript">
    function loadCommentInfo(json) {
        if (json.loginStatus.loggedIn) {
           $('input[id=city1]').attr(
           {value: json.loginStatus.city });
           $('input[id=name1]').attr(
           {value: json.loginStatus.userName });
           $('input[id=email1]').attr(
           {value: json.loginStatus.email });
           $('[id=country1]').attr(
           {value: json.loginStatus.country });
		   
		   $('input[id=city2]').attr(
           {value: json.loginStatus.city });
           $('input[id=name2]').attr(
           {value: json.loginStatus.userName });
           $('input[id=email2]').attr(
           {value: json.loginStatus.email });
           $('[id=country2]').attr(
           {value: json.loginStatus.country });
        } 
    }

    $(document).ready(function() {
        $.getJSON('/ajaxservlet?action=loginStatus', loadCommentInfo);
    });
