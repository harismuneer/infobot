			$(document).ready(function () {
				$('input:button[name=submit_et]').click(function () {
					var $this = $(this);
					var lid = $this.siblings('input:hidden[name=lid]').val();

					$.ajax({
						url: 'http://www.sacbee.com/cgi-bin/php/et/et_handler.php',
						dataType: 'json',
						data: {
							lid: '1118676',
							MID: '52372',
							thx: 'http://www.sacbee.com/cgi-bin/php/et/et_handler.php?wc_result=thx',
							err: 'http://www.sacbee.com/cgi-bin/php/et/et_handler.php?wc_result=err',
							SubAction: 'sub',
							'Email Address': $('input:text[name=email]').val()
						},
						success: function (result) {
							var resultPane = result.status;
							if (result.status === 'error' && result.errorcode === 8) {
								resultPane = "resub";
							}

							$this.siblings('.etresult.' + resultPane).animate({
								left: 0
							}, 500);
						}
					});
				});

				$('.tryagain').click(function () {
					$(this).parent().animate({
						left: 660
					}, 500);
					return false;
				});
			});
		</script>
