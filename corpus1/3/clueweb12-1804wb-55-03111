			var cc2Var = "";
				cc2Var = "Client Servicing";
			var cc3Var = "landingPage";
				cc3Var = "Tax Info";
				var cc4Var="2011 Distributions";
			//var usertype=getCleanCookie("FPUSERTYPE"); // Fix : EBBLKUSRS-783 [Arun B]
			var usertype=getCleanCookie("SITECORE_usertype");
			if((!usertype || 0 === usertype.length)) {
				usertype="II";
			}		
			var analyticsData = {
				"cc1" : cc1Var,
				"cc2" : cc2Var,
				"cc3" : cc3Var,
				"cc4" : cc4Var,
				"un"  : getCleanCookie("SITECORE_un"),
				"usertype"  : usertype,
				"investor_type"  : getCleanCookie("SITECORE_investor_type"),
				"status"  : getCleanCookie("SITECORE_status")
			};
			BLK.EBIZ.trace2("BLK.EBIZ.webAnalytics.getAnalyticsData. . .cc1=" + analyticsData.cc1 + 
