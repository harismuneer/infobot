<script type="text/javascript">

    function vote() 
	{
		document.getElementById('votediv').innerHTML = '&nbsp;<img width=42 height=11 src="images/transind.gif">';
	
		var http_request = getXmlHttpRequestObject();

        if (!http_request) {
            alert('Giving up :( Cannot create an XMLHTTP instance');
            return false;
        }
        http_request.onreadystatechange = function() 
		{ 
		    if (http_request.readyState != 4 || http_request.status != 200)
				return;
			

			document.getElementById('votes').innerHTML = '1';
			document.getElementById('votediv').innerHTML = '&nbsp;<i>Thank you!</i>';
		};
		
		var url = 'http://www.lyrics.net/voteartist.aspx?artist=' + encodeURIComponent('Kurupt');
		
        http_request.open('GET', url , true);
        http_request.send(null);
    }


function MyListener()
{
}

var curpage = 0;
var gpage = 0;

MyListener.prototype.handleCallback = function ()
{
	if (curpage != gpage)
	{
		curpage = gpage;
		searcher.gotoPage(curpage);
		return;
	}

    var resultsHTML = "";

	var thisResult = searcher.results[0];
	resultsHTML += "<img class=disc src=\"" + thisResult.tbUrl + "\">";

	
    resultsPanel.innerHTML = resultsHTML;
}                         

var aListener = new MyListener();
var searcher = new GimageSearch();

searcher.setRestriction(GSearch.RESTRICT_SAFESEARCH,
                        GSearch.SAFESEARCH_STRICT);

searcher.setRestriction(google.search.ImageSearch.RESTRICT_COLORIZATION,
                        google.search.ImageSearch.COLORIZATION_GRAYSCALE);
						
var options = new GsearcherOptions();
searcher.setNoHtmlGeneration();
searcher.setResultSetSize(searcher.LARGE_RESULTSET);
searcher.setSearchCompleteCallback(aListener, MyListener.prototype.handleCallback );

function performQuery(ipage)
{        
	gpage = ipage;
    searcher.execute( "Kurupt");
}

var checkedStyle = "mla";
function displayCitation(format){

	checkedStyle = format;
	previewPane = document.getElementById("preview");

	if(format == "mla")
	  previewPane.innerHTML = "\"I Will Survive.\" <u>Lyrics.net</u>. <i>STANDS4 LLC, 2012.</i>  24&nbsp;March. 2012. <a style=\"text-decoration:bold;color:#666666;\" href=\"http://www.lyrics.net/lyric/4244459\">http://www.lyrics.net/lyric/4244459</a>";
	else if(format == "chicago")
	  previewPane.innerHTML = "I Will Survive.  Lyrics.net, STANDS4 LLC, 2012. <a style=\"text-decoration:bold;color:#666666;\" href=\"http://www.lyrics.net/lyric/4244459\">http://www.lyrics.net/lyric/4244459</a>, accessed March&nbsp;24, 2012.";
	else if(format == "apa")
	  previewPane.innerHTML = "I Will Survive. (n.d.).  <i>Lyrics.net</i>. Retrieved March&nbsp;24, 2012, from Lyrics.net Web site: <a style=\"text-decoration:bold;color:#666666;\" href=\"http://www.lyrics.net/lyric/4244459\">http://www.lyrics.net/lyric/4244459</a>";
}

function translate() 
{	
	  document.getElementById('result').innerHTML = '<img width=42 height=11 src="images/transind.gif">';
	  
	  var transdd = document.getElementById('trans');
	  var tl = transdd.options[transdd.selectedIndex].value;
	
	  var newScript = document.createElement('script');
	  newScript.type = 'text/javascript';
	  var source = 'https://www.googleapis.com/language/translate/v2?key=AIzaSyDnwrsw6r1n8CqPRhP9AK8la5o6DMLMDl0&source=en&target=' + tl + '&callback=translateText&q=' + 'Technique]  <br>By dawn\'s early light  <br>I shine brighter than the stars in the night  <br>And \'bout ready for a fight  <br>Come noon, I\'ve consumed enough juice  <br>To cut loose and throw a mic to the moon  <br>Since I\'m the sun, I\'ll make you one, it\'s all for fun  <br>I\'m torching hot and all the same  <br>From playing war games with Saddam Hussein  <br>Now who\'s sane? I\'m insane, my membrane\'s on cold-grown  <br>The tempature\'s rising, to fry the cells in your dome  <br>It might be on, and if you think so, you know me  <br>It\'s Tech Caponey, the Doggfather\'s little homey  <br>[Snoop Doggy Dogg]  <br>Ain\'t that somethin\', how I got little homeys now?  <br>I remember when I was just a little bow-wow  <br>Writing for my rep, calling \'fore I step  <br>Now I put niggas in the game just to stay on deck  <br>High profile, but my style low-pro  <br>Top dogg on the Row, Doggystyle C.E.O.  <br>And I blow the best indo, that you know of  <br>So when I\'m in your town, dip down, and show love  <br>You know what? What\'s up? Shit, I\'m feeling good now  <br>You better come and get me \'fore I ';
	  newScript.src = source;
	  document.getElementsByTagName('head')[0].appendChild(newScript);
}	
function translateText(response) {
	var transdd = document.getElementById('trans');
	var tl = transdd.options[transdd.selectedIndex].value;
	document.getElementById('result').innerHTML = '<span style="background-color:#ffffd7;"><i>' + response.data.translations[0].translatedText + '</i> &nbsp;<img width=16 height=11 src=images/' + tl + '_flag.jpg></span>';
}
