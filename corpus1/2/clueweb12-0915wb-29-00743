<script>
		    var showSubject = "false";
		var keyInfo;
		keyInfo = "Afghanistan Country Flag";
							var subjectsArray = [
				'Inquiry from @@keywords@@'
				];
			var len = subjectsArray.length;
			var random = Math.floor(Math.random()*(len-1));
			var regEx = /@@keywords@@/g;
			var subjectValue = subjectsArray[random].replace(regEx,keyInfo);
			 
var fastFbUrl = "http://message.alibaba.com/msgsend/fast_feedback_inquiry.htm?subject="+encodeURIComponent(subjectValue)+"&id=215423732&domain=1&kw="+encodeURIComponent(encodeURIComponent('Afghanistan Country Flag'))+"&rm=8pctgRBMALOKabaHnF9ghsVmqqPI6Iyv&rn=Ms. Penny Chen&cn=fastFeedbackContent&s_cn=true&s_st="+showSubject+"&s_em=showTradeAlert";
YAHOO.util.Get.script(fastFbUrl, {charset: 'utf-8'});
//显示tradealert的回调方法
showTradeAlert=function(div , result){
  // TA订阅子模块
        var taConfig = {
            keyword: 'Afghanistan Country Flag',
            container:div.id,
        };
         
        var taSubscribe = AE.run.taSubscribe.getInc( taConfig );
         
        // 订阅获取状态事件
        taSubscribe.on( 'onChangeSubscribeStatus', function( e, args ){
            var s = args[0].status;
            if( this.needSubscribe( s ) ){
                this.render();
            }
        } );
         
        // 订阅成功之后操作
        taSubscribe.on( 'onSubscribed', function( e, args ){
            var result = args[0].status;
        } );
        
       //取这个帐号的订阅状态
       taSubscribe.getSubscribeStatus("nextagehk");
}
</script>
