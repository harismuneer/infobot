		function deferredLoad() {
			if (!jsLoaded) {
				jsLoaded = true;
				
				fmr.rct.util.removeEvt(document, "mousemove", deferredLoad);
				fmr.rct.util.removeEvt(document, "mouswheel", deferredLoad);
				fmr.rct.util.removeEvt(document, "keyup", deferredLoad);
				
				var scripts = [
					"https://www.fidelity.com:443/rct/1.1.1/dojo/dojo/dojo.xd.js",
					"https://www.fidelity.com:443/rct/1.1.1/rct-ui/rct-ui.js",
					"https://scs.fidelity.com:443/common/always-cached/embeddedquotes/1.0/js/embeddedquotes.js.shtml",
					"/pf/common/js/newsandinsight/newsandinsight_measurement.js"
				] ;
							
				fmr.rct.util.UL.exec( scripts, function() {			
					try {
						if(typeof postDeferredLoad == 'function')
						{
							postDeferredLoad();
						}
						//if disable links are present call it otherwise not
						if(typeof initDisableLinks == 'function')
						{
							initDisableLinks();
						}
					} catch (e) {}
					
					try {
						fmr.rct.Container.getInstance().addProvider(new fmr.rct.Provider("eventing"));
						fmr.rct.Container.getInstance().getProvider("eventing").addServiceProvider(new fmr.rct.eventing.DojoEventingImpl());
						fmr.rct.Eventing.getInstance("init").setDefaultProvider("dojo");		
						fmr.rct.Eventing.getInstance().subscribe("onFmrAttachEnd", window, "addMeasurementForMSLinks");
					} catch (e) {}
				});
			}
		}	
		fmr.rct.util.addEvt(document, "mousemove", deferredLoad);
		fmr.rct.util.addEvt(document, "mousewheel", deferredLoad);
		fmr.rct.util.addEvt(document, "keyup", deferredLoad);
		
