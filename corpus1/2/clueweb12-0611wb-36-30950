        var today = new Date();
        var choiceInstance = new choices();
        var Banner = choiceInstance[(today.getSeconds() % choiceInstance[0]) + 1];
        return Banner;
}        
        
function checkNavigator() {
                if (navigator.appVersion.charAt(0) < "2")
                        return;
                else {
                        grabBanner();
                        }
                }
