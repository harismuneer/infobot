	$('.create-account').click(function() {
		$("#dialog").dialog({
			bgiframe: true,
			autoOpen: false,
			height: 'auto',
			width: 660,
			modal: false,
			position: ['center',90],
			close: function(event, ui) { dialogContent.html(''); }
		});
		
		var dialog			= $('#dialog');
		var dialogContent	= $('#dialog-content');
		/*$('#ad-header-728').hide();*/
		$('.widget').remove();
		dialog.dialog('open');
		dialog.dialog('option', 'title', 'Sign Up');
		dialogContent.fadeIn('slow');
		$.post('/ajax/dialog.php', {type:75278 }, function(callback){
			dialogContent.html(callback);
		});
		return false;
	});
	// login
	$('.global-login').click(function() {
		var offset			= $(this).offset();
		var elementWidth 	= $(this).outerWidth();
		var left			= offset.left + elementWidth + 30;
		var top				= offset.top - 2;
		/*$('#ad-header-728').hide();*/
		$('#footerPlaceholder').after('<div style="position:absolute; display:block; z-index:1000; top:'+top+'px; left:'+left+'px;"><div class="widget login border-round"><div class="pointer up right"></div><div class="close hand"></div><div class="content"><form id="login" class="embedLogin">E-mail address:<br /><input class="pretty" type="text" name="emailLogin" /><br />Password:<br /><input class="pretty" type="password" name="passwordLogin" /><br />Remember me <input type="checkbox" name="rememberLogin" /><br /><br /><div id="globalLoginSubmit" class="small hand button green">Submit</div></form></div></div></div>');
		$('.widget.login .close').click(function(){ $('.widget.login').remove() });
		// submit login
		$('#globalLoginSubmit').live('click', function(){
			//$(this).after(loading).addClass('black').removeClass('green');
			$(this).after(loading).attr('disabled', '').removeClass('black').addClass('green').removeClass('disabled');
			embedLogin();
		});
		return false;
	});
	$('#nav-search input[type=text]').inputDesc({ description:'search' });	
	$('#globalsearch').focus(function(){
		//$('#screenOverlay').setOverlay({ status:1 });
		if ($('#global-searchresults').length == 0) {
		 	$('#footerPlaceholder').append('<div class="widget livesearch  border-round"><div id="global-searchresults" class="widget-content"><p>Start typing to search</p></div></div>');
			// hide when clicked outside area
		 	$('body').click(function() { $('.livesearch').remove(); });
			$('#globalsearch, .livesearch').click(function(event){ event.stopPropagation(); });	
		}
	});

	$('#globalsearch').keyup(function(event){
		var keyID	= event.which;
		if (keyID != 13) {
			if (keyID != 40 && keyID != 38) {
				var searchQuery	= $(this).val();
				var length 		= searchQuery.length;	
				if (length > 1) {
					if (!searchQuery) {
						$('#nav-search-loading').addClass('hidden');
						$('#searchresults').fadeOut();
					} else {
						$('#nav-search-loading').removeClass('hidden');
						getSearchResults(searchQuery, 'globalsearch');
					}
				}	
			}
		}
	});
	$('.poptip').poptip({ 'delay':0, 'fadeIn':0, 'fadeOut':0 });
	$('.poptip-l').poptip({ 'delay':0, 'maxWidth':'350px', 'fadeIn':0, 'fadeOut':0 });
	
	});

