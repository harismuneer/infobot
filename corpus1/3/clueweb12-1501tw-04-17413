				var real_document_write = document.write;
				var addthis_data = '<a class="addthis_button" href="http://addthis.com/bookmark.php?v=250&amp;pub=xa-4ac67d4a3f948267"><img src="http://s7.addthis.com/static/btn/v2/lg-share-en.gif" width="125" height="16" alt="Bookmark and Share" style="border:0"/></a>';
				document.write = function(s) {
					addthis_data += s;
				}
			</script>
