    </div>

			<script type="text/javascript">
				function enabledSubmit(){
					document.getElementById('submitComment').disabled=false;
				              if(document.getElementById('kaptcha_answer')){
                                                var kaptcha_value=(document.getElementById('kaptcha_answer')).value;
                                                if(kaptcha_value==""){
                                                   alert("Enter the characters in the image above:");
                                                   document.getElementById('submitComment').disabled=true;
                                                   return false;
                                                   }
                                        }
				}
				function checkComment(){
				        if(document.getElementById('body')){
                                         var comment_value=(document.getElementById('body')).value;
            if(!document.getElementById('accept').checked)
		alert("Please check the \"I am not posting spam\" box");
                                         if( comment_value == ""){
                                                alert("Please enter a comment.");
                                                document.getElementById('submitComment').disabled=true;
                                                return false;
                                          }
                                        }
				}
				function checkCaptcha(){
                                              if(document.getElementById('kaptcha_answer')){
                                               var kaptchaEntry=(document.getElementById('kaptcha_answer')).value;
                                                if(kaptchaEntry==""){
                                                   alert("Enter the characters in the image above:");
                                                   document.getElementById('submitComment').disabled=true;
                                                   return false;
                                                   }
					}
				}
                                function enableButton(){
                                        document.getElementById('submitComment').disabled=false;
                                }

				function getnewurl()
				{

					var formurl=(document.getElementsByName('formURL'))[0].value;
					if(formurl.indexOf("?")!=-1){
						formurl=formurl.substring(0,formurl.indexOf("?"));
					}
					(document.getElementsByName('formURL'))[0].value=formurl;
					(document.getElementsByName('formID'))[0].value="comments";
				}
				function getlogout()
				{
					getnewurl();
					window.location= "/jsp/logout.jsp?returnTo="+(document.getElementsByName('formURL'))[0].value;
				}
			</script>

		<input type="submit" class="buttonComment" id="submitComment" name="submitComment" value="Submit" onfocus="javascript:checkComment();checkCaptcha();" onclick="javascript:getnewurl();"/>
                        </fieldset>
                        <a href="javascript:getlogout();">log out</a>
                    </form>


		</div>

                <p class="error"></p>
        
    
</div>
</div>
		<script type="text/javascript">
		     var curUrl=document.URL;
		function refresh(){
		     if(curUrl.indexOf("&http")!=-1){
			curUrl=curUrl.substring(curUrl.indexOf("&http"));
			curUrl=curUrl.substring(curUrl.indexOf("&http")+1,curUrl.indexOf("="));
			curUrl=curUrl+"?formURL=refresh";
				setTimeout("window.location=curUrl",1000);
		//	javascript:location.reload(true);
		  }
		}
		</script>






   	



    



    

 
 



<script type="text/javascript">
		  javascript:refresh();
</script>
                       <!-- <a href="javascript:window.location.reload();">refresh</a> -->




