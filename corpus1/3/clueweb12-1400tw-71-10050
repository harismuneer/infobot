		<script type='text/javascript'>

		function toggle(obj) {
			var el = document.getElementById(obj);
			if ( el.style.display != 'none' ) {
				el.style.display = 'none';
			}
			else {
				el.style.display = '';
			}
		}

		
			function checkBoxes()
			{
				var items = document.getElementsByName('topicMoveDelete[]');
				var list = '';
				var firstElement = 0;
				

				
				for(var i=0; i< items.length; i++)
				{
					if(items[i].checked)
					{
						if(firstElement==0)
							list += items[i].value;
						else
							list += ';'+items[i].value;
						firstElement++;
					}
				}
				
				if(list.length < 1){
					alert("Please select one or more topics.");
					return false;
				}
				
				document.getElementById('checkedBoxes').value = list;
				return true;
			}

			function setTopicFormAction(action){
				document.topicAction.action = action;
			}

			function setButtonIMG(image,id) {
				document.getElementById(id).src = image;
			}
			
			//----------------------------------------------------------------------------------
			//		Comment menu JS
			//----------------------------------------------------------------------------------
			var hideMenu = new Array();
			function findCommentOptionMenuItem(id) {
				var i;
				for (i=0; i<hideMenu.length; i++) {
					if (hideMenu[i][0] == id) {
						return i;
					}
				}
				hideMenu.push(new Array(id,'0'));
				return hideMenu.length-1;
			}
			function showCommentOptionMenu(id, btn) {
				hideMenu[findCommentOptionMenuItem(id)][1] = '0';
			}
			function hideCommentOptionMenu(id) {
				if (document.getElementById(id)) {
					hideMenu[findCommentOptionMenuItem(id)][1] = '1';
					var t=setTimeout("closeCommentOptionMenu('"+id+"')",333);
				}
			}
			function closeCommentOptionMenu(id) {
				if (hideMenu[findCommentOptionMenuItem(id)][1] == '1') {
					document.getElementById(id).style.display='none';
				}
				hideMenu[findCommentOptionMenuItem(id)][1] = '0';
			}
			function toggleCommentOptionMenu(id, btn)
			{
				var commentMenu = document.getElementById(id);
				commentMenu.style.position='absolute';
				if (commentMenu.style.display == 'none')
				{
					commentMenu.style.left = '-9999px';
					
					commentMenu.style.display = 'block';

					var f = parseInt(YAHOO.util.Dom.getStyle(id, 'font-size'), 10);
					if (f<12) f=12;
					var width = f * 10;
					commentMenu.style.width = width+'px';
					
					var menuR = YAHOO.util.Region.getRegion(commentMenu);
					var btnR = YAHOO.util.Region.getRegion(btn);
					var top = btnR.height + 5;
					var left = btnR.width - menuR.width;

					commentMenu.style.top = top+'px';
					commentMenu.style.left = left+'px';
				} else {
					commentMenu.style.display = 'none';
				}
			}

			//----------------------------------------------------------------------------------
			//		Tag UI JS
			//----------------------------------------------------------------------------------
			var hideTagMenu = new Array();
			function findTagOptionMenuItem(id) {
				var i;
				for (i=0; i<hideTagMenu.length; i++) {
					if (hideTagMenu[i][0] == id) {
						return i;
					}
				}
				hideTagMenu.push(new Array(id,'0','0'));
				return hideTagMenu.length-1;
			}
			function showTagOptionMenu(id) {
				hideTagMenu[findTagOptionMenuItem(id)][1] = '0';
			}
			function hideTagOptionMenu(id) {
				if (document.getElementById(id)) {
					hideTagMenu[findTagOptionMenuItem(id)][1] = '1';
					var t=setTimeout("closeTagOptionMenu('"+id+"')",333);
				}
			}
			function closeTagOptionMenu(id) {
				index = findTagOptionMenuItem(id);
				//console.log(hideTagMenu[index][0]+" "+hideTagMenu[index][1]+" "+hideTagMenu[index][2]);
				if (hideTagMenu[index][1] == '1' && hideTagMenu[index][2] != '1') {
					document.getElementById(id).style.display='none';
					hideTagMenu[findTagOptionMenuItem(id)][1] = '0';
				}
			}
			function toggleTagOptionMenu(id, tagMenuButton, tagMenuInput) {
				var tagMenu = document.getElementById(id);
				if (tagMenu.style.display == 'none') {
					var tagMenuButtonRegion = YAHOO.util.Region.getRegion(tagMenuButton);
					var x = tagMenuButtonRegion.right;
					var y = tagMenuButtonRegion.top;
					y -= tagMenuButtonRegion.height - 8;

					tagMenu.style.display = 'block'; // without this 'w' will be NaN

					var tagMenuRegion = YAHOO.util.Region.getRegion(tagMenu);
					var w = tagMenuRegion.width;
					var h = tagMenuRegion.height;
					x += 4;

					rx = x + w;
					if (rx > DOM.getDocumentWidth()) {
						x -= w;
						y = tagMenuButtonRegion.top + tagMenuButtonRegion.height + 4;
					}
						
					by = y + h;
					if (by > DOM.getDocumentHeight()) {
						y = tagMenuButtonRegion.top - h - 4;
					}
						
					tagMenu.style.left = x.toString()+'px';
					tagMenu.style.top = y.toString()+'px';

					document.getElementById(tagMenuInput).focus();
				} else {
					tagMenu.style.display = 'none';
				}
			}
			function justRegisterRightClickForTagOptionMenu(id) {
				hideTagMenu[findTagOptionMenuItem(id)][2] = '1';
			}
			function registerRightClickForTagOptionMenu(id) {
				hideTagMenu[findTagOptionMenuItem(id)][2] = '1';
				var t=setTimeout("justClearRightClickForTagOptionMenu('"+id+"')",3000);
			}
			function justClearRightClickForTagOptionMenu(id) {
				hideTagMenu[findTagOptionMenuItem(id)][2] = '0';
			}
			function clearRightClickForTagOptionMenu(id) {
				hideTagMenu[findTagOptionMenuItem(id)][2] = '0';
				closeTagOptionMenu(id);
			}
		</script>	
					<script type="text/javascript">
				customPos = -1;
				
				function hilite(e, c) {
					if (e && c)
						e.className=c;
				}
				
				function lolite(e, c) {
					if (e && c)
						e.className=c;
				}
			</script>
	
					<script type="text/javascript" src="/js.ln/bb_edit.js"></script>
					<script type="text/javascript">
				function recordTopicTag(aBID, topicID, abKey, image) {
					tag = document.getElementById("tagInput"+topicID).value;
					document.getElementById("tagInput"+topicID).value = "";
					document.getElementById("tagInputAddButton"+topicID).src = image;
					if (tag !== null && tag !== undefined) {
						var callback = {
							argument : [topicID],
							success : handleTagSuccess
						};
                        YAHOO.util.Connect.initHeader('Content-Type', 'text/html; charset=UTF-8');
						var postData = "aBID="+aBID+"&topicID="+topicID+"&tag="+encodeURI(tag).replace(/\+/g, "%2B")+"&abKey="+abKey+"&action=83";
						var cObj = YAHOO.util.Connect.asyncRequest('POST', '/tagit.spark', callback, postData);
					}
				}

				function removeTopicTag(tagID, aBID, topicID, abKey, image) {
					if (true) {
						document.getElementById("tag"+tagID).src = image;
						if (tagID !== null && tagID !== undefined) {
							var callback = {
								argument : [topicID],
								success : handleTagSuccess
							};
                            YAHOO.util.Connect.initHeader('Content-Type', 'text/html; charset=UTF-8');
							var postData = "aBID="+aBID+"&topicID="+topicID+"&tagID="+tagID+"&abKey="+abKey+"&action=70"; 
							var cObj = YAHOO.util.Connect.asyncRequest('POST', '/tagit.spark', callback, postData);
						}
					}
				}
				
				var handleTagSuccess = function(o) {
					document.getElementById("tagUI"+o.argument[0]).innerHTML = o.responseText; 
					toggleTagOptionMenu("tagsMenu"+o.argument[0], document.getElementById("tagsMenuButton"+o.argument[0]), "tagInput"+o.argument[0]);
				};
			</script>
				
		
