<script type="text/javascript"> 
function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}
var dfp_rsi_segs = '';
var rsi_segs_all = readCookie('rsi_segs');
if(rsi_segs_all) {
	rsi_segs_array = rsi_segs_all.split('|');
	for(var i=0;i < rsi_segs_array.length;i++) {
		rsi_segs_array_temp = rsi_segs_array[i].split('_');
		if(rsi_segs_array_temp[1]) {
			dfp_rsi_segs = dfp_rsi_segs + "rs=" + rsi_segs_array_temp[1] + ";";
		}
	}
}
</script>
