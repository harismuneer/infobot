this[10] = "<a href=\"../catalog/scindex.htm\"><img alt=\"AD (4)\" border=\"0\" height=\"160\" width=\"120\" src=\"ads/ad4.jpg\"></a><p><center><a href=\"../catalog/gaorder.htm\"><img alt=\"AD (5)\" border=\"0\" height=\"160\" width=\"120\" src=\"ads/ad5.jpg\"></a>";

}

function grabBanner(){
        var today = new Date();
        var choiceInstance = new choices();
        var Banner = choiceInstance[(today.getSeconds() % choiceInstance[0]) + 1];
        return Banner;
}        
        
function checkNavigator() {
                if (navigator.appVersion.charAt(0) < "2")
                        return;
                else {
                        grabBanner();
                        }
                }
checkNavigator();
// End Hiding -->
</script>
