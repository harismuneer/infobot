<script type="text/javascript" language="javascript">

function lyricsalign(align)
{	
	var ids=align.id;

	if(ids == "leftalign")
	{
		document.getElementById("lyrics_body").style.textAlign="left";
	}
	if(ids == "centeralign")
	{
		document.getElementById("lyrics_body").style.textAlign="center";
	}
	if(ids == "rightalign")
	{
		document.getElementById("lyrics_body").style.textAlign="right";
	}
}
/********************* Report Ajax Call **************************/
 
	var xmlhttp;
	function GetXmlHttpObject()
	{
		if (window.XMLHttpRequest)
	    {
	    	// code for IE7+, Firefox, Chrome, Opera, Safari
	        return new XMLHttpRequest();
	    }
	    if (window.ActiveXObject)
	    {
	    	// code for IE6, IE5
	        return new ActiveXObject("Microsoft.XMLHTTP");
	    }
	    return null;
	}

	function checkReport()
	{							
		var report_body = document.getElementById("report_body").value;
				
		if(report_body)
		{
			reportAjax(report_body);
			return false;
		}
		else
		{
			alert("Report Body Can not be Blank");
			return false;
		}
	}	
	
	function reportAjax(report)
	{
		xmlhttp=GetXmlHttpObject();
	    if (xmlhttp==null)
	    {
	    	alert ("Browser does not support HTTP Request");
	        return;
	    }
	    var url= "/lyrics/reportajax/lid/58002";
	    url=url+"?report="+report; 
	   
	    //url=url+"&sid="+Math.random();
	    xmlhttp.onreadystatechange=function()
	    {
	    	if (xmlhttp.readyState==4)
		    {  		
			    var response = eval('(' + xmlhttp.responseText + ')');
			   	document.getElementById("report_body-label").innerHTML= response["response"];
		    }
		}; 
	    xmlhttp.open("GET",url,true);
	    xmlhttp.send(null);
	 }
 </script>
</head>
