    <script type="text/javascript">
        //<![CDATA[
        (function() {
		        var links = document.getElementsByTagName('a');
		        var query = '?';
		        for(var i = 0; i < links.length; i++) {
			        var dsqid = links[i].getAttribute('dsqid');
                    if(dsqid) {
                        query += 'dsqid' + i + '=' + dsqid + '&';
                    }

		        }
		        document.write('<script charset="utf-8" type="text/javascript" src="http://www.disqus.com/forums/lifescript/get_num_replies_from_dsqid.js' + query + '"></' + 'script>');
	        })();
        //]]>
