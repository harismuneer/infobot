        function goto_page(url) {
            new Effect.Parallel([
                new Effect.Opacity("volatileResults",{from:1.0,to:0}),
                new Effect.Opacity("volatileResultsLoading",{from:0,to:1.0})],
                {afterFinish: function(effect) {
            	mytt_gaTrack(url);
                var myAjax = new Ajax.Request(url, {method: "get", onComplete: done_goto_page});
                }});
        }
        function done_goto_page(originalRequest) {
            new Effect.Opacity("volatileResults",{from:0,to:1.0});
            new Effect.Opacity("volatileResultsLoading",{from:1.0,to:0});
            $("volatileResults").innerHTML = originalRequest.responseText;
        }
