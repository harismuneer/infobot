  <script type="text/javascript" charset="utf-8">

    $(function(){
      $("#dealfinderSearchOptions").buttonset();

      function showStartingSearchOption(){
        if($('input[@name="radio"]:checked', "#dealfinderSearchOptions").val() == 'car'){
          $("#vehicleSearch").show();
          $("#bodyStyleSearch").hide();
        } else if( $('input[@name="radio"]:checked', "#dealfinderSearchOptions").val() == 'bodyStyle' ){
          $("#vehicleSearch").hide();
          $("#bodyStyleSearch").show();
        }
      }

      showStartingSearchOption();
      $(".toggleSearchType").click(showStartingSearchOption);


      function resetCar2YearOnSimilar(field) {
          var dropdown = field;
          var entityIdFormField = document.getElementById('entitySelectingHelper.selectedEntity2');
          var selectedOptionIndex = dropdown.selectedIndex;
          if (entityIdFormField != null && dropdown.options[selectedOptionIndex].value == "Similar") {
              entityIdFormField.value = null;
          }
      }

    });
  </script>
