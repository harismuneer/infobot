<script type="text/javascript">

    function show_hide(id1) {
      if(document.getElementById(id1).style.display=='none'){
            document.getElementById(id1).style.display='block';
      } else {
            document.getElementById(id1).style.display='none';
      }
    }

	function quick_find(cn)
	{
		var url = 'http://www.trailerdownload.net/ajax/ajax_code.php';
		var movie_name = $('#go_search').val();
		
		$.post(url,{action:'search_movie',movie_name: movie_name},
				function (data){
					var data_int = parseInt(data.status);
					
					if(data_int == 1)
					{
						window.location = 'http://www.trailerdownload.net/' + 'movies/Show/' + data.msg;
					}
					else
					{
						alert(data.msg);
					}
					
				},'json'
			);
		
	}
    
    function autosearch(cn)
	{
		var url = 'http://www.trailerdownload.net/ajax/ajax_search.php';	
		var q = $('#go_search').val();
        
        
       // alert(cn);
		$.post(url,{cn: cn,q: q},
				function (data){
				    
					$("#searchresultshow").css('display','block');
					$("#searchresultshow").html(data);
				});
		
	}
    function X(){
		$("#searchresultshow").html('');
        $("#searchresultshow").css('display','none');
	}
   function XX(v){
        //alert(v);    
		$("#scr"+v).html('');
 	    $("#scr"+v).css('display','none');
	}
    
</script>

