		function tabToNextElement(field) {
		  var fieldFound = false;
		  var form = field.form;
		  for (var e = 0; e < form.elements.length; e++) {
			if (fieldFound && form.elements[e].type == 'text')
			  break;
			if (field == form.elements[e])
			  fieldFound = true;
		  }
		  form.elements[e % form.elements.length].focus();
		  form.elements[e % form.elements.length].select();

		  //return form.elements[e % form.elements.length];
		}
		function findID(formname,name)
		{
			var fieldFound = false;
			var form = formname.form;
			for (var e = 0; e < form.elements.length; e++) {
				if (name == form.elements[e].name)
					return e;
			}
		}
