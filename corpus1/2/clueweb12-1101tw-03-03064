<script id="ajaxInStockScript" type="text/javascript">

    function PerformInStockFilter(value, outermin, outermax, min, max) {
        value = value.replace(/&amp;/g,"&");
        $.ajax({
            url: value,
            cache: false,
            statusCode: {
                404: function(xhr, statusText, errorThrown) {
                    AjaxReplacementFilters(xhr.responseText, outermin, outermax, min, max);
                }
            },
            success: function (data) {
                AjaxReplacementFilters(data, outermin, outermax, min, max);
                ajaxSaveState(value, data);
                var auxUrl = $('#ddlSortAux').val()
                if(auxUrl != undefined) {
                    auxUrl = auxUrl.replace(/&amp;/g,"&").replace("&ris", "").replace("?ris", "?");
                    if($('#insfchk:checked').length >0)
                        auxUrl = auxUrl+"&ris";
                    LoadAuxResults(auxUrl);
                }
            }
        });
    }

    function AjaxReplacementFilters(data, outermin, outermax, min, max) {

        ajaxUpdateListComplete(data);

        
            var aUrl = 'http://www.wiggle.co.uk/ajax/es/cycling-shoes/';
            if (outermin != undefined){
                initSlider(aUrl, new PriceSliderValues(10, min, max, outermin, outermax, 'GBP', 1));
            }else
                if($('#slider').length==1){
                    initSliderDefault(aUrl);
                    }
        

    }
</script>
<div id="propertyfilters" class="filters">
    In Stock Only
