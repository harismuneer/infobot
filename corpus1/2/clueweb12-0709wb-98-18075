				<form method="get" action="http://www.yamahamusicsoft.com/en/catalog?currency=USD" name="siteKeywordForm"  onSubmit="return validateSearch()">
					<input class="example" type="text" value="e.g. Elton John, PianoSoft" id="searchMain" name="keyword" onfocus="prepSearch(this)"/>
					<input type="submit" class="search_main_submit uppercase" value="Search" onclick="return checkSiteKeyword();"/>
				</form>
			</div>
			<div class="clear_both"></div>
		</div>
	</div>



