   function rateHi(count, id){
	var lis = $('#rateHanger_' + id +" li");
	lis.removeClass("active");

	lis.each(function(i,v){
          if(i+1 <= count){
		 $(v).addClass("active");
	  }
          //else $(v).removeClass("active");
        });
        
   }

