function CheckwSS()
{
theFrm = document.wfrmSS;
hasDot = theFrm.Email.value.indexOf(".");
hasAt = theFrm.Email.value.indexOf("@");
if (hasDot == -1 || hasAt == -1)
{
alert("Please enter a valid email address.");
theFrm.Email.focus();
theFrm.Email.select();
return false;
}
return true;
}
