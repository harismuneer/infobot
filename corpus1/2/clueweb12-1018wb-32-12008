<script type="text/javascript">
    $(document).ready(function () {
        var localPathname = location.pathname;
        var selector = "";

        if (localPathname == "/" || localPathname == "/Default.aspx") {
            selector = "#tabHome";
        }
        else if (localPathname.indexOf("/Audio") >= 0 || localPathname.indexOf("/Movie-Reviews") >= 0) {
            selector = "#tabMovieReviews";
        }
        else if (localPathname.indexOf("/Podcasts") >= 0) {
            selector = "#tabPodcasts";
        }
        else if (localPathname.indexOf("/Actor") >= 0) {
            selector = "#tabActors";
        }
        else if (localPathname.indexOf("/Store") >= 0) {
            selector = "#tabStore"
        }

        if (selector != "") {
            $(selector).addClass("selected");
        }

        // MOUSE OVER EVENT HANDLER FOR NAVIGATION
        $("ul.toolbar li").hover(
            function () {
                $("ul.rolloverMenu", this).show();
            },
            function () {
                $("ul.rolloverMenu", this).hide();
            }
        );
    });
</script>

