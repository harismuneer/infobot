elbreadcrumbtextsearch' );this.searchForm.setSearchText( text );this.setPercentMode( false );this.percentModeState.set( 'value', false );this.currentPage = 1;this.currentPageState.set( 'value', 1 );this.performSearch( 0 );});dojo.query( '.resultsstatus .breadcrumbs .breadcrumb[jazelbreadcrumbtarget]', this.node ).connect( 'click', this, function( event ) {var key = dojo.attr( event.currentTarget, 'jazelbreadcrumbtarget' );var obj = this.searchForm.getPartialCriteriaObject( criteria, key );this.searchForm.setSearchDropDowns( obj );this.setPercentMode( true );this.percentModeState.set( 'value', true );this.currentPage = 1;this.currentPageState.set( 'value', 1 );this.performSearch( 2 );});},updateNavigation: function( total ) {var resultsPerPage = this.getResultsPerPage();this.numPages = (resultsPerPage != 'all') ? (Math.floor( ( total - 1 ) / resultsPerPage ) + 1) : 1;this.currentPage = Math.min( Math.max( this.currentPage, 1 ), this.numPages );var maxLinksOnSingleSide = 2;var maxPageLinks = maxLinksOnSingleSide * 2 + 1;var firstPage = this.currentPage - maxLinksOnSingleSide;var lastPage = this.currentPage + maxLinksOnSingleSide;if( firstPage < 1 ) {firstPage = 1;lastPage = Math.min( maxPageLinks, this.numPages );} else if( lastPage > this.numPages ) {lastPage = this.numPages;firstPage = Math.max( lastPage - maxPageLinks + 1, 1 );}	var outputArray = [];if( firstPage > 1 ) {outputArray.push( this._createLinkN( 1 ) );}if( firstPage > 2 ) {outputArray.push( '. . .' );}for( var i = firstPage; i <= lastPage; i++ ) {outputArray.push( this._createLinkN( i, (i == this.currentPage) ) );}if( lastPage < this.numPages - 1 ) {outputArray.push( '. . .' );}if( lastPage < this.numPages ) {outputArray.push( this._createLinkN( this.numPages ) );}dojo.query( '.resultsnavigationbar span.page-navigation', this.node ).html( outputArray.join( '' ) );if( this.currentPage == 1 ) {dojo.query( '.resultsnavigationbar .prevPageButton img', this.node ).addClass( 'inactive' );} else {dojo.query( '.