function toggleMobileMenu() {
	var ele = document.getElementById("nav");
	var text = document.getElementById("phoneMenuButton");
	if(ele.style.display == "block") {
	ele.style.display = "none";
		//text.innerHTML = "Show Menu";
		$('#phoneMenu').removeClass("selected");
  	}
	else {
		ele.style.display = "block";
		//text.innerHTML = "Hide Menu";
		$('#phoneMenu').addClass("selected");
	}
}

$(window).resize(function(){
	var ele = document.getElementById("nav");
	if(ele.style.display == "none" && window.innerWidth > 390) {ele.style.display='block'};
})

