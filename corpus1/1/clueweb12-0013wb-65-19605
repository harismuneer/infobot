<script type="text/javascript">
function MailSubValidateSubmit_414208()
{
    
	document.getElementById('MailSubscribeError_414208').innerHTML = '';
	var inputId = 'IngeniMailSubscribeEmailInput_414208';

	var Input = document.getElementById(inputId);
	var val = '' + Input.value;
	var rx = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/ig;
	var matches = rx.exec(val);

	if (matches != null && val == matches[0]) {
		MailSubscribe.MailSubscribeClick(414208,179596);
	}
	else {
		document.getElementById('MailSubscribeError_414208').innerHTML = 'Invalid email';
	}
}

function MailSubUnsubscribe(email)
{
	var Container = document.getElementById('IngeniMailSubscribeContainer_414208');
	Container.innerHTML = "<input id='IngeniMailSubscribeEmailInput_414208' type='hidden' value='" + email + "'/>";
	MailSubscribe.MailSubscribeClick(414208,179596);
}
