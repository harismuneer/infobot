	function acesearchsubmit(){
		var moquery = document.getElementById("qr").value.length;
		
		if (moquery >= "3"  ) {
			return true;
		} 
		else {
			alert("Please, make sure that your query length is higher than 3 characters.");
			return false;
		}
	}
	
</script>

