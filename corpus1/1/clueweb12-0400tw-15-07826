    twitterWidget10119983 = new Squarespace.Social.TwitterWidget(10119983, { preset : 'basic-preset', pageSize : 3, followMeText : 'Follow @username', filter : '', filterType : 'any', filterAtReplies: true, filterRetweets : true, userIds : '14520186,18322062,' });
    twitterWidget10119983.show();      
    
  });
  
  
