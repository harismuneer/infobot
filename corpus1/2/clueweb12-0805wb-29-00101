	<script type="text/javascript">
	    //we are definining a base for the relative URLs in the XML document: this makes the swf get relative to itself, not the parent HTML document
	    var flashvars = {}
	    var params = {
	        base: ".",
	        wmode: "transparent"
	    }
	    var attributes = {}




	    // "video.swf" that the swfobject embed below refers to is the "player" only. Will need to edit the embed URL to wherever you place the file.

	    swfobject.embedSWF("video/video.swf", "video", "365", "350", "9.0.0", false, flashvars, params, attributes);
	    // arguments: url, div id, width, height, min flash version
	</script>
	
