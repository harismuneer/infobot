<script type="text/javascript">
/* <![CDATA[ */
$('#layout_stockUpdates_email').focus(function(){
	if($('#layout_stockUpdates_email').val()=='Enter Your Email Address'){
		$('#layout_stockUpdates_email').val('');
	}
});
$('#layout_stockUpdates_email').blur(function(){
	if($('#layout_stockUpdates_email').val()==''){
		$('#layout_stockUpdates_email').val('Enter Your Email Address');
	}
});
$('#stockUpdates_miniForm').submit(function(){
    if($('#layout_stockUpdates_email').val() == '' || $('#layout_stockUpdates_email').val() == 'Enter Your Email Address'){
        alert("You must enter an email address");
        return false;
    }
    else{
        return true;
    }

});
/* ]]> */
</script>
                            
                
                

        
