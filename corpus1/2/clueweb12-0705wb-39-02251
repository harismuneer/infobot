<script type="text/javascript">
     PAGESETUP.addControl(function() {
         var links = YAHOO.util.Selector.query('a.canonicalLink'); /*get all canonicalLinks*/
         for(i=0; i < links.length; i++){/*loop through links*/
             var qstringParam = 'data-qstring';
             if(links[i].getAttribute(qstringParam)){ /*verify attribute exists*/
                 links[i].href = links[i].href + links[i].getAttribute(qstringParam); /*update href with query params*/
             }
         }
     });
 </script>
