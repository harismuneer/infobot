    <div id="blogCalendar5"></div><script language="javascript" type="text/javascript">
                var calBlogDate5 = new Date(2012,02,12);
            month5 = { "month": { "day": [ {"d": "11", "id": "03112012", "q": "?y=2012&amp;m=3&amp;d=11" } ], "key": "-2PKrkrsLzmWmi!5qflALI3bSllklfAy", "month": "3", "name": "March", "q": "?y=2012&amp;m=3", "root": "http:\/\/k13.mysbrforum.com\/blog\/", "year": "2012" }};
    if (typeof(cal5) == 'undefined') {
      cal5 = new Calendar({
        holderId: 'blogCalendar5',
        startDate: calBlogDate5,
        calendarClass: 'blogCal',
        showPrevious: true,
        showNext: true,
        autostart: false,
        WeekDays: new Array('Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'),
        onCreate:function(c){
          var thisMonth = month5;
          try {
            if (typeof(thisMonth) == 'undefined') return;
            if (thisMonth.month.day){
              $(thisMonth.month.day).each(function(d){
                if ($('cal_' + d.id))
                  $('cal_' + d.id).className = 'blogCal_hasBlog';
                  $('cal_' + d.id).update('<a href="' + thisMonth.month.root + d.q + '">' + d.d + '</a>');
              });
              cal5.monthHeader.update('<a href="' + thisMonth.month.root + thisMonth.month.q + '">' + cal5.monthHeader.innerHTML + '</a>');
            }
          } catch (e) {}
        },
        onPrevious:function(cells, m){
          new Ajax.Request('/ajax/?action=getBlogData', {
            method:'post',
            parameters:{key:month5.month.key,month:m.id + 1,year:m.year},
            requestHeaders: {Accept: 'application/json'},
            onSuccess: function(t){
              month5 = t.responseText.evalJSON(true);
              cal5.vars.onCreate();
            }
          });
        },
        onNext:function(cells, m){
          new Ajax.Request('/ajax/?action=getBlogData', {
            method:'post',
            parameters:{key:month5.month.key,month:m.id + 1,year:m.year},
            requestHeaders: {Accept: 'application/json'},
            onSuccess: function(t){
              month5 = t.responseText.evalJSON(true);
              cal5.vars.onCreate();
            }
          });
        }
      });
    }
    cal5.reInit();
    
  </script>
