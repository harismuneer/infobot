<script type="text/javascript">


var currentFeaturedImage = 0;
jQuery('#featuredNewsTotal').html(jQuery('#pnlFeaturedNews .featuredNewsItem').length);
jQuery('#pnlFeaturedNews .featuredNewsItem:eq(0)').addClass('active');
function nextFeaturedImage() {
	if (++currentFeaturedImage >= jQuery('#pnlFeaturedNews .featuredNewsItem').length) { 
		currentFeaturedImage = 0;	
	}
	jQuery('#featuredNewsIndex').html(currentFeaturedImage + 1);
	jQuery('#pnlFeaturedNews .featuredNewsItem').removeClass('active');
	jQuery('#pnlFeaturedNews .featuredNewsItem:eq('+currentFeaturedImage+')').addClass('active');
}
function prevFeaturedImage() {
	if (--currentFeaturedImage < 0) { 
		currentFeaturedImage = jQuery('#pnlFeaturedNews .featuredNewsItem').length - 1;
	}
	jQuery('#featuredNewsIndex').html(currentFeaturedImage + 1);
	jQuery('#pnlFeaturedNews .featuredNewsItem').removeClass('active');
	jQuery('#pnlFeaturedNews .featuredNewsItem:eq('+currentFeaturedImage+')').addClass('active');
}
</script>
