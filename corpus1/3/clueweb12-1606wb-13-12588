                <style>
			#videoPlayerColor span.up{
				color:#c05a28;
			}
			#videoPlayerColor span.over{
				color:#3477a9;
			}
		</style>
                <div id="videoPlayerColor" style="display:none;">
                  <span class="up">
                    <span class="videoUpColor">
                    </span>
                  </span>
                  <span class="over">
                    <span class="videoOverColor">
                    </span>
