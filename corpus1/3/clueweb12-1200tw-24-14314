           function openBookmarkLink(link)  {
                window.open(link, 'Bookmark ' + main_article_title, 'width=850px,height=650px,status=0,location=0,resizable=1,scrollbars=1,left=200,top=50',0);
            }

        function switchElem(nr)  {
            var i = 0;
            var elem = document.getElementById('elem' + i);
            while (elem != null)  {
                elem.style.display = (i == nr) ? "block" : "none";
                document.getElementById('e' + i).className = (i == nr) ? "h": "";
         i++;
                elem = document.getElementById('elem' + i);
            }
        }
