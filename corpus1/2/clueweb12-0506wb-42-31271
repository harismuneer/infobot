					function scmobFacebookShare() {
						window.location = 'http://www.facebook.com/sharer.php?u=' + document.URL + '&t=' + scmobEncode(document.title);
						return false;
					}

					function scmobTwitterTweet() {
						window.location = 'http://twitter.com/?status=' + document.URL + ' -- ' + scmobEncode(document.title);
						return false;
