// privacy
function update_tribute_privacy(e){
	YAHOO.util.Event.stopEvent(e);
	var v = YAHOO.util.Dom.get('tribute_set_private').checked;
	if(v){
		fetch_privacy('yes');
	}else{
		fetch_privacy('no');
	}
}
function fetched_privacy(oResponse) {
    var oR = eval('('+oResponse.responseText+')');
	if(oR.tribute_privacy){
		var sp = YAHOO.util.Dom.get('tribute_set_private');
		var icon = YAHOO.util.Dom.get('lock_icon');
		if(oR.tribute_privacy == 'yes'){
			sp.checked =true;
			icon.style.visibility = 'visible';
		}else{
			sp.checked = false;
			icon.style.visibility = 'hidden';
		}
	}
}

function fetch_privacy(sPrivacy) {
    var sURL = '/tribute/set_private.json?t=2&set_private='+sPrivacy;
    var cb = {
        success: fetched_privacy,
        failure: function() { }
    };
    YAHOO.util.Connect.asyncRequest('POST',sURL,cb);
}




function account_settings_expanding(sType, args) {
    if ( bConfirmation ) {
        YAHOO.util.Dom.setStyle("account_settings_confirmation", "display", "none");
    }
}

function account_settings_expanded(sType, args) {
    initAccountSettingsForm();
}


/* SLIDESHOW */

var current = 0;
var aPhotos = [];
var oInterval = null;

function remove_children(elt) {
    while( elt.childNodes.length ) {
        elt.removeChild(elt.childNodes[0]);
    }
}

function show_next() {
    // FADE OUT FIRST
    var img = YAHOO.util.Dom.get('tPhotoImg');
    if( img ) {
        YAHOO.util.Dom.setStyle(img,'position','absolute');
        var hAtt = { opacity: { to: 0} };
        var anim = new YAHOO.util.Anim(img, hAtt, 1 , YAHOO.util.Easing.easeOut);
        var fn = function(e,f) {
            var img = YAHOO.util.Dom.get('tPhotoImg');
            if( img ) {
                img.parentNode.removeChild(img);
            }
        }
        anim.onComplete.subscribe(fn);
        anim.animate();
    }

    // FADE IN NEW ONE
    var img2 = document.createElement('IMG');
    img2.setAttribute('id','tPhotoImg2');
    YAHOO.util.Dom.setStyle(img2,'position','absolute');
    img2.src = aPhotos[current]['m']['url'];
    img2.height = aPhotos[current]['m']['height'];
    img2.width = aPhotos[current]['m']['width'];

    var x = Math.floor((mH-img2.height)/2);
    var y = Math.floor((mW-img2.width)/2);
    x = x > 3 ? x - 3 : 0;
    y = y > 3 ? y - 3 : 0;
    YAHOO.util.Dom.setStyle(img2,'top',x+'px');
    YAHOO.util.Dom.setStyle(img2,'left',y+'px');
    //YAHOO.util.Dom.setStyle(img2,'margin-top',x+'px');
    //YAHOO.util.Dom.setStyle(img2,'margin-left',y+'px');
    YAHOO.util.Dom.setStyle(img2,'opacity','0.0');

    if( aPhotos[current]['url'] ) {
        var sURL = aPhotos[current]['url'];
        YAHOO.util.Event.on(img2,'click',function(e) { YAHOO.util.Event.stopEvent(e); document.location = sURL; });
        YAHOO.util.Dom.setStyle(img2,'cursor','pointer');
    }

    var elt = YAHOO.util.Dom.get('tPhotoSlideshowContainer');
    elt.appendChild(img2);

    var hAtt = { opacity: { to: 1 } };
    var anim2 = new YAHOO.util.Anim(img2,hAtt,2,YAHOO.util.Easing.easeOut);
    var fn = function(e,f) {
       var img = YAHOO.util.Dom.get('tPhotoImg2');
        if( img ) {
            img.setAttribute('id','tPhotoImg');
        }
        current = (current + 1 ) % aPhotos.length;
    }
    anim2.onComplete.subscribe(fn);
    anim2.animate();

}

function start_slideshow() {
    show_next();
    oInterval = setInterval(show_next,4000);
    var elt = YAHOO.util.Dom.get('tribute_slideshow_play');
    elt.innerHTML = 'stop';
}

function fetched_slideshow(oResponse) {
    var oR = eval('('+oResponse.responseText+')');
    if( oR.photos ) {
        aPhotos = oR.photos;
        prepare_slideshow();
    }
    start_slideshow();
}

function fetch_slideshow() {
    var sURL = '/tribute/fetch_photos.json?t=2';
    var cb = {
        success: fetched_slideshow,
        failure: function() { }
    };
    YAHOO.util.Connect.asyncRequest('POST',sURL,cb);
}

function play_slideshow(e) {
	var sURL = '/photoshow.html?t=2';
	self.location = sURL;
	return;
    YAHOO.util.Event.stopEvent(e);

    YAHOO.util.Event.removeListener('tPhotoImg','mouseover');
    YAHOO.util.Event.removeListener('tPhotoImg','mouseout');
    if( oHint ) {
        YAHOO.util.Dom.setStyle(oHint,'display','none');
    }
    
    if( aPhotos.length == 0 ) {
        fetch_slideshow();
    } else {
        if( oInterval ) {
            stop_slideshow();
        } else {
            start_slideshow();
        }
    }
}

        var mH = 0, mW = 0;
function prepare_slideshow() {
    var a = YAHOO.util.Dom.get('tPhoto');
    var b = YAHOO.util.Dom.get('tPhotoImg');
    var c = document.createElement('DIV');
    //YAHOO.util.Dom.setStyle(c,'border','1px solid transparent');
    YAHOO.util.Dom.setStyle(c,'position','relative');

    if( aPhotos.length > 0 ) {
        for(var i = 0; i < aPhotos.length; i++ ) {
            var h = parseInt(aPhotos[i]['m']['height']);
            var w = parseInt(aPhotos[i]['m']['width']);
            mH= h > mH ? h : mH;
            mW= w > mW ? w : mW;
        }
        // account for padding, borders and stuff
        mH += 10;
        mW += 10;
        YAHOO.util.Dom.setStyle(c,'height',mH+'px');
        YAHOO.util.Dom.setStyle(c,'width',mW+'px');
    }
    
    c.setAttribute('id','tPhotoSlideshowContainer');
    a.replaceChild(c,b);
    c.appendChild(b);

}

function stop_slideshow() {
    clearInterval(oInterval);
    oInterval = null;

    var elt = YAHOO.util.Dom.get('tribute_slideshow_play');
    elt.innerHTML = 'play';
}

var oHint = null;

function center_slideshow_hint() {
    if( oHint ) {
        var img = YAHOO.util.Dom.get('tPhotoImg');

        var h = oHint.height ? oHint.height: 88;
        var w = oHint.width ? oHint.width : 88;
        
        var x = 0;
        x += Math.floor((img.width - h)/2);
        x += YAHOO.util.Dom.getX(img);
        var y = 0;
        y += Math.floor((img.height - w)/2);
        y += YAHOO.util.Dom.getY(img);
        
        YAHOO.util.Dom.setX(oHint,x);
        YAHOO.util.Dom.setY(oHint,y);
    }
}

function show_slideshow_hint() {
    if( !oHint ) {
        oHint = document.createElement('IMG');
        oHint.src = sImageServer + 'icons/embed.gif';
        oHint.setAttribute('id','slideshow_hint');
        YAHOO.util.Dom.setStyle(oHint,'position','absolute');
        YAHOO.util.Dom.setStyle(oHint,'border','0');
        YAHOO.util.Dom.setStyle(oHint,'opacity','0.5');
        center_slideshow_hint();

        var elt = YAHOO.util.Dom.get('tPhoto');
        elt.appendChild(oHint);

        YAHOO.util.Event.on(oHint,'click',play_slideshow);
    }

    center_slideshow_hint();
    YAHOO.util.Dom.setStyle(oHint,'display','block');
}

function hide_slideshow_hint(e) {
    var t = YAHOO.util.Event.getRelatedTarget(e);
    if( t && t.id != 'slideshow_hint' ) {
        if( oHint ) {
            YAHOO.util.Dom.setStyle(oHint,'display','none');
        }
    }
}

function init_slideshow() {
    YAHOO.util.Event.on('tPhotoImg','mouseover',show_slideshow_hint);
    YAHOO.util.Event.on('tPhotoImg','mouseout',hide_slideshow_hint);

    YAHOO.util.Event.on('tPhotoImg','click',play_slideshow);

    YAHOO.util.Event.on('tribute_slideshow_play','click',play_slideshow);
}

YAHOO.util.Event.onAvailable('tribute_slideshow_play',init_slideshow);



</script>                                       
