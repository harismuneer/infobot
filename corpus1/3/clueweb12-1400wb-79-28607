<script type="text/javascript" language="Javascript">

function checkLocal(field){
  var zip = trim($(field).value);
  document.forms.toQuote.input.value = (field == 'dol_zipcode'?'daysonlot':'');
  document.forms.toQuote.zipcode.value = zip;
  if (checkZIP(zip)) {
    ajaxCheckZIP(zip);
    return false;
  } else {
    alert('Please enter a valid U.S. ZIP code to continue.');
    return false;
  }
}

function resZIP(originalRequest, json){
  var feed = originalRequest.responseText.evalJSON();
  if (feed.valid) document.forms.toQuote.submit();
  else alert('Please enter a valid U.S. ZIP code to continue.');
}
</script>


