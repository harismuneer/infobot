userActionAlt.setLinkBack(url);
userActionAlt.setTitle(title);
userActionAlt.setDescription(description);
userActionAlt.addMediaItem({
type: 'image',
src: srcAlt,
href: url
});


function checkCommentsClick(e) { 
if(e.shareItem.provider == "comments") { 
var gigya_position = $("#comments").offset().top - 30; 
$('html, body').animate({scrollTop:gigya_position}, 500); 
} 
}  

