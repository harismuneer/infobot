<!-- rebates widget-->
<!-- rebates widget -->
<script type="text/javascript" src="http://images.newcars.com/js-libs/prod/rebates_widget/dataconnector.js"></script>
<script type="text/javascript" src="http://images.newcars.com/js-libs/prod/rebates_widget/widget.js"></script>
<script type="text/javascript">


if ("undefined" === typeof(Local)) {
  var Local = {};
}

/***
 * Add onclick events to rebates widget links to show popup
 */
Local.enablePopupLinks = function () {
  var
    banner = Com.$("REB_callout_banner");

  Com.addEvent(Com.$("REB_rebates_link"), 'click', function (evt) {
      Com.stopEvent(evt);
      return Reb.popup.show("Rebate Widget More Links URL"); }
  );
  Com.addEvent(Com.$("REB_callout_link"), 'click', function (evt) {
      Com.stopEvent(evt);
      return Reb.popup.show("Rebate Widget Callout"); }
  );

  if (banner !== null) {
    banner.style.display = "block";
  }
};

Local.initRebatesWidget = function () {
  var
    options = {
      popup        : "Reb.rebates_widget",
      zip           : "",
      car_identity  : {
          year      : "2011",
          make_name : "Ford",
          model_name: "Edge"
      },
      success_handler: Local.enablePopupLinks
    };
  Reb.popup = new Reb.Widget(options);
  Reb.popup.init();
};

Local.initRebatesWidget();


</script>
<!-- end rebates widget --><!-- end rebates widget -->

<!-- NCI Search Box widget-->
 
<script type="text/javascript" src="http://images.newcars.com/js-libs/prod/new_car_locator/nci_linker.js"></script>
<script type="text/javascript">

if (typeof Local === 'undefined') 
{
    Local = {}
}
Local.initNCILinkerWidget = function() {
    var 
    // car creates the information to deliver to the widget below (keeping the
    // smarty elements separate from the rest of hte code.
    car =       {
                    
                    make:  'Ford',
                    model: 'Edge'
                    
                },
    omniture =  {
                    events  : 'event36',
                    eVar56  : 'New Car Inventory from Make Model Legacy Page'            
                },
    // Create the nci linker object here.
    nci_linker = Local.NCI_Linker({
                    div:"NCI_Link_box",
                    prefix: "nci_linker",
                    omniture: omniture,
                    car: car
                });
    nci_linker.init();
};
Local.initNCILinkerWidget();

</script><!-- end NCI Search Box rebates widget --><!-- end model page javascript -->



