              <script id="SimilarVehicleTemplate" type="text/html">
			~% for (PrintCounter = StartAt; xslLessthan(PrintCounter, EndAt); PrintCounter++ ) { %~
			<div class="SimilarVehicle"><a class="SimilarVehicleImageLink" href="~%=LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].VehicleUrl  %~"><img src="~%=	LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].primaryPhoto + '?w=169' %~" alt="~%= LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].modelYear %~ ~%= LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].make %~ ~%= LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].model %~ ~%= LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].trim %~ " /></a><div class="SimilarVehicleName"><a href="~%=LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].VehicleUrl %~">~%=	'New' + ' ' + LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].modelYear +  ' ' + LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].make + ' ' + LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].model + ' ' + LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].trim %~</a></div><div class="SimilarVehiclePrice">
					~% if(xslLessthan(LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].price, 1)) {  %~
					~%=	ZeroDollarValue %~
					~%} else {%~
					~%=	formatCurrency(LocalVehicles.SimilarVehicles.Vehicle[PrintCounter].price, false)  %~
					~%} %~
				</div></div>
			~% } %~
		</script>
              <script type="text/javascript">
				var SimilarVehciles1 = null;
				
				$(window).load(function() {
				SimilarVehciles1 = new SimilarVehicles({
				TemplateId : "SimilarVehicleTemplate",
				ParentId : "similarVehicles",
				EndAt : 20,
				PageSize: 8,
				Horizontal : false,
				ShowFilters : true,
				Area: 'Red Bank NJ',
				Department: 'New'
			});

				SimilarVehciles1.UiRefresh();
			});

		</script>
              <script type="text/javascript">
				$('#similarVehicles img').error(function () { $(this).attr('src', 'http://assets.clickmotive.com/NotAvailable.jpg?w=169'); });

				var SearchOptions = null;

				$(window).load(function() {

				SearchOptions =
				{
				All : (new  VehicleSearchUrl({
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
						DepartmentId: 2, 
						ModelId: 450, 
						AllSimilar: true})),
				NewMakeModel : (new VehicleSearchUrl( {
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
					DepartmentId: 2,  
					ModelId: 450})),
				UsedMakeModel: (new VehicleSearchUrl( {
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
					DepartmentId: 3, 
					ModelId: 450})),
				CertifiedMakeModel: (new VehicleSearchUrl( {
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
					DepartmentId: 3, 
					Certified: true, 
					ModelId: 450})),
				NewMakeProduct: (new VehicleSearchUrl( {
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
					DepartmentId: 2, 
					ProductId: 1  })),
				UsedMakeProduct: (new VehicleSearchUrl( {
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
					DepartmentId: 3, 
					ProductId: 1})),
				CertifiedMakeProduct: (new VehicleSearchUrl( {
			CustomerId : 30221, 
			VehicleId : 8416794,
			MakeId: 12, 
					DepartmentId: 3, 
					Certified: true, 
					ProductId: 1})),
				UpdateFilter: function (displayFilter) {
				switch (displayFilter) {
				case 'All':
				SimilarVehciles1.RefreshData(SearchOptions.All.GetSearchString());
				break;
				case 'NewMakeModel':
				SimilarVehciles1.RefreshData(SearchOptions.NewMakeModel.GetSearchString());
				break;
				case 'UsedMakeModel':
				SimilarVehciles1.RefreshData(SearchOptions.UsedMakeModel.GetSearchString());
				break;
				case 'CertifiedMakeModel':
				SimilarVehciles1.RefreshData(SearchOptions.CertifiedMakeModel.GetSearchString());
				break;
				case 'NewMakeProduct':
				SimilarVehciles1.RefreshData(SearchOptions.NewMakeProduct.GetSearchString());
				break;
				case 'UsedMakeProduct':
				SimilarVehciles1.RefreshData(SearchOptions.UsedMakeProduct.GetSearchString());
				break;
				case 'CertifiedMakeProduct':
				SimilarVehciles1.RefreshData(SearchOptions.CertifiedMakeProduct.GetSearchString());
				break;
				}
				}
				};
				});
			</script>
            </div>
          </div>
        </div>
      </div>
      <script type="text/javascript">
			var videoURL = "";

			

			$(document).ready(function () {

				// NOTE:  This var is defined in the main VDP.js file.
				mediaCenterEnabled = true;

				$('a.vehicle-thumbnails.without-video').fancybox(
        {
          href : '#media-center',
          padding: 0,
          showNavArrows: false,
          onStart: function(){$('#media-center').tabs('select', 0);},
          onComplete: function() {  mcDisplayImage( parseInt($(this.orig[0]).attr('thumbnailPosition'))  ); },
          onCleanup: function() {   $(".tooltip").click();}
        });

				$('a.vehicle-video').fancybox({href : '#media-center', padding: 0, showNavArrows: false, onComplete: function() {  mcDisplayImage( "video" );}, onCleanup: function() {   $(".tooltip").click();}});
				$(', a.vehicle-thumbnails.with-video').fancybox({href : '#media-center', padding: 0, showNavArrows: false, onComplete: function() {  mcDisplayImage( "video" );}, onCleanup: function() {   $(".tooltip").click();}});

				$('#vehicle-hero-photogallery-control span').bind('click', function (e) {
					var clickedImage;
					if (imagePosition == 0) { 
						clickedImage = $('a.vehicle-thumbnails')[1];
					} else {
						clickedImage = $('a.vehicle-thumbnails')[imagePosition];
					}
					$(clickedImage).click();
				});
				 
				$('#vehicle-hero-shot, #vehicle-play-button').bind('click', function () {
					if ($('#vehicle-hero-shot').hasClass('with-video')) {
						$('a.vehicle-video').click();
					} else {
						if (imagePosition == 0) {
							imagePosition = 1;
						}
						var popupImage = $('a.vehicle-thumbnails')[imagePosition];
						$(popupImage).click();
					}
				});

				$('#media-center').tabs();

				$('#media-center').unbind('tabsshow').bind("tabsshow", function (event, ui) {
					if (ui.index == 0) {
						$('#media-center-video').attr('src', '');
					} else {
						$('#media-center-video').attr('src', videoURL);
					}
				});

			});

			</script>
