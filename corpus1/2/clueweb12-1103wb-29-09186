function check_network_blocks() {

      var onnetwork = false;
      var central   = false;
      var alldata   = false;

      // Get the IP address using SSI
      
      var ip = '128.2.207.79';
      ip = ip.split(",")[0]
      

      for (i in ip_blocks.onnetwork) {
            if (network_contains_ip(ip, ip_blocks.onnetwork[i])) {
                  onnetwork = true;
            }
      }

      for (i in ip_blocks.central) {
            if (network_contains_ip(ip, ip_blocks.central[i])) {
                  central = true;
            }
      }

      for (i in ip_blocks.alldata) {
            if (network_contains_ip(ip, ip_blocks.alldata[i])) {
                  alldata = true;
            }
      }

// test
// alert ("For IP: " + ip + "\r\n\r\nonnetwork: " + onnetwork + "\r\ncentral: " + central + "\r\nalldata: " + alldata);


      if (onnetwork)
            {document.write('<style type="text/css">.inhouselink {display:inline;} .remotetext {display:none;}</style>');}
      else
            {document.write('<style type="text/css">.inhouselink {display:none;} .remotetext {display:inline;}</style>');}
 
      if (central)
            {document.write('<style type="text/css">.ceninhouselink {display:inline;} .cenremotetext {display:none;}</style>');}
      else
            {document.write('<style type="text/css">.ceninhouselink {display:none;} .cenremotetext {display:inline;}</style>');}
                   
      if (alldata)
            {document.write('<style type="text/css">.alldatainhouselink {display:inline;} .alldataremotetext {display:none;}</style>');}
      else
            {document.write('<style type="text/css">.alldatainhouselink {display:none;} .alldataremotetext {display:inline;}</style>');}

}

check_network_blocks();

</script>

