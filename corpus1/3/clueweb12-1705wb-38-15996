<script>
    var width = 9 * 200 + 200;
    $("#artistimgs").css({width: width.toString() + "px"});

    $('.artistimg:eq(0)').fadeIn();

    $('.artistimg').live('click', function () {
        $('#modalTitle').html($(this).children('img').attr('alt'));
        $('#modalBody').html("<img src='" + $(this).children('span').html().split('|')[0] + "'/>");
        $('#modal').css('width', $(this).children('span').html().split('|')[1] + 'px');
        $('#modalPopup').show();
    });

    $('#artistimgs .controls a').live('click', function () {
        var index = $(this).index();
        $('#artistimgs .controls a').removeClass('active');
        $(this).addClass('active');
        $("#artistimgs div:eq(0)").animate({ marginLeft: (-200*index).toString() + "px" });
    });
</script></div>
