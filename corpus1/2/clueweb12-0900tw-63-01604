	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_372010").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/372010?view=json", function(json){
				jQuery("#upv_372010").empty();
				if (json.stats) {
					jQuery("#upv_372010").append(json.stats.uniqueVisitors + ' (+' + json.stats.uniqueVisitorsProgression + ')');				
				} else {
					jQuery("#upv_372010").append("Not available");
				}
			});
		}
	}
