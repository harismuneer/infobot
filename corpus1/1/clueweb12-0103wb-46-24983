	$(function () {
		$('#slider1').anythingSlider({
			buildNavigation	: false,
			autoPlay		: false,
			startStopped    : true, // If autoPlay is on, this can force it to start stopped
			width : 219, // Override the default CSS width
			height           : 405,  // Override the default CSS width
			buildArrows: false,
			theme           : 'cs-portfolio',
			themeDirectory      : '/alumni/ee/css/theme-{themeName}.css', 
			onSlideComplete : function(slider){
				if(slider.currentPage==11){
					slider.gotoPage(1);
				}
				}
		});
		
	});
		
	</script>
