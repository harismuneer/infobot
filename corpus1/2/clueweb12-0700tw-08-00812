	<!-- Now, goes hack's for different layouts -->


	
			    <div class="facebook-like-box float_left">
				<h4>share this story</h4>
				<div class="padding_5"><fb:like  entry_id="825978"  hp_track="Badges"  width="244" href="http%3A%2F%2Fwww.huffingtonpost.com%2Fjohn-robbins%2Fchocolates-startling-heal_b_825978.html" class="" action="recommend" show_faces="false" font="lucida grande" locale="en_US"></fb:like>
