          function bccItemShow(itemID, descriptionID)
            {
                $(itemID).children("a.bcClassShowLink").css("display", "none");
                $(itemID).children("a.bcClassHideLink").css("display", "inline");
                $(itemID).css("background-color", "#F2F1DC");
                $(descriptionID).slideDown(800);
            }

            function bccItemHide(itemID, descriptionID)
            {
                $(itemID).children("a.bcClassHideLink").css("display", "none");
                $(itemID).children("a.bcClassShowLink").css("display", "inline");
                $(itemID).css("background-color", "#FFFFFF");
                $(descriptionID).slideUp(400);
            }

            function hideCurBccItem()
            {
                var x = 1;
                
                for(x; x <= 10; x++)
                {
                    var descriptionID = "#bcc" + x + "Desc";
                    var itemID = "#bcc" + x + "Item";
                    var isVisible = $(descriptionID).is(":visible");
                    if(isVisible)
                    {
                        bccItemHide(itemID, descriptionID);
                    }
                }
            }

            $("a.bcClassShowLink").click(function(event){
                event.preventDefault();
                var curID = $(this).attr("id");
                curID = "#" + curID;
                descriptionID = curID + "Desc";
                itemID = curID + "Item";
                hideCurBccItem();
                bccItemShow(itemID, descriptionID);
                $(".bcClassifiedWidget").mouseleave(function(){
                    setTimeout(function(){hideCurBccItem();}, 2800);
                });
            });
            
            $("a.bcClassHideLink").click(function(event){
                event.preventDefault();
                var curID = $(this).attr("id");
                curID = "#" + curID;
                descriptionID = curID + "Desc";
                itemID = curID + "Item";
                bccItemHide(itemID, descriptionID);
            });


            $("#keywordSearch").focus(function(){
                if(this.value == "Search Site")this.value = "";
            });
		});
    </script>

