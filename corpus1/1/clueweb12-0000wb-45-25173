 function init() {
// quit if this function has already been called
if (arguments.callee.done) return;
// flag this function so we don't do the same thing twice
arguments.callee.done = true;
// run the target initilization code
onReadyInit();
 };
/* for Mozilla */
if (document.addEventListener) {
    document.addEventListener("DOMContentLoaded", init, false);
}
// for Internet Explorer (using conditional comments)
/*@cc_on @*/
/*@if (@_win32)
document.write("<script id=__ie_onload defer src=javascript:void(0)><\/script>");
var script = document.getElementById("__ie_onload");
script.onreadystatechange = function() {
 if (this.readyState == "complete") {
   init(); // call the onload handler
 }
};
/*@end @*/
// for Safari
if (/WebKit/i.test(navigator.userAgent)) { // sniff
  var _timer = setInterval(function() {
    if (/loaded|complete/.test(document.readyState)) {
      clearInterval(_timer);
      init(); // call the onload handler
    }
  }, 10);
}
/* for all other browsers */
window.onload = init;
</script>
<script language="javascript">
<!--
// Option support classes
var classifications = [];
classifications[0] = new Classification(1, false, true);
classifications[0].addSize(new Size(1, 'X-Small', 510, 340, false, false));
classifications[0].addSize(new Size(2, 'Small', 1200, 800, true, false));
classifications[0].addSize(new Size(3, 'Medium', 2610, 1740, false, false));
classifications[0].addSize(new Size(4, 'Large', 4290, 2860, false, false));
function selectClassification(id) {
for (var i=0; i<classifications.length; i++) {
if (classifications[i].id == id) {
classifications[i].select();
} else {
classifications[i].deselect();
}
}
}
function selectSize(id) {
for (var i=0; i<classifications.length; i++) {
for (var j=0; j<classifications[i].sizes.length; j++) {
if (classifications[i].sizes[j].id == id) {
classifications[i].deselectAllSizes();
classifications[i].sizes[j].select();
return;
}
}
}
}
function selectClassificationForSize(id) {
for (var i=0; i<classifications.length; i++) {
for (var j=0; j<classifications[i].sizes.length; j++) {
if (classifications[i].sizes[j].id == id) {
selectClassification(classifications[i].id);
return;
}
}
}
}
function onReadyInit() {
var size_id = params("size_id");
if (size_id != null) {
 	selectClassificationForSize(size_id);
     	selectSize(size_id);
}
}
function Classification(id, free_license_available, selected) {
this.id = id;
this.selected = selected || false;
this.free_license_available = free_license_available || false;
this.sizes = [];
this.id_prefix = 'classification-id-';
this.display_type = 'table';
this.nav_id_prefix = 'classification-nav-id-';
this.addSize = function(size) {
var i = this.sizes.length;
this.sizes[i] = size;
this.sizes[i]._parent = this;
}
this.select = function() {
this.selected = true;
$(this.id_prefix + this.id).style.display = this.display_type;
$(this.nav_id_prefix + this.id).addClassName('selected');
this.refreshSizes();
this.refreshFileFormats();
this.refreshLinkBackLicense();
}
this.deselect = function() {
this.selected = false;
$(this.id_prefix + this.id).style.display = 'none';
$(this.nav_id_prefix + this.id).removeClassName('selected');
}
this.deselectAllSizes = function() {
for (var i=0; i<this.sizes.length; i++) {
this.sizes[i].deselect();
}
}
this.refreshSizes = function() {
for (var i=0; i<this.sizes.length; i++) {
if (this.sizes[i].selected == true) {
this.sizes[i].select();
}
}
}
this.refreshLinkBackLicense = function() {
if (this.free_license_available == true) {
if ($('linkBackLicense').style.display == 'none')
Effect.BlindDown('linkBackLicense', { duration: 0.3 });
} else {
Effect.BlindUp('linkBackLicense', { duration: 0.3});
}
}
this.refreshFileFormats = function() {
elements = document.getElementsByClassName('fileFormat');
for (var i=0; i<elements.length; i++) {
if(elements[i].hasClassName('file-format-for-classification-id-' + this.id)) {
elements[i].style.display = 'block';
} else {
elements[i].style.display = 'none';
}
}
}
}
function Size(id, size, width, height, selected, free_license_available) {
this.id = id;
this.size = size;
this.width = width;
this.height = height;
this.selected = selected || false;
this.free_license_available = free_license_available || false;
this.id_prefix = 'size-id-';
this.select = function() {
this.selected = true;
$(this.id_prefix + this.id).addClassName('selected');
$(this.id_prefix + this.id).removeClassName('highlighted');
this.updateCartLink();
this.updatePreview();
this.updateFreeDownloadLink();
}
this.updateCartLink = function() {
this.updateLinkSize('addToCart');
}
this.updatePreview = function() {
if ($('viewerImage') != null) {
if(this.width <= 400 && this.height <= 267) {
$('viewerImage').width = this.width;
$('viewerImage').height = this.height;
$('zoomIcon').style.display = 'none';
$('actualSizeLabel').style.display = 'block';
if (false == false) {
<!-- Do the following if image is not transparent -->
$('viewerImage').style.border = 'none';
$('compFrame').style.backgroundColor = '#e8e8e8';
$('actualSizeLabel').style.color = '#333';
$('enlargeCaption').style.display = 'none';
}
} else {
$('viewerImage').width = 400;
$('viewerImage').height = 267;
$('zoomIcon').style.display = 'inline';
$('actualSizeLabel').style.display = 'none';
if (false == false) {
<!-- Do the following if image is not transparent -->
$('viewerImage').style.border = '1px solid #bfbfbf';
$('compFrame').style.backgroundColor = '#fff';
$('enlargeCaption').style.display = 'block';
}
}
}
}
this.deselect = function() {
this.selected = false;
$(this.id_prefix + this.id).removeClassName('selected');
}
this.updateLinkSize = function(element) {
$(element).href = $(element).href.gsub(/\d+$/, this.id );
}
this.updateFreeDownloadLink = function() {
if (this._parent.free_license_available == true) {
if (this.free_license_available == true) {
$('freeDownloadAvailable').style.display = 'inline';
$('freeDownloadUnavailable').style.display = 'none';
this.updateLinkSize('freeDownload');
$('freeDownloadDescription').update('(' + this.size.toLowerCase() +')');
} else {
$('freeDownloadAvailable').style.display = 'none';
$('freeDownloadUnavailable').style.display = 'inline';
}
} 
}
}
function isSelected(obj) {
if (obj.className.search('selected') >= 0) {
return true;
} else {
return false;
}
}
function selectDimensions(units) {
selectUnits('dimensions', units)
}
function selectFileSize(units) {
selectUnits('fileSize', units)
}
function selectUnits(property, units) {
elements = document.getElementsByClassName(property);
for (var i=0; i<elements.length; i++) {
if (elements[i].hasClassName('header') != true) {
if (elements[i].hasClassName(units) == true) {
elements[i].style.display = 'inline';
} else {
elements[i].style.display = 'none';
}
} else {
if (elements[i].hasClassName(units) == true) {
elements[i].addClassName('selected');
} else {
elements[i].removeClassName('selected');
}
}
}
}
// Viewer Support Classes
function changeBackground(new_value) {
if (new_value != 'customBackground') { 
$('compFrame').style.backgroundColor = null;
Control.colorPickers.each(function(colorPicker) { colorPicker.close(); });  // Close any open color pickers
}
$('compFrame').className = new_value + ' transparent';
clearBackgroundIndicators();
$(new_value).className += " selected";
updateActualSizeLabel(new_value);
}
function clearBackgroundIndicators() {
var backgrounds = ['customBackground', 'gridBackground', 'gradientBackground', 'blackBackground', 'whiteBackground']
for (var i in backgrounds) {
$(backgrounds[i]).className = backgrounds[i];
}
}
function updateActualSizeLabel(background_value) {
if (background_value == 'whiteBackground' || background_value == 'gridBackground') {
$('actualSizeLabel').style.color = '#333';
} else {
$('actualSizeLabel').style.color = '#fff';
}
}
function params( name )	{
  name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
  var regexS = "[\\?&]"+name+"=([^&#]*)";
  var regex = new RegExp( regexS );
  var results = regex.exec( window.location.href );
  if( results == null )
    return null;
  else
    return results[1];
}
-->
