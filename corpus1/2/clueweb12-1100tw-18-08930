<script language="javascript" type="text/javascript" >
//<![CDATA[
//Browser Support Code
function sendRating()
{
	disableDiv();

	var ajaxRequest;  
	try
	{
		// Opera 8.0+, Firefox, Safari
		ajaxRequest = new XMLHttpRequest();
	} 
	catch (e)
	{
		// Internet Explorer Browsers
		try
		{
			ajaxRequest = new ActiveXObject("Msxml2.XMLHTTP");
		} 
		catch (e)
		{
			try
			{
				ajaxRequest = new ActiveXObject("Microsoft.XMLHTTP");
			}
			catch (e)
			{
				// Something went wrong
				alert("Error.");
				return false;
			}
		}
	}
	
	// Create a function that will receive data sent from the server
	ajaxRequest.onreadystatechange = function()
	{
		if(ajaxRequest.readyState == 4)
		{
			
			var newData = ajaxRequest.responseText.split("|");
			
			submitSuccess(newData[0], newData[1], newData[2]);
		}
	}
	
	//append random number to ensure that page does not get cached any where.
	randomVar = Math.random();
	
	dataURL = "../processRatings.aspx?rating=" + document.getElementById("userRating_PresentationModeContainer1_rating").value + "&comment=" + document.getElementById("comment").value + "&guid=" + document.getElementById("guid").value + "&url=" + document.getElementById("url").value + "&random=" + randomVar;
	//alert(dataURL);
	ajaxRequest.open("GET", dataURL, true);
	ajaxRequest.send(null); 
}
	
showFlag = 0;

function removeText(elem)
{
	document.getElementById(elem).value = "";
}

function showHideRatings()
{
	if(showFlag == 0)
	{
		document.getElementById("ratingsBox").style.display = "block";
		document.getElementById("rateBtn").value = "Cancel";
		showFlag = 1;
	}
	else
	{
		document.getElementById("ratingsBox").style.display = "none";
		document.getElementById("rateBtn").value = "Rate";
		//document.getElementById("expandednoJS").style.display = "none";
		showFlag = 0;
	}
}

function disableDiv()
{
	//drop opacity on ratingsBox
	document.getElementById("ratingsBox").style.opacity = 0.5;
	document.getElementById("ratingsBox").style.filter = 'alpha(opacity=50)';

	//disable form elements, change value to "Sending..."
	document.getElementById("userRating_PresentationModeContainer1_rating").disabled = true;
	document.getElementById("comment").disabled = true;
	document.getElementById("submitBtn").disabled = true;
	document.getElementById("submitBtn").value = "Sending...";
	
	//disable rate btn
	document.getElementById("rateBtn").value = "Rate";
	document.getElementById("rateBtn").disabled = true;
}

function submitSuccess(newRating, newVoteCount, ratingsMsg)
{
	//inc opacity on ratingsBox
	document.getElementById("ratingsBox").style.opacity = 1;
	document.getElementById("ratingsBox").style.filter = 'alpha(opacity=100)';
	
	//change innerHTML to "Thank you for submitting your vote. <close>"
	document.getElementById("ratingsBox").innerHTML = "<p>Thank you for your vote. <a href='javascript:closeDiv()'>close</a></p>";
	
	//remove vote button from containerBox, update contents (new rating)
	document.getElementById("intraContainer").innerHTML = ratingsMsg + "<br /><img src=\"../Images/" + newRating + "_star.gif\" alt=\"This page is rated" + newRating + "/5.\" border=\"0\"><input type=\"button\" name=\"rateBtn\" id=\"rateBtn\" onClick=\"showHideRatings()\" value=\"Rated\" style=\"width:70px;\" disabled>"

	//after 5 secs, hide this div
	var t=setTimeout("closeDiv()",10000);
}

function closeDiv()
{
	document.getElementById("ratingsBox").style.display = "none";
}
