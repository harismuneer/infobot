<script>
	function Login_checkAnyFilledField() {
		var e = getElementsByClassName(document, "login_vldAll");
		for (var i=0; i < e.length; i++) {
			if (e[i].value.trim() == "") {
				openStaticModalWindow('Please enter username and password', 'Alert');
				return false;
			}
		}
		return true;
    }
	
	function checkUserPassword() {
		return checkUserPasswordScript(document.getElementById('_ctl0__ctl0__ctl0_ucMenu1_login_userName').value, document.getElementById('_ctl0__ctl0__ctl0_ucMenu1_login_password').value)
    }

    function checkUserPasswordScript(usrName, pass) {
	    var loginResult = ucLogin.isValidUserPassword(usrName,pass).value;
	    if (loginResult > 0)
	        return true;
		switch (loginResult) {
			case -1 :
			    openWindow({ modal: true, title: 'Instructions to activate your account', url: '/auto/lease/Myleasetrader/InstructionsActiveAccountUser.aspx?go=Close', width: 540, height: 350, scrolling: false }); 
				break;
			case -2:
			    window.location = "/newUser.xhtml"; 
				break;		 
			case -3:	
			    var LoginValue = ucLogin.RedirectLink().value; 
			    openStaticModalWindow('Your IP does not belong to this country. Redirecting...', 'Error', 300);	
    											
			    var obs = {onClose: function() {						
			        window.top.Windows.removeObserver(obs);	
			        window.top.location.replace('http://'+LoginValue);
			    }};
			    window.top.Windows.addObserver(obs);
			    break;
			default:
			    openStaticModalWindow('Invalid username/password', 'Error');
			    break;
        }
        return false;
	}
</script>



