function validate() {
    var f = document.form1;
    var err = '';
    if (f.subject.value == 'Subject')
        err = err + 'Please Enter An Email Subject\n';
    if (f.firstName.value == 'Firstname')
        err = err + 'Please enter your firstname\n';
    if (f.lastName.value == 'Lastname')
        err = err + 'Please enter your lastname\n';
    if (f.email.value == 'Email')
        err = err + 'Please enter email address\n';
    if (f.Comments.value == '')
        err = err + 'Please enter your comments\n';
    if (err != '') {
        alert(err); 
        return false;
    }
    else {
        return true;
    }
}
</script>
