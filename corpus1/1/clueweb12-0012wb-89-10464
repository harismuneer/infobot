<script language="javascript" type="text/javascript">
    function ds(theStringToDisplay,theItemToSet) {
        if (theItemToSet==1) {
            document.getElementById("electionWinnerDem").innerHTML = theStringToDisplay;
                } else if (theItemToSet==2) {
                  var electoralPct = ((theStringToDisplay.slice(0,(theStringToDisplay.length-1))-0)*1);
                        document.getElementById("electionWinnerRep").innerHTML = theStringToDisplay;
                        if (electoralPct<50.0) {
                                document.getElementById("demsOrRepubsTxt").innerHTML = "Obama";
                        } else {
                                document.getElementById("demsOrRepubsTxt").innerHTML = "McCain";
                        }
                        document.getElementById("demsOrRepubsPct").innerHTML = (Math.round(electoralPct)+"%");
        } else if (theItemToSet==3) {
            document.getElementById("electionWinnerTies").innerHTML = theStringToDisplay;
        } else if (theItemToSet==4) {
            document.getElementById("avgElectoralVotesDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==5) {
            document.getElementById("avgElectoralVotesRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==6) {
            document.getElementById("mostCommonVotesDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==7) {
            document.getElementById("mostCommonVotesRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==8) {
            document.getElementById("rangeOfVotesDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==9) {
            document.getElementById("rangeOfVotesRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==10) {
            document.getElementById("medianElectoralVotesDem").innerHTML = theStringToDisplay;
            document.getElementById("medianElectoralVotesDemTop").innerHTML = theStringToDisplay;
        } else if (theItemToSet==11) {
            document.getElementById("medianElectoralVotesRep").innerHTML = theStringToDisplay;
            document.getElementById("medianElectoralVotesRepTop").innerHTML = theStringToDisplay;
        }
    }
    function dp(theStringToDisplay,theItemToSet) {
        if (theItemToSet==1) {
            document.getElementById("fiftyDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==2) {
            document.getElementById("seventyFiveDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==3) {
            document.getElementById("ninetyDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==4) {
            document.getElementById("oneHundredDem").innerHTML = theStringToDisplay;
        } else if (theItemToSet==5) {
            document.getElementById("fiftyRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==6) {
            document.getElementById("seventyFiveRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==7) {
            document.getElementById("ninetyRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==8) {
            document.getElementById("oneHundredRep").innerHTML = theStringToDisplay;
        } else if (theItemToSet==9) {
            document.getElementById("asterixRange").innerHTML = theStringToDisplay;
        }
    }
</script>
