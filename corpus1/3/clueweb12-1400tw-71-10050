function _doPostback_comp4f886294c9ba9(a,p,msgPage,msgs,aBUserID,mode,align,messageID){_setData_comp4f886294c9ba9(a,p,msgPage,msgs,aBUserID,mode,align,messageID);document.form__comp4f886294c9ba9.submit();}
function handleSuccess_comp4f886294c9ba9(o){ div = document.getElementById('_comp4f886294c9ba9');	if(o.responseText !== undefined){ div.innerHTML = o.responseText; } toggleLoading_comp4f886294c9ba9(false); }
