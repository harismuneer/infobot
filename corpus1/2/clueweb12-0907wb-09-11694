<script type="text/javascript">
var OAS = {

   /**
    * Built a query string from all the contextural data we have
    */
    getQueryString :function()
    {
        q  = {'context' : 'Pets',
              'weather': 'mist'.toLowerCase(),
              'site_search': '',
              'asSegments': audienceScienceSegments,
              'crtSegments' : crtg_content,
              
              'qcSegments' : quantSegs
              }
        query = qs.objToQueryString(q);

        query += '';

        return query;

    },

    getUrl : function(posList)
    {
        base = 'http://oas.newsquestdigital.co.uk/RealMedia/ads/';
        page = this.getSitePage();
        positions = posList.join(',');
        randomNumber = Math.random().toString().substr(2,11);
        query = this.getQueryString();

        url = base + 'adstream_mjx.ads/' + page + '/1' + randomNumber + '@' + positions +'?' + query;
        return url;
    },

    getSitePage : function()
    {
        // set a base depenging on martini
        // and addt the current path
        page = 'www.burytimes.co.uk' +  window.location.pathname;

        // if we don't have a page, add one
        if (page.charAt(page.length - 1) == '/')
        {
            page += 'index.php';
        }

        page = page.replace('--','');

        return page;
    },
    getAllValidPositions : function ()
    {
        positions = ['Bottom3', 'Bottom2', 'Bottom1', 'Bottom', 'BottomLeft', 'BottomRight',
                     'Frame2', 'Frame1',
                     'Left3', 'Left2', 'Left1', 'Left',
                     'Right3', 'Right2', 'Right1', 'Right',
                     'Middle3', 'Middle2', 'Middle1', 'Middle',
                     'Position3', 'Position2', 'Position1', 
                     'Right3', 'Right2', 'Right1', 'Right',
                     'Top3', 'Top2', 'Top1', 'Top', 'TopRight', 'TopLeft']

        for (i=1; i < 97; i++)
        {
            if (i < 10)
            {
                i = '0'+i;
            }
            positions.push('x'+i);
        }

        return this.validatePositionList(positions);
    },

    validatePositionList : function(positions)
    {
        validPositions = [];

        // iterate through each position and check it exists in the dom (that we need it)
        $.each(positions, function (p, name)
        {
            if ($('#OAS_'+name).length > 0)
            {
                validPositions.push(name);
            }
        });

        return validPositions;
    },

    load : function (posList)
    {
        $.each(posList, function(i, name)
        {
            var from = document.getElementById('OAS_AD_'+name);
            var to   = document.getElementById('OAS_'+name);
            if (OAS.checkForRealAd(from))
            {
                to.appendChild(from);
                from.style.visibility = "visible";
                from.className = '';
            }
            else
            {
                $(to).remove();
                $(from).remove();
            }
        });
        $('.OAS_HOLDER').remove();
    },

    checkForRealAd: function (el)
    {
        if (el === null)
        {
            return false;
        }
        var c = $(el).find('img').attr('src');
        if (typeof(c) != "undefined" && c.indexOf('empty.gif') !== -1)
        {
            return false;
        }
        return true;
    }
}
var posList = OAS.getAllValidPositions();

var pathName = document.location.pathname.toString().substr(0, 7);
if (pathName !== '/admin/')
{
    document.write('<scr'+'ipt type="text/javascript" src="'+OAS.getUrl(posList)+'"><\/script>');
}
else
{
    OAS_RICH = function() {};
}
</script>
