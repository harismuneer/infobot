		function() { $("#top-nav li").removeClass("sfhover"); $(this).addClass("sfhover"); },
		function() { $("#top-nav li").removeClass("sfhover"); }
	);
	 
	var readmore = 'View more ...'; var showless = 'Show less';
	$('.show-hide').each(function () { var tag = this.tagName; $(this).after(' <' + tag + ' class="read-more">' + readmore + '</' + tag + '>'); }).hide();
	$('.read-more').toggle(
		function(){ $(this).text(showless); $(this).prev().show(); },
		function(){ $(this).text(readmore); $(this).prev().hide(); }
	);

	$('.mobile #main-col img')
		.filter(function() { return $(this).width() > 500; })
		.css("height", function() { return $(this).height() * 500 / $(this).width(); })
		.css("width", 500)
