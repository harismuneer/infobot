'writeAtt':function() {
var sc = this.getCookie("SC");
if ( (sc != null) && (sc.length > 16) ) {

this.$("navbar_portfolio").href = "https://scs.fidelity.com/customeronly/portfolio.shtml?bar=p";
this.$("navbar_accountPositions").href = "https://scs.fidelity.com/customeronly/accountposition.shtml?bar=p";
this.$("navbar_trade").href = "https://scs.fidelity.com/customeronly/trade.shtml?bar=p";
this.$("navbar_transferMoney").href = "https://scs.fidelity.com/customeronly/trans.shtml?bar=p";
this.$("navbar_billPay").href = "https://scs.fidelity.com/customeronly/billpay.shtml?bar=p";
this.$("navbar_updateAccounts").href = "http://personal.fidelity.com/accounts/services/updateacctsframe.shtml?bar=p";
this.$("navbar_statements").href = "https://scs.fidelity.com/customeronly/statements.shtml?bar=p";
this.$("navbar_fullView").href = "https://scs.fidelity.com/customeronly/fullview.shtml?bar=p";

}
},

'writeLog':function() {

var sc = NavBar.getCookie("SC");
var login = this.$("navbar_log");
if ( (sc != null) && (sc.length > 16) ) {

login.ltag = "VSDESC^LogOut";

login.href = "javascript:NavBar.goTo('https://login.fidelity.com/ftgw/Fidelity/RtlCust/Logout/Init?AuthRedUrl=https://www.fidelity.com/tpv/logout_webxpress.shtml');";

login.innerHTML = '<b>Log Out</b>';

}
else {

login.href = "javascript:NavBar.goTo('https://login.fidelity.com/ftgw/Fas/Fidelity/RtlCust/Login/Init?AuthRedUrl=https://oltx.fidelity.com/ftgw/fbc/ofsummary/defaultPage');";

login.innerHTML = '<b>Log In</b>';

}

},

'addVSTracking':function(){
	var eb = NavBar.getCookie("cinfo");
	var sc = NavBar.getCookie("SC");
	var vsdesc; 
	var ebFlag = eb + ":ebflag[1]";
	var cookieDomain = "https://www.fidelity.com";
	cookieDomain = cookieDomain.split(":");
	cookieDomain = cookieDomain[1].split(".");
	cookieDomain = "."+cookieDomain[1]+"."+cookieDomain[2];
	var refToButton = NavBar.$("eb_tracker");
    try {
	
	if(eb != null) {
	vsdesc = /mstatus(\[*.\])/ig.exec(eb);
	vsdesc = vsdesc[1];
	vsdesc = vsdesc.replace("[", "");
	vsdesc = vsdesc.replace("]", "");
	}
	var strTagValue = "VSPAGE^STATUS_MIGRATION_EVENT~VSDESC^"+vsdesc+"~VSEVENT^STATUS_MIGRATION_EVENT~VSMODULE^TOPNAV_MIGRATION";
    NavBar.$("eb_tracker").setAttribute("ltag", strTagValue);
	if (eb.indexOf("F") != -1 || eb.indexOf("P") != -1) {
	if ( (sc != null) && (sc.length > 16) ) {
	if(eb.indexOf("ebflag[1]") == -1) {
	VsTrack(refToButton);
	NavBar.setCookie("cinfo", ebFlag, "", "/", cookieDomain);
	}
	else {
	return;
	}
	}
	}
	
	}
	catch(e) {
	return;
	}
},


'addVSTracking2':function(){
    var strTagValue = "VSDESC^Quote " + document.popup_quote_form.SID_VALUE_ID.value + "~VSEVENT^CallToAction~VSMODULE^TopNav";
    this.$("navbar_nb-quote").setAttribute("ltag", strTagValue);
	var refToButton = this.$("navbar_nb-quote");
    try {
	VsTrack(refToButton);
	}
	catch(e) {
	return;
	}
}

};

</script>


