<script type="text/javascript" src="http://www.zap2it.com/javascript/jquery-1.3.2.js"></script>
<script type="text/javascript">
    function displayTvlistings(data) {
        var total = 3;
        var previous = null;
        var current = 0;
        var date;
        var dateString;
        var timeString;
        var days = new Array("Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday");
        var html = '<div class="zcc-tvlistings-box">';
        if (data.results.smallImagePath != null && data.results.smallImagePath.length > 0) {
            html += '<div class="zcc-image"><a href="' + data.results.celebrityDetailLink + '">';
            html += '<img src="' + data.results.smallImagePath + '"/></a></div>';
            html += '<div class="zcc-image-promo">Get the latest TV appearances, news and photos on your favorite celebrities</div>';
            html += '<div class="clear">&nbsp;</div>';
        }

if (data.results.schedules != null && data.results.schedules.length > 0) {
        html += '<table cellspacing="0" cellpadding="0" border="0" class="zcc-tvlistings">';
        html += '<tr><th class="zcc-show-column">Show/Movie</th><th class="zcc-datetime-column">When (' + data.results.schedules[0].timezone + ')</th><th class="zcc-network-column">Network</th></tr>';
        jQuery.each(data.results.schedules, function(){
            dateString = this.date.split("-");
            timeString = this.time.split(" ");

			var d=new Date(dateString[2], dateString[0]-1, dateString[1], timeString[0].split(":")[0], timeString[0].split(":")[1], 0, 0);
            
            date = new Date(dateString[2], dateString[0], dateString[1]);
            html += '<tr><td class="';
            if (previous != null && previous != this.link) {
                html += 'zcc-border ';
            }
            html += 'zcc-show-column">';

            if (previous != null && previous == this.link) {
                html += "";
            } else if (this.programType == "MV") {
                html += '<a href="' + this.link + '">' + this.title + '</a>';
            } else if (this.episodeTitle != null) {
               html += '<a href="' + this.link + '">' + this.title + '</a>';
            } else {
                html += '<a href="' + this.showcardLink + '">' + this.title + '</a>';
            }
            html += '</td>';

            html += '<td class="zcc-datetime-column">';

            month = date.getMonth() + 1;
            html += days[d.getDay()] + " " + (d.getMonth() + 1) + "/" + d.getDate() + " " + timeString[0] + " " + timeString[1];
            html += '</td>';
            html += '<td class="zcc-network-column">';
            if (this.channel != null) {
                html += '<span class="zcc-channel">' + this.channel + '</span>';
            }
            if (this.stationName != null && this.stationName.length > 0) {
                html += this.stationName;
            } else {
                html += "Check Local Listings";
            }
            html += '</td></tr>';
            previous = this.link;
            current++;
            if (current == total) {
                return false;
            }
        });
        html += '</table>';
       }  
        
        html = html + '</div> <a href="' + data.results.celebrityDetailLink + '" class="more">&raquo;&nbsp;More ' + data.results.personName + ' News</a>';
        $(html).appendTo("#tvlistings");
        $("#tvlistings").show();
    }

    jQuery.ajax({
        type: 'GET',
        url: 'http://api.zap2it.com/tvlistings/webservices/peopleFinder',
        data: {'name':'Nicolas Cage','callback':'displayTvlistings'},
        dataType: 'script',
        cache: true,
        success: function(data){
            displayTvlistings(data);
        }
    });
</script>

