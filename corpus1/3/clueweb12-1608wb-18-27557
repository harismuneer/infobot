'sst':function(tabnumber) {

var tab = "navbar_tab"+tabnumber
var tabLink = "navbar_tab"+tabnumber+"link";
var subTab = "navbar_subtab"+tabnumber;
var subTabList = "navbar_subtablist"+tabnumber;
var selectedTabClass = "selectedtab"+tabnumber;

this.isSelected = tabnumber;

for (i=0;i<this.numberOfTabs;i++) {
if (i != tabnumber) {
var unselectedTab = "navbar_tab"+i;
var unselectedTabClass = "tab"+i;
var unselectedTabLink = "navbar_tab"+i+"link";
var unselectedSubTab = "navbar_subtab"+i;
var unsubTabList = "navbar_subtablist"+i;

this.$(unselectedTab).className=unselectedTabClass;
this.$(unselectedTab).style.backgroundColor=this.darkgreen;
this.$(unselectedTab).style.border="1px solid #fff";
this.$(unselectedTab).style.borderLeft="1px solid #fff";
this.$(unselectedTab).style.borderRight="1px solid #fff";
this.$(unselectedTabLink).style.color=this.white;
this.$(unselectedTabLink).style.font="bold 11px Arial";
this.$(unselectedSubTab).style.visibility = "hidden";
this.$(unsubTabList).style.display = "none";
}
}
if(tabnumber !=5 || tabnumber !=3) {
this.$(tab).className=selectedTabClass;
this.$(tab).style.backgroundColor=this.gray;
this.$(tab).style.borderTop="1px black solid";
this.$(tab).style.borderLeft="1px black solid";
this.$(tab).style.borderRight="1px black solid";
this.$(tab).style.borderBottom="0px black solid";
this.$(tabLink).style.color=this.black;
this.$(tabLink).style.font="bold 12px Arial";
this.$(subTabList).style.display = "block";
this.$(subTab).style.visibility = "visible";
this.$(subTab).style.zIndex = "2";
this.$(subTabList).style.zIndex = "2";
}
if(!this.reflowFlag) {
this.forcePageReflow(1);
}

},

'ht':function(tab, state) {
	var tabId="navbar_tab"+tab;
	if (tab != this.isSelected) {
		if(state) {
		this.$(tabId).style.backgroundColor=this.lightgreen;
		}
		else {
		this.$(tabId).style.backgroundColor=this.darkgreen;
		}
	}
},

'ha':function() {
for (i=0;i<this.numberOfTabs;i++) {
var unselectedTab = "navbar_tab"+i;
var unselectedTabClass = "tab"+i;
var unselectedTabLink = "navbar_tab"+i+"link";
var unselectedSubTab = "navbar_subtab"+i;
var unsubTabList = "navbar_subtablist"+i;

this.$(unselectedTab).className=unselectedTabClass;
this.$(unselectedTab).style.backgroundColor=this.darkgreen;
this.$(unselectedTab).style.border="1px solid #fff";
this.$(unselectedTab).style.borderLeft="1px solid #fff";
this.$(unselectedTab).style.borderRight="1px solid #fff";
