function socialNetwork(networkName){
	var netwindow,url,currentUrl,title,content;
	currentUrl=encodeURIComponent(document.location.href);
	title=encodeURIComponent(document.title);
	switch(networkName)
		{
			case 'twitter':
			  url='http://twitter.com/home?status=Looking at: ' + currentUrl;
				break;    
			case 'facebook':
			 url='http://www.facebook.com/sharer.php?u=' + currentUrl + '&t='  + title;
			 break;
			case 'myspace':
			 title=document.title;
			 content='<img src="http://media.dealeron.com/PHOTO/col320/sscusa/ford/edge/2012/5od-4%20se/dx.jpg" /><br><p>' + title + '</p>';
		   url= 'http://www.myspace.com/index.cfm?fuseaction=postto&t=' + title+ '&c=' + content + '&u=' + currentUrl;
		 	 break; 
		 	case 'del':
		 	  url= 'http://del.icio.us/post?url='+ currentUrl +'&title=' + title;
		 	  break; 
		 	case 'digg':
		 	  url= 'http://digg.com/submit?phase=2&url=' + currentUrl;
		 	  break; 
		 	case 'linkedin':
		 		url= 'http://www.linkedin.com/shareArticle?mini=true&url=' + currentUrl +'&title='+ title +'&source=' + currentUrl +'&summary=';
		 	  break; 		 	  
			}
 		netwindow=window.open(url,'sharer','toolbar=0,status=0,scrollbars=1,width=626,height=436');
}

