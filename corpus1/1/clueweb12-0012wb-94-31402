function getc(cname) {          // 4 times
    var dc = document.cookie;
    var prefix = cname + "=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1) {
        begin = dc.indexOf(prefix);
        if (begin != 0) return null;
    } else  {
        begin += 2;
    }
    var end = document.cookie.indexOf(";", begin);
    if (end == -1) end = dc.length;
    rtn = unescape(dc.substring(begin + prefix.length, end));
    nrtn = parseFloat(rtn)      // or NaN if not numeric
    return unescape(dc.substring(begin + prefix.length, end));
}
//  search coding:
var NS4 = (navigator.appName.indexOf("Netscape") >= 0 && parseFloat(navigator.appVersion) >= 4) ? 1 : 0;
var IE4 = (document.all);
var win = this;    // Page to search
var n   = 0;
// Set up keypress event capturing
if (NS4)
  document.captureEvents(Event.KEYPRESS);
document.onkeypress = getKey;

function findinpage(str) {
  var txt, i, found;
  if (str == "")
    return false;
// Find next occurence of the given string on the page, wrap around to the start of the page if necessary.
  if (NS4) {
// Look for match starting at the current point. If not found, rewind back to the first match.
    if (!win.find(str))
      while(win.find(str, false, true))
        n++;
    else
      n++;
    if (n == 0)
        alert("Sorry; we could not find "+str+".");
  }
//
  if (IE4) {
    txt = win.document.body.createTextRange();
// Find the nth match from the top of the page.
    for (i = 0; i <= n && (found = txt.findText(str)) != false; i++) {
      txt.moveStart("character", 1);
      txt.moveEnd("textedit");
    }
    if (found) {        // mark it and scroll it into view.
      txt.moveStart("character", -1);
      txt.findText(str);
      txt.select();
      txt.scrollIntoView();
      n++;
    }
// Otherwise, start over at the top of the page and find first match.
    else {
      if (n > 0) {
        n = 0;
        findinpage(str);
      }
      else
        alert("Sorry; we could not find "+str+".");
    }
  }
  return false;
}
function getKey(e) {
  var key;
// Check for the ENTER key.
  if ((IE4 && event.keyCode == 13) || (NS4 && e.which == 13)) {
    findinpage(document.forms["search"].string.value);
    return false;
  }
}
// -------------------
rtn = 0
nrtn = 0
function cntreq(songid)  {
    var expdate = new Date()
    expdate.setTime(expdate.getTime() + (1 * (24 * 60 * 60 * 1000)))
    var testcook = "testcook=lyrics; expires=" + expdate.toGMTString() + "; path=/;  domain=.bbhq.com"
    document.cookie = testcook;
    if (document.cookie.length == 0 && mcflg == 0)    {
        alert("Your Internet browser is not configured to accept cookies.  You must accept cookies in order to view lyrics ay BBHQ.\n\n(See our Internet Primer for more information on cookies.)")
        return
    }
//
    var cnt = 0
    if (getc("cklcnt"))   {      // is already there?
        cnt = nrtn
    }
    cnt = cnt + 1
    setc("cklcnt",cnt)
    if (getc("cklcnt"))     {
        document.lyrform.songid.value = songid
        document.lyrform.usercnt.value = cnt
        document.lyrform.submit()
    } else {    // did not write it
        alert("Your Internet browser is not configured to accept cookies.  You must accept cookies in order to view lyrics ay BBHQ.\n\n(See our Internet Primer for more information on cookies.)")
        return
    }
}
