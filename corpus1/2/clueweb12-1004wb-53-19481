        <!-- EXTERNAL JAVASCRIPT LIBRARIES -->
        <script type="text/javascript" src="http://www.wreathsacrossamerica.org/wp-content/themes/wreaths/js/jquery-ui-1.8.11.custom.min.js"></script>
        <script type="text/javascript" src="http://www.wreathsacrossamerica.org/wp-content/themes/wreaths/js/jquery.lavalamp-1.3.5-hacked.js"></script>
        <script type="text/javascript" src="http://www.wreathsacrossamerica.org/wp-content/themes/wreaths/js/jquery.lavalamp-1.3.5.min.js"></script>
        <script type="text/javascript" src="http://www.wreathsacrossamerica.org/wp-content/themes/wreaths/js/jquery.easing.1.3.js"></script>
        <script type="text/javascript">
			//$(document).ready(function(){
			jQuery(function($){
				//Activate our primary masthead navigation lava
				$('#primary-navigation-row .centered-content').lavaLamp({
					fx: 'linear',
					speed: 200,
					startItem: 1				});
				//Add some focus / unfocus action to our newsletter signup forms (located in both the homepage connect bar and the global site footer)
				$('.email-input').focusin(function(){
					if ($(this).val() == 'Enter E-Mail') {
						$(this).val('')
					}
				}).focusout(function(){
					if ($(this).val() == '') {
						$(this).val('Enter E-Mail')
					}
				});
					$('.newsletter-signup-form').submit(function(){
						var email = $(this).find('#Contact0Email').val()
						if (email == "Enter E-Mail" || email == "") {
							return false;
						}
					});
				//Select the share url when the user focuses on the input, for easier copying
				$('#share-url').focus(function(){ $(this).select() });
				$('#connect-bar-share-trigger').hover(function(){
					//Over
					//Position our share popup directly on the share button trigger
					var p = $(this).position()
					$(this).find('.share-popup').css('left', p.left - 170).css('top', p.top + 17).show();
				}, function() {
					//Out
					$(this).find('.share-popup').hide();
				});
				$('#footer-share-trigger').hover(function(){
					//Over
					//Position our share popup directly on the share button trigger
					var p = $(this).position()
					$(this).find('.share-popup').css('left', p.left - 170).css('top', p.top - 145).show();
				}, function() {
					//Out
					$(this).find('.share-popup').hide();
				});
				
			})
		</script>
		    </body>
</html>