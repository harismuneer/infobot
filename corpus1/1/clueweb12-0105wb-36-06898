  <script type="text/javascript">
      function clearEmptyFilterField() {
          checkEmptyField('fullTextSearchInput', '(eg. diesel, sunroof)');
      }

      function emptyFullTextSearchFilterField() {
          $('#fullTextSearchInput').val("").blur();
          fullTextSearchFilter.clearSearch();
          fullTextSearchFilter.handleChange();
      }

      $(document).ready(function() {
          initEmptyField('fullTextSearchInput', '(eg. diesel, sunroof)');
      });

      function submitFullTextSearchFilterQuery() {
          var query = $('#fullTextSearchInput').val();

          if (query == null || query == "" || query.trim() == "") {
              alert("Please enter one or more search terms to filter these listings.");
              $('#fullTextSearchInput').blur();
              return;
          }

          // show searching clue
          $('#fullTextSearchLoadingImage').show();
          $('#searchInputButtons').hide();

          var successCallback = function () {
              $('#fullTextSearchLoadingImage').hide();
              $('#searchInputButtons').show();
          };

          var errorCallback = function() {
              $('#fullTextSearchLoadingImage').hide();
              $('#searchInputButtons').show();

              alert("Sorry no matching results!");
          };

          fullTextSearchFilter.performSearch(query, successCallback, errorCallback, true);
      }
  </script>
  <div id="fullTextSearchFilterContainer" class="cg-dealFinder-fullTextSearchFilter-wrap">




        
<form id="criteriaForm" name="criteriaForm" onsubmit="clearEmptyFilterField();submitFullTextSearchFilterQuery();return false;" action="/Cars/l.action" method="post">          <input id="fullTextSearchInput" class="cg-dealFinder-fullTextSearchFilter-input" type="text" maxlength="200" value="(eg. diesel, sunroof)"></input>
          <div id="searchInputButtons" class="cg-dealFinder-fullTextSearchFilter-buttons">








<input type="image" alt="Apply" src="http://static.cargurus.com/gfx/icons/silk/zoom.png" id="fullTextSearchFilterSubmitButton" value="Apply"

 title="Enter search terms against which to filter these listings."/>
              <img onclick="emptyFullTextSearchFilterField();" src="http://static.cargurus.com/gfx/icons/silk/cross.png" title="Clear full text search filter." alt="Clear filter."></img>
          </div>
          <img id="fullTextSearchLoadingImage" src="http://static.cargurus.com/gfx/loading_F0F0F0.gif" style="width:16px;line-height:22px; vertical-align:middle; display:none;"></img>




</form>



  </div>
</div>
