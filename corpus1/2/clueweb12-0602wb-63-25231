 <script type="text/javascript"> 
 var srchtype;
            
$('search_string').observe('keydown',function()
{          
        if(document.getElementById('select_filter').value == 'lyrics')
        {
            //srchtype = 'lyrics';  
            new AutoComplete('search_string', '/search/lyrics/searchlyrics/?sty=lyrics&m=json&s=',{
            delay: .25,
            resultFormat: AutoComplete.Options.RESULT_FORMAT_JSON     
            });          
        }   
        else if(document.getElementById('select_filter').value == 'artist')
        {
            srchtype = 'artist';
            new AutoComplete('search_string', '/search/lyrics/searchlyrics/?sty=artist&m=json&s=',{
            delay: .25,
            resultFormat: AutoComplete.Options.RESULT_FORMAT_JSON     
            });            
        } 
        else if(document.getElementById('select_filter').value == 'albums')
        {
            srchtype = 'albums';
            new AutoComplete('search_string', '/search/lyrics/searchlyrics/?sty=albums&m=json&s=',{
            delay: .25,
            resultFormat: AutoComplete.Options.RESULT_FORMAT_JSON     
            });            
        }
        else if(document.getElementById('select_filter').value == 'lyrics_text')
        {
            srchtype = 'lyrics_text';
            new AutoComplete('search_string', '/search/lyrics/searchlyrics/?sty=lyrics_text&m=json&s=',{
            delay: .25,
            resultFormat: AutoComplete.Options.RESULT_FORMAT_JSON     
            });            
        }     
        
});
</script>         


<script>
function setlaguage(lang,url)
{  
    document.location.href = url+"?select_language="+lang;   
}

function checkstr()
{
	if($('search_string').value == "")
	{
		alert("Please enter keyword to search");
		return false;
	}
}
