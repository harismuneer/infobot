    $('zoomMedia').setStyle({display:'block',left:'455px'});
		movieName = "izmoColor";
  break;
  

default:
  alert("This option is not available.");
}

}
function OpenZoomWindow(){
  var screen_width = window.screen.availWidth;
  var screen_height = window.screen.availHeight;
  var left_point = parseInt(screen_width/2)-(490);
  var top_point = parseInt(screen_height/2)-(420);
  var mediaFile;
   
	  
	switch(galleryType)
		{
	case "video":
		mediaFile='<embed height="100%" width="100%" allowscriptaccess="always" wmode="transparent" quality="high" bgcolor="#FFFFFF" name="videospin" id="videospin" src="http://automotiontv.com/flash/automotion_player_lite.swf?id=4237&asset_id=&code=39651411017&playermode=get_display_data_lite&dn=http://automotiontv.com" type="application/x-shockwave-flash"/>';
		break;
		case "int":
			mediaFile='<embed height="100%" width="100%" allowscriptaccess="always" base="http://media.dealeron.com/photo/panos/SSCUSA/FORD/EDGE/2012/5OD-4 SE/" wmode="transparent" quality="high" bgcolor="#FFFFFF" name="intspin" id="intspin" src="http://media.dealeron.com/photo/panos/SSCUSA/FORD/EDGE/2012/5OD-4 SE/INT360.SWF" type="application/x-shockwave-flash"/>';
			break;    
		case "ext":
			mediaFile='<di<embed height="100%" width="100%" wmode="transparent" align="bottom" quality="high" bgcolor="#FFFFFF" name="extSpinIzmo" id="extSpinIzmo" src="http://media.dealeron.com/photo/flix/SSCUSA/Ford/Edge/2012/5OD-4 SE/360.swf" type="application/x-shockwave-flash"/></div>';
			break;
		case "color":
			mediaFile='<embed height="100%" width="100%" swliveconnect="true" wmode="transparent" quality="high" bgcolor="#FFFFFF" name="izmoColor" id="izmoColor" src="http://media.dealeron.com/photo/flix/SSCUSA/FORD/EDGE/2012/5OD-4 SE/COLORS.SWF" type="application/x-shockwave-flash"/></div>';
			break;
		default:
			alert("This option is not available.");
			mediaFile='';
		}  
  
  var generator=window.open('','name','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,copyhistory=no,height=420,width=980');
  generator.moveTo(left_point,top_point);
   
  generator.document.write('<html><head><title>2012 Ford Edge</title>');
  generator.document.write('</head><body>');
  generator.document.write(mediaFile);
  generator.document.write('</body></html>');
  generator.document.close();
 return; 
}

