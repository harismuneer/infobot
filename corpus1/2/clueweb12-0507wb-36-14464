					function scmobEncode(string) {
						return escape(string.replace(/\//g, "%2F"));
					}

					function scmobFacebookShare() {
						window.location = 'http://www.facebook.com/sharer.php?u=' + document.URL + '&t=' + scmobEncode(document.title);
						return false;
					}

					function scmobTwitterTweet() {
						window.location = 'http://twitter.com/?status=' + document.URL + ' -- ' + scmobEncode(document.title);
						return false;
					}

					function scmobLinkedInShare() {
						// See: http://developer.linkedin.com/docs/DOC-1075
						window.location = 'http://www.linkedin.com/shareArticle?mini=true&url=' + scmobEncode(document.URL) + '&title=' + scmobEncode(document.title);
						return false;
					}
