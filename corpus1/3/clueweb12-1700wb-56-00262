   <script type="text/javascript">
$(document).ready(function() {
    var height=$(document).height();
    //keeps track whether the flyout has been flown
    var flown=false;
    //keeps track once ope if flyout is open or closed
    var fly_is_open=false;
    
    //the flyout get loaded.  On page load check where we are.  If we are at the bottom of the page fly
    if($(document).scrollTop()>(height/4) && !flown){
        flyinOpen('#flyin-article',368);
        flown=true;
    }    

    //bind an event listener to the scroll event
    //check if view port is halfway down the page
    $(document).scroll(function(){
        if($(document).scrollTop()>(height/4) && !flown){
            flyinOpen('#flyin-article', 368);
            flown=true;
        }
        if($(document).scrollTop()<(height/4) && flown){
            flown=false;
            //if it is open, close it and remove the controlbar
            if(fly_is_open){
                flyinClose('#flyin-article',368);
                //after close remove the controlbar
                $('#flyin-article').width(360);
                $('#flyin-controlbar').hide();
            }
            //otherwise just remove the controlbar
            else{
                $("#flyin-article").animate({"right": "-=8px"}, "slow", function(){
                    $('#flyin-article').width(360);
                    $('#flyin-controlbar').hide();
                });
            }

        }
    });
    
    
    $("#flyin-close").click(function(){
        flyinClose('#flyin-article', 359);
        ads_click('FLY','close', '', '', '', '');
    });
    $("#flyin-open").click(function(){
        flyinOpen("#flyin-article", 359);
        ads_click('FLY','open', '', '', '', '');
    });
    

    
    function flyinClose(item, move){
        $('#flyin-controlbar').show();
        var flyin=$(item);
        flyin.width(380);
        $("#flyin-article").animate({"right": "-="+move+"px"}, "slow");
        fly_is_open=false;
    }
    function flyinOpen(item, move){
        var flyin=$(item);
        $("#flyin-article").animate({"right": "+="+move+"px"}, "slow", function(){
            flyin.width(360);
            $("#flyin-controlbar").hide();
        });        
        fly_is_open=true;
    }
});

</script>


