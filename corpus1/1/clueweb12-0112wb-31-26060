<script type="text/javascript">


jQuery(document).ready(function ($) {
	$('#video_icon').click(function () {
		$('#photo_icon').removeClass('darkBox').addClass('beigeBox');
		$('#video_icon').removeClass('beigeBox').addClass('darkBox');
		$('#gallery').hide();
		$('#video').show();		
		return false;
	});
	
	$('#photo_icon').click(function () {
		$('#photo_icon').removeClass('beigeBox').addClass('darkBox');
		$('#video_icon').removeClass('darkBox').addClass('beigeBox');
		$('#gallery').show();
		$('#video').hide();
		return false;
	});

	
});

jQuery.noConflict();
function startGallery() {
		var myGallery = new gallery($('gallery'), {
			timed: true,
			showArrows: true,
			showCarousel: true,
			delay: 5000,
			slideInfoZoneSlide: false,
			embedLinks: true,
			thumbIdleOpacity: 0.8,
			textShowCarousel: 'Image {0}/{1}'
		});
}
window.addEvent('domready', startGallery);


