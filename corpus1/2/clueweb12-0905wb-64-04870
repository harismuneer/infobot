<script type="text/javascript" charset="utf-8">
jQuery(document).ready(function(){
  // Sticky header 
   // http://jqueryfordesigners.com/fixed-floating-elements/

  

  
  
  

  
  $(function () {

    var msie6 = $.browser == 'msie' && $.browser.version < 7;
  
    if (!msie6) {
      var top = $('.product_header').offset().top - 38;
      $(window).scroll(function (event) {
        // what the y position of the scroll is
        var y = $(this).scrollTop();

        // whether that's below the form
        if (y >= top) {
          // if so, add the fixed class
          $('.product_header').css({
            'position' : 'fixed',
            'top' : '38px'
          });
        } else {
          // otherwise remove it
          $('.product_header').css({
            'position' : 'relative',
            'top' : '0'
          });
        }
      });
    }  
  });
  
  
  
  $(function () {

    var msie6 = $.browser == 'msie' && $.browser.version < 7;
  
    if (!msie6) {
      var topShop = $('#shop').offset().top; // - parseFloat($('#shop').css('margin-top').replace(/auto/, 0));
      var topHeader = $('#header').offset().top + 79;
      $(window).scroll(function (event) {
        // what the y position of the scroll is
        var y = $(this).scrollTop();

        // whether that's below the form
        if (y >= topShop) {
          // if so, add the fixed class
          $('#shop').css({
            'position' : 'fixed',
            'top' : '0'
          });
        } else {
          // otherwise remove it
          $('#shop').css({
            'position' : 'relative',
            'top' : '0'
          });
        }
        if (y >= topHeader) {
          // if so, add the fixed class
          $('#header').css({
            'margin-bottom' : '143px'
          });
        } else {
          // otherwise remove it

          $('#header').css({
            'margin-bottom' : '0'
          });
        }
      });
    }  
  });
  
   
});
</script>



