			// back to init state
			jQuery("#media_tab4_div .audio_item .player_holder").hide();
			jQuery("#media_tab4_div .audio_item .player_holder").html('');			
			jQuery("#media_tab4_div .audio_item .player_image").show();
			jQuery("#media_tab4_div .audio_item").removeClass('activeitem');
			
			sEmbed = urldecode($(this).parent().find('.player_embed').html());
			$(this).parent().find('.player_holder').html('<div class="player_buffer">'+sEmbed+'<div>');
			
			$(this).hide();
			
			$(this).parent().addClass("activeitem");				
		});
		
				
	}); //onready
		
	function loadBlogVideo(content) 
	{		
		jQuery("#pnlBlogVideo").html('<div>' + urldecode(content) + '</div>');
		return false;
	}

	function activate_tab(sDiv)
	{		
		// deactivate all tabs and div content
		jQuery("#divVideoLabelsInterior > div").removeClass('active');
		jQuery("#media_tabs_content > .media_tab ").css('display', 'none');		
		jQuery("#media_tab2_div > div").css('display', 'none');
		
		// activate the selected tab and div
		jQuery("#"+sDiv).addClass('active');
		jQuery("#"+sDiv+"_div").css('display', 'block');
		jQuery("#flickr_options").css('display', 'block');
		return false;
	}
	
	
	function activate_tab_flickr(sDiv)
	{
		var flickrUserID;
		var flickrUserPath;
		
		if (sDiv=='flickr_content1') {
			flickrUserID = '29456680@N06';
			flickrUserPath = 'isafmedia';
		} else {
			flickrUserID = '35102820@N06';
			flickrUserPath = 'ccdet';
		}
		
		var content = [];
		content.push('<object width="250" height="200">');
		content.push('  <param name="flashvars" value="offsite=true&lang=en-us&page_show_url=%2Fphotos%2F' + flickrUserPath + '%2Fshow%2F&page_show_back_url=%2Fphotos%2F' + flickrUserPath + '%2F&user_id=' + flickrUserID + '&jump_to="></param>');
		content.push('  <param name="movie" value="http://www.flickr.com/apps/slideshow/show.swf?v=71649"></param>');
		content.push('  <param name="allowFullScreen" value="true"></param>');
		content.push('  <embed type="application/x-shockwave-flash" src="http://www.flickr.com/apps/slideshow/show.swf?v=71649" allowFullScreen="true" flashvars="offsite=true&lang=en-us&page_show_url=%2Fphotos%2F' + flickrUserPath + '%2Fshow%2F&page_show_back_url=%2Fphotos%2F' + flickrUserPath + '%2F&user_id=' + flickrUserID + '&jump_to=" width="250" height="200"></embed>');
		content.push('</object>');

		jQuery("#flickr_options").css('display', 'none');		
		jQuery("#flickr_content").css('display', 'block');
		jQuery("#flickr_content_holder").html(content.join("\n")); 
	}
	
	function activate_audio(sDiv)
	{
		
	}
//-->
</script>


