myOnBeforeAnimation = function(carousel, first, i, prevFirst, prevLast, size) {
            jQuery('.jcarousel-item-' + i).hide();
            jQuery('.jcarousel-item-' + i).fadeIn('slow');

            var ind = (i - 3 < 0) ? i + 3 : i - 3;

            jQuery('.jcarousel-item-' +ind).fadeOut('slow', function() { jQuery('.jcarousel-item-' + ind).show(); });
			return;
};
   //]]>

function mycarousel_initCallback(carousel) {
	carousel.clip.hover(function() { carousel.stopAuto(); }, function() { carousel.startAuto(); });
};

jQuery(document).ready(function() {
	var open = false;
	$('.fot a').click(function() {
		if ($(this).hasClass('show')){
			$(this).removeClass('show');
			$(this).addClass('hide');}
		else if($(this).hasClass('hide')){
			$(this).removeClass('hide');
			$(this).addClass('show');}
		$(this).parents('li').children('.jtxt').children('.jtxt_cont').slideToggle();
	});
    if (open) { $('.jtxt_cont').show(); $('.fot a').removeClass('hide'); $('.fot a').addClass('show');  }
	jQuery('#mycarousel').jcarousel({ auto: 6, wrap: 'circular', animation: 'slow', scroll: 1, itemVisibleInCallback: { onBeforeAnimation: myOnBeforeAnimation }, initCallback: mycarousel_initCallback });

});

</script>

