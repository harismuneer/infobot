//<![CDATA[
    // This assumes jQuery and jQuery UI are both available.
    $(document).ready(function() {
        $("#edit_console").draggable({ distance: 10 });
        
        $("#edit_console").mouseup(function() {
            var pos = $("#edit_console").position();
            var top = parseInt(pos.top);
            var left = parseInt(pos.left);
            
            if (isNaN(edit_console_top)) edit_console_top = 25;
            if (isNaN(edit_console_left)) edit_console_left = 25;
            
            $.cookies.set("edit_console_top", pos.top);
            $.cookies.set("edit_console_left", pos.left);
        });
        
        var edit_console_top = $.cookies.get("edit_console_top") || 25;
        var edit_console_left = $.cookies.get("edit_console_left") || 25;
        
        $("#edit_console").css({ top: parseInt(edit_console_top), left: parseInt(edit_console_left) });
        $("#edit_console").css({ opacity: 0.5 });
        $("#edit_console").show();
        
        $("#edit_console").hover(function() {
            $(this).css({ opacity: 1.0 });
        }, function() {
            $(this).css({ opacity: 0.5 });
        });
    });
//]]>
</script>
        
