<script type="text/javascript" src="/outils/js?js=/js/jquery.ui.datepicker-fr.js,/js/jquery.qtip.min.js,/js/form.js,/js/placeholder.js,/js/tracking.js,/js/html5.js,/js/slider.js,/js/nav.js,/js/jquery.colorbox-min.js,/js/searchBox.js,/js/jquery.ui.selectmenu.js,/js/ui.dropdownchecklist-1.4-min.js&expires=1331750439624"></script>
<script type="text/javascript">
    $(document).ready(function(){
        $.fn.qtip.zindex = 9999999;

        $.trSm360({context: ''});
        $.trSm360("trackCurrentPage");

        $.ajaxSetup({cache: false});

        $("#search-field").searchBox({location: '/en/search.html'});
        $("#search-btn").live("click", function() {
            $("#search-field").searchBox("search");
        });

        $("#search-field").placeholder({placeholder: 'Search...'});
        $("#contact input, #contact textarea").placeholder();

        resizePromo();
        $(window).resize(function() {
            resizePromo();
        });

        var timeoutId;
        $("#nav-dealership").mouseenter(function(){
            clearTimeout(timeoutId);
            $("#main-nav-dealership").slideDown(500, 'linear');
        });

        $("#nav-dealership").mouseleave(function(){
            timeoutId = setTimeout(function() { $("#main-nav-dealership").slideUp(500, 'linear'); }, 500);
        });

        $("#main-nav-dealership").mouseleave(function(){
            timeoutId = setTimeout(function() { $("#main-nav-dealership").slideUp(500, 'linear'); }, 500);
        });

        $("#main-nav-dealership").mouseenter(function(){
            clearTimeout(timeoutId);
        });
    });

    function resizePromo(){
        if($(this).width() > 1400){
            $('#header-slider').css("width", "1400px");
            $('#header-slider').css("left", "-200px");
            $('#header-slider-wrapper').css("left", "0px");
        } else if($(this).width() <= 990){
            $('#header-slider').css("width", "960px");
            $('#header-slider').css("left", "0px");
            $('#header-slider-wrapper').css("left", "-200px");
        } else {
            $('#header-slider').css("left", ((990-$(this).width())/2) + "px");
            $('#header-slider').css("width", ($(this).width()-1) +"px");
            $('#header-slider-wrapper').css("left", -200 - ((990-$(this).width())/2 ) + "px");
        }
    }
    function equalHeight(selector) {
       var tallest = 0;
       selector.each(function() {
          var thisHeight = $(this).height();
          if(thisHeight > tallest) {
             tallest = thisHeight;
          }
       });
       selector.height(tallest);
    }
</script>


