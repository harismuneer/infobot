<script type="text/javascript">

    function IsEmailValid(email) {
        return /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i.test(email);
    }

    function validateForm(buttonID) {
        $LS('#' + buttonID).parent().find(".fromEmailError").removeClass("show").addClass("hide");
        $LS('#' + buttonID).parent().find(".toEmailError").removeClass("show").addClass("hide");

        var fromValid = IsEmailValid($LS('#' + buttonID).parent().find(".tbxFromEmail").val());
        var toValid = IsEmailValid($LS('#' + buttonID).parent().find(".tbxToEmail").val());
        if (!fromValid)
            $LS('#' + buttonID).parent().find(".fromEmailError").removeClass("hide").addClass("show");
        if (!toValid)
            $LS('#' + buttonID).parent().find(".toEmailError").removeClass("hide").addClass("show");
        if (!fromValid || !toValid) {
            return false;
        } else {
            TrackArticlePageViewSendEmail(); 
            return true;
        }
    }

    function ShowEmailPage() {
        $LS("#emailSection").removeClass("hide").addClass("show");
    }

    function ShowEmailPageSocial(uniqueClass) {
        $LS(".emailSection" + uniqueClass).removeClass("hide").addClass("show");
    }

    function CancelSendEmail() {
        $LS("#emailSection").removeClass("show").addClass("hide");
        //for ConversationV2
        $LS(".conversationComments").find("div#emailSection").each(function () {
            $LS(this).removeClass("show").addClass("hide");
        });
        //window.scrollTo(0, 0);
    }

    function ClearFromToEmail(id) {
        if ($LS("#" + id).val() == "email address") {
            $LS("#" + id).val("");
            $LS("#" + id).css("font-style", "normal");
            $LS("#" + id).css("color", "#333");
        }
        else if ($LS.trim($LS("#" + id).val()) == "") {
            $LS("#" + id).val("email address");
            $LS("#" + id).css("font-style", "italic");
            $LS("#" + id).css("color", "#bbbbbb");
        }
    }
</script>
