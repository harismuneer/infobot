  <param name="playerKey" value="AQ~~,AAAAACpvMpk~,rAvHhAS7JOpa4tlt0CXVebDvGzQCdYY2" />
  <param name="isVid" value="true" />
  <param name="isUI" value="true" />
  <param name="dynamicStreaming" value="true" />
  
  <param name="@videoPlayer" value="1276897685001" />
