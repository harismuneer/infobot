    <script type="text/javascript" src="http://static.websimages.com/static/global/js/jquery/plugins/urlparser/jquery.url.packed.js"></script>
	<script type="text/javascript" src="http://static.websimages.com/Platform/JS/Ajax.js"></script>
	<link href="http://static.websimages.com/static/projects/comments/fwcomments.css" rel="stylesheet" media="screen">
    <script type="text/javascript" src="http://static.websimages.com/static/projects/comments/fwcomments.js"></script>

    <style>
        #commentForm form textarea, .commentBody form textarea {
            width: 100%;
        }
    </style>

	<script>var commentTooLongWarning = 'Your comment is too long!!!';</script>
	<script>var flagConfirm = 'Are you sure you want to report this comment as spam?';</script>
	<script>var resetConfirm = 'Are you sure you want to reset the flags for this comment?';</script>
	<script>var deleteConfirm = 'Are you sure you want to delete this comment?';</script>


    <script type="text/javascript">

		function flagComment(id) {
			if (confirm(flagConfirm)) {
				jQuery('#flagCommentIdInput').val(id);
				jQuery('#flagCommentForm').submit();
			}
			return false;
		}
		function resetFlagCount(id) {
			if (confirm(resetConfirm)) {
				jQuery('#resetFlagCountIdInput').val(id);
				jQuery('#resetFlagCountForm').submit();
			}
			return false;
		}

		function deleteComment(id) {
			if (confirm(deleteConfirm)) {
				jQuery('#deleteCommentIdInput').val(id);
				jQuery('#deleteCommentForm').submit();
			}
			return false;
		}

        function fillErrorFields(){
            setCommenterName();
            setCommenterEmail();
            setCommentBody();
            return false;
        }


        function setCommenterName() {

            var commenterName = jQuery.url.param("fw_comments_commenterName");

            if (commenterName) {
                commenterName = decodeURIComponent(commenterName);
                jQuery("#commenterName").val(commenterName);
            }
            return false;
        }


        function setCommenterEmail() {

            var commenterEmail = jQuery.url.param("fw_comments_commenterEmail");

            if (commenterEmail) {
                commenterEmail = decodeURIComponent(commenterEmail);
                jQuery("#commenterEmail").val(commenterEmail);
            }
            return false;
        }


        function setCommentBody() {

            var commentBody = jQuery.url.param("fw_comments_body");

            if (commentBody) {
                commentBody = decodeURIComponent(commentBody);
                jQuery("#commentBody").val(commentBody);
            }
            return false;
        }

        function commentsReturnURLPopulate() {
            var cleanHref = window.location.href;
            cleanHref = cleanHref.replace(/[&]?fw_comments.*?=[^&]*/img, "");

            jQuery('.returnUrlInput').each(function () {
                if (jQuery(this).val() == '') {
                    jQuery(this).val(cleanHref);
                }
            });
            return false;
        }

		function setupPaging() {
            jQuery("#commentPagination a").each(
                function () {
                    var page = jQuery(this).attr("title");
                    jQuery(this).click(function () {
                        jQuery('.commentListLoadingOverlay').fadeIn();
                        getCommentPage(page);
                        return false;
                    });
                }
            );
            return false;
		}

		function getCommentPage(page) {
			var params = {
				"fw_comments_page": page,
                "fw_comments_per_page": "20",
                "fw_comments_order": "ASC",
				"xid": "entry_comments_5693915",
				"namespace": "default",
				"linkbackURL": "http://servicemanuals.webs.com/apps/blog/show/5693915-ford-mondeo-manual/",
				"thumbnailURL": "",
				"returnURL": "",
				"appId": "525",
                "richText": "false",
                "allowAnonymous": "true"
			};

            var ajax = new fw.Ajax();
            ajax.responseType = Ajax.FWML;
            ajax.ondone = function(data) {
                jQuery("#commentList").html(data);
                commentsReturnURLPopulate();
                setupPaging();
                return false;
            };

            ajax.post("http://members.webs.com/s/app/comments/list", params);
            return false;
		}


        function validateComment(theBody){
            if(theBody.length > 5000){
                alert(commentTooLongWarning);
                return false;
            }

	    return true;
        }

	function publishComment(theBody) {
		// hack the FB object because it doesn't update the session
		FB.Connect._singleton._userInfo.shortStorySetting = FB.FeedStorySetting.autoaccept;
		FB.Connect.streamPublish(theBody, {'name': '- ' + document.title, 'href': window.location.href}, [{'href': window.location.href, 'text': 'Visit Site'}], null, null, function(post_id, ex) { jQuery("#comment_form").submit() }, true, null);
	}

	function onSubmit(commentId) {
		var theBody = (typeof punymce != "undefined") ? eval(commentId).getContent({save : true}) : document.getElementById(commentId).value;

		if (!validateComment(theBody))
			return false;

		if (jQuery("#facebookPublish").attr('checked')) {
			FB.Facebook.apiClient.users_hasAppPermission("publish_stream", function(p) {
				if (p == "1") {
					publishComment(theBody);
				} else {
					FB.Connect.showPermissionDialog("publish_stream", function(perms) {
						if (perms != "")
							publishComment(theBody);
						else
							jQuery("#comment_form").submit();

					});
				}
			});

			return false;
		}

		return true;
	}


        jQuery(document).ready(function() {

            var validator = jQuery.url.param("fw_comments_validator");

            if (validator) {
                var nameError = validator.indexOf('name') > -1;
                var emailError = validator.indexOf('email') > -1;
				var bodyError = validator.indexOf('body') > -1;
				var captchaError = validator.indexOf('captcha') > -1;

                if (nameError || emailError || bodyError) {
                    jQuery(".commentErrorBox").show();
                } else if (captchaError) {
					jQuery(".captchaErrorBox").show();
				}

                if (nameError && emailError && bodyError) {
                    showError("commentForm");
                }
                else {
					if (captchaError) {
						showError("captcha");
					}
                    if (nameError) {
                        showError("commenterName");
                    }
                    if (emailError) {
                        showError("commenterEmail");
                    }
                    if (bodyError) {
                            showError("commentBody");
                    }
                }
                fillErrorFields();
            }

            commentsReturnURLPopulate();
            setupPaging();
        });

    </script>
