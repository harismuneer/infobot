<div style="width: 336px; height: 280px; float: right; margin-left: 5px; margin-bottom: 5px;">
<script type="text/javascript"><!--
google_ad_client = "pub-3431743524996604";
/* Agonist Content Right 336x280 */
google_ad_slot = "5086360025";
