            <ul class="panel-controller">
                <li class="next-previous"><a class="btn_arrow_left"></a></li>
                                <li><a class="dot dot-active panel-target-1" title="1"></a></li>
                                <li><a class="dot panel-target-2" title="2"></a></li>
                                <li><a class="dot panel-target-3" title="3"></a></li>
                                <li><a class="dot panel-target-4" title="4"></a></li>
                                <li><a class="dot panel-target-5" title="5"></a></li>
                                <li class="next-previous"><a class="btn_arrow_right"></a></li>
            </ul>
            
            <script>
			jQuery(document).ready(function() {
				
				window.panel_current_slide = 0;
				
				jQuery("ul.panel-controller li a").click(function(event) {
					event.preventDefault();
					
					if (jQuery(this).hasClass("btn_arrow_left")) {
						if (window.panel_current_slide > 0) {
							window.panel_current_slide--;
							var slide = window.panel_current_slide;
						} else {
							var slide = window.panel_current_slide;	
						}
					} else if (jQuery(this).hasClass("btn_arrow_right")) {
						if (window.panel_current_slide < 4) {
							window.panel_current_slide++;
							var slide = window.panel_current_slide;
						} else {
							var slide = window.panel_current_slide;	
						}
					} else {
						var slide = parseInt(jQuery(this).attr("title")) - 1;
						window.panel_current_slide = slide;
					}
					
					var width = 294;
					var offset = "-" + (slide*width) + "px";
					
					jQuery(this).parent().parent().parent().children(".recent-photo-galleries").children(".recent-photo-galleries-showcase").animate({
						marginLeft: offset
					}, {
						duration: 400, 
						easing: 'easeInOutCubic'
					});
					
					jQuery("ul.panel-controller li a").removeClass("dot-active");
					jQuery(".panel-target-" + (window.panel_current_slide+1)).addClass("dot-active");
				});
			});
			</script>
