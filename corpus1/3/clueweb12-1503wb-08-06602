var HTTP = {
  // {{{ parse_http_params(params)

  parse_params: function(params) {
    var tmp = params.split(';');
    if(tmp) {
      var pieces = new Object;
      for(var i = 0; i < tmp.length; i++) {
        var c = tmp[i].split("=");
        if(c[0].charAt(0) == " ") {
          c[0] = c[0].substring(1);
        }
        if(c[1] && c[1].charAt(1) == " ") {
            c[1] = c[1].substring(1);
        }
        try {
          eval("pieces[\"" + c[0] + "\"] = \"" + c[1] + "\"");
        } catch(e) {
          // do nothing
        }
      }
      return pieces;
    } else {
      return undefined;
    }
    return undefined;
  }

// }}}
};

var cookies = HTTP.parse_params(document.cookie);
var check = new String(document.location);
if(check.substr(check.length - 5) == "_show" && cookies.keyword) {
  document.write("<div class=\"not_what_you_are_looking_for\"><a id=\"nwylf\" href=\"/cc-common/RadioKeywordSystemSearch/search3.php?show_results&keyword=" + cookies.keyword + "\">This is a special promotions page. If you'd like Google web search results for <span class=\"not_what_you_are_looking_for_keyword\">" + cookies.keyword.replace(/[+]/g,' ') + "</span>, click here.</a> </div>");
