<script type="text/javaScript">
$(document).ready(function() {

    var y;
    var pos;
    var bottom;
    var banner = $('#ad_skyArtist');


    if (banner.length != 0) {
        pos = banner.offset().top - 75;        
        bottom = $('#footer').offset().top - 600;        
    }

    $(window).scroll(function () {
        
        // if banner doesn't exist than return
        if (banner.length == 0) return;

        y = getScrollXY()[1];
        //alert(y + " " + pos);

        if (y < pos) {
            banner.removeClass('fixed_skyArtist');
            banner.removeAttr('style');

        } else if ((y > pos) && (y < bottom)) {
            banner.addClass('fixed_skyArtist');
            banner.removeAttr('style');

        } else if (y > bottom) {
            banner.removeClass('fixed_skyArtist');
            banner.attr('style', 'position: relative;top: ' + (bottom - pos - 75) + 'px');
        }

        //console.log(banner.offset().top);
    })

    function getScrollXY() {
        var scrOfX = 0, scrOfY = 0;
        if( typeof( window.pageYOffset ) == 'number' ) {
        //Netscape compliant
        scrOfY = window.pageYOffset;
        scrOfX = window.pageXOffset;
        } else if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
        //DOM compliant
        scrOfY = document.body.scrollTop;
        scrOfX = document.body.scrollLeft;
        } else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
        //IE6 standards compliant mode
        scrOfY = document.documentElement.scrollTop;
        scrOfX = document.documentElement.scrollLeft;
        }
        return [ scrOfX, scrOfY ];
    }

})
</script>               </div>

