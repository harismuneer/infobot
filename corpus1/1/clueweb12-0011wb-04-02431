	function sharing_plusone( obj ) { 
		jQuery.ajax( {
			url: 'http://smartpeopleiknow.wordpress.com/2011/02/03/the-nicolas-cage-movie-matrix/?share=google-plus-1',
			type: 'POST',
			data: obj
		} );
	}
	</script>
	<script type="text/javascript" src="http://apis.google.com/js/plusone.js"></script>
		<script type="text/javascript" charset="utf-8">
		jQuery(document).ready(function(){
			jQuery( '.share-linkedin' ).click(function(){
				window.open( jQuery(this).attr( 'href' ), 'wpcom-linkedin', 'menubar=1,resizable=1,width=580,height=450' );
				return false;
			});
		});
		</script>
