					function showTab2(tabsid,cls,ths,tmpvar,spanid){
					try
					{
					
/* tabsid will contain all tabids with # seperation and class contain on off class with # seperation*/
var allTabsid = tabsid.split("#");
var classname = cls.split("#");
var allSpansid = spanid.split("#");
for(tid = 0; tid < allTabsid.length; tid++){
	if(ths == allTabsid[tid]){	
		getObj(allTabsid[tid]).className=''+classname[0];
		getObj(allTabsid[tid] + 'cnt').style.display='inline-block';
		for(sid = 0; sid < allTabsid.length; sid++){
			if(ths == allTabsid[sid]){		
				for(sid = 0; sid < allSpansid.length; sid++){
					if(ths == allTabsid[sid]){getObj(allSpansid[sid]).style.display='block';}
					else{getObj(allSpansid[sid]).style.display='none';}
				}
			}
			else {for(sid = 0; sid < allSpansid.length; sid++){
					if(ths != allTabsid[sid]){getObj(allSpansid[sid]).style.display='none';}
					else{getObj(allSpansid[sid]).style.display='block';}
				}
			}
		}
	}else{
		getObj(allTabsid[tid]).className=''+classname[1];
		getObj(allTabsid[tid] + 'cnt').style.display='none';
		for(sid = 0; sid < allTabsid.length; sid++){
			if(ths == allTabsid[sid]){
				for(sid = 0; sid < allSpansid.length; sid++){
					if(ths == allTabsid[sid]){getObj(allSpansid[sid]).style.display='block';}
					else{getObj(allSpansid[sid]).style.display='none';}
				}
			}
			else {for(sid = 0; sid < allSpansid.length; sid++){
					if(ths != allTabsid[sid]){getObj(allSpansid[sid]).style.display='none';}
					else
					{getObj(allSpansid[sid]).style.display='block';}
				}
			}
		}
	}
}
return false;
}
catch(ex){}
}

