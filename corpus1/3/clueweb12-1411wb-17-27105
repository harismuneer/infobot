chk=1;
document.getElementById('span_model2').className='missrow';
}else{
document.getElementById('span_model2').className='dropdowntext';

}
if (document.doublecombo2.contact_zipcode.value.length < 5){

chk=1;
document.getElementById('div_zip2').className='missrow';
}else{
document.getElementById('div_zip2').className='dropdowntext';

}

if(chk == 1){
return;
}else{
document.doublecombo2.submit();
}}
