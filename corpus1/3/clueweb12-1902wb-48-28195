var HTTP = {
  // {{{ parse_http_params(params)

  parse_params: function(params) {
    var tmp = params.split(';');
    if(tmp) {
      var pieces = new Object;
      for(var i = 0; i < tmp.length; i++) {
        var c = tmp[i].split("=");
        if(c[0].charAt(0) == " ") {
          c[0] = c[0].substring(1);
        }
        if(c[1] && c[1].charAt(1) == " ") {
            c[1] = c[1].substring(1);
        }
        try {
          eval("pieces[\"" + c[0] + "\"] = \"" + c[1] + "\"");
        } catch(e) {
          // do nothing
        }
      }
      return pieces;
    } else {
      return undefined;
    }
    return undefined;
  }

// }}}
};

var cookies = HTTP.parse_params(document.cookie);
