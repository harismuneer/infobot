<script>
function reload_comment_stream() { 
	window.location.href = 'http://www.uproxx.com/webculture/2012/02/100-greatest-nicolas-cage-movie-lines/#comments';
	// window.location.href = unescape(uproxxsso_back);
	window.location.reload(true);
}

var uproxxsso_num = 2;
var uproxxsso_original_num = uproxxsso_num;

function uproxxsso_update_num_comments(new_num){
	if(new_num > uproxxsso_num){
		var dif = new_num - uproxxsso_original_num;
		var text = "";
		if(dif == 1){
			text = "One new comment has been made. Click here to reload.";
		}else {
			text = dif + " new comments have been made. Click to reload.";
		}
		$("#uproxxsso_num_comments").html(text).slideDown().delay(35000).slideUp("slow");
		$("#num_comments").text(new_num);
		uproxxsso_num = new_num;
	}
	uproxxsso_request_comments_num();
}

function uproxxsso_request_comments_num(){
	setTimeout(uproxxsso_load_comments_num, 20000);
}
function uproxxsso_load_comments_num(){
	jQuery.getScript("http://profile.uproxx.com/api/num_comments.php?post_id=56619&blog_id=10&rand="+Math.random());
}
</script>
<script>
$(document).ready(uproxxsso_load_comments_num);
</script>

