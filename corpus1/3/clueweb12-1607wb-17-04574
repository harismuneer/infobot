    <script type="text/javascript">
			$(document).ready(function(){
    		var adapter = new Object(),
					event = "",
					webPageName = 'NewInventorySearch';
				switch (webPageName.toLowerCase()) {
					case "newinventorysearch":
					case "usedinventorysearch":
						adapter = new CM.SisterInventoryPage();
						event = "updatePage_complete"
						break;
					case "vehicle":
						adapter = new CM.SisterVehiclePage();
						break;
					case "mobile":
						var vehicleId = 0;
						adapter = new CM.SisterMobileVehiclePage(vehicleId);
						break;
				}
				var options = {
						adapter: adapter,
						event: event,
						webPageName: webPageName
					},
					sisterIntegration = new CM.SisterIntegration(options);
				$.event.trigger("Sister_Data_Loaded", sisterIntegration);
      });
		</script>
    <script type="text/javascript">
				(function() {
					var publishedEvent = NextGenIntegration().events.incentivePublished;

					CM.subscribe("Pages/InventorySearch/ViewBuildComplete", function() {
						
						setTimeout(function(){
							var ngInventorySearch = NextGenInventorySearch();

							ngInventorySearch.init();
							CM.subscribe(publishedEvent, function(incentives) {
								ngInventorySearch.applyIncentivesToDOM(incentives);
							});
						}, 1000);

					});

				}());
		</script>
    <script type="text/x-mustache-template" id="incentive-list-template">
      <div class="incentive-container">
        <ul>
					{{#incentives}}
					<li class="incentive-list-item"><div class="incentive-details"><div class="cash-details"><h5>{{Details.Name}}<sup>{{index}}{{#hasAPRIncentive}}a{{/hasAPRIncentive}}</sup></h5><p>${{Details.Amount}}</p></div>
							{{#hasAPRIncentive}}
