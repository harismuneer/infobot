	        function MainTemplate_ctl02DoSearch()
	        {
		        if(typeof encodeURIComponent != "function")
		        {
			        alert('Your web browser does not support this function. Please upgrade to IE 5.5 or above, Netscape 6 or above, or the latest version of your current browser.');
			        return;
		        }
        		
		        var qsearch = document.getElementById('MainTemplate_ctl02QuickSearch');
		        if(qsearch.value == '')
		        {
			        alert('You must enter a search phrase.');
			        return;
		        }
		        document.location.href = '/Search/SearchResultsGrouped.pg?SearchPhrase=' + encodeURIComponent(qsearch.value) + '&SearchType=text&SearchOperator=And';
	        }

	        function MainTemplate_ctl02KeyCheck(e)
	        {
		        if(window.event && e.keyCode == 13)
		        {
			        MainTemplate_ctl02DoSearch();
			        return false;
		        }
		        else if(e.which && e.which == 13)
		        {
			        MainTemplate_ctl02DoSearch();
			        return false;
		        }
		        else			
			        return true;
	        }
        </script>
    

