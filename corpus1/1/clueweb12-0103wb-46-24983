<script type="text/javascript">
$(document).ready(function() {
	$("#openCloseIdentifier_ea").hide();
	$("#openCloseIdentifier_uga").hide();
});
$(document).ready(function() {
	$("#quick_links_button").click( function() {
		closeea();
        if ($("#openCloseIdentifier_uga").is(":hidden")) {
								
			$("#uga_sliderContent").show(0,	function(){
				$("#uga_brand_bar").animate({ 
					marginTop: "151px"
					}, 500 ,function() {
						$("#openCloseIdentifier_uga").show();
					});								
				});				
        } else {
					$("#uga_brand_bar").animate({ 
						marginTop: "0px"
						}, 400);			
					$("#openCloseIdentifier_uga").hide(0,closeea());					

        }		
    }); 
});
$(document).ready(function() {
	$("#ea_brand_bar").click( function() {
		closeuga();
        if ($("#openCloseIdentifier_ea").is(":hidden")) {
									
			$("#ea_sliderContent").show(0,	function(){
				$("#ea_brand_bar").animate({ 
					marginTop: "161px"
					}, 500 ,function() {
						$("#openCloseIdentifier_ea").show();
					});								
				});				
        } else {
					$("#ea_brand_bar").animate({ 
						marginTop: "0px"
						}, 400 );			
					$("#openCloseIdentifier_ea").hide(0,closeea());					

        }		
    }); 
});
function closeuga(){
	if ($("#openCloseIdentifier_uga").is(":hidden")!=true) {
		$("#uga_brand_bar").animate({ 
			marginTop: "0px"
			}, 400, function(){
				$("#uga_sliderContent").hide();
				$("#openCloseIdentifier_uga").hide();
			}
			);
	}
}
function closeea(){	
	if ($("#openCloseIdentifier_ea").is(":hidden")!=true) {
		$("#ea_brand_bar").animate({ 
			marginTop: "0px"
			}, 400, function(){
				$("#ea_sliderContent").hide();
				$("#openCloseIdentifier_ea").hide();
			}
			);
	}		
}
</script>
