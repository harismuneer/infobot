function toggle_hotel_rooms(hotel_id)
{
  var e = _ge('hotel_rooms_'+hotel_id);
  var a = _ge('hotel_btn_'+hotel_id);
  if (e && a)
  {
    a.value = e.style.display != 'block' ? 'Hide rooms' : 'Show rooms';
    e.style.display = e.style.display != 'block' ? 'block' : 'none';
  }
}

function make_order(hotel_id)
{
  var total_beds = 0;
  var party_size = getQueryVariable('party_size');
  party_size = party_size ? parseInt(party_size) : 1;
  if (!party_size) return;
  var duration = getQueryVariable('duration');
 duration = duration ? parseInt(duration) : 5;
  if (!duration) return;
  var flexibility = getQueryVariable('flexibility');
  flexibility = flexibility ? parseInt(flexibility) : 0;
  if (!d1_ar[hotel_id]) d1_ar[hotel_id] = 0;
  //if (!d1_ar[hotel_id]) return;
  var top_page_search = getQueryVariable('top_page_search');
  if (!top_page_search) top_page_search = '';
  var cost = 0;
  var q = 'target_id=';
  if (rooms_ar[hotel_id])
  { //rooms
  for (var i in rooms_ar[hotel_id])
  {
  if (i == "in_array") continue;
  var _id = rooms_ar[hotel_id][i].id;
  var _beds = rooms_ar[hotel_id][i].beds;
  var _price = rooms_ar[hotel_id][i].price;
  var _e = _ge('room_cb_'+_id);
  if (_e && _e.checked)
  {
  cost += _price;
  total_beds += _beds;
         q += _id+',';
      }
    }
    if (total_beds < party_size)
    {
      alert('You have chosen rooms with '+total_beds+' beds summary, but your party size is '+party_size+'!');
      return;
   }
    var book_type = 'room';
  }
  else
  { //whole hotel
    q += hotel_id;
    var book_type = 'hotel';
 }
  if (q.substr(q.length - 1, 1) == ',') q = q.substr(0, q.length - 1);
  q += '&d1='+d1_ar[hotel_id]+'&duration='+duration+'&party_size='+party_size+'&flexibility='+flexibility;
 if (top_page_search) q += '&top_page_search='+top_page_search;
  window.location.href = '/'+book_path+'/?type='+book_type+'&'+q;
}
    </script>
